+				// We need to precompute the watches first then open a watch in the cache.
+				sw.watches.addWatch(typeURL, &watch{
+					cancel:   s.cache.CreateWatch(req, sw.streamState, responder),
+					response: responder,
+				})
+			}
+		}
+	}
