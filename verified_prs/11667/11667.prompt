117: 	auto &child_type = child->return_type;
118: 	unnest_level--;
119: 
120: 	if (unnest_level > 0) {
121: 		throw BinderException(
122: 		    function,
123: 		    "Nested UNNEST calls are not supported - use UNNEST(x, recursive := true) to unnest multiple levels");
124: 	}
125: 
126: 	switch (child_type.id()) {
127: 	case LogicalTypeId::UNKNOWN:
128: 		throw ParameterNotResolvedException();
129: 	case LogicalTypeId::LIST:
130: 	case LogicalTypeId::STRUCT:
131: 	case LogicalTypeId::SQLNULL:
132: 		break;
