65: 			if (has_parameter) {
66: 				return BindResult(BinderException(function, "UNNEST() only supports a single additional argument"));
67: 			}
68: 			if (function.children[i]->HasParameter()) {
69: 				throw ParameterNotAllowedException("Parameter not allowed in unnest parameter");
70: 			}
71: 			if (!function.children[i]->IsScalar()) {
72: 				break;
73: 			}
74: 			auto alias = function.children[i]->alias;
75: 			BindChild(function.children[i], depth, error);
76: 			if (error.HasError()) {
77: 				return BindResult(std::move(error));
78: 			}
79: 			auto &const_child = BoundExpression::GetExpression(*function.children[i]);
80: 			auto value = ExpressionExecutor::EvaluateScalar(context, *const_child, true);
81: 			if (alias == "recursive") {
82: 				auto recursive = value.GetValue<bool>();
83: 				if (recursive) {
84: 					max_depth = NumericLimits<idx_t>::Maximum();
85: 				}
86: 			} else if (alias == "max_depth") {
87: 				max_depth = value.GetValue<uint32_t>();
88: 				if (max_depth == 0) {
89: 					throw BinderException("UNNEST cannot have a max depth of 0");
90: 				}
91: 			} else if (!alias.empty()) {
92: 				throw BinderException("Unsupported parameter \"%s\" for unnest", alias);
93: 			} else {
94: 				break;
95: 			}
96: 			has_parameter = true;
97: 			supported_argument = true;
98: 		}
99: 		if (!supported_argument) {
