46: BindResult SelectBinder::BindUnnest(FunctionExpression &function, idx_t depth, bool root_expression) {
47: 	// bind the children of the function expression
48: 	if (depth > 0) {
49: 		return BindResult(BinderException(function, "UNNEST() for correlated expressions is not supported yet"));
50: 	}
51: 
52: 	ErrorData error;
53: 	if (function.children.empty()) {
54: 		return BindResult(BinderException(function, "UNNEST() requires a single argument"));
55: 	}
56: 	if (inside_window) {
