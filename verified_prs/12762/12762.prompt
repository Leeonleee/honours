870: template <unsigned BASE_BITS, typename Char, typename UInt>
871: inline Char* format_uint(Char* buffer, UInt value, int num_digits,
872:                          bool upper = false) {
873:   buffer += num_digits;
874:   Char* end = buffer;
875:   do {
876:     const char* digits = upper ? "0123456789ABCDEF" : data::hex_digits;
877:     unsigned digit = (value & ((1 << BASE_BITS) - 1));
878:     *--buffer = static_cast<Char>(BASE_BITS < 4 ? static_cast<char>('0' + digit)
879:                                                 : digits[digit]);
880:   } while ((value >>= BASE_BITS) != 0);
881:   return end;
882: }
883: 
884: template <unsigned BASE_BITS, typename Char>
885: Char* format_uint(Char* buffer, internal::fallback_uintptr n, int num_digits,
886:                   bool = false) {
887:   auto char_digits = std::numeric_limits<unsigned char>::digits / 4;
888:   int start = (num_digits + char_digits - 1) / char_digits - 1;
889:   if (int start_digits = num_digits % char_digits) {
890:     unsigned value = n.value[start--];
891:     buffer = format_uint<BASE_BITS>(buffer, value, start_digits);
892:   }
893:   for (; start >= 0; --start) {
894:     unsigned value = n.value[start];
895:     buffer += char_digits;
896:     auto p = buffer;
897:     for (int i = 0; i < char_digits; ++i) {
898:       unsigned digit = (value & ((1 << BASE_BITS) - 1));
899:       *--p = static_cast<Char>(data::hex_digits[digit]);
900:       value >>= BASE_BITS;
901:     }
902:   }
903:   return buffer;
904: }
905: 
906: template <unsigned BASE_BITS, typename Char, typename It, typename UInt>
907: inline It format_uint(It out, UInt value, int num_digits, bool upper = false) {
908:   // Buffer should be large enough to hold all digits (digits / BASE_BITS + 1).
909:   char buffer[num_bits<UInt>() / BASE_BITS + 1];
910:   format_uint<BASE_BITS>(buffer, value, num_digits, upper);
911:   return internal::copy_str<Char>(buffer, buffer + num_digits, out);
912: }
