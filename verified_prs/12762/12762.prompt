817: // Formats a decimal unsigned integer value writing into buffer.
818: // add_thousands_sep is called after writing each char to add a thousands
819: // separator if necessary.
820: template <typename UInt, typename Char, typename F>
821: inline Char* format_decimal(Char* buffer, UInt value, int num_digits,
822:                             F add_thousands_sep) {
823:   FMT_ASSERT(num_digits >= 0, "invalid digit count");
824:   buffer += num_digits;
825:   Char* end = buffer;
826:   while (value >= 100) {
827:     // Integer division is slow so do it for a group of two digits instead
828:     // of for every digit. The idea comes from the talk by Alexandrescu
829:     // "Three Optimization Tips for C++". See speed-test for a comparison.
830:     auto index = static_cast<unsigned>((value % 100) * 2);
831:     value /= 100;
832:     *--buffer = static_cast<Char>(data::digits[index + 1]);
833:     add_thousands_sep(buffer);
834:     *--buffer = static_cast<Char>(data::digits[index]);
835:     add_thousands_sep(buffer);
836:   }
837:   if (value < 10) {
838:     *--buffer = static_cast<Char>('0' + value);
839:     return end;
840:   }
841:   auto index = static_cast<unsigned>(value * 2);
842:   *--buffer = static_cast<Char>(data::digits[index + 1]);
843:   add_thousands_sep(buffer);
844:   *--buffer = static_cast<Char>(data::digits[index]);
845:   return end;
846: }
