2579: template <typename Char, typename ErrorHandler, typename... Args>
2580: class format_string_checker {
2581:  public:
2582:   explicit FMT_CONSTEXPR format_string_checker(
2583:       basic_string_view<Char> format_str, ErrorHandler eh)
2584:       : arg_id_(-1),
2585:         context_(format_str, eh),
2586:         parse_funcs_{&parse_format_specs<Args, parse_context_type>...} {}
2587: 
2588:   FMT_CONSTEXPR void on_text(const Char*, const Char*) {}
2589: 
2590:   FMT_CONSTEXPR void on_arg_id() {
2591:     arg_id_ = context_.next_arg_id();
2592:     check_arg_id();
2593:   }
2594:   FMT_CONSTEXPR void on_arg_id(int id) {
2595:     arg_id_ = id;
2596:     context_.check_arg_id(id);
2597:     check_arg_id();
2598:   }
2599:   FMT_CONSTEXPR void on_arg_id(basic_string_view<Char>) {
2600:     on_error("compile-time checks don't support named arguments");
