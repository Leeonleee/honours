1607:  public:
1608:   explicit basic_writer(Range out, locale_ref loc = locale_ref())
1609:       : out_(out.begin()), locale_(loc) {}
1610: 
1611:   iterator out() const { return out_; }
1612: 
1613:   // Writes a value in the format
1614:   //   <left-padding><value><right-padding>
1615:   // where <value> is written by f(it).
1616:   template <typename F> void write_padded(const format_specs& specs, F&& f) {
1617:     // User-perceived width (in code points).
1618:     unsigned width = to_unsigned(specs.width);
1619:     size_t size = f.size();  // The number of code units.
1620:     size_t num_code_points = width != 0 ? f.width() : size;
1621:     if (width <= num_code_points) return f(reserve(size));
1622:     auto&& it = reserve(width + (size - num_code_points));
1623:     char_type fill = specs.fill[0];
1624:     std::size_t padding = width - num_code_points;
1625:     if (specs.align == align::right) {
1626:       it = std::fill_n(it, padding, fill);
1627:       f(it);
1628:     } else if (specs.align == align::center) {
1629:       std::size_t left_padding = padding / 2;
1630:       it = std::fill_n(it, left_padding, fill);
1631:       f(it);
1632:       it = std::fill_n(it, padding - left_padding, fill);
1633:     } else {
1634:       f(it);
1635:       it = std::fill_n(it, padding, fill);
1636:     }
1637:   }
