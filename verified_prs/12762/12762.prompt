2414: // Parses standard format specifiers and sends notifications about parsed
2415: // components to handler.
2416: template <typename Char, typename SpecHandler>
2417: FMT_CONSTEXPR const Char* parse_format_specs(const Char* begin, const Char* end,
2418:                                              SpecHandler&& handler) {
2419:   if (begin == end || *begin == '}') return begin;
2420: 
2421:   begin = parse_align(begin, end, handler);
2422:   if (begin == end) return begin;
2423: 
2424:   // Parse sign.
2425:   switch (static_cast<char>(*begin)) {
2426:   case '+':
2427:     handler.on_plus();
2428:     ++begin;
2429:     break;
2430:   case '-':
2431:     handler.on_minus();
2432:     ++begin;
2433:     break;
2434:   case ' ':
2435:     handler.on_space();
2436:     ++begin;
2437:     break;
2438:   case ',':
2439:     handler.on_comma();
2440:     ++begin;
2441:     break;
2442:   case '_':
2443:     handler.on_underscore();
2444:     ++begin;
2445:     break;
2446:   case '\'':
2447:     handler.on_single_quote();
2448:     ++begin;
2449:     break;
2450:   case 't':
2451:     ++begin;
2452:     if (begin == end) return begin;
2453:     handler.on_thousands(*begin);
2454:     ++begin;
2455:     break;
2456:   }
2457:   if (begin == end) return begin;
2458: 
2459:   if (*begin == '#') {
2460:     handler.on_hash();
2461:     if (++begin == end) return begin;
2462:   }
2463: 
2464:   // Parse zero flag.
2465:   if (*begin == '0') {
2466:     handler.on_zero();
2467:     if (++begin == end) return begin;
2468:   }
2469: 
2470:   begin = parse_width(begin, end, handler);
2471:   if (begin == end) return begin;
2472: 
2473:   // Parse precision.
2474:   if (*begin == '.') {
2475:     begin = parse_precision(begin, end, handler);
2476:   }
2477: 
2478:   // Parse type.
2479:   if (begin != end && *begin != '}') handler.on_type(*begin++);
2480:   return begin;
2481: }
