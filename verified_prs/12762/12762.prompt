392: template <typename OutputIt>
393: class truncating_iterator<OutputIt, std::false_type>
394:     : public truncating_iterator_base<OutputIt> {
395:   using traits = std::iterator_traits<OutputIt>;
396: 
397:   mutable typename traits::value_type blackhole_;
398: 
399:  public:
400:   using value_type = typename traits::value_type;
401: 
402:   truncating_iterator(OutputIt out, std::size_t limit)
403:       : truncating_iterator_base<OutputIt>(out, limit) {}
404: 
405:   truncating_iterator& operator++() {
406:     if (this->count_++ < this->limit_) ++this->out_;
407:     return *this;
408:   }
409: 
410:   truncating_iterator operator++(int) {
411:     auto it = *this;
412:     ++*this;
413:     return it;
414:   }
415: 
416:   value_type& operator*() const {
417:     return this->count_ < this->limit_ ? *this->out_ : blackhole_;
