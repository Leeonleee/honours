2712: /** Fast integer formatter. */
2713: class format_int {
2714:  private:
2715:   // Buffer should be large enough to hold all digits (digits10 + 1),
2716:   // a sign and a null character.
2717:   enum { buffer_size = std::numeric_limits<unsigned long long>::digits10 + 3 };
2718:   mutable char buffer_[buffer_size];
2719:   char* str_;
2720: 
2721:   // Formats value in reverse and returns a pointer to the beginning.
2722:   char* format_decimal(unsigned long long value) {
2723:     char* ptr = buffer_ + (buffer_size - 1);  // Parens to workaround MSVC bug.
2724:     while (value >= 100) {
2725:       // Integer division is slow so do it for a group of two digits instead
2726:       // of for every digit. The idea comes from the talk by Alexandrescu
2727:       // "Three Optimization Tips for C++". See speed-test for a comparison.
2728:       auto index = static_cast<unsigned>((value % 100) * 2);
2729:       value /= 100;
2730:       *--ptr = internal::data::digits[index + 1];
2731:       *--ptr = internal::data::digits[index];
2732:     }
2733:     if (value < 10) {
2734:       *--ptr = static_cast<char>('0' + value);
2735:       return ptr;
2736:     }
2737:     auto index = static_cast<unsigned>(value * 2);
2738:     *--ptr = internal::data::digits[index + 1];
2739:     *--ptr = internal::data::digits[index];
2740:     return ptr;
2741:   }
2742: 
2743:   void format_signed(long long value) {
2744:     auto abs_value = static_cast<unsigned long long>(value);
2745:     bool negative = value < 0;
2746:     if (negative) abs_value = 0 - abs_value;
2747:     str_ = format_decimal(abs_value);
2748:     if (negative) *--str_ = '-';
2749:   }
2750: 
2751:  public:
2752:   explicit format_int(int value) { format_signed(value); }
2753:   explicit format_int(long value) { format_signed(value); }
2754:   explicit format_int(long long value) { format_signed(value); }
2755:   explicit format_int(unsigned value) : str_(format_decimal(value)) {}
2756:   explicit format_int(unsigned long value) : str_(format_decimal(value)) {}
2757:   explicit format_int(unsigned long long value) : str_(format_decimal(value)) {}
2758: 
2759:   /** Returns the number of characters written to the output buffer. */
2760:   std::size_t size() const {
2761:     return internal::to_unsigned(buffer_ - str_ + buffer_size - 1);
2762:   }
2763: 
2764:   /**
2765:     Returns a pointer to the output buffer content. No terminating null
2766:     character is appended.
2767:    */
2768:   const char* data() const { return str_; }
2769: 
2770:   /**
2771:     Returns a pointer to the output buffer content with terminating null
2772:     character appended.
2773:    */
2774:   const char* c_str() const {
2775:     buffer_[buffer_size - 1] = '\0';
2776:     return str_;
2777:   }
2778: 
2779:   /**
2780:     \rst
2781:     Returns the content of the output buffer as an ``std::string``.
2782:     \endrst
2783:    */
2784:   std::string str() const { return std::string(str_, size()); }
2785: };
