2602: 
2603:   FMT_CONSTEXPR void on_replacement_field(const Char*) {}
2604: 
2605:   FMT_CONSTEXPR const Char* on_format_specs(const Char* begin, const Char*) {
2606:     advance_to(context_, begin);
2607:     return arg_id_ < num_args ? parse_funcs_[arg_id_](context_) : begin;
2608:   }
2609: 
2610:   FMT_CONSTEXPR void on_error(std::string message) {
2611:     context_.on_error(message);
2612:   }
2613: 
2614:  private:
2615:   using parse_context_type = basic_format_parse_context<Char, ErrorHandler>;
2616:   enum { num_args = sizeof...(Args) };
2617: 
2618:   FMT_CONSTEXPR void check_arg_id() {
2619:     if (arg_id_ >= num_args) context_.on_error("argument index out of range");
2620:   }
2621: 
2622:   // Format specifier parsing function.
2623:   using parse_func = const Char* (*)(parse_context_type&);
2624: 
2625:   int arg_id_;
2626:   parse_context_type context_;
2627:   parse_func parse_funcs_[num_args > 0 ? num_args : 1];
