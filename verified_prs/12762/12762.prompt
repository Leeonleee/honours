1025:   template <typename It> It prettify(It it) const {
1026:     // pow(10, full_exp - 1) <= v <= pow(10, full_exp).
1027:     int full_exp = num_digits_ + exp_;
1028:     if (specs_.format == float_format::exp) {
1029:       // Insert a decimal point after the first digit and add an exponent.
1030:       *it++ = static_cast<Char>(*digits_);
1031:       int num_zeros = specs_.precision - num_digits_;
1032:       bool trailing_zeros = num_zeros > 0 && specs_.trailing_zeros;
1033:       if (num_digits_ > 1 || trailing_zeros) *it++ = decimal_point_;
1034:       it = copy_str<Char>(digits_ + 1, digits_ + num_digits_, it);
1035:       if (trailing_zeros)
1036:         it = std::fill_n(it, num_zeros, static_cast<Char>('0'));
1037:       *it++ = static_cast<Char>(specs_.upper ? 'E' : 'e');
1038:       return write_exponent<Char>(full_exp - 1, it);
1039:     }
1040:     if (num_digits_ <= full_exp) {
1041:       // 1234e7 -> 12340000000[.0+]
1042:       if (specs_.thousand_sep != '\0' && full_exp > 3) {
1043:         // thousand separator
1044:         // we need to print the thousand separator every 3 digits
1045:         // the loop is essentially write 3 digits, write the thousand separator
1046:         // the exception is the first batch of digits which are 1-3 digits
1047:         int digit_count = full_exp % 3 == 0 ? 3 : full_exp % 3;
1048:         for(int i = 0; i < full_exp; i += digit_count, digit_count = 3) {
1049:           if (i > 0) {
1050:             *it++ = specs_.thousand_sep;
1051:           }
1052:           if (i < num_digits_) {
1053:             // we still need to write digits
1054:             int write_count = std::min<int>(num_digits_ - i, digit_count);
1055:             it = copy_str<Char>(digits_ + i, digits_ + i + write_count, it);
1056:             if (write_count < digit_count) {
1057:               // write any trailing zeros that belong to this batch
1058:               it = std::fill_n(it, digit_count - write_count, static_cast<Char>('0'));
1059:             }
1060:           } else {
1061:             // we only need to write trailing zeros
1062:             it = std::fill_n(it, digit_count, static_cast<Char>('0'));
1063:           }
1064:         }
1065:       } else {
1066:         it = copy_str<Char>(digits_, digits_ + num_digits_, it);
1067:         it = std::fill_n(it, full_exp - num_digits_, static_cast<Char>('0'));
1068:       }
1069:       if (specs_.trailing_zeros) {
1070:         *it++ = decimal_point_;
1071:         int num_zeros = specs_.precision - full_exp;
1072:         if (num_zeros <= 0) {
1073:           if (specs_.format != float_format::fixed)
1074:             *it++ = static_cast<Char>('0');
1075:           return it;
1076:         }
1077: #ifdef FUZZING_BUILD_MODE_UNSAFE_FOR_PRODUCTION
1078:         if (num_zeros > 1000)
1079:           throw std::runtime_error("fuzz mode - avoiding excessive cpu use");
1080: #endif
1081:         it = std::fill_n(it, num_zeros, static_cast<Char>('0'));
1082:       }
1083:     } else if (full_exp > 0) {
1084:       // 1234e-2 -> 12.34[0+]
1085:       if (specs_.thousand_sep != '\0' && full_exp > 3) {
1086:         // thousand separator
1087:         // we need to print the thousand separator every 3 digits
1088:         // the loop is essentially write 3 digits, write the thousand separator
1089:         // the exception is the first batch of digits which are 1-3 digits
1090:         int digit_count = full_exp % 3 == 0 ? 3 : full_exp % 3;
1091:         for(int i = 0; i < full_exp; i += digit_count, digit_count = 3) {
1092:           if (i > 0) {
1093:             *it++ = specs_.thousand_sep;
1094:           }
1095:           it = copy_str<Char>(digits_ + i, digits_ + i + digit_count, it);
1096:         }
1097:       } else {
1098:         it = copy_str<Char>(digits_, digits_ + full_exp, it);
1099:       }
1100:       if (!specs_.trailing_zeros) {
1101:         // Remove trailing zeros.
1102:         int num_digits = num_digits_;
1103:         while (num_digits > full_exp && digits_[num_digits - 1] == '0')
1104:           --num_digits;
1105:         if (num_digits != full_exp) *it++ = decimal_point_;
1106:         return copy_str<Char>(digits_ + full_exp, digits_ + num_digits, it);
1107:       }
1108:       *it++ = decimal_point_;
1109:       it = copy_str<Char>(digits_ + full_exp, digits_ + num_digits_, it);
1110:       if (specs_.precision > num_digits_) {
1111:         // Add trailing zeros.
1112:         int num_zeros = specs_.precision - num_digits_;
1113:         it = std::fill_n(it, num_zeros, static_cast<Char>('0'));
1114:       }
1115:     } else {
1116:       // 1234e-6 -> 0.001234
1117:       *it++ = static_cast<Char>('0');
1118:       int num_zeros = -full_exp;
1119:       if (specs_.precision >= 0 && specs_.precision < num_zeros)
1120:         num_zeros = specs_.precision;
1121:       int num_digits = num_digits_;
1122:       if (!specs_.trailing_zeros)
1123:         while (num_digits > 0 && digits_[num_digits - 1] == '0') --num_digits;
1124:       if (num_zeros != 0 || num_digits != 0) {
1125:         *it++ = decimal_point_;
1126:         it = std::fill_n(it, num_zeros, static_cast<Char>('0'));
1127:         it = copy_str<Char>(digits_, digits_ + num_digits, it);
1128:       }
1129:     }
1130:     return it;
1131:   }
