2900: // A formatter for types known only at run time such as variant alternatives.
2901: //
2902: // Usage:
2903: //   using variant = std::variant<int, std::string>;
2904: //   template <>
2905: //   struct formatter<variant>: dynamic_formatter<> {
2906: //     void format(buffer &buf, const variant &v, context &ctx) {
2907: //       visit([&](const auto &val) { format(buf, val, ctx); }, v);
2908: //     }
2909: //   };
2910: template <typename Char = char> class dynamic_formatter {
2911:  private:
2912:   struct null_handler : internal::error_handler {
2913:     void on_align(align_t) {}
2914:     void on_plus() {}
2915:     void on_minus() {}
2916:     void on_space() {}
2917:     void on_hash() {}
2918:   };
2919: 
2920:  public:
2921:   template <typename ParseContext>
2922:   auto parse(ParseContext& ctx) -> decltype(ctx.begin()) {
2923:     format_str_ = ctx.begin();
2924:     // Checks are deferred to formatting time when the argument type is known.
2925:     internal::dynamic_specs_handler<ParseContext> handler(specs_, ctx);
2926:     return parse_format_specs(ctx.begin(), ctx.end(), handler);
2927:   }
2928: 
2929:   template <typename T, typename FormatContext>
2930:   auto format(const T& val, FormatContext& ctx) -> decltype(ctx.out()) {
2931:     handle_specs(ctx);
2932:     internal::specs_checker<null_handler> checker(
2933:         null_handler(),
2934:         internal::mapped_type_constant<T, FormatContext>::value);
2935:     checker.on_align(specs_.align);
2936:     switch (specs_.sign) {
2937:     case sign::none:
2938:       break;
2939:     case sign::plus:
2940:       checker.on_plus();
2941:       break;
2942:     case sign::minus:
2943:       checker.on_minus();
2944:       break;
2945:     case sign::space:
2946:       checker.on_space();
2947:       break;
2948:     }
2949:     if (specs_.alt) checker.on_hash();
2950:     if (specs_.precision >= 0) checker.end_precision();
2951:     using range = internal::output_range<typename FormatContext::iterator,
2952:                                          typename FormatContext::char_type>;
2953:     visit_format_arg(arg_formatter<range>(ctx, nullptr, &specs_),
2954:                      internal::make_arg<FormatContext>(val));
2955:     return ctx.out();
2956:   }
2957: 
2958:  private:
2959:   template <typename Context> void handle_specs(Context& ctx) {
2960:     internal::handle_dynamic_spec<internal::width_checker>(
2961:         specs_.width, specs_.width_ref, ctx);
2962:     internal::handle_dynamic_spec<internal::precision_checker>(
2963:         specs_.precision, specs_.precision_ref, ctx);
2964:   }
2965: 
2966:   internal::dynamic_format_specs<Char> specs_;
2967:   const Char* format_str_;
2968: };
