561: /**
562:   \rst
563:   A dynamically growing memory buffer for trivially copyable/constructible types
564:   with the first ``SIZE`` elements stored in the object itself.
565: 
566:   You can use one of the following type aliases for common character types:
567: 
568:   +----------------+------------------------------+
569:   | Type           | Definition                   |
570:   +================+==============================+
571:   | memory_buffer  | basic_memory_buffer<char>    |
572:   +----------------+------------------------------+
573:   | wmemory_buffer | basic_memory_buffer<wchar_t> |
574:   +----------------+------------------------------+
575: 
576:   **Example**::
577: 
578:      fmt::memory_buffer out;
579:      format_to(out, "The answer is {}.", 42);
580: 
581:   This will append the following output to the ``out`` object:
582: 
583:   .. code-block:: none
584: 
585:      The answer is 42.
586: 
587:   The output can be converted to an ``std::string`` with ``to_string(out)``.
588:   \endrst
589:  */
590: template <typename T, std::size_t SIZE = inline_buffer_size,
591:           typename Allocator = std::allocator<T>>
592: class basic_memory_buffer : private Allocator, public internal::buffer<T> {
593:  private:
594:   T store_[SIZE];
595: 
596:   // Deallocate memory allocated by the buffer.
597:   void deallocate() {
598:     T* data = this->data();
599:     if (data != store_) Allocator::deallocate(data, this->capacity());
600:   }
601: 
602:  protected:
603:   void grow(std::size_t size) FMT_OVERRIDE;
604: 
605:  public:
606:   using value_type = T;
607:   using const_reference = const T&;
608: 
609:   explicit basic_memory_buffer(const Allocator& alloc = Allocator())
610:       : Allocator(alloc) {
611:     this->set(store_, SIZE);
612:   }
613:   ~basic_memory_buffer() FMT_OVERRIDE { deallocate(); }
614: 
615:  private:
616:   // Move data from other to this buffer.
617:   void move(basic_memory_buffer& other) {
618:     Allocator &this_alloc = *this, &other_alloc = other;
619:     this_alloc = std::move(other_alloc);
620:     T* data = other.data();
621:     std::size_t size = other.size(), capacity = other.capacity();
622:     if (data == other.store_) {
623:       this->set(store_, capacity);
624:       std::uninitialized_copy(other.store_, other.store_ + size,
625:                               internal::make_checked(store_, capacity));
626:     } else {
627:       this->set(data, capacity);
628:       // Set pointer to the inline array so that delete is not called
629:       // when deallocating.
630:       other.set(other.store_, 0);
631:     }
632:     this->resize(size);
633:   }
634: 
635:  public:
636:   /**
637:     \rst
638:     Constructs a :class:`fmt::basic_memory_buffer` object moving the content
639:     of the other object to it.
640:     \endrst
641:    */
642:   basic_memory_buffer(basic_memory_buffer&& other) FMT_NOEXCEPT { move(other); }
643: 
644:   /**
645:     \rst
646:     Moves the content of the other ``basic_memory_buffer`` object to this one.
647:     \endrst
648:    */
649:   basic_memory_buffer& operator=(basic_memory_buffer&& other) FMT_NOEXCEPT {
650:     FMT_ASSERT(this != &other, "");
651:     deallocate();
652:     move(other);
653:     return *this;
654:   }
655: 
656:   // Returns a copy of the allocator associated with this buffer.
657:   Allocator get_allocator() const { return *this; }
658: };
659: 
660: template <typename T, std::size_t SIZE, typename Allocator>
661: void basic_memory_buffer<T, SIZE, Allocator>::grow(std::size_t size) {
662: #ifdef FUZZING_BUILD_MODE_UNSAFE_FOR_PRODUCTION
663:   if (size > 1000) throw std::runtime_error("fuzz mode - won't grow that much");
664: #endif
665:   std::size_t old_capacity = this->capacity();
666:   std::size_t new_capacity = old_capacity + old_capacity / 2;
667:   if (size > new_capacity) new_capacity = size;
668:   T* old_data = this->data();
669:   T* new_data = std::allocator_traits<Allocator>::allocate(*this, new_capacity);
670:   // The following code doesn't throw, so the raw pointer above doesn't leak.
671:   std::uninitialized_copy(old_data, old_data + this->size(),
672:                           internal::make_checked(new_data, new_capacity));
673:   this->set(new_data, new_capacity);
674:   // deallocate must not throw according to the standard, but even if it does,
675:   // the buffer already uses the new storage and will deallocate it in
676:   // destructor.
677:   if (old_data != store_) Allocator::deallocate(old_data, old_capacity);
678: }
