1171: template <typename Spec, typename Handler>
1172: FMT_CONSTEXPR void handle_int_type_spec(const Spec& specs, Handler&& handler) {
1173:   if (specs.thousands != '\0') {
1174:     handler.on_num();
1175:     return;
1176:   }
1177:   switch (specs.type) {
1178:   case 0:
1179:   case 'd':
1180:     handler.on_dec();
1181:     break;
1182:   case 'x':
1183:   case 'X':
1184:     handler.on_hex();
1185:     break;
1186:   case 'b':
1187:   case 'B':
1188:     handler.on_bin();
1189:     break;
1190:   case 'o':
1191:     handler.on_oct();
1192:     break;
1193:   case 'n':
1194:   case 'l':
1195:   case 'L':
1196:     handler.on_num();
1197:     break;
1198:   default:
1199:     handler.on_error("Invalid type specifier \"" + std::string(1, specs.type) + "\" for formatting a value of type int");
1200:   }
1201: }
