2795:   // Parses format specifiers stopping either at the end of the range or at the
2796:   // terminating '}'.
2797:   template <typename ParseContext>
2798:   FMT_CONSTEXPR auto parse(ParseContext& ctx) -> decltype(ctx.begin()) {
2799:     using handler_type = internal::dynamic_specs_handler<ParseContext>;
2800:     auto type = internal::type_constant<T, Char>::value;
2801:     internal::specs_checker<handler_type> handler(handler_type(specs_, ctx),
2802:                                                   type);
2803:     auto it = parse_format_specs(ctx.begin(), ctx.end(), handler);
2804:     auto eh = ctx.error_handler();
2805:     switch (type) {
2806:     case internal::none_type:
2807:     case internal::named_arg_type:
2808:       FMT_ASSERT(false, "invalid argument type");
2809:       break;
2810:     case internal::int_type:
2811:     case internal::uint_type:
2812:     case internal::long_long_type:
2813:     case internal::ulong_long_type:
2814:     case internal::int128_type:
2815:     case internal::uint128_type:
2816:     case internal::bool_type:
2817:       handle_int_type_spec(specs_.type,
2818:                            internal::int_type_checker<decltype(eh)>(eh));
2819:       break;
2820:     case internal::char_type:
2821:       handle_char_specs(
2822:           &specs_, internal::char_specs_checker<decltype(eh)>(specs_.type, eh));
2823:       break;
2824:     case internal::float_type:
2825:     case internal::double_type:
2826:     case internal::long_double_type:
2827:       internal::parse_float_type_spec(specs_, eh);
2828:       break;
2829:     case internal::cstring_type:
2830:       internal::handle_cstring_type_spec(
2831:           specs_.type, internal::cstring_type_checker<decltype(eh)>(eh));
2832:       break;
2833:     case internal::string_type:
2834:       internal::check_string_type_spec(specs_.type, eh);
2835:       break;
2836:     case internal::pointer_type:
2837:       internal::check_pointer_type_spec(specs_.type, eh);
2838:       break;
2839:     case internal::custom_type:
