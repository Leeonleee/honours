1360: // This template provides operations for formatting and writing data into a
1361: // character range.
1362: template <typename Range> class basic_writer {
1363:  public:
1364:   using char_type = typename Range::value_type;
1365:   using iterator = typename Range::iterator;
1366:   using format_specs = basic_format_specs<char_type>;
1367: 
1368:  private:
1369:   iterator out_;  // Output iterator.
1370:   locale_ref locale_;
1371: 
1372:   // Attempts to reserve space for n extra characters in the output range.
1373:   // Returns a pointer to the reserved range or a reference to out_.
1374:   auto reserve(std::size_t n) -> decltype(internal::reserve(out_, n)) {
1375:     return internal::reserve(out_, n);
1376:   }
1377: 
1378:   template <typename F> struct padded_int_writer {
1379:     size_t size_;
1380:     string_view prefix;
1381:     char_type fill;
1382:     std::size_t padding;
1383:     F f;
1384: 
1385:     size_t size() const { return size_; }
1386:     size_t width() const { return size_; }
1387: 
1388:     template <typename It> void operator()(It&& it) const {
1389:       if (prefix.size() != 0)
1390:         it = copy_str<char_type>(prefix.begin(), prefix.end(), it);
1391:       it = std::fill_n(it, padding, fill);
1392:       f(it);
1393:     }
1394:   };
1395: 
1396:   // Writes an integer in the format
1397:   //   <left-padding><prefix><numeric-padding><digits><right-padding>
1398:   // where <digits> are written by f(it).
1399:   template <typename F>
1400:   void write_int(int num_digits, string_view prefix, format_specs specs, F f) {
1401:     std::size_t size = prefix.size() + to_unsigned(num_digits);
1402:     char_type fill = specs.fill[0];
1403:     std::size_t padding = 0;
1404:     if (specs.align == align::numeric) {
1405:       auto unsiged_width = to_unsigned(specs.width);
1406:       if (unsiged_width > size) {
1407:         padding = unsiged_width - size;
1408:         size = unsiged_width;
1409:       }
1410:     } else if (specs.precision > num_digits) {
1411:       size = prefix.size() + to_unsigned(specs.precision);
1412:       padding = to_unsigned(specs.precision - num_digits);
1413:       fill = static_cast<char_type>('0');
1414:     }
1415:     if (specs.align == align::none) specs.align = align::right;
1416:     write_padded(specs, padded_int_writer<F>{size, prefix, fill, padding, f});
1417:   }
1418: 
1419:   // Writes a decimal integer.
1420:   template <typename Int> void write_decimal(Int value) {
1421:     auto abs_value = static_cast<uint32_or_64_or_128_t<Int>>(value);
1422:     bool negative = is_negative(value);
1423:     // Don't do -abs_value since it trips unsigned-integer-overflow sanitizer.
1424:     if (negative) abs_value = ~abs_value + 1;
1425:     int num_digits = count_digits(abs_value);
1426:     auto&& it = reserve((negative ? 1 : 0) + static_cast<size_t>(num_digits));
1427:     if (negative) *it++ = static_cast<char_type>('-');
1428:     it = format_decimal<char_type>(it, abs_value, num_digits);
1429:   }
1430: 
1431:   // The handle_int_type_spec handler that writes an integer.
1432:   template <typename Int, typename Specs> struct int_writer {
1433:     using unsigned_type = uint32_or_64_or_128_t<Int>;
1434: 
1435:     basic_writer<Range>& writer;
1436:     const Specs& specs;
1437:     unsigned_type abs_value;
1438:     char prefix[4];
1439:     unsigned prefix_size;
1440: 
1441:     string_view get_prefix() const { return string_view(prefix, prefix_size); }
1442: 
1443:     int_writer(basic_writer<Range>& w, Int value, const Specs& s)
1444:         : writer(w),
1445:           specs(s),
1446:           abs_value(static_cast<unsigned_type>(value)),
1447:           prefix_size(0) {
1448:       if (is_negative(value)) {
1449:         prefix[0] = '-';
1450:         ++prefix_size;
1451:         abs_value = 0 - abs_value;
1452:       } else if (specs.sign != sign::none && specs.sign != sign::minus) {
1453:         prefix[0] = specs.sign == sign::plus ? '+' : ' ';
1454:         ++prefix_size;
1455:       }
1456:     }
