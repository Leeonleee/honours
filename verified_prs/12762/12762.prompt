994: // Writes the exponent exp in the form "[+-]d{2,3}" to buffer.
995: template <typename Char, typename It> It write_exponent(int exp, It it) {
996:   FMT_ASSERT(-10000 < exp && exp < 10000, "exponent out of range");
997:   if (exp < 0) {
998:     *it++ = static_cast<Char>('-');
999:     exp = -exp;
1000:   } else {
1001:     *it++ = static_cast<Char>('+');
1002:   }
1003:   if (exp >= 100) {
1004:     const char* top = data::digits + (exp / 100) * 2;
1005:     if (exp >= 1000) *it++ = static_cast<Char>(top[0]);
1006:     *it++ = static_cast<Char>(top[1]);
1007:     exp %= 100;
1008:   }
1009:   const char* d = data::digits + exp * 2;
1010:   *it++ = static_cast<Char>(d[0]);
1011:   *it++ = static_cast<Char>(d[1]);
1012:   return it;
1013: }
