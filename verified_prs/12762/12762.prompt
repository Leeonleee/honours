1203: template <typename ErrorHandler = error_handler, typename Char>
1204: FMT_CONSTEXPR float_specs parse_float_type_spec(
1205:     const basic_format_specs<Char>& specs, ErrorHandler&& eh = {}) {
1206: 
1207:   auto result = float_specs();
1208:   result.thousand_sep = specs.thousands;
1209:   result.trailing_zeros = specs.alt;
1210:   switch (specs.type) {
1211:   case 0:
1212:     result.format = float_format::general;
1213:     result.trailing_zeros |= specs.precision != 0;
1214:     break;
1215:   case 'G':
1216:     result.upper = true;
1217:     FMT_FALLTHROUGH;
1218:   case 'g':
1219:     result.format = float_format::general;
1220:     break;
1221:   case 'E':
1222:     result.upper = true;
1223:     FMT_FALLTHROUGH;
1224:   case 'e':
1225:     result.format = float_format::exp;
1226:     result.trailing_zeros |= specs.precision != 0;
1227:     break;
1228:   case 'F':
1229:     result.upper = true;
1230:     FMT_FALLTHROUGH;
1231:   case 'f':
1232:     result.format = float_format::fixed;
1233:     result.trailing_zeros |= specs.precision != 0;
1234:     break;
1235: #if FMT_DEPRECATED_PERCENT
1236:   case '%':
1237:     result.format = float_format::fixed;
1238:     result.percent = true;
1239:     break;
1240: #endif
1241:   case 'A':
1242:     result.upper = true;
1243:     FMT_FALLTHROUGH;
1244:   case 'a':
1245:     result.format = float_format::hex;
1246:     break;
1247:   case 'n':
1248:   case 'l':
1249:   case 'L':
1250:     result.locale = true;
1251:     break;
1252:   default:
1253:     eh.on_error("Invalid type specifier \"" + std::string(1, specs.type) + "\" for formatting a value of type float");
1254:     break;
1255:   }
1256:   return result;
1257: }
