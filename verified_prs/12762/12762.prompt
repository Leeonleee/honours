724: #ifdef FMT_BUILTIN_CLZLL
725: // Returns the number of decimal digits in n. Leading zeros are not counted
726: // except for n == 0 in which case count_digits returns 1.
727: inline int count_digits(uint64_t n) {
728:   // Based on http://graphics.stanford.edu/~seander/bithacks.html#IntegerLog10
729:   // and the benchmark https://github.com/localvoid/cxx-benchmark-count-digits.
730:   int t = (64 - FMT_BUILTIN_CLZLL(n | 1)) * 1233 >> 12;
731:   return t - (n < data::zero_or_powers_of_10_64[t]) + 1;
732: }
733: #else
734: // Fallback version of count_digits used when __builtin_clz is not available.
735: inline int count_digits(uint64_t n) {
736:   int count = 1;
737:   for (;;) {
738:     // Integer division is slow so do it for a group of four digits instead
739:     // of for every digit. The idea comes from the talk by Alexandrescu
740:     // "Three Optimization Tips for C++". See speed-test for a comparison.
741:     if (n < 10) return count;
742:     if (n < 100) return count + 1;
743:     if (n < 1000) return count + 2;
744:     if (n < 10000) return count + 3;
745:     n /= 10000u;
746:     count += 4;
747:   }
748: }
749: #endif
750: 
751: #if FMT_USE_INT128
752: inline int count_digits(uint128_t n) {
753:   int count = 1;
754:   for (;;) {
755:     // Integer division is slow so do it for a group of four digits instead
756:     // of for every digit. The idea comes from the talk by Alexandrescu
757:     // "Three Optimization Tips for C++". See speed-test for a comparison.
758:     if (n < 10) return count;
759:     if (n < 100) return count + 1;
760:     if (n < 1000) return count + 2;
761:     if (n < 10000) return count + 3;
762:     n /= 10000U;
763:     count += 4;
764:   }
765: }
766: #endif
