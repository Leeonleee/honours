2492: template <>
2493: inline bool find<false, char>(const char* first, const char* last, char value,
2494:                               const char*& out) {
2495:   out = static_cast<const char*>(
2496:       std::memchr(first, value, internal::to_unsigned(last - first)));
2497:   return out != nullptr;
2498: }
