1652:   template <typename T, typename Spec>
1653:   void write_int(T value, const Spec& spec) {
1654:     handle_int_type_spec(spec, int_writer<T, Spec>(*this, value, spec));
1655:   }
1656: 
1657:   template <typename T, FMT_ENABLE_IF(std::is_floating_point<T>::value)>
1658:   void write(T value, format_specs specs = {}) {
1659:     float_specs fspecs = parse_float_type_spec(specs);
1660:     fspecs.sign = specs.sign;
1661:     if (std::signbit(value)) {  // value < 0 is false for NaN so use signbit.
1662:       fspecs.sign = sign::minus;
1663:       value = -value;
1664:     } else if (fspecs.sign == sign::minus) {
1665:       fspecs.sign = sign::none;
1666:     }
1667: 
1668:     if (!std::isfinite(value)) {
1669:       auto str = std::isinf(value) ? (fspecs.upper ? "INF" : "inf")
1670:                                    : (fspecs.upper ? "NAN" : "nan");
1671:       return write_padded(specs, nonfinite_writer<char_type>{fspecs.sign, str});
1672:     }
1673: 
1674:     if (specs.align == align::none) {
1675:       specs.align = align::right;
1676:     } else if (specs.align == align::numeric) {
1677:       if (fspecs.sign) {
1678:         auto&& it = reserve(1);
1679:         *it++ = static_cast<char_type>(data::signs[fspecs.sign]);
1680:         fspecs.sign = sign::none;
1681:         if (specs.width != 0) --specs.width;
1682:       }
1683:       specs.align = align::right;
1684:     }
1685: 
1686:     memory_buffer buffer;
1687:     if (fspecs.format == float_format::hex) {
1688:       if (fspecs.sign) buffer.push_back(data::signs[fspecs.sign]);
1689:       snprintf_float(promote_float(value), specs.precision, fspecs, buffer);
1690:       write_padded(specs, str_writer<char>{buffer.data(), buffer.size()});
1691:       return;
1692:     }
1693:     int precision = specs.precision >= 0 || !specs.type ? specs.precision : 6;
1694:     if (fspecs.format == float_format::exp) ++precision;
1695:     if (const_check(std::is_same<T, float>())) fspecs.binary32 = true;
1696:     fspecs.use_grisu = use_grisu<T>();
1697:     if (const_check(FMT_DEPRECATED_PERCENT) && fspecs.percent) value *= 100;
1698:     int exp = format_float(promote_float(value), precision, fspecs, buffer);
1699:     if (const_check(FMT_DEPRECATED_PERCENT) && fspecs.percent) {
1700:       buffer.push_back('%');
1701:       --exp;  // Adjust decimal place position.
1702:     }
1703:     fspecs.precision = precision;
1704:     char_type point;
1705:     if (fspecs.locale) {
1706:       point = decimal_point<char_type>(locale_);
1707:     } else if (fspecs.thousand_sep == '.') {
1708:       // if the thousand separator is a point, we automatically switch the decimal separator to comma
1709:       point =static_cast<char_type>(',');
1710:     } else {
1711:       point = static_cast<char_type>('.');
1712:     }
1713:     write_padded(specs, float_writer<char_type>(buffer.data(),
1714:                                                 static_cast<int>(buffer.size()),
1715:                                                 exp, fspecs, point));
1716:   }
