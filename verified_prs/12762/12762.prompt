2847:   template <typename FormatContext>
2848:   auto format(const T& val, FormatContext& ctx) -> decltype(ctx.out()) {
2849:     internal::handle_dynamic_spec<internal::width_checker>(
2850:         specs_.width, specs_.width_ref, ctx);
2851:     internal::handle_dynamic_spec<internal::precision_checker>(
2852:         specs_.precision, specs_.precision_ref, ctx);
2853:     using range_type =
2854:         internal::output_range<typename FormatContext::iterator,
2855:                                typename FormatContext::char_type>;
2856:     return visit_format_arg(arg_formatter<range_type>(ctx, nullptr, &specs_),
2857:                             internal::make_arg<FormatContext>(val));
2858:   }
2859: 
2860:  private:
2861:   internal::dynamic_format_specs<Char> specs_;
