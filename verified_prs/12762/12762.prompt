2334: // Parses fill and alignment.
2335: template <typename Char, typename Handler>
2336: FMT_CONSTEXPR const Char* parse_align(const Char* begin, const Char* end,
2337:                                       Handler&& handler) {
2338:   FMT_ASSERT(begin != end, "");
2339:   auto align = align::none;
2340:   int i = 0;
2341:   if (begin + 1 != end) ++i;
2342:   do {
2343:     switch (static_cast<char>(begin[i])) {
2344:     case '<':
2345:       align = align::left;
2346:       break;
2347:     case '>':
2348:       align = align::right;
2349:       break;
2350: #if FMT_NUMERIC_ALIGN
2351:     case '=':
2352:       align = align::numeric;
2353:       break;
2354: #endif
2355:     case '^':
2356:       align = align::center;
2357:       break;
2358:     }
2359:     if (align != align::none) {
2360:       if (i > 0) {
2361:         auto c = *begin;
2362:         if (c == '{')
2363:           return handler.on_error("invalid fill character '{'"), begin;
2364:         begin += 2;
2365:         handler.on_fill(c);
2366:       } else
2367:         ++begin;
2368:       handler.on_align(align);
2369:       break;
2370:     }
2371:   } while (i-- > 0);
2372:   return begin;
2373: }
2374: 
2375: template <typename Char, typename Handler>
2376: FMT_CONSTEXPR const Char* parse_width(const Char* begin, const Char* end,
2377:                                       Handler&& handler) {
2378:   FMT_ASSERT(begin != end, "");
2379:   if ('0' <= *begin && *begin <= '9') {
2380:     handler.on_width(parse_nonnegative_int(begin, end, handler));
2381:   } else if (*begin == '{') {
2382:     ++begin;
2383:     if (begin != end)
2384:       begin = parse_arg_id(begin, end, width_adapter<Handler, Char>(handler));
2385:     if (begin == end || *begin != '}')
2386:       return handler.on_error("invalid format string"), begin;
2387:     ++begin;
2388:   }
2389:   return begin;
2390: }
2391: 
2392: template <typename Char, typename Handler>
2393: FMT_CONSTEXPR const Char* parse_precision(const Char* begin, const Char* end,
2394:                                           Handler&& handler) {
2395:   ++begin;
2396:   auto c = begin != end ? *begin : Char();
2397:   if ('0' <= c && c <= '9') {
2398:     handler.on_precision(parse_nonnegative_int(begin, end, handler));
2399:   } else if (c == '{') {
2400:     ++begin;
2401:     if (begin != end) {
2402:       begin =
2403:           parse_arg_id(begin, end, precision_adapter<Handler, Char>(handler));
2404:     }
2405:     if (begin == end || *begin++ != '}')
2406:       return handler.on_error("invalid format string"), begin;
2407:   } else {
2408:     return handler.on_error("missing precision specifier"), begin;
2409:   }
2410:   handler.end_precision();
2411:   return begin;
2412: }
