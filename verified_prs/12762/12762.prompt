2512: template <bool IS_CONSTEXPR, typename Char, typename Handler>
2513: FMT_CONSTEXPR void parse_format_string(basic_string_view<Char> format_str,
2514:                                        Handler&& handler) {
2515:   struct pfs_writer {
2516:     FMT_CONSTEXPR void operator()(const Char* begin, const Char* end) {
2517:       if (begin == end) return;
2518:       for (;;) {
2519:         const Char* p = nullptr;
2520:         if (!find<IS_CONSTEXPR>(begin, end, '}', p))
2521:           return handler_.on_text(begin, end);
2522:         ++p;
2523:         if (p == end || *p != '}')
2524:           return handler_.on_error("unmatched '}' in format string");
2525:         handler_.on_text(begin, p);
2526:         begin = p + 1;
2527:       }
2528:     }
2529:     Handler& handler_;
2530:   } write{handler};
2531:   auto begin = format_str.data();
2532:   auto end = begin + format_str.size();
2533:   while (begin != end) {
2534:     // Doing two passes with memchr (one for '{' and another for '}') is up to
2535:     // 2.5x faster than the naive one-pass implementation on big format strings.
2536:     const Char* p = begin;
2537:     if (*begin != '{' && !find<IS_CONSTEXPR>(begin, end, '{', p))
2538:       return write(begin, end);
2539:     write(begin, p);
2540:     ++p;
2541:     if (p == end) return handler.on_error("invalid format string");
2542:     if (static_cast<char>(*p) == '}') {
2543:       handler.on_arg_id();
2544:       handler.on_replacement_field(p);
2545:     } else if (*p == '{') {
2546:       handler.on_text(p, p + 1);
2547:     } else {
2548:       p = parse_arg_id(p, end, id_adapter<Handler, Char>{handler});
2549:       Char c = p != end ? *p : Char();
2550:       if (c == '}') {
2551:         handler.on_replacement_field(p);
2552:       } else if (c == ':') {
2553:         p = handler.on_format_specs(p + 1, end);
2554:         if (p == end || *p != '}')
2555:           return handler.on_error("unknown format specifier");
2556:       } else {
2557:         return handler.on_error("missing '}' in format string");
2558:       }
2559:     }
2560:     begin = p + 1;
2561:   }
2562: }
