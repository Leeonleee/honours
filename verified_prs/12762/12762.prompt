2789: template <typename T, typename Char>
2790: struct formatter<T, Char,
2791:                  enable_if_t<internal::type_constant<T, Char>::value !=
2792:                              internal::custom_type>> {
2793:   FMT_CONSTEXPR formatter() = default;
