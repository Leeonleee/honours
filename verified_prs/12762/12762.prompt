1639:   void write(int value) { write_decimal(value); }
1640:   void write(long value) { write_decimal(value); }
1641:   void write(long long value) { write_decimal(value); }
1642: 
1643:   void write(unsigned value) { write_decimal(value); }
1644:   void write(unsigned long value) { write_decimal(value); }
1645:   void write(unsigned long long value) { write_decimal(value); }
