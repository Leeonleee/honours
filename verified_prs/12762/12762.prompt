1330: void arg_map<Context>::init(const basic_format_args<Context>& args) {
1331:   if (map_) return;
1332:   map_ = new entry[internal::to_unsigned(args.max_size())];
1333:   if (args.is_packed()) {
1334:     for (int i = 0;; ++i) {
1335:       internal::type arg_type = args.type(i);
1336:       if (arg_type == internal::none_type) return;
1337:       if (arg_type == internal::named_arg_type) push_back(args.values_[i]);
1338:     }
1339:   }
1340:   for (int i = 0, n = args.max_size(); i < n; ++i) {
1341:     auto type = args.args_[i].type_;
1342:     if (type == internal::named_arg_type) push_back(args.args_[i].value_);
1343:   }
1344: }
