2483: // Return the result via the out param to workaround gcc bug 77539.
2484: template <bool IS_CONSTEXPR, typename T, typename Ptr = const T*>
2485: FMT_CONSTEXPR bool find(Ptr first, Ptr last, T value, Ptr& out) {
2486:   for (out = first; out != last; ++out) {
2487:     if (*out == value) return true;
2488:   }
2489:   return false;
2490: }
