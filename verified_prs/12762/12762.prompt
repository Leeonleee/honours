1525:     struct num_writer {
1526:       unsigned_type abs_value;
1527:       int size;
1528:       const std::string& groups;
1529:       char_type sep;
1530: 
1531:       template <typename It> void operator()(It&& it) const {
1532:         basic_string_view<char_type> s(&sep, sep_size);
1533:         // Index of a decimal digit with the least significant digit having
1534:         // index 0.
1535:         int digit_index = 0;
1536:         std::string::const_iterator group = groups.cbegin();
1537:         it = format_decimal<char_type>(
1538:             it, abs_value, size,
1539:             [this, s, &group, &digit_index](char_type*& buffer) {
1540:               if (*group <= 0 || ++digit_index % *group != 0 ||
1541:                   *group == max_value<char>())
1542:                 return;
1543:               if (group + 1 != groups.cend()) {
1544:                 digit_index = 0;
1545:                 ++group;
1546:               }
1547:               buffer -= s.size();
1548:               std::uninitialized_copy(s.data(), s.data() + s.size(),
1549:                                       make_checked(buffer, s.size()));
1550:             });
1551:       }
1552:     };
1553: 
1554:     void on_num() {
1555:       std::string groups = grouping<char_type>(writer.locale_);
1556:       if (groups.empty()) return on_dec();
1557:       auto sep = specs.thousands;
1558:       if (!sep) return on_dec();
1559:       int num_digits = count_digits(abs_value);
1560:       int size = num_digits;
1561:       std::string::const_iterator group = groups.cbegin();
1562:       while (group != groups.cend() && num_digits > *group && *group > 0 &&
1563:              *group != max_value<char>()) {
1564:         size += sep_size;
1565:         num_digits -= *group;
1566:         ++group;
1567:       }
1568:       if (group == groups.cend())
1569:         size += sep_size * ((num_digits - 1) / groups.back());
1570:       writer.write_int(size, get_prefix(), specs,
1571:                        num_writer{abs_value, size, groups, static_cast<char_type>(sep)});
1572:     }
