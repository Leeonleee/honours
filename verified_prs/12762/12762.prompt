2271: template <typename Char, typename IDHandler>
2272: FMT_CONSTEXPR const Char* parse_arg_id(const Char* begin, const Char* end,
2273:                                        IDHandler&& handler) {
2274:   FMT_ASSERT(begin != end, "");
2275:   Char c = *begin;
2276:   if (c == '}' || c == ':') {
2277:     handler();
2278:     return begin;
2279:   }
2280:   if (c >= '0' && c <= '9') {
2281:     int index = parse_nonnegative_int(begin, end, handler);
2282:     if (begin == end || (*begin != '}' && *begin != ':'))
2283:       handler.on_error("invalid format string");
2284:     else
2285:       handler(index);
2286:     return begin;
2287:   }
2288:   if (!is_name_start(c)) {
2289:     handler.on_error("invalid format string");
2290:     return begin;
2291:   }
2292:   auto it = begin;
2293:   do {
2294:     ++it;
2295:   } while (it != end && (is_name_start(c = *it) || ('0' <= c && c <= '9')));
2296:   handler(basic_string_view<Char>(begin, to_unsigned(it - begin)));
2297:   return it;
2298: }
