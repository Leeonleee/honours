2985: template <typename ArgFormatter, typename Char, typename Context>
2986: struct format_handler : internal::error_handler {
2987:   using range = typename ArgFormatter::range;
2988: 
2989:   format_handler(range r, basic_string_view<Char> str,
2990:                  basic_format_args<Context> format_args,
2991:                  internal::locale_ref loc)
2992:       : parse_context(str), context(r.begin(), format_args, loc) {}
2993: 
2994:   void on_text(const Char* begin, const Char* end) {
2995:     auto size = internal::to_unsigned(end - begin);
2996:     auto out = context.out();
2997:     auto&& it = internal::reserve(out, size);
2998:     it = std::copy_n(begin, size, it);
2999:     context.advance_to(out);
3000:   }
3001: 
3002:   void get_arg(int id) { arg = internal::get_arg(context, id); }
3003: 
3004:   void on_arg_id() { get_arg(parse_context.next_arg_id()); }
3005:   void on_arg_id(int id) {
3006:     parse_context.check_arg_id(id);
3007:     get_arg(id);
3008:   }
3009:   void on_arg_id(basic_string_view<Char> id) { arg = context.arg(id); }
3010: 
3011:   void on_replacement_field(const Char* p) {
3012:     advance_to(parse_context, p);
3013:     context.advance_to(
3014:         visit_format_arg(ArgFormatter(context, &parse_context), arg));
3015:   }
3016: 
3017:   const Char* on_format_specs(const Char* begin, const Char* end) {
3018:     advance_to(parse_context, begin);
3019:     internal::custom_formatter<Context> f(parse_context, context);
3020:     if (visit_format_arg(f, arg)) return parse_context.begin();
3021:     basic_format_specs<Char> specs;
3022:     using internal::specs_handler;
3023:     using parse_context_t = basic_format_parse_context<Char>;
3024:     internal::specs_checker<specs_handler<parse_context_t, Context>> handler(
3025:         specs_handler<parse_context_t, Context>(specs, parse_context, context),
3026:         arg.type());
3027:     begin = parse_format_specs(begin, end, handler);
3028:     if (begin == end || *begin != '}') on_error("missing '}' in format string");
3029:     advance_to(parse_context, begin);
3030:     context.advance_to(
3031:         visit_format_arg(ArgFormatter(context, &parse_context, &specs), arg));
3032:     return begin;
3033:   }
3034: 
3035:   basic_format_parse_context<Char> parse_context;
3036:   Context context;
3037:   basic_format_arg<Context> arg;
3038: };
