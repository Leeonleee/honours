1915: // Parses the range [begin, end) as an unsigned integer. This function assumes
1916: // that the range is non-empty and the first character is a digit.
1917: template <typename Char, typename ErrorHandler>
1918: FMT_CONSTEXPR int parse_nonnegative_int(const Char*& begin, const Char* end,
1919:                                         ErrorHandler&& eh) {
1920:   FMT_ASSERT(begin != end && '0' <= *begin && *begin <= '9', "");
1921:   if (*begin == '0') {
1922:     ++begin;
1923:     return 0;
1924:   }
1925:   unsigned value = 0;
1926:   // Convert to unsigned to prevent a warning.
1927:   constexpr unsigned max_int = max_value<int>();
1928:   unsigned big = max_int / 10;
1929:   do {
1930:     // Check for overflow.
1931:     if (value > big) {
1932:       value = max_int + 1;
1933:       break;
1934:     }
1935:     value = value * 10 + unsigned(*begin - '0');
1936:     ++begin;
1937:   } while (begin != end && '0' <= *begin && *begin <= '9');
1938:   if (value > max_int) eh.on_error("number is too big");
1939:   return static_cast<int>(value);
1940: }
