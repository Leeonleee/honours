223: void Binder::BindLogicalType(LogicalType &type, optional_ptr<Catalog> catalog, const string &schema) {
224: 	if (type.id() == LogicalTypeId::LIST || type.id() == LogicalTypeId::MAP) {
225: 		auto child_type = ListType::GetChildType(type);
226: 		BindLogicalType(child_type, catalog, schema);
227: 		auto alias = type.GetAlias();
228: 		auto modifiers = type.GetModifiersCopy();
229: 		if (type.id() == LogicalTypeId::LIST) {
230: 			type = LogicalType::LIST(child_type);
231: 		} else {
232: 			D_ASSERT(child_type.id() == LogicalTypeId::STRUCT); // map must be list of structs
233: 			type = LogicalType::MAP(child_type);
234: 		}
235: 
236: 		type.SetAlias(alias);
237: 		type.SetModifiers(modifiers);
238: 	} else if (type.id() == LogicalTypeId::STRUCT) {
239: 		auto child_types = StructType::GetChildTypes(type);
240: 		for (auto &child_type : child_types) {
241: 			BindLogicalType(child_type.second, catalog, schema);
242: 		}
243: 		// Generate new Struct Type
244: 		auto alias = type.GetAlias();
245: 		auto modifiers = type.GetModifiersCopy();
246: 		type = LogicalType::STRUCT(child_types);
247: 		type.SetAlias(alias);
248: 		type.SetModifiers(modifiers);
249: 	} else if (type.id() == LogicalTypeId::ARRAY) {
250: 		auto child_type = ArrayType::GetChildType(type);
251: 		auto array_size = ArrayType::GetSize(type);
252: 		BindLogicalType(child_type, catalog, schema);
253: 		auto alias = type.GetAlias();
254: 		auto modifiers = type.GetModifiersCopy();
255: 		type = LogicalType::ARRAY(child_type, array_size);
256: 		type.SetAlias(alias);
257: 		type.SetModifiers(modifiers);
258: 	} else if (type.id() == LogicalTypeId::UNION) {
259: 		auto member_types = UnionType::CopyMemberTypes(type);
260: 		for (auto &member_type : member_types) {
261: 			BindLogicalType(member_type.second, catalog, schema);
262: 		}
263: 		// Generate new Union Type
264: 		auto alias = type.GetAlias();
265: 		auto modifiers = type.GetModifiersCopy();
266: 		type = LogicalType::UNION(member_types);
267: 		type.SetAlias(alias);
268: 		type.SetModifiers(modifiers);
269: 	} else if (type.id() == LogicalTypeId::USER) {
270: 		auto user_type_name = UserType::GetTypeName(type);
271: 		auto user_type_mods = UserType::GetTypeModifiers(type);
272: 
273: 		bind_type_modifiers_function_t user_bind_modifiers_func = nullptr;
274: 
275: 		if (catalog) {
276: 			// The search order is:
277: 			// 1) In the same schema as the table
278: 			// 2) In the same catalog
279: 			// 3) System catalog
280: 			auto entry = entry_retriever.GetEntry(CatalogType::TYPE_ENTRY, *catalog, schema, user_type_name,
281: 			                                      OnEntryNotFound::RETURN_NULL);
282: 			if (!entry || entry->Cast<TypeCatalogEntry>().user_type.id() == LogicalTypeId::INVALID) {
283: 				entry = entry_retriever.GetEntry(CatalogType::TYPE_ENTRY, *catalog, INVALID_SCHEMA, user_type_name,
284: 				                                 OnEntryNotFound::RETURN_NULL);
285: 				if (!entry || entry->Cast<TypeCatalogEntry>().user_type.id() == LogicalTypeId::INVALID) {
286: 					entry = entry_retriever.GetEntry(CatalogType::TYPE_ENTRY, INVALID_CATALOG, INVALID_SCHEMA,
287: 					                                 user_type_name, OnEntryNotFound::THROW_EXCEPTION);
288: 				}
289: 			}
290: 			auto &type_entry = entry->Cast<TypeCatalogEntry>();
291: 			type = type_entry.user_type;
292: 			user_bind_modifiers_func = type_entry.bind_modifiers;
293: 		} else {
294: 			string type_catalog = UserType::GetCatalog(type);
295: 			string type_schema = UserType::GetSchema(type);
296: 
297: 			BindSchemaOrCatalog(context, type_catalog, type_schema);
298: 			auto entry = entry_retriever.GetEntry(CatalogType::TYPE_ENTRY, type_catalog, type_schema, user_type_name);
299: 			auto &type_entry = entry->Cast<TypeCatalogEntry>();
300: 			type = type_entry.user_type;
301: 			user_bind_modifiers_func = type_entry.bind_modifiers;
302: 		}
303: 
304: 		BindLogicalType(type, catalog, schema);
305: 
306: 		// Apply the type modifiers (if any)
307: 		if (user_bind_modifiers_func) {
308: 			// If an explicit bind_modifiers function was provided, use that to set the type modifier
309: 			BindTypeModifiersInput input {context, type, user_type_mods};
310: 			type = user_bind_modifiers_func(input);
311: 		} else if (type.HasModifiers()) {
312: 			// If the type already has modifiers, try to replace them with the user-provided ones if they are compatible
313: 			// This enables registering custom types with "default" type modifiers that can be overridden, without
314: 			// having to provide a custom bind_modifiers function
315: 			auto type_mods_size = type.GetModifiers()->size();
316: 
317: 			// Are we trying to pass more type modifiers than the type has?
318: 			if (user_type_mods.size() > type_mods_size) {
319: 				throw BinderException(
320: 				    "Cannot apply '%d' type modifier(s) to type '%s' taking at most '%d' type modifier(s)",
321: 				    user_type_mods.size(), user_type_name, type_mods_size);
322: 			}
323: 
324: 			// Deep copy the type so that we can replace the type modifiers
325: 			type = type.DeepCopy();
326: 
327: 			// Re-fetch the type modifiers now that we've deduplicated the ExtraTypeInfo
328: 			auto &type_mods = *type.GetModifiers();
329: 
330: 			// Replace them in order, casting if necessary
331: 			for (idx_t i = 0; i < MinValue(type_mods.size(), user_type_mods.size()); i++) {
332: 				auto &type_mod = type_mods[i];
333: 				auto user_type_mod = user_type_mods[i];
334: 				if (type_mod.type() == user_type_mod.type()) {
335: 					type_mod = std::move(user_type_mod);
336: 				} else if (user_type_mod.DefaultTryCastAs(type_mod.type())) {
337: 					type_mod = std::move(user_type_mod);
338: 				} else {
339: 					throw BinderException("Cannot apply type modifier '%s' to type '%s', expected value of type '%s'",
340: 					                      user_type_mod.ToString(), user_type_name, type_mod.type().ToString());
341: 				}
342: 			}
343: 		} else if (!user_type_mods.empty()) {
344: 			// We're trying to pass type modifiers to a type that doesnt have any
345: 			throw BinderException("Type '%s' does not take any type modifiers", user_type_name);
346: 		}
347: 	}
348: }
