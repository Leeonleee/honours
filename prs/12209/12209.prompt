371: idx_t DBConfig::ParseMemoryLimit(const string &arg) {
372: 	if (arg[0] == '-' || arg == "null" || arg == "none") {
373: 		// infinite
374: 		return NumericLimits<idx_t>::Maximum();
375: 	}
376: 	// split based on the number/non-number
377: 	idx_t idx = 0;
378: 	while (StringUtil::CharacterIsSpace(arg[idx])) {
379: 		idx++;
380: 	}
381: 	idx_t num_start = idx;
382: 	while ((arg[idx] >= '0' && arg[idx] <= '9') || arg[idx] == '.' || arg[idx] == 'e' || arg[idx] == 'E' ||
383: 	       arg[idx] == '-') {
384: 		idx++;
385: 	}
386: 	if (idx == num_start) {
387: 		throw ParserException("Memory limit must have a number (e.g. SET memory_limit=1GB");
388: 	}
389: 	string number = arg.substr(num_start, idx - num_start);
390: 
391: 	// try to parse the number
392: 	double limit = Cast::Operation<string_t, double>(string_t(number));
393: 
394: 	// now parse the memory limit unit (e.g. bytes, gb, etc)
395: 	while (StringUtil::CharacterIsSpace(arg[idx])) {
396: 		idx++;
397: 	}
398: 	idx_t start = idx;
399: 	while (idx < arg.size() && !StringUtil::CharacterIsSpace(arg[idx])) {
400: 		idx++;
401: 	}
402: 	if (limit < 0) {
403: 		// limit < 0, set limit to infinite
404: 		return (idx_t)-1;
405: 	}
406: 	string unit = StringUtil::Lower(arg.substr(start, idx - start));
407: 	idx_t multiplier;
408: 	if (unit == "byte" || unit == "bytes" || unit == "b") {
409: 		multiplier = 1;
410: 	} else if (unit == "kilobyte" || unit == "kilobytes" || unit == "kb" || unit == "k") {
411: 		multiplier = 1000LL;
412: 	} else if (unit == "megabyte" || unit == "megabytes" || unit == "mb" || unit == "m") {
413: 		multiplier = 1000LL * 1000LL;
414: 	} else if (unit == "gigabyte" || unit == "gigabytes" || unit == "gb" || unit == "g") {
415: 		multiplier = 1000LL * 1000LL * 1000LL;
416: 	} else if (unit == "terabyte" || unit == "terabytes" || unit == "tb" || unit == "t") {
417: 		multiplier = 1000LL * 1000LL * 1000LL * 1000LL;
418: 	} else if (unit == "kib") {
419: 		multiplier = 1024LL;
420: 	} else if (unit == "mib") {
421: 		multiplier = 1024LL * 1024LL;
422: 	} else if (unit == "gib") {
423: 		multiplier = 1024LL * 1024LL * 1024LL;
424: 	} else if (unit == "tib") {
425: 		multiplier = 1024LL * 1024LL * 1024LL * 1024LL;
426: 	} else {
427: 		throw ParserException("Unknown unit for memory_limit: %s (expected: KB, MB, GB, TB for 1000^i units or KiB, "
428: 		                      "MiB, GiB, TiB for 1024^i unites)");
429: 	}
430: 	return NumericCast<idx_t>(multiplier * limit);
431: }
