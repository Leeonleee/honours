470: static bool AnyConstraintReferencesGeneratedColumn(CreateTableInfo &table_info) {
471: 	unordered_set<string> generated_columns;
472: 	for (auto &col : table_info.columns.Logical()) {
473: 		if (!col.Generated()) {
474: 			continue;
475: 		}
476: 		generated_columns.insert(col.Name());
477: 	}
478: 	if (generated_columns.empty()) {
479: 		return false;
480: 	}
481: 
482: 	for (auto &constr : table_info.constraints) {
483: 		switch (constr->type) {
484: 		case ConstraintType::CHECK: {
485: 			auto &constraint = constr->Cast<CheckConstraint>();
486: 			auto &expr = constraint.expression;
487: 			bool contains_generated_column = false;
488: 			ExpressionContainsGeneratedColumn(*expr, generated_columns, contains_generated_column);
489: 			if (contains_generated_column) {
490: 				return true;
491: 			}
492: 			break;
493: 		}
494: 		case ConstraintType::NOT_NULL: {
495: 			auto &constraint = constr->Cast<NotNullConstraint>();
496: 			if (table_info.columns.GetColumn(constraint.index).Generated()) {
497: 				return true;
498: 			}
499: 			break;
500: 		}
501: 		case ConstraintType::UNIQUE: {
502: 			auto &constraint = constr->Cast<UniqueConstraint>();
503: 			if (!constraint.HasIndex()) {
504: 				for (auto &col : constraint.GetColumnNames()) {
505: 					if (generated_columns.count(col)) {
506: 						return true;
507: 					}
508: 				}
509: 			} else {
510: 				if (table_info.columns.GetColumn(constraint.GetIndex()).Generated()) {
511: 					return true;
512: 				}
513: 			}
514: 			break;
515: 		}
516: 		case ConstraintType::FOREIGN_KEY: {
517: 			// If it contained a generated column, an exception would have been thrown inside AddDataTableIndex earlier
518: 			break;
519: 		}
520: 		default: {
521: 			throw NotImplementedException("ConstraintType not implemented");
522: 		}
523: 		}
524: 	}
525: 	return false;
526: }
