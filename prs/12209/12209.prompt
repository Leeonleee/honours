861: void ForceCompressionSetting::SetGlobal(DatabaseInstance *db, DBConfig &config, const Value &input) {
862: 	auto compression = StringUtil::Lower(input.ToString());
863: 	if (compression == "none" || compression == "auto") {
864: 		config.options.force_compression = CompressionType::COMPRESSION_AUTO;
865: 	} else {
866: 		auto compression_type = CompressionTypeFromString(compression);
867: 		if (CompressionTypeIsDeprecated(compression_type)) {
868: 			throw ParserException("Attempted to force a deprecated compression type (%s)",
869: 			                      CompressionTypeToString(compression_type));
870: 		}
871: 		if (compression_type == CompressionType::COMPRESSION_AUTO) {
872: 			auto compression_types = StringUtil::Join(ListCompressionTypes(), ", ");
873: 			throw ParserException("Unrecognized option for PRAGMA force_compression, expected %s", compression_types);
874: 		}
875: 		config.options.force_compression = compression_type;
876: 	}
877: }
