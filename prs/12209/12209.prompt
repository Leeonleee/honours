423: static void FindForeignKeyIndexes(const ColumnList &columns, const vector<string> &names,
424:                                   vector<PhysicalIndex> &indexes) {
425: 	D_ASSERT(indexes.empty());
426: 	D_ASSERT(!names.empty());
427: 	for (auto &name : names) {
428: 		if (!columns.ColumnExists(name)) {
429: 			throw BinderException("column \"%s\" named in key does not exist", name);
430: 		}
431: 		auto &column = columns.GetColumn(name);
432: 		if (column.Generated()) {
433: 			throw BinderException("Failed to create foreign key: referenced column \"%s\" is a generated column",
434: 			                      column.Name());
435: 		}
436: 		indexes.push_back(column.Physical());
437: 	}
438: }
