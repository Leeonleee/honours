1041: //===--------------------------------------------------------------------===//
1042: // Maximum Temp Directory Size
1043: //===--------------------------------------------------------------------===//
1044: void MaximumTempDirectorySize::SetGlobal(DatabaseInstance *db, DBConfig &config, const Value &input) {
1045: 	auto maximum_swap_space = DBConfig::ParseMemoryLimit(input.ToString());
1046: 	if (maximum_swap_space == DConstants::INVALID_INDEX) {
1047: 		// We use INVALID_INDEX to indicate that the value is not set by the user
1048: 		// use one lower to indicate 'unlimited'
1049: 		maximum_swap_space--;
1050: 	}
1051: 	if (!db) {
1052: 		config.options.maximum_swap_space = maximum_swap_space;
1053: 		return;
1054: 	}
1055: 	auto &buffer_manager = BufferManager::GetBufferManager(*db);
1056: 	buffer_manager.SetSwapLimit(maximum_swap_space);
1057: 	config.options.maximum_swap_space = maximum_swap_space;
1058: }
1059: 
1060: void MaximumTempDirectorySize::ResetGlobal(DatabaseInstance *db, DBConfig &config) {
1061: 	config.options.maximum_swap_space = DConstants::INVALID_INDEX;
1062: 	if (!db) {
1063: 		return;
1064: 	}
1065: 	auto &buffer_manager = BufferManager::GetBufferManager(*db);
1066: 	buffer_manager.SetSwapLimit();
1067: }
1068: 
1069: Value MaximumTempDirectorySize::GetSetting(const ClientContext &context) {
1070: 	auto &config = DBConfig::GetConfig(context);
1071: 	if (config.options.maximum_swap_space != DConstants::INVALID_INDEX) {
1072: 		// Explicitly set by the user
1073: 		return Value(StringUtil::BytesToHumanReadableString(config.options.maximum_swap_space));
1074: 	}
1075: 	auto &buffer_manager = BufferManager::GetBufferManager(context);
1076: 	// Database is initialized, use the setting from the temporary directory
1077: 	auto max_swap = buffer_manager.GetMaxSwap();
1078: 	if (max_swap.IsValid()) {
1079: 		return Value(StringUtil::BytesToHumanReadableString(max_swap.GetIndex()));
1080: 	} else {
1081: 		// The temp directory has not been used yet
1082: 		return Value(StringUtil::BytesToHumanReadableString(0));
1083: 	}
1084: }
