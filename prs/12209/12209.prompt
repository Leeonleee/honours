957: void LogQueryPathSetting::SetLocal(ClientContext &context, const Value &input) {
958: 	auto &client_data = ClientData::Get(context);
959: 	auto path = input.ToString();
960: 	if (path.empty()) {
961: 		// empty path: clean up query writer
962: 		client_data.log_query_writer = nullptr;
