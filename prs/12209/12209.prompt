453: void ExpressionContainsGeneratedColumn(const ParsedExpression &expr, const unordered_set<string> &gcols,
454:                                        bool &contains_gcol) {
455: 	if (contains_gcol) {
456: 		return;
457: 	}
458: 	if (expr.type == ExpressionType::COLUMN_REF) {
459: 		auto &column_ref = expr.Cast<ColumnRefExpression>();
460: 		auto &name = column_ref.GetColumnName();
461: 		if (gcols.count(name)) {
462: 			contains_gcol = true;
463: 			return;
464: 		}
465: 	}
466: 	ParsedExpressionIterator::EnumerateChildren(
467: 	    expr, [&](const ParsedExpression &child) { ExpressionContainsGeneratedColumn(child, gcols, contains_gcol); });
468: }
