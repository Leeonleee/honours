357: idx_t DBConfig::GetSystemMaxThreads(FileSystem &fs) {
358: #ifndef DUCKDB_NO_THREADS
359: 	idx_t physical_cores = std::thread::hardware_concurrency();
360: #ifdef __linux__
361: 	auto cores_available_per_period = CGroupBandwidthQuota(physical_cores, fs);
362: 	return MaxValue<idx_t>(cores_available_per_period, 1);
363: #else
364: 	return physical_cores;
365: #endif
366: #else
367: 	return 1;
368: #endif
369: }
