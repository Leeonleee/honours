611: void EnableProfilingSetting::SetLocal(ClientContext &context, const Value &input) {
612: 	auto parameter = StringUtil::Lower(input.ToString());
613: 
614: 	auto &config = ClientConfig::GetConfig(context);
615: 	if (parameter == "json") {
616: 		config.profiler_print_format = ProfilerPrintFormat::JSON;
617: 	} else if (parameter == "query_tree") {
618: 		config.profiler_print_format = ProfilerPrintFormat::QUERY_TREE;
619: 	} else if (parameter == "query_tree_optimizer") {
620: 		config.profiler_print_format = ProfilerPrintFormat::QUERY_TREE_OPTIMIZER;
621: 	} else {
622: 		throw ParserException(
623: 		    "Unrecognized print format %s, supported formats: [json, query_tree, query_tree_optimizer]", parameter);
624: 	}
625: 	config.enable_profiler = true;
626: 	config.emit_profiler_output = true;
627: }
628: 
629: Value EnableProfilingSetting::GetSetting(const ClientContext &context) {
630: 	auto &config = ClientConfig::GetConfig(context);
631: 	if (!config.enable_profiler) {
632: 		return Value();
633: 	}
634: 	switch (config.profiler_print_format) {
635: 	case ProfilerPrintFormat::JSON:
636: 		return Value("json");
637: 	case ProfilerPrintFormat::QUERY_TREE:
638: 		return Value("query_tree");
639: 	case ProfilerPrintFormat::QUERY_TREE_OPTIMIZER:
640: 		return Value("query_tree_optimizer");
641: 	default:
642: 		throw InternalException("Unsupported profiler print format");
643: 	}
644: }
