308: idx_t CGroupBandwidthQuota(idx_t physical_cores, FileSystem &fs) {
309: 	static constexpr const char *CPU_MAX = "/sys/fs/cgroup/cpu.max";
310: 	static constexpr const char *CFS_QUOTA = "/sys/fs/cgroup/cpu/cpu.cfs_quota_us";
311: 	static constexpr const char *CFS_PERIOD = "/sys/fs/cgroup/cpu/cpu.cfs_period_us";
312: 
313: 	int64_t quota, period;
314: 	char byte_buffer[1000];
315: 	unique_ptr<FileHandle> handle;
316: 	int64_t read_bytes;
317: 
318: 	if (fs.FileExists(CPU_MAX)) {
319: 		// cgroup v2
320: 		// https://www.kernel.org/doc/html/latest/admin-guide/cgroup-v2.html
321: 		handle = fs.OpenFile(CPU_MAX, FileFlags::FILE_FLAGS_READ);
322: 		read_bytes = fs.Read(*handle, (void *)byte_buffer, 999);
323: 		byte_buffer[read_bytes] = '\0';
324: 		if (std::sscanf(byte_buffer, "%" SCNd64 " %" SCNd64 "", &quota, &period) != 2) {
325: 			return physical_cores;
326: 		}
327: 	} else if (fs.FileExists(CFS_QUOTA) && fs.FileExists(CFS_PERIOD)) {
328: 		// cgroup v1
329: 		// https://www.kernel.org/doc/html/latest/scheduler/sched-bwc.html#management
330: 
331: 		// Read the quota, this indicates how many microseconds the CPU can be utilized by this cgroup per period
332: 		handle = fs.OpenFile(CFS_QUOTA, FileFlags::FILE_FLAGS_READ);
333: 		read_bytes = fs.Read(*handle, (void *)byte_buffer, 999);
334: 		byte_buffer[read_bytes] = '\0';
335: 		if (std::sscanf(byte_buffer, "%" SCNd64 "", &quota) != 1) {
336: 			return physical_cores;
337: 		}
338: 
339: 		// Read the time period, a cgroup can utilize the CPU up to quota microseconds every period
340: 		handle = fs.OpenFile(CFS_PERIOD, FileFlags::FILE_FLAGS_READ);
341: 		read_bytes = fs.Read(*handle, (void *)byte_buffer, 999);
342: 		byte_buffer[read_bytes] = '\0';
343: 		if (std::sscanf(byte_buffer, "%" SCNd64 "", &period) != 1) {
344: 			return physical_cores;
345: 		}
346: 	} else {
347: 		// No cgroup quota
348: 		return physical_cores;
349: 	}
350: 	if (quota > 0 && period > 0) {
351: 		return idx_t(std::ceil((double)quota / (double)period));
352: 	} else {
353: 		return physical_cores;
354: 	}
355: }
