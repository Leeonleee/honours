1259: void ProfilingModeSetting::SetLocal(ClientContext &context, const Value &input) {
1260: 	auto parameter = StringUtil::Lower(input.ToString());
1261: 	auto &config = ClientConfig::GetConfig(context);
1262: 	if (parameter == "standard") {
1263: 		config.enable_profiler = true;
1264: 		config.enable_detailed_profiling = false;
1265: 		config.emit_profiler_output = true;
1266: 	} else if (parameter == "detailed") {
1267: 		config.enable_profiler = true;
1268: 		config.enable_detailed_profiling = true;
1269: 		config.emit_profiler_output = true;
1270: 	} else {
1271: 		throw ParserException("Unrecognized profiling mode \"%s\", supported formats: [standard, detailed]", parameter);
1272: 	}
1273: }
