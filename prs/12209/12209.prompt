350: static void FindMatchingPrimaryKeyColumns(const ColumnList &columns, const vector<unique_ptr<Constraint>> &constraints,
351:                                           ForeignKeyConstraint &fk) {
352: 	// find the matching primary key constraint
353: 	bool found_constraint = false;
354: 	// if no columns are defined, we will automatically try to bind to the primary key
355: 	bool find_primary_key = fk.pk_columns.empty();
356: 	for (auto &constr : constraints) {
357: 		if (constr->type != ConstraintType::UNIQUE) {
358: 			continue;
359: 		}
360: 		auto &unique = constr->Cast<UniqueConstraint>();
361: 		if (find_primary_key && !unique.IsPrimaryKey()) {
362: 			continue;
363: 		}
364: 		found_constraint = true;
365: 
366: 		vector<string> pk_names;
367: 		if (unique.HasIndex()) {
368: 			pk_names.push_back(columns.GetColumn(LogicalIndex(unique.GetIndex())).Name());
369: 		} else {
370: 			pk_names = unique.GetColumnNames();
371: 		}
372: 		if (find_primary_key) {
373: 			// found matching primary key
374: 			if (pk_names.size() != fk.fk_columns.size()) {
375: 				auto pk_name_str = StringUtil::Join(pk_names, ",");
376: 				auto fk_name_str = StringUtil::Join(fk.fk_columns, ",");
377: 				throw BinderException(
378: 				    "Failed to create foreign key: number of referencing (%s) and referenced columns (%s) differ",
379: 				    fk_name_str, pk_name_str);
380: 			}
381: 			fk.pk_columns = pk_names;
382: 			return;
383: 		}
384: 		if (pk_names.size() != fk.fk_columns.size()) {
385: 			// the number of referencing and referenced columns for foreign keys must be the same
386: 			continue;
387: 		}
388: 		bool equals = true;
389: 		for (idx_t i = 0; i < fk.pk_columns.size(); i++) {
390: 			if (!StringUtil::CIEquals(fk.pk_columns[i], pk_names[i])) {
391: 				equals = false;
392: 				break;
393: 			}
394: 		}
395: 		if (!equals) {
396: 			continue;
397: 		}
398: 		// found match
399: 		return;
400: 	}
401: 	// no match found! examine why
402: 	if (!found_constraint) {
403: 		// no unique constraint or primary key
404: 		string search_term = find_primary_key ? "primary key" : "primary key or unique constraint";
405: 		throw BinderException("Failed to create foreign key: there is no %s for referenced table \"%s\"", search_term,
406: 		                      fk.info.table);
407: 	}
408: 	// check if all the columns exist
409: 	for (auto &name : fk.pk_columns) {
410: 		bool found = columns.ColumnExists(name);
411: 		if (!found) {
412: 			throw BinderException(
413: 			    "Failed to create foreign key: referenced table \"%s\" does not have a column named \"%s\"",
414: 			    fk.info.table, name);
415: 		}
416: 	}
417: 	auto fk_names = StringUtil::Join(fk.pk_columns, ",");
418: 	throw BinderException("Failed to create foreign key: referenced table \"%s\" does not have a primary key or unique "
419: 	                      "constraint on the columns %s",
420: 	                      fk.info.table, fk_names);
421: }
