528: unique_ptr<LogicalOperator> DuckCatalog::BindCreateIndex(Binder &binder, CreateStatement &stmt,
529:                                                          TableCatalogEntry &table, unique_ptr<LogicalOperator> plan) {
530: 	D_ASSERT(plan->type == LogicalOperatorType::LOGICAL_GET);
531: 	auto &base = stmt.info->Cast<CreateIndexInfo>();
532: 
533: 	auto &get = plan->Cast<LogicalGet>();
534: 	// bind the index expressions
535: 	IndexBinder index_binder(binder, binder.context);
536: 	auto &dependencies = base.dependencies;
537: 	auto &catalog = Catalog::GetCatalog(binder.context, base.catalog);
538: 	index_binder.SetCatalogLookupCallback([&dependencies, &catalog](CatalogEntry &entry) {
539: 		if (&catalog != &entry.ParentCatalog()) {
540: 			// Don't register any cross-catalog dependencies
541: 			return;
542: 		}
543: 		dependencies.AddDependency(entry);
544: 	});
545: 	vector<unique_ptr<Expression>> expressions;
546: 	expressions.reserve(base.expressions.size());
547: 	for (auto &expr : base.expressions) {
548: 		expressions.push_back(index_binder.Bind(expr));
549: 	}
550: 
551: 	auto create_index_info = unique_ptr_cast<CreateInfo, CreateIndexInfo>(std::move(stmt.info));
552: 	for (auto &column_id : get.column_ids) {
553: 		if (column_id == COLUMN_IDENTIFIER_ROW_ID) {
554: 			throw BinderException("Cannot create an index on the rowid!");
555: 		}
556: 		create_index_info->scan_types.push_back(get.returned_types[column_id]);
557: 	}
558: 	create_index_info->scan_types.emplace_back(LogicalType::ROW_TYPE);
559: 	create_index_info->names = get.names;
560: 	create_index_info->column_ids = get.column_ids;
561: 	auto &bind_data = get.bind_data->Cast<TableScanBindData>();
562: 	bind_data.is_create_index = true;
563: 	get.column_ids.push_back(COLUMN_IDENTIFIER_ROW_ID);
564: 
565: 	// the logical CREATE INDEX also needs all fields to scan the referenced table
566: 	auto result = make_uniq<LogicalCreateIndex>(std::move(create_index_info), std::move(expressions), table);
567: 	result->children.push_back(std::move(plan));
568: 	return std::move(result);
569: }
