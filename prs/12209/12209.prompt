232: void DBConfig::ResetOption(const string &name) {
233: 	lock_guard<mutex> l(config_lock);
234: 	auto extension_option = extension_parameters.find(name);
235: 	D_ASSERT(extension_option != extension_parameters.end());
236: 	auto &default_value = extension_option->second.default_value;
237: 	if (!default_value.IsNull()) {
238: 		// Default is not NULL, override the setting
239: 		options.set_variables[name] = default_value;
240: 	} else {
241: 		// Otherwise just remove it from the 'set_variables' map
242: 		options.set_variables.erase(name);
243: 	}
244: }
