184: void DBConfig::SetOptionByName(const string &name, const Value &value) {
185: 	auto option = DBConfig::GetOptionByName(name);
186: 	if (option) {
187: 		SetOption(*option, value);
188: 		return;
189: 	}
190: 
191: 	auto param = extension_parameters.find(name);
192: 	if (param != extension_parameters.end()) {
193: 		Value target_value = value.DefaultCastAs(param->second.type);
194: 		SetOption(name, std::move(target_value));
195: 	} else {
196: 		options.unrecognized_options[name] = value;
197: 	}
198: }
