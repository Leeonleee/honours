484: SerializationCompatibility SerializationCompatibility::FromString(const string &input) {
485: 	if (input.empty()) {
486: 		throw InvalidInputException("Version string can not be empty");
487: 	}
488: 
489: 	auto serialization_version = GetSerializationVersion(input.c_str());
490: 	if (!serialization_version.IsValid()) {
491: 		auto candidates = GetSerializationCandidates();
492: 		throw InvalidInputException("The version string '%s' is not a valid DuckDB version, valid options are: %s",
493: 		                            input, StringUtil::Join(candidates, ", "));
494: 	}
495: 	SerializationCompatibility result;
496: 	result.duckdb_version = input;
497: 	result.serialization_version = serialization_version.GetIndex();
498: 	result.manually_set = true;
499: 	return result;
500: }
