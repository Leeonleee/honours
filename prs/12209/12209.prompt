167: SchemaCatalogEntry &Binder::BindCreateFunctionInfo(CreateInfo &info) {
168: 	auto &base = info.Cast<CreateMacroInfo>();
169: 	auto &scalar_function = base.function->Cast<ScalarMacroFunction>();
170: 
171: 	if (scalar_function.expression->HasParameter()) {
172: 		throw BinderException("Parameter expressions within macro's are not supported!");
173: 	}
174: 
175: 	// create macro binding in order to bind the function
176: 	vector<LogicalType> dummy_types;
177: 	vector<string> dummy_names;
178: 	// positional parameters
179: 	for (auto &param_expr : base.function->parameters) {
180: 		auto param = param_expr->Cast<ColumnRefExpression>();
181: 		if (param.IsQualified()) {
182: 			throw BinderException("Invalid parameter name '%s': must be unqualified", param.ToString());
183: 		}
184: 		dummy_types.emplace_back(LogicalType::SQLNULL);
185: 		dummy_names.push_back(param.GetColumnName());
186: 	}
187: 	// default parameters
188: 	for (auto &entry : base.function->default_parameters) {
189: 		auto &val = entry.second->Cast<ConstantExpression>();
190: 		dummy_types.push_back(val.value.type());
191: 		dummy_names.push_back(entry.first);
192: 	}
193: 	auto this_macro_binding = make_uniq<DummyBinding>(dummy_types, dummy_names, base.name);
194: 	macro_binding = this_macro_binding.get();
195: 
196: 	// create a copy of the expression because we do not want to alter the original
197: 	auto expression = scalar_function.expression->Copy();
198: 	ExpressionBinder::QualifyColumnNames(*this, expression);
199: 
200: 	// bind it to verify the function was defined correctly
201: 	ErrorData error;
202: 	BoundSelectNode sel_node;
203: 	BoundGroupInformation group_info;
204: 	SelectBinder binder(*this, context, sel_node, group_info);
205: 	auto &dependencies = base.dependencies;
206: 	auto &catalog = Catalog::GetCatalog(context, info.catalog);
207: 	binder.SetCatalogLookupCallback([&dependencies, &catalog](CatalogEntry &entry) {
208: 		if (&catalog != &entry.ParentCatalog()) {
209: 			// Don't register any cross-catalog dependencies
210: 			return;
211: 		}
212: 		// Register any catalog entry required to bind the macro function
213: 		dependencies.AddDependency(entry);
214: 	});
215: 	error = binder.Bind(expression, 0, false);
216: 	if (error.HasError()) {
217: 		error.Throw();
218: 	}
219: 
220: 	return BindCreateSchema(info);
221: }
