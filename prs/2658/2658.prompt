15989: static int arListCommand(ArCommand *pAr){
15990:   const char *zSql = "SELECT %s FROM %s WHERE %s";
15991:   const char *azCols[] = {
15992:     "name",
15993:     "lsmode(mode), sz, datetime(mtime, 'unixepoch'), name"
15994:   };
15995: 
15996:   char *zWhere = 0;
15997:   sqlite3_stmt *pSql = 0;
15998:   int rc;
15999: 
16000:   rc = arCheckEntries(pAr);
16001:   arWhereClause(&rc, pAr, &zWhere);
16002: 
16003:   shellPreparePrintf(pAr->db, &rc, &pSql, zSql, azCols[pAr->bVerbose],
16004:                      pAr->zSrcTable, zWhere);
16005:   if( pAr->bDryRun ){
16006:     utf8_printf(pAr->p->out, "%s\n", sqlite3_sql(pSql));
16007:   }else{
16008:     while( rc==SQLITE_OK && SQLITE_ROW==sqlite3_step(pSql) ){
16009:       if( pAr->bVerbose ){
16010:         utf8_printf(pAr->p->out, "%s % 10d  %s  %s\n",
16011:             sqlite3_column_text(pSql, 0),
16012:             sqlite3_column_int(pSql, 1),
16013:             sqlite3_column_text(pSql, 2),
16014:             sqlite3_column_text(pSql, 3)
16015:         );
16016:       }else{
16017:         utf8_printf(pAr->p->out, "%s\n", sqlite3_column_text(pSql, 0));
16018:       }
16019:     }
16020:   }
16021:   shellFinalize(&rc, pSql);
16022:   sqlite3_free(zWhere);
16023:   return rc;
16024: }
