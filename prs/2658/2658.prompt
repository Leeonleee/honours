12581: /* Draw horizontal line N characters long using unicode box
12582: ** characters
12583: */
12584: static void print_box_line(FILE *out, int N){
12585:   const char zDash[] =
12586:       BOX_24 BOX_24 BOX_24 BOX_24 BOX_24 BOX_24 BOX_24 BOX_24 BOX_24 BOX_24
12587:       BOX_24 BOX_24 BOX_24 BOX_24 BOX_24 BOX_24 BOX_24 BOX_24 BOX_24 BOX_24;
12588:   const int nDash = sizeof(zDash) - 1;
12589:   N *= 3;
12590:   while( N>nDash ){
12591:     utf8_printf(out, zDash);
12592:     N -= nDash;
12593:   }
12594:   utf8_printf(out, "%.*s", N, zDash);
12595: }
