12033: /*
12034: ** Execute a query statement that will generate SQL output.  Print
12035: ** the result columns, comma-separated, on a line and then add a
12036: ** semicolon terminator to the end of that line.
12037: **
12038: ** If the number of columns is 1 and that column contains text "--"
12039: ** then write the semicolon on a separate line.  That way, if a
12040: ** "--" comment occurs at the end of the statement, the comment
12041: ** won't consume the semicolon terminator.
12042: */
12043: static int run_table_dump_query(
12044:   ShellState *p,           /* Query context */
12045:   const char *zSelect      /* SELECT statement to extract content */
12046: ){
12047:   sqlite3_stmt *pSelect;
12048:   int rc;
12049:   int nResult;
12050:   int i;
12051:   const char *z;
12052:   rc = sqlite3_prepare_v2(p->db, zSelect, -1, &pSelect, 0);
12053:   if( rc!=SQLITE_OK || !pSelect ){
12054:     utf8_printf(p->out, "/**** ERROR: (%d) %s *****/\n", rc,
12055:                 sqlite3_errmsg(p->db));
12056:     if( (rc&0xff)!=SQLITE_CORRUPT ) p->nErr++;
12057:     return rc;
12058:   }
12059:   rc = sqlite3_step(pSelect);
12060:   nResult = sqlite3_column_count(pSelect);
12061:   while( rc==SQLITE_ROW ){
12062:     z = (const char*)sqlite3_column_text(pSelect, 0);
12063:     utf8_printf(p->out, "%s", z);
12064:     for(i=1; i<nResult; i++){
12065:       utf8_printf(p->out, ",%s", sqlite3_column_text(pSelect, i));
12066:     }
12067:     if( z==0 ) z = "";
12068:     while( z[0] && (z[0]!='-' || z[1]!='-') ) z++;
12069:     if( z[0] ){
12070:       raw_printf(p->out, "\n;\n");
12071:     }else{
12072:       raw_printf(p->out, ";\n");
12073:     }
12074:     rc = sqlite3_step(pSelect);
12075:   }
12076:   rc = sqlite3_finalize(pSelect);
12077:   if( rc!=SQLITE_OK ){
12078:     utf8_printf(p->out, "/**** ERROR: (%d) %s *****/\n", rc,
12079:                 sqlite3_errmsg(p->db));
12080:     if( (rc&0xff)!=SQLITE_CORRUPT ) p->nErr++;
12081:   }
12082:   return rc;
12083: }
