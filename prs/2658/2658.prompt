105: unique_ptr<FileHandle> LocalFileSystem::OpenFile(const string &path, uint8_t flags, FileLockType lock_type,
106:                                                  FileCompressionType compression, FileOpener *opener) {
107: 	if (compression != FileCompressionType::UNCOMPRESSED) {
108: 		throw NotImplementedException("Unsupported compression type for default file system");
109: 	}
110: 
111: 	AssertValidFileFlags(flags);
112: 
113: 	int open_flags = 0;
114: 	int rc;
115: 	bool open_read = flags & FileFlags::FILE_FLAGS_READ;
116: 	bool open_write = flags & FileFlags::FILE_FLAGS_WRITE;
117: 	if (open_read && open_write) {
118: 		open_flags = O_RDWR;
119: 	} else if (open_read) {
120: 		open_flags = O_RDONLY;
121: 	} else if (open_write) {
122: 		open_flags = O_WRONLY;
123: 	} else {
124: 		throw InternalException("READ, WRITE or both should be specified when opening a file");
125: 	}
126: 	if (open_write) {
127: 		// need Read or Write
128: 		D_ASSERT(flags & FileFlags::FILE_FLAGS_WRITE);
129: 		open_flags |= O_CLOEXEC;
130: 		if (flags & FileFlags::FILE_FLAGS_FILE_CREATE) {
131: 			open_flags |= O_CREAT;
132: 		} else if (flags & FileFlags::FILE_FLAGS_FILE_CREATE_NEW) {
133: 			open_flags |= O_CREAT | O_TRUNC;
134: 		}
135: 		if (flags & FileFlags::FILE_FLAGS_APPEND) {
136: 			open_flags |= O_APPEND;
137: 		}
138: 	}
139: 	if (flags & FileFlags::FILE_FLAGS_DIRECT_IO) {
140: #if defined(__sun) && defined(__SVR4)
141: 		throw Exception("DIRECT_IO not supported on Solaris");
142: #endif
143: #if defined(__DARWIN__) || defined(__APPLE__) || defined(__OpenBSD__)
144: 		// OSX does not have O_DIRECT, instead we need to use fcntl afterwards to support direct IO
145: 		open_flags |= O_SYNC;
146: #else
147: 		open_flags |= O_DIRECT | O_SYNC;
148: #endif
149: 	}
150: 	int fd = open(path.c_str(), open_flags, 0666);
151: 	if (fd == -1) {
152: 		throw IOException("Cannot open file \"%s\": %s", path, strerror(errno));
153: 	}
154: 	// #if defined(__DARWIN__) || defined(__APPLE__)
155: 	// 	if (flags & FileFlags::FILE_FLAGS_DIRECT_IO) {
156: 	// 		// OSX requires fcntl for Direct IO
157: 	// 		rc = fcntl(fd, F_NOCACHE, 1);
158: 	// 		if (fd == -1) {
159: 	// 			throw IOException("Could not enable direct IO for file \"%s\": %s", path, strerror(errno));
160: 	// 		}
161: 	// 	}
162: 	// #endif
163: 	if (lock_type != FileLockType::NO_LOCK) {
164: 		// set lock on file
165: 		// but only if it is not an input/output stream
166: 		auto file_type = GetFileTypeInternal(fd);
167: 		if (file_type != FileType::FILE_TYPE_FIFO && file_type != FileType::FILE_TYPE_SOCKET) {
168: 			struct flock fl;
169: 			memset(&fl, 0, sizeof fl);
170: 			fl.l_type = lock_type == FileLockType::READ_LOCK ? F_RDLCK : F_WRLCK;
171: 			fl.l_whence = SEEK_SET;
172: 			fl.l_start = 0;
173: 			fl.l_len = 0;
174: 			rc = fcntl(fd, F_SETLK, &fl);
175: 			if (rc == -1) {
176: 				throw IOException("Could not set lock on file \"%s\": %s", path, strerror(errno));
177: 			}
178: 		}
179: 	}
180: 	return make_unique<UnixFileHandle>(*this, path, fd);
181: }
