809: static void GlobFiles(FileSystem &fs, const string &path, const string &glob, bool match_directory,
810:                       vector<string> &result, bool join_path) {
811: 	fs.ListFiles(path, [&](const string &fname, bool is_directory) {
812: 		if (is_directory != match_directory) {
813: 			return;
814: 		}
815: 		if (LikeFun::Glob(fname.c_str(), fname.size(), glob.c_str(), glob.size())) {
816: 			if (join_path) {
817: 				result.push_back(fs.JoinPath(path, fname));
818: 			} else {
819: 				result.push_back(fname);
820: 			}
821: 		}
822: 	});
823: }
