15087:   }
15088:   sqlite3_bind_text(pStmt, 1, zDb, -1, SQLITE_STATIC);
15089:   if( sqlite3_step(pStmt)==SQLITE_ROW
15090:    && sqlite3_column_bytes(pStmt,0)>100
15091:   ){
15092:     memcpy(aHdr, sqlite3_column_blob(pStmt,0), 100);
15093:     sqlite3_finalize(pStmt);
15094:   }else{
15095:     raw_printf(stderr, "unable to read database header\n");
