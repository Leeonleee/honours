20017: /*
20018: ** Read input from *in and process it.  If *in==0 then input
20019: ** is interactive - the user is typing it it.  Otherwise, input
20020: ** is coming from a file or device.  A prompt is issued and history
20021: ** is saved only if input is interactive.  An interrupt signal will
20022: ** cause this routine to exit immediately, unless input is interactive.
20023: **
20024: ** Return the number of errors.
20025: */
20026: static int process_input(ShellState *p){
20027:   char *zLine = 0;          /* A single input line */
20028:   char *zSql = 0;           /* Accumulated SQL text */
20029:   int nLine;                /* Length of current line */
20030:   int nSql = 0;             /* Bytes of zSql[] used */
20031:   int nAlloc = 0;           /* Allocated zSql[] space */
20032:   int nSqlPrior = 0;        /* Bytes of zSql[] used by prior line */
20033:   int rc;                   /* Error code */
20034:   int errCnt = 0;           /* Number of errors seen */
20035:   int startline = 0;        /* Line number for start of current input */
20036:   int numCtrlC = 0;
20037:   p->lineno = 0;
20038:   while( errCnt==0 || !bail_on_error || (p->in==0 && stdin_is_interactive) ){
20039:     fflush(p->out);
20040:     zLine = one_input_line(p->in, zLine, nSql>0);
20041:     if( zLine==0 ){
20042:       /* End of input */
20043:       if( p->in==0 && stdin_is_interactive ) printf("\n");
20044:       break;
20045:     }
20046:     if (*zLine == '\3') {
20047:       // ctrl c: reset sql statement
20048:       if (nSql == 0 && zLine[1] == '\0' && stdin_is_interactive) {
20049:         // if in interactive mode and we press ctrl c twice
20050:         // on an empty line, we exit
20051:         numCtrlC++;
20052:         if (numCtrlC >= 2) {
20053:           break;
20054:         }
20055:       }
20056:       nSql = 0;
20057:       continue;
20058:     } else {
20059:       numCtrlC = 0;
20060:     }
20061:     if( seenInterrupt ){
20062:       if( p->in!=0 ) break;
20063:       seenInterrupt = 0;
20064:     }
20065:     p->lineno++;
20066:     if( nSql==0 && _all_whitespace(zLine) ){
20067:       if( ShellHasFlag(p, SHFLG_Echo) ) printf("%s\n", zLine);
20068:       continue;
20069:     }
20070:     if( zLine && (zLine[0]=='.' || zLine[0]=='#') && nSql==0 ){
20071:       if( ShellHasFlag(p, SHFLG_Echo) ) printf("%s\n", zLine);
20072:       if( zLine[0]=='.' ){
20073:         rc = do_meta_command(zLine, p);
20074:         if( rc==2 ){ /* exit requested */
20075:           break;
20076:         }else if( rc ){
20077:           errCnt++;
20078:         }
20079:       }
20080:       continue;
20081:     }
20082:     if( line_is_command_terminator(zLine) && line_is_complete(zSql, nSql) ){
20083:       memcpy(zLine,";",2);
20084:     }
20085:     nLine = strlen30(zLine);
20086:     if( nSql+nLine+2>=nAlloc ){
20087:       nAlloc = nSql+nLine+100;
20088:       zSql = realloc(zSql, nAlloc);
20089:       if( zSql==0 ) shell_out_of_memory();
20090:     }
20091:     nSqlPrior = nSql;
20092:     if( nSql==0 ){
20093:       int i;
20094:       for(i=0; zLine[i] && IsSpace(zLine[i]); i++){}
20095:       assert( nAlloc>0 && zSql!=0 );
20096:       memcpy(zSql, zLine+i, nLine+1-i);
20097:       startline = p->lineno;
20098:       nSql = nLine-i;
20099:     }else{
20100:       zSql[nSql++] = '\n';
20101:       memcpy(zSql+nSql, zLine, nLine+1);
20102:       nSql += nLine;
20103:     }
20104:     if( nSql && line_contains_semicolon(&zSql[nSqlPrior], nSql-nSqlPrior)
20105:                 && sqlite3_complete(zSql) ){
20106:       errCnt += runOneSqlLine(p, zSql, p->in, startline);
20107:       nSql = 0;
20108:       if( p->outCount ){
20109:         output_reset(p);
20110:         p->outCount = 0;
20111:       }else{
20112:         clearTempFile(p);
20113:       }
20114:     }else if( nSql && _all_whitespace(zSql) ){
20115:       if( ShellHasFlag(p, SHFLG_Echo) ) printf("%s\n", zSql);
20116:       nSql = 0;
20117:     }
20118:   }
20119:   if( nSql && !_all_whitespace(zSql) ){
20120:     errCnt += runOneSqlLine(p, zSql, p->in, startline);
20121:   }
20122:   free(zSql);
20123:   free(zLine);
20124:   return errCnt>0;
20125: }
