79: void LogicalOperator::Verify() {
80: #ifdef DEBUG
81: 	// verify expressions
82: 	for (idx_t expr_idx = 0; expr_idx < expressions.size(); expr_idx++) {
83: 		// verify that we can (correctly) copy this expression
84: 		auto copy = expressions[expr_idx]->Copy();
85: 		auto original_hash = expressions[expr_idx]->Hash();
86: 		auto copy_hash = copy->Hash();
87: 		// copy should be identical to original
88: 		D_ASSERT(expressions[expr_idx]->ToString() == copy->ToString());
89: 		D_ASSERT(original_hash == copy_hash);
90: 		D_ASSERT(Expression::Equals(expressions[expr_idx].get(), copy.get()));
91: 
92: 		D_ASSERT(!Expression::Equals(expressions[expr_idx].get(), nullptr));
93: 		for (idx_t other_idx = 0; other_idx < expr_idx; other_idx++) {
94: 			// comparison with other expressions
95: 			auto other_hash = expressions[other_idx]->Hash();
96: 			bool expr_equal = Expression::Equals(expressions[expr_idx].get(), expressions[other_idx].get());
97: 			if (original_hash != other_hash) {
98: 				// if the hashes are not equal the expressions should not be equal either
99: 				D_ASSERT(!expr_equal);
100: 			}
101: 		}
102: 	}
103: 	D_ASSERT(!ToString().empty());
104: 	for (auto &child : children) {
105: 		child->Verify();
106: 	}
107: #endif
108: }
