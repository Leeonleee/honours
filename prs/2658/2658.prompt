14531: static int sql_trace_callback(
14532:   unsigned mType,         /* The trace type */
14533:   void *pArg,             /* The ShellState pointer */
14534:   void *pP,               /* Usually a pointer to sqlite_stmt */
14535:   void *pX                /* Auxiliary output */
14536: ){
