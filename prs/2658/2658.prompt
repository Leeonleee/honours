15764: /*
15765: ** Parse the command line for an ".ar" command. The results are written into
15766: ** structure (*pAr). SQLITE_OK is returned if the command line is parsed
15767: ** successfully, otherwise an error message is written to stderr and
15768: ** SQLITE_ERROR returned.
15769: */
15770: static int arParseCommand(
15771:   char **azArg,                   /* Array of arguments passed to dot command */
15772:   int nArg,                       /* Number of entries in azArg[] */
15773:   ArCommand *pAr                  /* Populate this object */
15774: ){
15775:   struct ArSwitch {
15776:     const char *zLong;
15777:     char cShort;
15778:     u8 eSwitch;
15779:     u8 bArg;
15780:   } aSwitch[] = {
15781:     { "create",    'c', AR_CMD_CREATE,       0 },
15782:     { "extract",   'x', AR_CMD_EXTRACT,      0 },
15783:     { "insert",    'i', AR_CMD_INSERT,       0 },
15784:     { "list",      't', AR_CMD_LIST,         0 },
15785:     { "update",    'u', AR_CMD_UPDATE,       0 },
15786:     { "help",      'h', AR_CMD_HELP,         0 },
15787:     { "verbose",   'v', AR_SWITCH_VERBOSE,   0 },
15788:     { "file",      'f', AR_SWITCH_FILE,      1 },
15789:     { "append",    'a', AR_SWITCH_APPEND,    1 },
15790:     { "directory", 'C', AR_SWITCH_DIRECTORY, 1 },
15791:     { "dryrun",    'n', AR_SWITCH_DRYRUN,    0 },
15792:   };
15793:   int nSwitch = sizeof(aSwitch) / sizeof(struct ArSwitch);
15794:   struct ArSwitch *pEnd = &aSwitch[nSwitch];
15795: 
15796:   if( nArg<=1 ){
15797:     utf8_printf(stderr, "Wrong number of arguments.  Usage:\n");
15798:     return arUsage(stderr);
15799:   }else{
15800:     char *z = azArg[1];
15801:     if( z[0]!='-' ){
15802:       /* Traditional style [tar] invocation */
15803:       int i;
15804:       int iArg = 2;
15805:       for(i=0; z[i]; i++){
15806:         const char *zArg = 0;
15807:         struct ArSwitch *pOpt;
15808:         for(pOpt=&aSwitch[0]; pOpt<pEnd; pOpt++){
15809:           if( z[i]==pOpt->cShort ) break;
15810:         }
15811:         if( pOpt==pEnd ){
15812:           return arErrorMsg(pAr, "unrecognized option: %c", z[i]);
15813:         }
15814:         if( pOpt->bArg ){
15815:           if( iArg>=nArg ){
15816:             return arErrorMsg(pAr, "option requires an argument: %c",z[i]);
15817:           }
15818:           zArg = azArg[iArg++];
15819:         }
15820:         if( arProcessSwitch(pAr, pOpt->eSwitch, zArg) ) return SQLITE_ERROR;
15821:       }
15822:       pAr->nArg = nArg-iArg;
15823:       if( pAr->nArg>0 ){
15824:         pAr->azArg = &azArg[iArg];
15825:       }
15826:     }else{
15827:       /* Non-traditional invocation */
15828:       int iArg;
15829:       for(iArg=1; iArg<nArg; iArg++){
15830:         int n;
15831:         z = azArg[iArg];
15832:         if( z[0]!='-' ){
15833:           /* All remaining command line words are command arguments. */
15834:           pAr->azArg = &azArg[iArg];
15835:           pAr->nArg = nArg-iArg;
15836:           break;
15837:         }
15838:         n = strlen30(z);
15839: 
15840:         if( z[1]!='-' ){
15841:           int i;
15842:           /* One or more short options */
15843:           for(i=1; i<n; i++){
15844:             const char *zArg = 0;
15845:             struct ArSwitch *pOpt;
15846:             for(pOpt=&aSwitch[0]; pOpt<pEnd; pOpt++){
15847:               if( z[i]==pOpt->cShort ) break;
15848:             }
15849:             if( pOpt==pEnd ){
15850:               return arErrorMsg(pAr, "unrecognized option: %c", z[i]);
15851:             }
15852:             if( pOpt->bArg ){
15853:               if( i<(n-1) ){
15854:                 zArg = &z[i+1];
15855:                 i = n;
15856:               }else{
15857:                 if( iArg>=(nArg-1) ){
15858:                   return arErrorMsg(pAr, "option requires an argument: %c",
15859:                                     z[i]);
15860:                 }
15861:                 zArg = azArg[++iArg];
15862:               }
15863:             }
15864:             if( arProcessSwitch(pAr, pOpt->eSwitch, zArg) ) return SQLITE_ERROR;
15865:           }
15866:         }else if( z[2]=='\0' ){
15867:           /* A -- option, indicating that all remaining command line words
15868:           ** are command arguments.  */
15869:           pAr->azArg = &azArg[iArg+1];
15870:           pAr->nArg = nArg-iArg-1;
15871:           break;
15872:         }else{
15873:           /* A long option */
15874:           const char *zArg = 0;             /* Argument for option, if any */
15875:           struct ArSwitch *pMatch = 0;      /* Matching option */
15876:           struct ArSwitch *pOpt;            /* Iterator */
15877:           for(pOpt=&aSwitch[0]; pOpt<pEnd; pOpt++){
15878:             const char *zLong = pOpt->zLong;
15879:             if( (n-2)<=strlen30(zLong) && 0==memcmp(&z[2], zLong, n-2) ){
15880:               if( pMatch ){
15881:                 return arErrorMsg(pAr, "ambiguous option: %s",z);
15882:               }else{
15883:                 pMatch = pOpt;
15884:               }
15885:             }
15886:           }
15887: 
15888:           if( pMatch==0 ){
15889:             return arErrorMsg(pAr, "unrecognized option: %s", z);
15890:           }
15891:           if( pMatch->bArg ){
15892:             if( iArg>=(nArg-1) ){
15893:               return arErrorMsg(pAr, "option requires an argument: %s", z);
15894:             }
15895:             zArg = azArg[++iArg];
15896:           }
15897:           if( arProcessSwitch(pAr, pMatch->eSwitch, zArg) ) return SQLITE_ERROR;
15898:         }
15899:       }
15900:     }
15901:   }
15902: 
15903:   return SQLITE_OK;
15904: }
