3278: /* Allocates the stack if it does not exist.
3279:  *  Guarantees space for at least one push.
3280:  */
3281: static void yyensure_buffer_stack (yyscan_t yyscanner)
3282: {
3283: 	yy_size_t num_to_alloc;
3284:     struct yyguts_t * yyg = (struct yyguts_t*)yyscanner;
3285: 
3286: 	if (!yyg->yy_buffer_stack) {
3287: 
3288: 		/* First allocation is just for 2 elements, since we don't know if this
3289: 		 * scanner will even need a stack. We use 2 instead of 1 to avoid an
3290: 		 * immediate realloc on the next call.
3291:          */
3292:       num_to_alloc = 1; /* After all that talk, this was set to 1 anyways... */
3293: 		yyg->yy_buffer_stack = (struct yy_buffer_state**)yyalloc
3294: 								(num_to_alloc * sizeof(struct yy_buffer_state*)
3295: 								, yyscanner);
3296: 		if ( ! yyg->yy_buffer_stack )
3297: 			YY_FATAL_ERROR( "out of dynamic memory in yyensure_buffer_stack()" );
3298: 
3299: 		memset(yyg->yy_buffer_stack, 0, num_to_alloc * sizeof(struct yy_buffer_state*));
3300: 
3301: 		yyg->yy_buffer_stack_max = num_to_alloc;
3302: 		yyg->yy_buffer_stack_top = 0;
3303: 		return;
3304: 	}
3305: 
3306: 	if (yyg->yy_buffer_stack_top >= (yyg->yy_buffer_stack_max) - 1){
3307: 
3308: 		/* Increase the buffer to prepare for a possible push. */
3309: 		yy_size_t grow_size = 8 /* arbitrary grow size */;
3310: 
3311: 		num_to_alloc = yyg->yy_buffer_stack_max + grow_size;
3312: 		yyg->yy_buffer_stack = (struct yy_buffer_state**)yyrealloc
3313: 								(yyg->yy_buffer_stack,
3314: 								num_to_alloc * sizeof(struct yy_buffer_state*)
3315: 								, yyscanner);
3316: 		if ( ! yyg->yy_buffer_stack )
3317: 			YY_FATAL_ERROR( "out of dynamic memory in yyensure_buffer_stack()" );
3318: 
3319: 		/* zero only the new slots.*/
3320: 		memset(yyg->yy_buffer_stack + yyg->yy_buffer_stack_max, 0, grow_size * sizeof(struct yy_buffer_state*));
3321: 		yyg->yy_buffer_stack_max = num_to_alloc;
3322: 	}
3323: }
