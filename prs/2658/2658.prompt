15318: /*
15319: ** The implementation of SQL scalar function fkey_collate_clause(), used
15320: ** by the ".lint fkey-indexes" command. This scalar function is always
15321: ** called with four arguments - the parent table name, the parent column name,
15322: ** the child table name and the child column name.
15323: **
15324: **   fkey_collate_clause('parent-tab', 'parent-col', 'child-tab', 'child-col')
15325: **
15326: ** If either of the named tables or columns do not exist, this function
15327: ** returns an empty string. An empty string is also returned if both tables
15328: ** and columns exist but have the same default collation sequence. Or,
15329: ** if both exist but the default collation sequences are different, this
15330: ** function returns the string " COLLATE <parent-collation>", where
15331: ** <parent-collation> is the default collation sequence of the parent column.
15332: */
15333: static void shellFkeyCollateClause(
15334:   sqlite3_context *pCtx,
15335:   int nVal,
15336:   sqlite3_value **apVal
15337: ){
15338:   sqlite3 *db = sqlite3_context_db_handle(pCtx);
15339:   const char *zParent;
15340:   const char *zParentCol;
15341:   const char *zParentSeq;
15342:   const char *zChild;
15343:   const char *zChildCol;
15344:   const char *zChildSeq = 0;  /* Initialize to avoid false-positive warning */
15345:   int rc;
15346: 
15347:   assert( nVal==4 );
15348:   zParent = (const char*)sqlite3_value_text(apVal[0]);
15349:   zParentCol = (const char*)sqlite3_value_text(apVal[1]);
15350:   zChild = (const char*)sqlite3_value_text(apVal[2]);
15351:   zChildCol = (const char*)sqlite3_value_text(apVal[3]);
15352: 
15353:   sqlite3_result_text(pCtx, "", -1, SQLITE_STATIC);
15354:   rc = sqlite3_table_column_metadata(
15355:       db, "main", zParent, zParentCol, 0, &zParentSeq, 0, 0, 0
15356:   );
15357:   if( rc==SQLITE_OK ){
15358:     rc = sqlite3_table_column_metadata(
15359:         db, "main", zChild, zChildCol, 0, &zChildSeq, 0, 0, 0
15360:     );
15361:   }
15362: 
15363:   if( rc==SQLITE_OK && sqlite3_stricmp(zParentSeq, zChildSeq) ){
15364:     char *z = sqlite3_mprintf(" COLLATE %s", zParentSeq);
15365:     sqlite3_result_text(pCtx, z, -1, SQLITE_TRANSIENT);
15366:     sqlite3_free(z);
15367:   }
15368: }
