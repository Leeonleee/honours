14106: /*
14107: ** Scalar function "shell_escape_crnl" used by the .recover command.
14108: ** The argument passed to this function is the output of built-in
14109: ** function quote(). If the first character of the input is "'",
14110: ** indicating that the value passed to quote() was a text value,
14111: ** then this function searches the input for "\n" and "\r" characters
14112: ** and adds a wrapper similar to the following:
14113: **
14114: **   replace(replace(<input>, '\n', char(10), '\r', char(13));
14115: **
14116: ** Or, if the first character of the input is not "'", then a copy
14117: ** of the input is returned.
14118: */
14119: static void shellEscapeCrnl(
14120:   sqlite3_context *context,
14121:   int argc,
14122:   sqlite3_value **argv
14123: ){
14124:   const char *zText = (const char*)sqlite3_value_text(argv[0]);
14125:   UNUSED_PARAMETER(argc);
14126:   if( zText[0]=='\'' ){
14127:     int nText = sqlite3_value_bytes(argv[0]);
14128:     int i;
14129:     char zBuf1[20];
14130:     char zBuf2[20];
14131:     const char *zNL = 0;
14132:     const char *zCR = 0;
14133:     int nCR = 0;
14134:     int nNL = 0;
14135: 
14136:     for(i=0; zText[i]; i++){
14137:       if( zNL==0 && zText[i]=='\n' ){
14138:         zNL = unused_string(zText, "\\n", "\\012", zBuf1);
14139:         nNL = (int)strlen(zNL);
14140:       }
14141:       if( zCR==0 && zText[i]=='\r' ){
14142:         zCR = unused_string(zText, "\\r", "\\015", zBuf2);
14143:         nCR = (int)strlen(zCR);
14144:       }
14145:     }
14146: 
14147:     if( zNL || zCR ){
14148:       int iOut = 0;
14149:       i64 nMax = (nNL > nCR) ? nNL : nCR;
14150:       i64 nAlloc = nMax * nText + (nMax+64)*2;
14151:       char *zOut = (char*)sqlite3_malloc64(nAlloc);
14152:       if( zOut==0 ){
14153:         sqlite3_result_error_nomem(context);
14154:         return;
14155:       }
14156: 
14157:       if( zNL && zCR ){
14158:         memcpy(&zOut[iOut], "replace(replace(", 16);
14159:         iOut += 16;
14160:       }else{
14161:         memcpy(&zOut[iOut], "replace(", 8);
14162:         iOut += 8;
14163:       }
14164:       for(i=0; zText[i]; i++){
14165:         if( zText[i]=='\n' ){
14166:           memcpy(&zOut[iOut], zNL, nNL);
14167:           iOut += nNL;
14168:         }else if( zText[i]=='\r' ){
14169:           memcpy(&zOut[iOut], zCR, nCR);
14170:           iOut += nCR;
14171:         }else{
14172:           zOut[iOut] = zText[i];
14173:           iOut++;
14174:         }
14175:       }
14176: 
14177:       if( zNL ){
14178:         memcpy(&zOut[iOut], ",'", 2); iOut += 2;
14179:         memcpy(&zOut[iOut], zNL, nNL); iOut += nNL;
14180:         memcpy(&zOut[iOut], "', char(10))", 12); iOut += 12;
14181:       }
14182:       if( zCR ){
14183:         memcpy(&zOut[iOut], ",'", 2); iOut += 2;
14184:         memcpy(&zOut[iOut], zCR, nCR); iOut += nCR;
14185:         memcpy(&zOut[iOut], "', char(13))", 12); iOut += 12;
14186:       }
14187: 
14188:       sqlite3_result_text(context, zOut, iOut, SQLITE_TRANSIENT);
14189:       sqlite3_free(zOut);
14190:       return;
14191:     }
14192:   }
14193: 
14194:   sqlite3_result_value(context, argv[0]);
14195: }
