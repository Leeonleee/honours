7219: typedef struct ZipfileBuffer ZipfileBuffer;
7220: struct ZipfileBuffer {
7221:   u8 *a;                          /* Pointer to buffer */
7222:   int n;                          /* Size of buffer in bytes */
7223:   int nAlloc;                     /* Byte allocated at a[] */
7224: };
