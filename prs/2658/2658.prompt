536: int64_t LocalFileSystem::Read(FileHandle &handle, void *buffer, int64_t nr_bytes) {
537: 	HANDLE hFile = ((WindowsFileHandle &)handle).fd;
538: 	DWORD bytes_read;
539: 	auto &pos = ((WindowsFileHandle &)handle).position;
540: 	OVERLAPPED ov = {};
541: 	ov.Internal = 0;
542: 	ov.InternalHigh = 0;
543: 	ov.Offset = pos & 0xFFFFFFFF;
544: 	ov.OffsetHigh = pos >> 32;
545: 	ov.hEvent = 0;
546: 	auto n = std::min<idx_t>(std::max<idx_t>(GetFileSize(handle), pos) - pos, nr_bytes);
547: 	ReadFile(hFile, buffer, (DWORD)n, NULL, &ov);
548: 	auto rc = GetOverlappedResult(hFile, &ov, &bytes_read, true);
549: 	if (rc == 0) {
550: 		auto error = GetLastErrorAsString();
551: 		throw IOException("Could not read file \"%s\": %s", handle.path, error);
552: 	}
553: 	pos += bytes_read;
554: 	return bytes_read;
555: }
