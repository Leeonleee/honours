515: void LocalFileSystem::Read(FileHandle &handle, void *buffer, int64_t nr_bytes, idx_t location) {
516: 	HANDLE hFile = ((WindowsFileHandle &)handle).fd;
517: 	DWORD bytes_read;
518: 	OVERLAPPED ov = {};
519: 	ov.Internal = 0;
520: 	ov.InternalHigh = 0;
521: 	ov.Offset = location & 0xFFFFFFFF;
522: 	ov.OffsetHigh = location >> 32;
523: 	ov.hEvent = 0;
524: 	ReadFile(hFile, buffer, (DWORD)nr_bytes, NULL, &ov);
525: 	auto rc = GetOverlappedResult(hFile, &ov, &bytes_read, true);
526: 	if (rc == 0) {
527: 		auto error = GetLastErrorAsString();
528: 		throw IOException("Could not read file \"%s\": %s", handle.path, error);
529: 	}
530: 	if (bytes_read != nr_bytes) {
531: 		throw IOException("Could not read all bytes from file \"%s\": wanted=%lld read=%lld", handle.path, nr_bytes,
532: 		                  bytes_read);
533: 	}
534: }
