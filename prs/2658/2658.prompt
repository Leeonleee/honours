14972: /*
14973: ** Change the output file back to stdout.
14974: **
14975: ** If the p->doXdgOpen flag is set, that means the output was being
14976: ** redirected to a temporary file named by p->zTempFile.  In that case,
14977: ** launch start/open/xdg-open on that temporary file.
14978: */
14979: static void output_reset(ShellState *p){
14980:   if( p->outfile[0]=='|' ){
14981: #ifndef SQLITE_OMIT_POPEN
14982:     pclose(p->out);
14983: #endif
14984:   }else{
14985:     output_file_close(p->out);
14986: #ifndef SQLITE_NOHAVE_SYSTEM
14987:     if( p->doXdgOpen ){
14988:       const char *zXdgOpenCmd =
14989: #if defined(_WIN32)
14990:       "start";
14991: #elif defined(__APPLE__)
14992:       "open";
14993: #else
14994:       "xdg-open";
14995: #endif
14996:       char *zCmd;
14997:       zCmd = sqlite3_mprintf("%s %s", zXdgOpenCmd, p->zTempFile);
14998:       if( system(zCmd) ){
14999:         utf8_printf(stderr, "Failed: [%s]\n", zCmd);
15000:       }else{
15001:         /* Give the start/open/xdg-open command some time to get
15002:         ** going before we continue, and potential delete the
15003:         ** p->zTempFile data file out from under it */
15004:         sqlite3_sleep(2000);
15005:       }
15006:       sqlite3_free(zCmd);
15007:       outputModePop(p);
15008:       p->doXdgOpen = 0;
15009:     }
15010: #endif /* !defined(SQLITE_NOHAVE_SYSTEM) */
15011:   }
15012:   p->outfile[0] = 0;
15013:   p->out = stdout;
15014: }
