11490:                 pNext ? "|--" : "`--", z);
11491:     if( n<(int)sizeof(p->sGraph.zPrefix)-7 ){
11492:       memcpy(&p->sGraph.zPrefix[n], pNext ? "|  " : "   ", 4);
11493:       eqp_render_level(p, pRow->iEqpId);
11494:       p->sGraph.zPrefix[n] = 0;
11495:     }
11496:   }
