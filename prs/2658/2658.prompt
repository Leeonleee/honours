626: /*
627: ** Compute a string length that is limited to what can be stored in
628: ** lower 30 bits of a 32-bit signed integer.
629: */
630: static int strlen30(const char *z){
631:   const char *z2 = z;
632:   while( *z2 ){ z2++; }
633:   return 0x3fffffff & (int)(z2 - z);
634: }
