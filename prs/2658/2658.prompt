15950: /*
15951: ** Format a WHERE clause that can be used against the "sqlar" table to
15952: ** identify all archive members that match the command arguments held
15953: ** in (*pAr). Leave this WHERE clause in (*pzWhere) before returning.
15954: ** The caller is responsible for eventually calling sqlite3_free() on
15955: ** any non-NULL (*pzWhere) value.
15956: */
15957: static void arWhereClause(
15958:   int *pRc,
15959:   ArCommand *pAr,
15960:   char **pzWhere                  /* OUT: New WHERE clause */
15961: ){
15962:   char *zWhere = 0;
15963:   if( *pRc==SQLITE_OK ){
15964:     if( pAr->nArg==0 ){
15965:       zWhere = sqlite3_mprintf("1");
15966:     }else{
15967:       int i;
15968:       const char *zSep = "";
15969:       for(i=0; i<pAr->nArg; i++){
15970:         const char *z = pAr->azArg[i];
15971:         zWhere = sqlite3_mprintf(
15972:           "%z%s name = '%q' OR substr(name,1,%d) = '%q/'",
15973:           zWhere, zSep, z, strlen30(z)+1, z
15974:         );
15975:         if( zWhere==0 ){
15976:           *pRc = SQLITE_NOMEM;
15977:           break;
15978:         }
15979:         zSep = " OR ";
15980:       }
15981:     }
15982:   }
15983:   *pzWhere = zWhere;
15984: }
