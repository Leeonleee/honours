16027: /*
16028: ** Implementation of .ar "eXtract" command.
16029: */
16030: static int arExtractCommand(ArCommand *pAr){
16031:   const char *zSql1 =
16032:     "SELECT "
16033:     " ($dir || name),"
16034:     " writefile(($dir || name), %s, mode, mtime) "
16035:     "FROM %s WHERE (%s) AND (data IS NULL OR $dirOnly = 0)"
16036:     " AND name NOT GLOB '*..[/\\]*'";
16037: 
16038:   const char *azExtraArg[] = {
16039:     "sqlar_uncompress(data, sz)",
16040:     "data"
16041:   };
16042: 
16043:   sqlite3_stmt *pSql = 0;
16044:   int rc = SQLITE_OK;
16045:   char *zDir = 0;
16046:   char *zWhere = 0;
16047:   int i, j;
16048: 
16049:   /* If arguments are specified, check that they actually exist within
16050:   ** the archive before proceeding. And formulate a WHERE clause to
16051:   ** match them.  */
16052:   rc = arCheckEntries(pAr);
16053:   arWhereClause(&rc, pAr, &zWhere);
16054: 
16055:   if( rc==SQLITE_OK ){
16056:     if( pAr->zDir ){
16057:       zDir = sqlite3_mprintf("%s/", pAr->zDir);
16058:     }else{
16059:       zDir = sqlite3_mprintf("");
16060:     }
16061:     if( zDir==0 ) rc = SQLITE_NOMEM;
16062:   }
16063: 
16064:   shellPreparePrintf(pAr->db, &rc, &pSql, zSql1,
16065:       azExtraArg[pAr->bZip], pAr->zSrcTable, zWhere
16066:   );
16067: 
16068:   if( rc==SQLITE_OK ){
16069:     j = sqlite3_bind_parameter_index(pSql, "$dir");
16070:     sqlite3_bind_text(pSql, j, zDir, -1, SQLITE_STATIC);
16071: 
16072:     /* Run the SELECT statement twice. The first time, writefile() is called
16073:     ** for all archive members that should be extracted. The second time,
16074:     ** only for the directories. This is because the timestamps for
16075:     ** extracted directories must be reset after they are populated (as
16076:     ** populating them changes the timestamp).  */
16077:     for(i=0; i<2; i++){
16078:       j = sqlite3_bind_parameter_index(pSql, "$dirOnly");
16079:       sqlite3_bind_int(pSql, j, i);
16080:       if( pAr->bDryRun ){
16081:         utf8_printf(pAr->p->out, "%s\n", sqlite3_sql(pSql));
16082:       }else{
16083:         while( rc==SQLITE_OK && SQLITE_ROW==sqlite3_step(pSql) ){
16084:           if( i==0 && pAr->bVerbose ){
16085:             utf8_printf(pAr->p->out, "%s\n", sqlite3_column_text(pSql, 0));
16086:           }
16087:         }
16088:       }
16089:       shellReset(&rc, pSql);
16090:     }
16091:     shellFinalize(&rc, pSql);
16092:   }
16093: 
16094:   sqlite3_free(zDir);
16095:   sqlite3_free(zWhere);
16096:   return rc;
16097: }
