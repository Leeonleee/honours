3224: /** Pushes the new state onto the stack. The new state becomes
3225:  *  the current state. This function will allocate the stack
3226:  *  if necessary.
3227:  *  @param new_buffer The new state.
3228:  *  @param yyscanner The scanner object.
3229:  */
3230: void yypush_buffer_state (YY_BUFFER_STATE new_buffer , yyscan_t yyscanner)
3231: {
3232:     struct yyguts_t * yyg = (struct yyguts_t*)yyscanner;
3233: 	if (new_buffer == NULL)
3234: 		return;
3235: 
3236: 	yyensure_buffer_stack(yyscanner);
3237: 
3238: 	/* This block is copied from yy_switch_to_buffer. */
3239: 	if ( YY_CURRENT_BUFFER )
3240: 		{
3241: 		/* Flush out information for old buffer. */
3242: 		*yyg->yy_c_buf_p = yyg->yy_hold_char;
3243: 		YY_CURRENT_BUFFER_LVALUE->yy_buf_pos = yyg->yy_c_buf_p;
3244: 		YY_CURRENT_BUFFER_LVALUE->yy_n_chars = yyg->yy_n_chars;
3245: 		}
3246: 
3247: 	/* Only push if top exists. Otherwise, replace top. */
3248: 	if (YY_CURRENT_BUFFER)
3249: 		yyg->yy_buffer_stack_top++;
3250: 	YY_CURRENT_BUFFER_LVALUE = new_buffer;
