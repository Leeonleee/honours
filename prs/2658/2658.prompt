11101: /*
11102: ** Output the given string as a quoted string using SQL quoting conventions.
11103: ** Additionallly , escape the "\n" and "\r" characters so that they do not
11104: ** get corrupted by end-of-line translation facilities in some operating
11105: ** systems.
11106: **
11107: ** This is like output_quoted_string() but with the addition of the \r\n
11108: ** escape mechanism.
11109: */
11110: static void output_quoted_escaped_string(FILE *out, const char *z){
11111:   int i;
11112:   char c;
11113:   setBinaryMode(out, 1);
11114:   for(i=0; (c = z[i])!=0 && c!='\'' && c!='\n' && c!='\r'; i++){}
11115:   if( c==0 ){
11116:     utf8_printf(out,"'%s'",z);
11117:   }else{
11118:     const char *zNL = 0;
11119:     const char *zCR = 0;
11120:     int nNL = 0;
11121:     int nCR = 0;
11122:     char zBuf1[20], zBuf2[20];
11123:     for(i=0; z[i]; i++){
11124:       if( z[i]=='\n' ) nNL++;
11125:       if( z[i]=='\r' ) nCR++;
11126:     }
11127:     if( nNL ){
11128:       raw_printf(out, "replace(");
11129:       zNL = unused_string(z, "\\n", "\\012", zBuf1);
11130:     }
11131:     if( nCR ){
11132:       raw_printf(out, "replace(");
11133:       zCR = unused_string(z, "\\r", "\\015", zBuf2);
11134:     }
11135:     raw_printf(out, "'");
11136:     while( *z ){
11137:       for(i=0; (c = z[i])!=0 && c!='\n' && c!='\r' && c!='\''; i++){}
11138:       if( c=='\'' ) i++;
11139:       if( i ){
11140:         utf8_printf(out, "%.*s", i, z);
11141:         z += i;
11142:       }
11143:       if( c=='\'' ){
11144:         raw_printf(out, "'");
11145:         continue;
11146:       }
11147:       if( c==0 ){
11148:         break;
11149:       }
11150:       z++;
11151:       if( c=='\n' ){
11152:         raw_printf(out, "%s", zNL);
11153:         continue;
11154:       }
11155:       raw_printf(out, "%s", zCR);
11156:     }
11157:     raw_printf(out, "'");
11158:     if( nCR ){
11159:       raw_printf(out, ",'%s',char(13))", zCR);
11160:     }
11161:     if( nNL ){
11162:       raw_printf(out, ",'%s',char(10))", zNL);
11163:     }
11164:   }
11165:   setTextMode(out, 1);
11166: }
