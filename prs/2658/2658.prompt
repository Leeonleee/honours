13767: /*
13768: ** Output help text.
13769: **
13770: ** zPattern describes the set of commands for which help text is provided.
13771: ** If zPattern is NULL, then show all commands, but only give a one-line
13772: ** description of each.
13773: **
13774: ** Return the number of matches.
13775: */
13776: static int showHelp(FILE *out, const char *zPattern){
13777:   int i = 0;
13778:   int j = 0;
13779:   int n = 0;
13780:   char *zPat;
13781:   if( zPattern==0
13782:    || zPattern[0]=='0'
13783:    || strcmp(zPattern,"-a")==0
13784:    || strcmp(zPattern,"-all")==0
13785:    || strcmp(zPattern,"--all")==0
13786:   ){
13787:     /* Show all commands, but only one line per command */
13788:     if( zPattern==0 ) zPattern = "";
13789:     for(i=0; i<ArraySize(azHelp); i++){
13790:       if( azHelp[i][0]=='.' || zPattern[0] ){
13791:         utf8_printf(out, "%s\n", azHelp[i]);
13792:         n++;
13793:       }
13794:     }
13795:   }else{
13796:     /* Look for commands that for which zPattern is an exact prefix */
13797:     zPat = sqlite3_mprintf(".%s*", zPattern);
13798:     for(i=0; i<ArraySize(azHelp); i++){
13799:       if( sqlite3_strglob(zPat, azHelp[i])==0 ){
13800:         utf8_printf(out, "%s\n", azHelp[i]);
13801:         j = i+1;
13802:         n++;
13803:       }
13804:     }
13805:     sqlite3_free(zPat);
13806:     if( n ){
13807:       if( n==1 ){
13808:         /* when zPattern is a prefix of exactly one command, then include the
13809:         ** details of that command, which should begin at offset j */
13810:         while( j<ArraySize(azHelp)-1 && azHelp[j][0]!='.' ){
13811:           utf8_printf(out, "%s\n", azHelp[j]);
13812:           j++;
13813:         }
13814:       }
13815:       return n;
13816:     }
13817:     /* Look for commands that contain zPattern anywhere.  Show the complete
13818:     ** text of all commands that match. */
13819:     zPat = sqlite3_mprintf("%%%s%%", zPattern);
13820:     for(i=0; i<ArraySize(azHelp); i++){
13821:       if( azHelp[i][0]=='.' ) j = i;
13822:       if( sqlite3_strlike(zPat, azHelp[i], 0)==0 ){
13823:         utf8_printf(out, "%s\n", azHelp[j]);
13824:         while( j<ArraySize(azHelp)-1 && azHelp[j+1][0]!='.' ){
13825:           j++;
13826:           utf8_printf(out, "%s\n", azHelp[j]);
13827:         }
13828:         i = j;
13829:         n++;
13830:       }
13831:     }
13832:     sqlite3_free(zPat);
13833:   }
13834:   return n;
13835: }
