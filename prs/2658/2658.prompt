16642: /*
16643: ** Return a RecoverTable object representing the orphans table.
16644: */
16645: static RecoverTable *recoverOrphanTable(
16646:   ShellState *pState,             /* Shell state object */
16647:   int *pRc,                       /* IN/OUT: Error code */
16648:   const char *zLostAndFound,      /* Base name for orphans table */
16649:   int nCol                        /* Number of user data columns */
16650: ){
16651:   RecoverTable *pTab = 0;
16652:   if( nCol>=0 && *pRc==SQLITE_OK ){
16653:     int i;
16654: 
16655:     /* This block determines the name of the orphan table. The prefered
16656:     ** name is zLostAndFound. But if that clashes with another name
16657:     ** in the recovered schema, try zLostAndFound_0, zLostAndFound_1
16658:     ** and so on until a non-clashing name is found.  */
16659:     int iTab = 0;
16660:     char *zTab = shellMPrintf(pRc, "%s", zLostAndFound);
16661:     sqlite3_stmt *pTest = 0;
16662:     shellPrepare(pState->db, pRc,
16663:         "SELECT 1 FROM recovery.schema WHERE name=?", &pTest
16664:     );
16665:     if( pTest ) sqlite3_bind_text(pTest, 1, zTab, -1, SQLITE_TRANSIENT);
16666:     while( *pRc==SQLITE_OK && SQLITE_ROW==sqlite3_step(pTest) ){
16667:       shellReset(pRc, pTest);
16668:       sqlite3_free(zTab);
16669:       zTab = shellMPrintf(pRc, "%s_%d", zLostAndFound, iTab++);
16670:       sqlite3_bind_text(pTest, 1, zTab, -1, SQLITE_TRANSIENT);
16671:     }
16672:     shellFinalize(pRc, pTest);
16673: 
16674:     pTab = (RecoverTable*)shellMalloc(pRc, sizeof(RecoverTable));
16675:     if( pTab ){
16676:       pTab->zQuoted = shellMPrintf(pRc, "\"%w\"", zTab);
16677:       pTab->nCol = nCol;
16678:       pTab->iPk = -2;
16679:       if( nCol>0 ){
16680:         pTab->azlCol = (char**)shellMalloc(pRc, sizeof(char*) * (nCol+1));
16681:         if( pTab->azlCol ){
16682:           pTab->azlCol[nCol] = shellMPrintf(pRc, "");
16683:           for(i=nCol-1; i>=0; i--){
16684:             pTab->azlCol[i] = shellMPrintf(pRc, "%s, NULL", pTab->azlCol[i+1]);
16685:           }
16686:         }
16687:       }
16688: 
16689:       if( *pRc!=SQLITE_OK ){
16690:         recoverFreeTable(pTab);
16691:         pTab = 0;
16692:       }else{
16693:         raw_printf(pState->out,
16694:             "CREATE TABLE %s(rootpgno INTEGER, "
16695:             "pgno INTEGER, nfield INTEGER, id INTEGER", pTab->zQuoted
16696:         );
16697:         for(i=0; i<nCol; i++){
16698:           raw_printf(pState->out, ", c%d", i);
16699:         }
16700:         raw_printf(pState->out, ");\n");
16701:       }
16702:     }
16703:     sqlite3_free(zTab);
16704:   }
16705:   return pTab;
16706: }
