16099: /*
16100: ** Run the SQL statement in zSql.  Or if doing a --dryrun, merely print it out.
16101: */
16102: static int arExecSql(ArCommand *pAr, const char *zSql){
16103:   int rc;
16104:   if( pAr->bDryRun ){
16105:     utf8_printf(pAr->p->out, "%s\n", zSql);
16106:     rc = SQLITE_OK;
16107:   }else{
16108:     char *zErr = 0;
16109:     rc = sqlite3_exec(pAr->db, zSql, 0, 0, &zErr);
16110:     if( zErr ){
16111:       utf8_printf(stdout, "ERROR: %s\n", zErr);
16112:       sqlite3_free(zErr);
16113:     }
16114:   }
16115:   return rc;
16116: }
