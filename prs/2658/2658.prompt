12115:       { "wchar: ",                  "Bytes sent to write():"    },
12116:       { "syscr: ",                  "Read() system calls:"      },
12117:       { "syscw: ",                  "Write() system calls:"     },
12118:       { "read_bytes: ",             "Bytes read from storage:"  },
12119:       { "write_bytes: ",            "Bytes written to storage:" },
12120:       { "cancelled_write_bytes: ",  "Cancelled write bytes:"    },
12121:     };
