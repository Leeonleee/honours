16590: /*
16591: ** This function is called to search the schema recovered from the
16592: ** sqlite_schema table of the (possibly) corrupt database as part
16593: ** of a ".recover" command. Specifically, for a table with root page
16594: ** iRoot and at least nCol columns. Additionally, if bIntkey is 0, the
16595: ** table must be a WITHOUT ROWID table, or if non-zero, not one of
16596: ** those.
16597: **
16598: ** If a table is found, a (RecoverTable*) object is returned. Or, if
16599: ** no such table is found, but bIntkey is false and iRoot is the
16600: ** root page of an index in the recovered schema, then (*pbNoop) is
16601: ** set to true and NULL returned. Or, if there is no such table or
16602: ** index, NULL is returned and (*pbNoop) set to 0, indicating that
16603: ** the caller should write data to the orphans table.
16604: */
16605: static RecoverTable *recoverFindTable(
16606:   ShellState *pState,             /* Shell state object */
16607:   int *pRc,                       /* IN/OUT: Error code */
16608:   int iRoot,                      /* Root page of table */
16609:   int bIntkey,                    /* True for an intkey table */
16610:   int nCol,                       /* Number of columns in table */
16611:   int *pbNoop                     /* OUT: True if iRoot is root of index */
16612: ){
16613:   sqlite3_stmt *pStmt = 0;
16614:   RecoverTable *pRet = 0;
16615:   int bNoop = 0;
16616:   const char *zSql = 0;
16617:   const char *zName = 0;
16618: 
16619:   /* Search the recovered schema for an object with root page iRoot. */
16620:   shellPreparePrintf(pState->db, pRc, &pStmt,
16621:       "SELECT type, name, sql FROM recovery.schema WHERE rootpage=%d", iRoot
16622:   );
16623:   while( *pRc==SQLITE_OK && SQLITE_ROW==sqlite3_step(pStmt) ){
16624:     const char *zType = (const char*)sqlite3_column_text(pStmt, 0);
16625:     if( bIntkey==0 && sqlite3_stricmp(zType, "index")==0 ){
16626:       bNoop = 1;
16627:       break;
16628:     }
16629:     if( sqlite3_stricmp(zType, "table")==0 ){
16630:       zName = (const char*)sqlite3_column_text(pStmt, 1);
16631:       zSql = (const char*)sqlite3_column_text(pStmt, 2);
16632:       pRet = recoverNewTable(pRc, zName, zSql, bIntkey, nCol);
16633:       break;
16634:     }
16635:   }
16636: 
16637:   shellFinalize(pRc, pStmt);
16638:   *pbNoop = bNoop;
16639:   return pRet;
16640: }
