16391: /*
16392: ** If *pRc is not SQLITE_OK when this function is called, it is a no-op.
16393: ** Otherwise, an attempt is made to allocate, zero and return a pointer
16394: ** to a buffer nByte bytes in size. If an OOM error occurs, *pRc is set
16395: ** to SQLITE_NOMEM and NULL returned.
16396: */
16397: static void *shellMalloc(int *pRc, sqlite3_int64 nByte){
16398:   void *pRet = 0;
16399:   if( *pRc==SQLITE_OK ){
16400:     pRet = sqlite3_malloc64(nByte);
16401:     if( pRet==0 ){
16402:       *pRc = SQLITE_NOMEM;
16403:     }else{
16404:       memset(pRet, 0, nByte);
16405:     }
16406:   }
16407:   return pRet;
16408: }
