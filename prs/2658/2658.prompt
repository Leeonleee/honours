14869: /*
14870: ** Try to transfer all rows of the schema that match zWhere.  For
14871: ** each row, invoke xForEach() on the object defined by that row.
14872: ** If an error is encountered while moving forward through the
14873: ** sqlite_schema table, try again moving backwards.
14874: */
14875: static void tryToCloneSchema(
14876:   ShellState *p,
14877:   sqlite3 *newDb,
14878:   const char *zWhere,
14879:   void (*xForEach)(ShellState*,sqlite3*,const char*)
14880: ){
14881:   sqlite3_stmt *pQuery = 0;
14882:   char *zQuery = 0;
14883:   int rc;
14884:   const unsigned char *zName;
14885:   const unsigned char *zSql;
14886:   char *zErrMsg = 0;
14887: 
14888:   zQuery = sqlite3_mprintf("SELECT name, sql FROM sqlite_schema"
14889:                            " WHERE %s", zWhere);
14890:   rc = sqlite3_prepare_v2(p->db, zQuery, -1, &pQuery, 0);
14891:   if( rc ){
14892:     utf8_printf(stderr, "Error: (%d) %s on [%s]\n",
14893:                     sqlite3_extended_errcode(p->db), sqlite3_errmsg(p->db),
14894:                     zQuery);
14895:     goto end_schema_xfer;
14896:   }
14897:   while( (rc = sqlite3_step(pQuery))==SQLITE_ROW ){
14898:     zName = sqlite3_column_text(pQuery, 0);
14899:     zSql = sqlite3_column_text(pQuery, 1);
14900:     printf("%s... ", zName); fflush(stdout);
14901:     sqlite3_exec(newDb, (const char*)zSql, 0, 0, &zErrMsg);
14902:     if( zErrMsg ){
14903:       utf8_printf(stderr, "Error: %s\nSQL: [%s]\n", zErrMsg, zSql);
14904:       sqlite3_free(zErrMsg);
14905:       zErrMsg = 0;
14906:     }
14907:     if( xForEach ){
14908:       xForEach(p, newDb, (const char*)zName);
14909:     }
14910:     printf("done\n");
14911:   }
14912:   if( rc!=SQLITE_DONE ){
14913:     sqlite3_finalize(pQuery);
14914:     sqlite3_free(zQuery);
14915:     zQuery = sqlite3_mprintf("SELECT name, sql FROM sqlite_schema"
14916:                              " WHERE %s ORDER BY rowid DESC", zWhere);
14917:     rc = sqlite3_prepare_v2(p->db, zQuery, -1, &pQuery, 0);
14918:     if( rc ){
14919:       utf8_printf(stderr, "Error: (%d) %s on [%s]\n",
14920:                       sqlite3_extended_errcode(p->db), sqlite3_errmsg(p->db),
14921:                       zQuery);
14922:       goto end_schema_xfer;
14923:     }
14924:     while( (rc = sqlite3_step(pQuery))==SQLITE_ROW ){
14925:       zName = sqlite3_column_text(pQuery, 0);
14926:       zSql = sqlite3_column_text(pQuery, 1);
14927:       printf("%s... ", zName); fflush(stdout);
14928:       sqlite3_exec(newDb, (const char*)zSql, 0, 0, &zErrMsg);
14929:       if( zErrMsg ){
14930:         utf8_printf(stderr, "Error: %s\nSQL: [%s]\n", zErrMsg, zSql);
14931:         sqlite3_free(zErrMsg);
14932:         zErrMsg = 0;
14933:       }
14934:       if( xForEach ){
14935:         xForEach(p, newDb, (const char*)zName);
14936:       }
14937:       printf("done\n");
14938:     }
14939:   }
14940: end_schema_xfer:
14941:   sqlite3_finalize(pQuery);
14942:   sqlite3_free(zQuery);
14943: }
