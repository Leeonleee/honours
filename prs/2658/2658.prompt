14464: /*
14465: ** Interpret zArg as either an integer or a boolean value.  Return 1 or 0
14466: ** for TRUE and FALSE.  Return the integer value if appropriate.
14467: */
14468: static int booleanValue(const char *zArg){
14469:   int i;
14470:   if( zArg[0]=='0' && zArg[1]=='x' ){
14471:     for(i=2; hexDigitValue(zArg[i])>=0; i++){}
14472:   }else{
14473:     for(i=0; zArg[i]>='0' && zArg[i]<='9'; i++){}
14474:   }
14475:   if( i>0 && zArg[i]==0 ) return (int)(integerValue(zArg) & 0xffffffff);
14476:   if( sqlite3_stricmp(zArg, "on")==0 || sqlite3_stricmp(zArg,"yes")==0 ){
14477:     return 1;
14478:   }
14479:   if( sqlite3_stricmp(zArg, "off")==0 || sqlite3_stricmp(zArg,"no")==0 ){
14480:     return 0;
14481:   }
14482:   utf8_printf(stderr, "ERROR: Not a boolean value: \"%s\". Assuming \"no\".\n",
14483:           zArg);
14484:   return 0;
14485: }
