4750:   int nSig, nFrac, nDigit;
4751:   int i, rc;
4752:   if( pA==0 ){
4753:     return;
4754:   }
4755:   if( pA->oom || pB==0 || pB->oom ){
4756:     pA->oom = 1;
4757:     return;
4758:   }
4759:   if( pA->isNull || pB->isNull ){
4760:     pA->isNull = 1;
4761:     return;
4762:   }
4763:   nSig = pA->nDigit - pA->nFrac;
4764:   if( nSig && pA->a[0]==0 ) nSig--;
4765:   if( nSig<pB->nDigit-pB->nFrac ){
4766:     nSig = pB->nDigit - pB->nFrac;
