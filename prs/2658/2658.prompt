5625: /*
5626: ** If string zIn is quoted, dequote it in place. Otherwise, if the string
5627: ** is not quoted, do nothing.
5628: */
5629: static void zipfileDequote(char *zIn){
5630:   char q = zIn[0];
5631:   if( q=='"' || q=='\'' || q=='`' || q=='[' ){
5632:     int iIn = 1;
5633:     int iOut = 0;
5634:     if( q=='[' ) q = ']';
5635:     while( ALWAYS(zIn[iIn]) ){
5636:       char c = zIn[iIn++];
5637:       if( c==q && zIn[iIn++]!=q ) break;
5638:       zIn[iOut++] = c;
5639:     }
5640:     zIn[iOut] = '\0';
5641:   }
5642: }
