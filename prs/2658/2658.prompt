12621: char *strdup_handle_newline(ShellState *p, const char *z) {
12622:   if (!z) {
12623:     return 0;
12624:   }
12625:   if (p->cMode != MODE_Box) {
12626:     return strdup(z);
12627:   }
12628:   int max_size = 80;
12629:   char *result = malloc(max_size * 2 + 10);
12630:   char *t = result;
12631:   int count = 0;
12632:   int interrupted = 0;
12633:   const char *s;
12634:   for(s = z; *s; s++, t++) {
12635:     if (*s == '\n') {
12636:       *t = '\\';
12637:       t++;
12638:       *t = 'n';
12639:     } else {
12640:       *t = *s;
12641:     }
12642:     count++;
12643:     if (count >= max_size && ((*s & 0xc0) != 0x80)) {
12644:       interrupted = 1;
12645:       break;
12646:     }
12647:   }
12648:   if (interrupted) {
12649:     *t++ = '.';
12650:     *t++ = '.';
12651:     *t++ = '.';
12652:   }
12653:   *t = '\0';
12654:   return result;
12655: }
