416: // Returns the last Win32 error, in string format. Returns an empty string if there is no error.
417: std::string GetLastErrorAsString() {
418: 	// Get the error message, if any.
419: 	DWORD errorMessageID = GetLastError();
420: 	if (errorMessageID == 0)
421: 		return std::string(); // No error message has been recorded
422: 
423: 	LPSTR messageBuffer = nullptr;
424: 	idx_t size =
425: 	    FormatMessageA(FORMAT_MESSAGE_ALLOCATE_BUFFER | FORMAT_MESSAGE_FROM_SYSTEM | FORMAT_MESSAGE_IGNORE_INSERTS,
426: 	                   NULL, errorMessageID, MAKELANGID(LANG_NEUTRAL, SUBLANG_DEFAULT), (LPSTR)&messageBuffer, 0, NULL);
427: 
428: 	std::string message(messageBuffer, size);
429: 
430: 	// Free the buffer.
431: 	LocalFree(messageBuffer);
432: 
433: 	return message;
434: }
