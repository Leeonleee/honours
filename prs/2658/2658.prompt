16119: /*
16120: ** Implementation of .ar "create", "insert", and "update" commands.
16121: **
16122: **     create    ->     Create a new SQL archive
16123: **     insert    ->     Insert or reinsert all files listed
16124: **     update    ->     Insert files that have changed or that were not
16125: **                      previously in the archive
16126: **
16127: ** Create the "sqlar" table in the database if it does not already exist.
16128: ** Then add each file in the azFile[] array to the archive. Directories
16129: ** are added recursively. If argument bVerbose is non-zero, a message is
16130: ** printed on stdout for each file archived.
16131: **
16132: ** The create command is the same as update, except that it drops
16133: ** any existing "sqlar" table before beginning.  The "insert" command
16134: ** always overwrites every file named on the command-line, where as
16135: ** "update" only overwrites if the size or mtime or mode has changed.
16136: */
16137: static int arCreateOrUpdateCommand(
16138:   ArCommand *pAr,                 /* Command arguments and options */
16139:   int bUpdate,                    /* true for a --create. */
16140:   int bOnlyIfChanged              /* Only update if file has changed */
16141: ){
16142:   const char *zCreate =
16143:       "CREATE TABLE IF NOT EXISTS sqlar(\n"
16144:       "  name TEXT PRIMARY KEY,  -- name of the file\n"
16145:       "  mode INT,               -- access permissions\n"
16146:       "  mtime INT,              -- last modification time\n"
16147:       "  sz INT,                 -- original file size\n"
16148:       "  data BLOB               -- compressed content\n"
16149:       ")";
16150:   const char *zDrop = "DROP TABLE IF EXISTS sqlar";
16151:   const char *zInsertFmt[2] = {
16152:      "REPLACE INTO %s(name,mode,mtime,sz,data)\n"
16153:      "  SELECT\n"
16154:      "    %s,\n"
16155:      "    mode,\n"
16156:      "    mtime,\n"
16157:      "    CASE substr(lsmode(mode),1,1)\n"
16158:      "      WHEN '-' THEN length(data)\n"
16159:      "      WHEN 'd' THEN 0\n"
16160:      "      ELSE -1 END,\n"
16161:      "    sqlar_compress(data)\n"
16162:      "  FROM fsdir(%Q,%Q) AS disk\n"
16163:      "  WHERE lsmode(mode) NOT LIKE '?%%'%s;"
16164:      ,
16165:      "REPLACE INTO %s(name,mode,mtime,data)\n"
16166:      "  SELECT\n"
16167:      "    %s,\n"
16168:      "    mode,\n"
16169:      "    mtime,\n"
16170:      "    data\n"
16171:      "  FROM fsdir(%Q,%Q) AS disk\n"
16172:      "  WHERE lsmode(mode) NOT LIKE '?%%'%s;"
16173:   };
16174:   int i;                          /* For iterating through azFile[] */
16175:   int rc;                         /* Return code */
16176:   const char *zTab = 0;           /* SQL table into which to insert */
16177:   char *zSql;
16178:   char zTemp[50];
16179:   char *zExists = 0;
16180: 
16181:   arExecSql(pAr, "PRAGMA page_size=512");
16182:   rc = arExecSql(pAr, "SAVEPOINT ar;");
16183:   if( rc!=SQLITE_OK ) return rc;
16184:   zTemp[0] = 0;
16185:   if( pAr->bZip ){
16186:     /* Initialize the zipfile virtual table, if necessary */
16187:     if( pAr->zFile ){
16188:       sqlite3_uint64 r;
16189:       sqlite3_randomness(sizeof(r),&r);
16190:       sqlite3_snprintf(sizeof(zTemp),zTemp,"zip%016llx",r);
16191:       zTab = zTemp;
16192:       zSql = sqlite3_mprintf(
16193:          "CREATE VIRTUAL TABLE temp.%s USING zipfile(%Q)",
16194:          zTab, pAr->zFile
16195:       );
16196:       rc = arExecSql(pAr, zSql);
16197:       sqlite3_free(zSql);
16198:     }else{
16199:       zTab = "zip";
16200:     }
16201:   }else{
16202:     /* Initialize the table for an SQLAR */
16203:     zTab = "sqlar";
16204:     if( bUpdate==0 ){
16205:       rc = arExecSql(pAr, zDrop);
16206:       if( rc!=SQLITE_OK ) goto end_ar_transaction;
16207:     }
16208:     rc = arExecSql(pAr, zCreate);
16209:   }
16210:   if( bOnlyIfChanged ){
16211:     zExists = sqlite3_mprintf(
16212:       " AND NOT EXISTS("
16213:           "SELECT 1 FROM %s AS mem"
16214:           " WHERE mem.name=disk.name"
16215:           " AND mem.mtime=disk.mtime"
16216:           " AND mem.mode=disk.mode)", zTab);
16217:   }else{
16218:     zExists = sqlite3_mprintf("");
16219:   }
16220:   if( zExists==0 ) rc = SQLITE_NOMEM;
16221:   for(i=0; i<pAr->nArg && rc==SQLITE_OK; i++){
16222:     char *zSql2 = sqlite3_mprintf(zInsertFmt[pAr->bZip], zTab,
16223:         pAr->bVerbose ? "shell_putsnl(name)" : "name",
16224:         pAr->azArg[i], pAr->zDir, zExists);
16225:     rc = arExecSql(pAr, zSql2);
16226:     sqlite3_free(zSql2);
16227:   }
16228: end_ar_transaction:
16229:   if( rc!=SQLITE_OK ){
16230:     sqlite3_exec(pAr->db, "ROLLBACK TO ar; RELEASE ar;", 0, 0, 0);
16231:   }else{
16232:     rc = arExecSql(pAr, "RELEASE ar;");
16233:     if( pAr->bZip && pAr->zFile ){
16234:       zSql = sqlite3_mprintf("DROP TABLE %s", zTemp);
16235:       arExecSql(pAr, zSql);
16236:       sqlite3_free(zSql);
16237:     }
16238:   }
16239:   sqlite3_free(zExists);
16240:   return rc;
16241: }
