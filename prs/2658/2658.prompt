364: bool LocalFileSystem::ListFiles(const string &directory, const std::function<void(string, bool)> &callback) {
365: 	if (!DirectoryExists(directory)) {
366: 		return false;
367: 	}
368: 	DIR *dir = opendir(directory.c_str());
369: 	if (!dir) {
370: 		return false;
371: 	}
372: 	struct dirent *ent;
373: 	// loop over all files in the directory
374: 	while ((ent = readdir(dir)) != nullptr) {
375: 		string name = string(ent->d_name);
376: 		// skip . .. and empty files
377: 		if (name.empty() || name == "." || name == "..") {
378: 			continue;
379: 		}
380: 		// now stat the file to figure out if it is a regular file or directory
381: 		string full_path = JoinPath(directory, name);
382: 		if (access(full_path.c_str(), 0) != 0) {
383: 			continue;
384: 		}
385: 		struct stat status;
386: 		stat(full_path.c_str(), &status);
387: 		if (!(status.st_mode & S_IFREG) && !(status.st_mode & S_IFDIR)) {
388: 			// not a file or directory: skip
389: 			continue;
390: 		}
391: 		// invoke callback
392: 		callback(name, status.st_mode & S_IFDIR);
393: 	}
394: 	closedir(dir);
395: 	return true;
396: }
