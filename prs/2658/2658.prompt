14615: /* Clean up resourced used by an ImportCtx */
14616: static void import_cleanup(ImportCtx *p){
14617:   if( p->in!=0 && p->xCloser!=0 ){
14618:     p->xCloser(p->in);
14619:     p->in = 0;
14620:   }
14621:   sqlite3_free(p->z);
14622:   p->z = 0;
14623: }
