14059: /*
14060: ** Scalar function "shell_int32". The first argument to this function
14061: ** must be a blob. The second a non-negative integer. This function
14062: ** reads and returns a 32-bit big-endian integer from byte
14063: ** offset (4*<arg2>) of the blob.
14064: */
14065: static void shellInt32(
14066:   sqlite3_context *context,
14067:   int argc,
14068:   sqlite3_value **argv
14069: ){
14070:   const unsigned char *pBlob;
14071:   int nBlob;
14072:   int iInt;
14073: 
14074:   UNUSED_PARAMETER(argc);
14075:   nBlob = sqlite3_value_bytes(argv[0]);
14076:   pBlob = (const unsigned char*)sqlite3_value_blob(argv[0]);
14077:   iInt = sqlite3_value_int(argv[1]);
14078: 
14079:   if( iInt>=0 && (iInt+1)*4<=nBlob ){
14080:     const unsigned char *a = &pBlob[iInt*4];
14081:     sqlite3_int64 iVal = ((sqlite3_int64)a[0]<<24)
14082:                        + ((sqlite3_int64)a[1]<<16)
14083:                        + ((sqlite3_int64)a[2]<< 8)
14084:                        + ((sqlite3_int64)a[3]<< 0);
14085:     sqlite3_result_int64(context, iVal);
14086:   }
14087: }
