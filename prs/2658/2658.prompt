15120:   }else if( strcmp(zDb,"temp")==0 ){
15121:     zSchemaTab = sqlite3_mprintf("%s", "sqlite_temp_schema");
15122:   }else{
15123:     zSchemaTab = sqlite3_mprintf("\"%w\".sqlite_schema", zDb);
15124:   }
15125:   for(i=0; i<ArraySize(aQuery); i++){
15126:     char *zSql = sqlite3_mprintf(aQuery[i].zSql, zSchemaTab);
15127:     int val = db_int(p, zSql);
15128:     sqlite3_free(zSql);
15129:     utf8_printf(p->out, "%-20s %d\n", aQuery[i].zName, val);
