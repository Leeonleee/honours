11953: /*
11954: ** Generate an appropriate SELFTEST table in the main database.
11955: */
11956: static void createSelftestTable(ShellState *p){
11957:   char *zErrMsg = 0;
11958:   sqlite3_exec(p->db,
11959:     "SAVEPOINT selftest_init;\n"
11960:     "CREATE TABLE IF NOT EXISTS selftest(\n"
11961:     "  tno INTEGER PRIMARY KEY,\n"   /* Test number */
11962:     "  op TEXT,\n"                   /* Operator:  memo run */
11963:     "  cmd TEXT,\n"                  /* Command text */
11964:     "  ans TEXT\n"                   /* Desired answer */
11965:     ");"
11966:     "CREATE TEMP TABLE [_shell$self](op,cmd,ans);\n"
11967:     "INSERT INTO [_shell$self](rowid,op,cmd)\n"
11968:     "  VALUES(coalesce((SELECT (max(tno)+100)/10 FROM selftest),10),\n"
11969:     "         'memo','Tests generated by --init');\n"
11970:     "INSERT INTO [_shell$self]\n"
11971:     "  SELECT 'run',\n"
11972:     "    'SELECT hex(sha3_query(''SELECT type,name,tbl_name,sql "
11973:                                  "FROM sqlite_schema ORDER BY 2'',224))',\n"
11974:     "    hex(sha3_query('SELECT type,name,tbl_name,sql "
11975:                           "FROM sqlite_schema ORDER BY 2',224));\n"
11976:     "INSERT INTO [_shell$self]\n"
11977:     "  SELECT 'run',"
11978:     "    'SELECT hex(sha3_query(''SELECT * FROM \"' ||"
11979:     "        printf('%w',name) || '\" NOT INDEXED'',224))',\n"
11980:     "    hex(sha3_query(printf('SELECT * FROM \"%w\" NOT INDEXED',name),224))\n"
11981:     "  FROM (\n"
11982:     "    SELECT name FROM sqlite_schema\n"
11983:     "     WHERE type='table'\n"
11984:     "       AND name<>'selftest'\n"
11985:     "       AND coalesce(rootpage,0)>0\n"
11986:     "  )\n"
11987:     " ORDER BY name;\n"
11988:     "INSERT INTO [_shell$self]\n"
11989:     "  VALUES('run','PRAGMA integrity_check','ok');\n"
11990:     "INSERT INTO selftest(tno,op,cmd,ans)"
11991:     "  SELECT rowid*10,op,cmd,ans FROM [_shell$self];\n"
11992:     "DROP TABLE [_shell$self];"
11993:     ,0,0,&zErrMsg);
11994:   if( zErrMsg ){
11995:     utf8_printf(stderr, "SELFTEST initialization failure: %s\n", zErrMsg);
11996:     sqlite3_free(zErrMsg);
11997:   }
11998:   sqlite3_exec(p->db, "RELEASE selftest_init",0,0,0);
11999: }
