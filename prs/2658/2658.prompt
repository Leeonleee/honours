14760: /*
14761: ** Try to transfer data for table zTable.  If an error is seen while
14762: ** moving forward, try to go backwards.  The backwards movement won't
14763: ** work for WITHOUT ROWID tables.
14764: */
14765: static void tryToCloneData(
14766:   ShellState *p,
14767:   sqlite3 *newDb,
14768:   const char *zTable
14769: ){
14770:   sqlite3_stmt *pQuery = 0;
14771:   sqlite3_stmt *pInsert = 0;
14772:   char *zQuery = 0;
14773:   char *zInsert = 0;
14774:   int rc;
14775:   int i, j, n;
14776:   int nTable = strlen30(zTable);
14777:   int k = 0;
14778:   int cnt = 0;
14779:   const int spinRate = 10000;
14780: 
14781:   zQuery = sqlite3_mprintf("SELECT * FROM \"%w\"", zTable);
14782:   rc = sqlite3_prepare_v2(p->db, zQuery, -1, &pQuery, 0);
14783:   if( rc ){
14784:     utf8_printf(stderr, "Error %d: %s on [%s]\n",
14785:             sqlite3_extended_errcode(p->db), sqlite3_errmsg(p->db),
14786:             zQuery);
14787:     goto end_data_xfer;
14788:   }
14789:   n = sqlite3_column_count(pQuery);
14790:   zInsert = sqlite3_malloc64(200 + nTable + n*3);
14791:   if( zInsert==0 ) shell_out_of_memory();
14792:   sqlite3_snprintf(200+nTable,zInsert,
14793:                    "INSERT OR IGNORE INTO \"%s\" VALUES(?", zTable);
14794:   i = strlen30(zInsert);
14795:   for(j=1; j<n; j++){
14796:     memcpy(zInsert+i, ",?", 2);
14797:     i += 2;
14798:   }
14799:   memcpy(zInsert+i, ");", 3);
14800:   rc = sqlite3_prepare_v2(newDb, zInsert, -1, &pInsert, 0);
14801:   if( rc ){
14802:     utf8_printf(stderr, "Error %d: %s on [%s]\n",
14803:             sqlite3_extended_errcode(newDb), sqlite3_errmsg(newDb),
14804:             zQuery);
14805:     goto end_data_xfer;
14806:   }
14807:   for(k=0; k<2; k++){
14808:     while( (rc = sqlite3_step(pQuery))==SQLITE_ROW ){
14809:       for(i=0; i<n; i++){
14810:         switch( sqlite3_column_type(pQuery, i) ){
14811:           case SQLITE_NULL: {
14812:             sqlite3_bind_null(pInsert, i+1);
14813:             break;
14814:           }
14815:           case SQLITE_INTEGER: {
14816:             sqlite3_bind_int64(pInsert, i+1, sqlite3_column_int64(pQuery,i));
14817:             break;
14818:           }
14819:           case SQLITE_FLOAT: {
14820:             sqlite3_bind_double(pInsert, i+1, sqlite3_column_double(pQuery,i));
14821:             break;
14822:           }
14823:           case SQLITE_TEXT: {
14824:             sqlite3_bind_text(pInsert, i+1,
14825:                              (const char*)sqlite3_column_text(pQuery,i),
14826:                              -1, SQLITE_STATIC);
14827:             break;
14828:           }
14829:           case SQLITE_BLOB: {
14830:             sqlite3_bind_blob(pInsert, i+1, sqlite3_column_blob(pQuery,i),
14831:                                             sqlite3_column_bytes(pQuery,i),
14832:                                             SQLITE_STATIC);
14833:             break;
14834:           }
14835:         }
14836:       } /* End for */
14837:       rc = sqlite3_step(pInsert);
14838:       if( rc!=SQLITE_OK && rc!=SQLITE_ROW && rc!=SQLITE_DONE ){
14839:         utf8_printf(stderr, "Error %d: %s\n", sqlite3_extended_errcode(newDb),
14840:                         sqlite3_errmsg(newDb));
14841:       }
14842:       sqlite3_reset(pInsert);
14843:       cnt++;
14844:       if( (cnt%spinRate)==0 ){
14845:         printf("%c\b", "|/-\\"[(cnt/spinRate)%4]);
14846:         fflush(stdout);
14847:       }
14848:     } /* End while */
14849:     if( rc==SQLITE_DONE ) break;
14850:     sqlite3_finalize(pQuery);
14851:     sqlite3_free(zQuery);
14852:     zQuery = sqlite3_mprintf("SELECT * FROM \"%w\" ORDER BY rowid DESC;",
14853:                              zTable);
14854:     rc = sqlite3_prepare_v2(p->db, zQuery, -1, &pQuery, 0);
14855:     if( rc ){
14856:       utf8_printf(stderr, "Warning: cannot step \"%s\" backwards", zTable);
14857:       break;
14858:     }
14859:   } /* End for(k=0...) */
14860: 
14861: end_data_xfer:
14862:   sqlite3_finalize(pQuery);
14863:   sqlite3_finalize(pInsert);
14864:   sqlite3_free(zQuery);
14865:   sqlite3_free(zInsert);
14866: }
