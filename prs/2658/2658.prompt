12510: /*
12511: ** Bind parameters on a prepared statement.
12512: **
12513: ** Parameter bindings are taken from a TEMP table of the form:
12514: **
12515: **    CREATE TEMP TABLE sqlite_parameters(key TEXT PRIMARY KEY, value)
12516: **    WITHOUT ROWID;
12517: **
12518: ** No bindings occur if this table does not exist.  The name of the table
12519: ** begins with "sqlite_" so that it will not collide with ordinary application
12520: ** tables.  The table must be in the TEMP schema.
12521: */
12522: static void bind_prepared_stmt(ShellState *pArg, sqlite3_stmt *pStmt){
12523:   int nVar;
12524:   int i;
12525:   int rc;
12526:   sqlite3_stmt *pQ = 0;
12527: 
12528:   nVar = sqlite3_bind_parameter_count(pStmt);
12529:   if( nVar==0 ) return;  /* Nothing to do */
12530:   if( sqlite3_table_column_metadata(pArg->db, "TEMP", "sqlite_parameters",
12531:                                     "key", 0, 0, 0, 0, 0)!=SQLITE_OK ){
12532:     return; /* Parameter table does not exist */
12533:   }
12534:   rc = sqlite3_prepare_v2(pArg->db,
12535:           "SELECT value FROM temp.sqlite_parameters"
12536:           " WHERE key=?1", -1, &pQ, 0);
12537:   if( rc || pQ==0 ) return;
12538:   for(i=1; i<=nVar; i++){
12539:     char zNum[30];
12540:     const char *zVar = sqlite3_bind_parameter_name(pStmt, i);
12541:     if( zVar==0 ){
12542:       sqlite3_snprintf(sizeof(zNum),zNum,"?%d",i);
12543:       zVar = zNum;
12544:     }
12545:     sqlite3_bind_text(pQ, 1, zVar, -1, SQLITE_STATIC);
12546:     if( sqlite3_step(pQ)==SQLITE_ROW ){
12547:       sqlite3_bind_value(pStmt, i, sqlite3_column_value(pQ, 0));
12548:     }else{
12549:       sqlite3_bind_null(pStmt, i);
12550:     }
12551:     sqlite3_reset(pQ);
12552:   }
12553:   sqlite3_finalize(pQ);
12554: }
