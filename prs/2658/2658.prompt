15371: /*
15372: ** The implementation of dot-command ".lint fkey-indexes".
15373: */
15374: static int lintFkeyIndexes(
15375:   ShellState *pState,             /* Current shell tool state */
15376:   char **azArg,                   /* Array of arguments passed to dot command */
15377:   int nArg                        /* Number of entries in azArg[] */
15378: ){
15379:   sqlite3 *db = pState->db;       /* Database handle to query "main" db of */
15380:   FILE *out = pState->out;        /* Stream to write non-error output to */
15381:   int bVerbose = 0;               /* If -verbose is present */
15382:   int bGroupByParent = 0;         /* If -groupbyparent is present */
15383:   int i;                          /* To iterate through azArg[] */
15384:   const char *zIndent = "";       /* How much to indent CREATE INDEX by */
15385:   int rc;                         /* Return code */
15386:   sqlite3_stmt *pSql = 0;         /* Compiled version of SQL statement below */
15387: 
15388:   /*
15389:   ** This SELECT statement returns one row for each foreign key constraint
15390:   ** in the schema of the main database. The column values are:
15391:   **
15392:   ** 0. The text of an SQL statement similar to:
15393:   **
15394:   **      "EXPLAIN QUERY PLAN SELECT 1 FROM child_table WHERE child_key=?"
15395:   **
15396:   **    This SELECT is similar to the one that the foreign keys implementation
15397:   **    needs to run internally on child tables. If there is an index that can
15398:   **    be used to optimize this query, then it can also be used by the FK
15399:   **    implementation to optimize DELETE or UPDATE statements on the parent
15400:   **    table.
15401:   **
15402:   ** 1. A GLOB pattern suitable for sqlite3_strglob(). If the plan output by
15403:   **    the EXPLAIN QUERY PLAN command matches this pattern, then the schema
15404:   **    contains an index that can be used to optimize the query.
15405:   **
15406:   ** 2. Human readable text that describes the child table and columns. e.g.
15407:   **
15408:   **       "child_table(child_key1, child_key2)"
15409:   **
15410:   ** 3. Human readable text that describes the parent table and columns. e.g.
15411:   **
15412:   **       "parent_table(parent_key1, parent_key2)"
15413:   **
15414:   ** 4. A full CREATE INDEX statement for an index that could be used to
15415:   **    optimize DELETE or UPDATE statements on the parent table. e.g.
15416:   **
15417:   **       "CREATE INDEX child_table_child_key ON child_table(child_key)"
15418:   **
15419:   ** 5. The name of the parent table.
15420:   **
15421:   ** These six values are used by the C logic below to generate the report.
15422:   */
15423:   const char *zSql =
15424:   "SELECT "
15425:     "     'EXPLAIN QUERY PLAN SELECT 1 FROM ' || quote(s.name) || ' WHERE '"
15426:     "  || group_concat(quote(s.name) || '.' || quote(f.[from]) || '=?' "
15427:     "  || fkey_collate_clause("
15428:     "       f.[table], COALESCE(f.[to], p.[name]), s.name, f.[from]),' AND ')"
15429:     ", "
15430:     "     'SEARCH TABLE ' || s.name || ' USING COVERING INDEX*('"
15431:     "  || group_concat('*=?', ' AND ') || ')'"
15432:     ", "
15433:     "     s.name  || '(' || group_concat(f.[from],  ', ') || ')'"
15434:     ", "
15435:     "     f.[table] || '(' || group_concat(COALESCE(f.[to], p.[name])) || ')'"
15436:     ", "
15437:     "     'CREATE INDEX ' || quote(s.name ||'_'|| group_concat(f.[from], '_'))"
15438:     "  || ' ON ' || quote(s.name) || '('"
15439:     "  || group_concat(quote(f.[from]) ||"
15440:     "        fkey_collate_clause("
15441:     "          f.[table], COALESCE(f.[to], p.[name]), s.name, f.[from]), ', ')"
15442:     "  || ');'"
15443:     ", "
15444:     "     f.[table] "
15445:     "FROM sqlite_schema AS s, pragma_foreign_key_list(s.name) AS f "
15446:     "LEFT JOIN pragma_table_info AS p ON (pk-1=seq AND p.arg=f.[table]) "
15447:     "GROUP BY s.name, f.id "
15448:     "ORDER BY (CASE WHEN ? THEN f.[table] ELSE s.name END)"
15449:   ;
15450:   const char *zGlobIPK = "SEARCH TABLE * USING INTEGER PRIMARY KEY (rowid=?)";
15451: 
15452:   for(i=2; i<nArg; i++){
15453:     int n = strlen30(azArg[i]);
15454:     if( n>1 && sqlite3_strnicmp("-verbose", azArg[i], n)==0 ){
15455:       bVerbose = 1;
15456:     }
15457:     else if( n>1 && sqlite3_strnicmp("-groupbyparent", azArg[i], n)==0 ){
15458:       bGroupByParent = 1;
15459:       zIndent = "    ";
15460:     }
15461:     else{
15462:       raw_printf(stderr, "Usage: %s %s ?-verbose? ?-groupbyparent?\n",
15463:           azArg[0], azArg[1]
15464:       );
15465:       return SQLITE_ERROR;
15466:     }
15467:   }
15468: 
15469:   /* Register the fkey_collate_clause() SQL function */
15470:   rc = sqlite3_create_function(db, "fkey_collate_clause", 4, SQLITE_UTF8,
15471:       0, shellFkeyCollateClause, 0, 0
15472:   );
15473: 
15474: 
15475:   if( rc==SQLITE_OK ){
15476:     rc = sqlite3_prepare_v2(db, zSql, -1, &pSql, 0);
15477:   }
15478:   if( rc==SQLITE_OK ){
15479:     sqlite3_bind_int(pSql, 1, bGroupByParent);
15480:   }
15481: 
15482:   if( rc==SQLITE_OK ){
15483:     int rc2;
15484:     char *zPrev = 0;
15485:     while( SQLITE_ROW==sqlite3_step(pSql) ){
15486:       int res = -1;
15487:       sqlite3_stmt *pExplain = 0;
15488:       const char *zEQP = (const char*)sqlite3_column_text(pSql, 0);
15489:       const char *zGlob = (const char*)sqlite3_column_text(pSql, 1);
15490:       const char *zFrom = (const char*)sqlite3_column_text(pSql, 2);
15491:       const char *zTarget = (const char*)sqlite3_column_text(pSql, 3);
15492:       const char *zCI = (const char*)sqlite3_column_text(pSql, 4);
15493:       const char *zParent = (const char*)sqlite3_column_text(pSql, 5);
15494: 
15495:       rc = sqlite3_prepare_v2(db, zEQP, -1, &pExplain, 0);
15496:       if( rc!=SQLITE_OK ) break;
15497:       if( SQLITE_ROW==sqlite3_step(pExplain) ){
15498:         const char *zPlan = (const char*)sqlite3_column_text(pExplain, 3);
15499:         res = (
15500:               0==sqlite3_strglob(zGlob, zPlan)
15501:            || 0==sqlite3_strglob(zGlobIPK, zPlan)
15502:         );
15503:       }
15504:       rc = sqlite3_finalize(pExplain);
15505:       if( rc!=SQLITE_OK ) break;
15506: 
15507:       if( res<0 ){
15508:         raw_printf(stderr, "Error: internal error");
15509:         break;
15510:       }else{
15511:         if( bGroupByParent
15512:         && (bVerbose || res==0)
15513:         && (zPrev==0 || sqlite3_stricmp(zParent, zPrev))
15514:         ){
15515:           raw_printf(out, "-- Parent table %s\n", zParent);
15516:           sqlite3_free(zPrev);
15517:           zPrev = sqlite3_mprintf("%s", zParent);
15518:         }
15519: 
15520:         if( res==0 ){
15521:           raw_printf(out, "%s%s --> %s\n", zIndent, zCI, zTarget);
15522:         }else if( bVerbose ){
15523:           raw_printf(out, "%s/* no extra indexes required for %s -> %s */\n",
15524:               zIndent, zFrom, zTarget
15525:           );
15526:         }
15527:       }
15528:     }
15529:     sqlite3_free(zPrev);
15530: 
15531:     if( rc!=SQLITE_OK ){
15532:       raw_printf(stderr, "%s\n", sqlite3_errmsg(db));
15533:     }
15534: 
15535:     rc2 = sqlite3_finalize(pSql);
15536:     if( rc==SQLITE_OK && rc2!=SQLITE_OK ){
15537:       rc = rc2;
15538:       raw_printf(stderr, "%s\n", sqlite3_errmsg(db));
15539:     }
15540:   }else{
15541:     raw_printf(stderr, "%s\n", sqlite3_errmsg(db));
15542:   }
15543: 
15544:   return rc;
15545: }
