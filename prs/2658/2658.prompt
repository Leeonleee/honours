16372: /*
16373: ** Like shellExec(), except that zFmt is a printf() style format string.
16374: */
16375: static void shellExecPrintf(sqlite3 *db, int *pRc, const char *zFmt, ...){
16376:   char *z = 0;
16377:   if( *pRc==SQLITE_OK ){
16378:     va_list ap;
16379:     va_start(ap, zFmt);
16380:     z = sqlite3_vmprintf(zFmt, ap);
16381:     va_end(ap);
16382:     if( z==0 ){
16383:       *pRc = SQLITE_NOMEM;
16384:     }else{
16385:       shellExec(db, pRc, z);
16386:     }
16387:     sqlite3_free(z);
16388:   }
16389: }
