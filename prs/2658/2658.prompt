455: unique_ptr<FileHandle> LocalFileSystem::OpenFile(const string &path, uint8_t flags, FileLockType lock_type,
456:                                                  FileCompressionType compression, FileOpener *opener) {
457: 	if (compression != FileCompressionType::UNCOMPRESSED) {
458: 		throw NotImplementedException("Unsupported compression type for default file system");
459: 	}
460: 	AssertValidFileFlags(flags);
461: 
462: 	DWORD desired_access;
463: 	DWORD share_mode;
464: 	DWORD creation_disposition = OPEN_EXISTING;
465: 	DWORD flags_and_attributes = FILE_ATTRIBUTE_NORMAL | FILE_FLAG_OVERLAPPED;
466: 	bool open_read = flags & FileFlags::FILE_FLAGS_READ;
467: 	bool open_write = flags & FileFlags::FILE_FLAGS_WRITE;
468: 	if (open_read && open_write) {
469: 		desired_access = GENERIC_READ | GENERIC_WRITE;
470: 		share_mode = 0;
471: 	} else if (open_read) {
472: 		desired_access = GENERIC_READ;
473: 		share_mode = FILE_SHARE_READ;
474: 	} else if (open_write) {
475: 		desired_access = GENERIC_WRITE;
476: 		share_mode = 0;
477: 	} else {
478: 		throw InternalException("READ, WRITE or both should be specified when opening a file");
479: 	}
480: 	if (open_write) {
481: 		if (flags & FileFlags::FILE_FLAGS_FILE_CREATE) {
482: 			creation_disposition = OPEN_ALWAYS;
483: 		} else if (flags & FileFlags::FILE_FLAGS_FILE_CREATE_NEW) {
484: 			creation_disposition = CREATE_ALWAYS;
485: 		}
486: 		if (flags & FileFlags::FILE_FLAGS_DIRECT_IO) {
487: 			flags_and_attributes |= FILE_FLAG_WRITE_THROUGH;
488: 		}
489: 	}
490: 	if (flags & FileFlags::FILE_FLAGS_DIRECT_IO) {
491: 		flags_and_attributes |= FILE_FLAG_NO_BUFFERING;
492: 	}
493: 	HANDLE hFile =
494: 	    CreateFileA(path.c_str(), desired_access, share_mode, NULL, creation_disposition, flags_and_attributes, NULL);
495: 	if (hFile == INVALID_HANDLE_VALUE) {
496: 		auto error = GetLastErrorAsString();
497: 		throw IOException("Cannot open file \"%s\": %s", path.c_str(), error);
498: 	}
499: 	auto handle = make_unique<WindowsFileHandle>(*this, path.c_str(), hFile);
500: 	if (flags & FileFlags::FILE_FLAGS_APPEND) {
501: 		auto file_size = GetFileSize(*handle);
502: 		SetFilePointer(*handle, file_size);
503: 	}
504: 	return move(handle);
505: }
