16435: /*
16436: ** When running the ".recover" command, each output table, and the special
16437: ** orphaned row table if it is required, is represented by an instance
16438: ** of the following struct.
16439: */
16440: typedef struct RecoverTable RecoverTable;
16441: struct RecoverTable {
16442:   char *zQuoted;                  /* Quoted version of table name */
16443:   int nCol;                       /* Number of columns in table */
16444:   char **azlCol;                  /* Array of column lists */
16445:   int iPk;                        /* Index of IPK column */
16446: };
16447: 
16448: /*
16449: ** Free a RecoverTable object allocated by recoverFindTable() or
16450: ** recoverOrphanTable().
16451: */
16452: static void recoverFreeTable(RecoverTable *pTab){
16453:   if( pTab ){
16454:     sqlite3_free(pTab->zQuoted);
16455:     if( pTab->azlCol ){
16456:       int i;
16457:       for(i=0; i<=pTab->nCol; i++){
16458:         sqlite3_free(pTab->azlCol[i]);
16459:       }
16460:       sqlite3_free(pTab->azlCol);
16461:     }
16462:     sqlite3_free(pTab);
16463:   }
16464: }
