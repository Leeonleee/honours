11066: /*
11067: ** Output the given string as a quoted string using SQL quoting conventions.
11068: **
11069: ** See also: output_quoted_escaped_string()
11070: */
11071: static void output_quoted_string(FILE *out, const char *z){
11072:   int i;
11073:   char c;
11074:   setBinaryMode(out, 1);
11075:   for(i=0; (c = z[i])!=0 && c!='\''; i++){}
11076:   if( c==0 ){
11077:     utf8_printf(out,"'%s'",z);
11078:   }else{
11079:     raw_printf(out, "'");
11080:     while( *z ){
11081:       for(i=0; (c = z[i])!=0 && c!='\''; i++){}
11082:       if( c=='\'' ) i++;
11083:       if( i ){
11084:         utf8_printf(out, "%.*s", i, z);
11085:         z += i;
11086:       }
11087:       if( c=='\'' ){
11088:         raw_printf(out, "'");
11089:         continue;
11090:       }
11091:       if( c==0 ){
11092:         break;
11093:       }
11094:       z++;
11095:     }
11096:     raw_printf(out, "'");
11097:   }
11098:   setTextMode(out, 1);
11099: }
