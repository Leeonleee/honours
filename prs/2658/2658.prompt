15109:     switch( ofst ){
15110:       case 56: {
15111:         if( val==1 ) raw_printf(p->out, " (utf8)");
15112:         if( val==2 ) raw_printf(p->out, " (utf16le)");
15113:         if( val==3 ) raw_printf(p->out, " (utf16be)");
15114:       }
15115:     }
15116:     raw_printf(p->out, "\n");
