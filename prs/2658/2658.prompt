14625: /* Append a single byte to z[] */
14626: static void import_append_char(ImportCtx *p, int c){
14627:   if( p->n+1>=p->nAlloc ){
14628:     p->nAlloc += p->nAlloc + 100;
14629:     p->z = sqlite3_realloc64(p->z, p->nAlloc);
14630:     if( p->z==0 ) shell_out_of_memory();
14631:   }
14632:   p->z[p->n++] = (char)c;
14633: }
14634: 
14635: /* Read a single field of CSV text.  Compatible with rfc4180 and extended
14636: ** with the option of having a separator other than ",".
14637: **
14638: **   +  Input comes from p->in.
14639: **   +  Store results in p->z of length p->n.  Space to hold p->z comes
14640: **      from sqlite3_malloc64().
14641: **   +  Use p->cSep as the column separator.  The default is ",".
14642: **   +  Use p->rSep as the row separator.  The default is "\n".
14643: **   +  Keep track of the line number in p->nLine.
14644: **   +  Store the character that terminates the field in p->cTerm.  Store
14645: **      EOF on end-of-file.
14646: **   +  Report syntax errors on stderr
14647: */
14648: static char *SQLITE_CDECL csv_read_one_field(ImportCtx *p){
14649:   int c;
14650:   int cSep = p->cColSep;
14651:   int rSep = p->cRowSep;
14652:   p->n = 0;
14653:   c = fgetc(p->in);
14654:   if( c==EOF || seenInterrupt ){
14655:     p->cTerm = EOF;
14656:     return 0;
14657:   }
14658:   if( c=='"' ){
14659:     int pc, ppc;
14660:     int startLine = p->nLine;
14661:     int cQuote = c;
14662:     pc = ppc = 0;
14663:     while( 1 ){
14664:       c = fgetc(p->in);
14665:       if( c==rSep ) p->nLine++;
14666:       if( c==cQuote ){
14667:         if( pc==cQuote ){
14668:           pc = 0;
14669:           continue;
14670:         }
14671:       }
14672:       if( (c==cSep && pc==cQuote)
14673:        || (c==rSep && pc==cQuote)
14674:        || (c==rSep && pc=='\r' && ppc==cQuote)
14675:        || (c==EOF && pc==cQuote)
14676:       ){
14677:         do{ p->n--; }while( p->z[p->n]!=cQuote );
14678:         p->cTerm = c;
14679:         break;
14680:       }
14681:       if( pc==cQuote && c!='\r' ){
14682:         utf8_printf(stderr, "%s:%d: unescaped %c character\n",
14683:                 p->zFile, p->nLine, cQuote);
14684:       }
14685:       if( c==EOF ){
14686:         utf8_printf(stderr, "%s:%d: unterminated %c-quoted field\n",
14687:                 p->zFile, startLine, cQuote);
14688:         p->cTerm = c;
14689:         break;
14690:       }
14691:       import_append_char(p, c);
14692:       ppc = pc;
14693:       pc = c;
14694:     }
14695:   }else{
14696:     /* If this is the first field being parsed and it begins with the
14697:     ** UTF-8 BOM  (0xEF BB BF) then skip the BOM */
14698:     if( (c&0xff)==0xef && p->bNotFirst==0 ){
14699:       import_append_char(p, c);
14700:       c = fgetc(p->in);
14701:       if( (c&0xff)==0xbb ){
14702:         import_append_char(p, c);
14703:         c = fgetc(p->in);
14704:         if( (c&0xff)==0xbf ){
14705:           p->bNotFirst = 1;
14706:           p->n = 0;
14707:           return csv_read_one_field(p);
14708:         }
14709:       }
14710:     }
14711:     while( c!=EOF && c!=cSep && c!=rSep ){
14712:       import_append_char(p, c);
14713:       c = fgetc(p->in);
14714:     }
14715:     if( c==rSep ){
14716:       p->nLine++;
14717:       if( p->n>0 && p->z[p->n-1]=='\r' ) p->n--;
14718:     }
14719:     p->cTerm = c;
14720:   }
14721:   if( p->z ) p->z[p->n] = 0;
14722:   p->bNotFirst = 1;
14723:   return p->z;
14724: }
14725: 
14726: /* Read a single field of ASCII delimited text.
14727: **
14728: **   +  Input comes from p->in.
14729: **   +  Store results in p->z of length p->n.  Space to hold p->z comes
14730: **      from sqlite3_malloc64().
14731: **   +  Use p->cSep as the column separator.  The default is "\x1F".
14732: **   +  Use p->rSep as the row separator.  The default is "\x1E".
14733: **   +  Keep track of the row number in p->nLine.
14734: **   +  Store the character that terminates the field in p->cTerm.  Store
14735: **      EOF on end-of-file.
14736: **   +  Report syntax errors on stderr
14737: */
14738: static char *SQLITE_CDECL ascii_read_one_field(ImportCtx *p){
14739:   int c;
14740:   int cSep = p->cColSep;
14741:   int rSep = p->cRowSep;
14742:   p->n = 0;
14743:   c = fgetc(p->in);
14744:   if( c==EOF || seenInterrupt ){
14745:     p->cTerm = EOF;
14746:     return 0;
14747:   }
14748:   while( c!=EOF && c!=cSep && c!=rSep ){
14749:     import_append_char(p, c);
14750:     c = fgetc(p->in);
14751:   }
14752:   if( c==rSep ){
14753:     p->nLine++;
14754:   }
14755:   p->cTerm = c;
14756:   if( p->z ) p->z[p->n] = 0;
14757:   return p->z;
14758: }
