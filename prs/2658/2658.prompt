12340: /*
12341: ** Parameter azArray points to a zero-terminated array of strings. zStr
12342: ** points to a single nul-terminated string. Return non-zero if zStr
12343: ** is equal, according to strcmp(), to any of the strings in the array.
12344: ** Otherwise, return zero.
12345: */
12346: static int str_in_array(const char *zStr, const char **azArray){
12347:   int i;
12348:   for(i=0; azArray[i]; i++){
12349:     if( 0==strcmp(zStr, azArray[i]) ) return 1;
12350:   }
12351:   return 0;
12352: }
