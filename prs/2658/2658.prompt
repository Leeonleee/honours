4782:           pA->a[i] = x - 10;
4783:         }else{
4784:           carry = 0;
4785:           pA->a[i] = x;
4786:         }
4787:       }
4788:     }else{
4789:       signed char *aA, *aB;
4790:       int borrow = 0;
4791:       rc = memcmp(pA->a, pB->a, nDigit);
4792:       if( rc<0 ){
4793:         aA = pB->a;
4794:         aB = pA->a;
4795:         pA->sign = !pA->sign;
4796:       }else{
4797:         aA = pA->a;
4798:         aB = pB->a;
4799:       }
4800:       for(i=nDigit-1; i>=0; i--){
4801:         int x = aA[i] - aB[i] - borrow;
4802:         if( x<0 ){
4803:           pA->a[i] = x+10;
4804:           borrow = 1;
4805:         }else{
4806:           pA->a[i] = x;
