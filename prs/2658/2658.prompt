14542:   if( mType==SQLITE_TRACE_CLOSE ){
14543:     utf8_printf(p->traceOut, "-- closing database connection\n");
14544:     return 0;
14545:   }
14546:   if( mType!=SQLITE_TRACE_ROW && ((const char*)pX)[0]=='-' ){
14547:     zSql = (const char*)pX;
14548:   }else{
14549:     pStmt = (sqlite3_stmt*)pP;
14550:     switch( p->eTraceType ){
14551:       case SHELL_TRACE_EXPANDED: {
14552:         zSql = sqlite3_expanded_sql(pStmt);
14553:         break;
14554:       }
14555: #ifdef SQLITE_ENABLE_NORMALIZE
14556:       case SHELL_TRACE_NORMALIZED: {
14557:         zSql = sqlite3_normalized_sql(pStmt);
14558:         break;
14559:       }
14560: #endif
14561:       default: {
14562:         zSql = sqlite3_sql(pStmt);
14563:         break;
14564:       }
14565:     }
14566:   }
14567:   if( zSql==0 ) return 0;
14568:   nSql = strlen30(zSql);
14569:   while( nSql>0 && zSql[nSql-1]==';' ){ nSql--; }
14570:   switch( mType ){
14571:     case SQLITE_TRACE_ROW:
14572:     case SQLITE_TRACE_STMT: {
14573:       utf8_printf(p->traceOut, "%.*s;\n", nSql, zSql);
14574:       break;
14575:     }
14576:     case SQLITE_TRACE_PROFILE: {
14577:       sqlite3_int64 nNanosec = *(sqlite3_int64*)pX;
14578:       utf8_printf(p->traceOut, "%.*s; -- %lld ns\n", nSql, zSql, nNanosec);
14579:       break;
14580:     }
14581:   }
14582:   return 0;
14583: }
14584: #endif
