14945: /*
14946: ** Open a new database file named "zNewDb".  Try to recover as much information
14947: ** as possible out of the main database (which might be corrupt) and write it
14948: ** into zNewDb.
14949: */
14950: static void tryToClone(ShellState *p, const char *zNewDb){
14951:   int rc;
14952:   sqlite3 *newDb = 0;
14953:   if( access(zNewDb,0)==0 ){
14954:     utf8_printf(stderr, "File \"%s\" already exists.\n", zNewDb);
14955:     return;
14956:   }
14957:   rc = sqlite3_open(zNewDb, &newDb);
14958:   if( rc ){
14959:     utf8_printf(stderr, "Cannot create output database: %s\n",
14960:             sqlite3_errmsg(newDb));
14961:   }else{
14962:     sqlite3_exec(p->db, "PRAGMA writable_schema=ON;", 0, 0, 0);
14963:     sqlite3_exec(newDb, "BEGIN EXCLUSIVE;", 0, 0, 0);
14964:     tryToCloneSchema(p, newDb, "type='table'", tryToCloneData);
14965:     tryToCloneSchema(p, newDb, "type!='table'", 0);
14966:     sqlite3_exec(newDb, "COMMIT;", 0, 0, 0);
14967:     sqlite3_exec(p->db, "PRAGMA writable_schema=OFF;", 0, 0, 0);
14968:   }
14969:   close_db(newDb);
14970: }
