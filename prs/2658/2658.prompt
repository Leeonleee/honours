13370: /*
13371: ** This is a different callback routine used for dumping the database.
13372: ** Each row received by this callback consists of a table name,
13373: ** the table type ("index" or "table") and SQL to create the table.
13374: ** This routine should print text sufficient to recreate the table.
13375: */
13376: static int dump_callback(void *pArg, int nArg, char **azArg, char **azNotUsed){
13377:   int rc;
13378:   const char *zTable;
13379:   const char *zType;
13380:   const char *zSql;
13381:   ShellState *p = (ShellState *)pArg;
13382: 
13383:   UNUSED_PARAMETER(azNotUsed);
13384:   if( nArg!=3 || azArg==0 ) return 0;
13385:   zTable = azArg[0];
13386:   zType = azArg[1];
13387:   zSql = azArg[2];
13388: 
13389:   if( strcmp(zTable, "sqlite_sequence")==0 ){
13390:     raw_printf(p->out, "DELETE FROM sqlite_sequence;\n");
13391:   }else if( sqlite3_strglob("sqlite_stat?", zTable)==0 ){
13392:     raw_printf(p->out, "ANALYZE sqlite_schema;\n");
13393:   }else if( strncmp(zTable, "sqlite_", 7)==0 ){
13394:     return 0;
13395:   }else if( strncmp(zSql, "CREATE VIRTUAL TABLE", 20)==0 ){
13396:     char *zIns;
13397:     if( !p->writableSchema ){
13398:       raw_printf(p->out, "PRAGMA writable_schema=ON;\n");
13399:       p->writableSchema = 1;
13400:     }
13401:     zIns = sqlite3_mprintf(
13402:        "INSERT INTO sqlite_schema(type,name,tbl_name,rootpage,sql)"
13403:        "VALUES('table','%q','%q',0,'%q');",
13404:        zTable, zTable, zSql);
13405:     utf8_printf(p->out, "%s\n", zIns);
13406:     sqlite3_free(zIns);
13407:     return 0;
13408:   }else{
13409:     printSchemaLine(p->out, zSql, ";\n");
13410:   }
13411: 
13412:   if( strcmp(zType, "table")==0 ){
13413:     ShellText sSelect;
13414:     ShellText sTable;
13415:     char **azCol;
13416:     int i;
13417:     char *savedDestTable;
13418:     int savedMode;
13419: 
13420:     azCol = tableColumnList(p, zTable);
13421:     if( azCol==0 ){
13422:       p->nErr++;
13423:       return 0;
13424:     }
13425: 
13426:     /* Always quote the table name, even if it appears to be pure ascii,
13427:     ** in case it is a keyword. Ex:  INSERT INTO "table" ... */
13428:     initText(&sTable);
13429:     appendText(&sTable, zTable, quoteChar(zTable));
13430:     /* If preserving the rowid, add a column list after the table name.
13431:     ** In other words:  "INSERT INTO tab(rowid,a,b,c,...) VALUES(...)"
13432:     ** instead of the usual "INSERT INTO tab VALUES(...)".
13433:     */
13434:     if( azCol[0] ){
13435:       appendText(&sTable, "(", 0);
13436:       appendText(&sTable, azCol[0], 0);
13437:       for(i=1; azCol[i]; i++){
13438:         appendText(&sTable, ",", 0);
13439:         appendText(&sTable, azCol[i], quoteChar(azCol[i]));
13440:       }
13441:       appendText(&sTable, ")", 0);
13442:     }
13443: 
13444:     /* Build an appropriate SELECT statement */
13445:     initText(&sSelect);
13446:     appendText(&sSelect, "SELECT ", 0);
13447:     if( azCol[0] ){
13448:       appendText(&sSelect, azCol[0], 0);
13449:       appendText(&sSelect, ",", 0);
13450:     }
13451:     for(i=1; azCol[i]; i++){
13452:       appendText(&sSelect, azCol[i], quoteChar(azCol[i]));
13453:       if( azCol[i+1] ){
13454:         appendText(&sSelect, ",", 0);
13455:       }
13456:     }
13457:     freeColumnList(azCol);
13458:     appendText(&sSelect, " FROM ", 0);
13459:     appendText(&sSelect, zTable, quoteChar(zTable));
13460: 
13461:     savedDestTable = p->zDestTable;
13462:     savedMode = p->mode;
13463:     p->zDestTable = sTable.z;
13464:     p->mode = p->cMode = MODE_Insert;
13465:     rc = shell_exec(p, sSelect.z, 0);
13466:     if( (rc&0xff)==SQLITE_CORRUPT ){
13467:       raw_printf(p->out, "/****** CORRUPTION ERROR *******/\n");
13468:       toggleSelectOrder(p->db);
13469:       shell_exec(p, sSelect.z, 0);
13470:       toggleSelectOrder(p->db);
13471:     }
13472:     p->zDestTable = savedDestTable;
13473:     p->mode = savedMode;
13474:     freeText(&sTable);
13475:     freeText(&sSelect);
13476:     if( rc ) p->nErr++;
13477:   }
13478:   return 0;
13479: }
