13077: /*
13078: ** Execute a statement or set of statements.  Print
13079: ** any result rows/columns depending on the current mode
13080: ** set via the supplied callback.
13081: **
13082: ** This is very similar to SQLite's built-in sqlite3_exec()
13083: ** function except it takes a slightly different callback
13084: ** and callback data argument.
13085: */
13086: static int shell_exec(
13087:   ShellState *pArg,                         /* Pointer to ShellState */
13088:   const char *zSql,                         /* SQL to be evaluated */
13089:   char **pzErrMsg                           /* Error msg written here */
13090: ){
13091:   sqlite3_stmt *pStmt = NULL;     /* Statement to execute. */
13092:   int rc = SQLITE_OK;             /* Return Code */
13093:   int rc2;
13094:   const char *zLeftover;          /* Tail of unprocessed SQL */
13095:   sqlite3 *db = pArg->db;
13096: 
13097:   if( pzErrMsg ){
13098:     *pzErrMsg = NULL;
13099:   }
13100: 
13101: #ifndef SQLITE_OMIT_VIRTUALTABLE
13102:   if( pArg->expert.pExpert ){
13103:     rc = expertHandleSQL(pArg, zSql, pzErrMsg);
13104:     return expertFinish(pArg, (rc!=SQLITE_OK), pzErrMsg);
13105:   }
13106: #endif
13107: 
13108:   while( zSql[0] && (SQLITE_OK == rc) ){
13109:     static const char *zStmtSql;
13110:     rc = sqlite3_prepare_v2(db, zSql, -1, &pStmt, &zLeftover);
13111:     if( SQLITE_OK != rc ){
13112:       if( pzErrMsg ){
13113:         *pzErrMsg = save_err_msg(db);
13114:       }
13115:     }else{
13116:       if( !pStmt ){
13117:         /* this happens for a comment or white-space */
13118:         zSql = zLeftover;
13119:         while( IsSpace(zSql[0]) ) zSql++;
13120:         continue;
13121:       }
13122:       zStmtSql = sqlite3_sql(pStmt);
13123:       if( zStmtSql==0 ) zStmtSql = "";
13124:       while( IsSpace(zStmtSql[0]) ) zStmtSql++;
13125: 
13126:       /* save off the prepared statment handle and reset row count */
13127:       if( pArg ){
13128:         pArg->pStmt = pStmt;
13129:         pArg->cnt = 0;
13130:       }
13131: 
13132:       /* echo the sql statement if echo on */
13133:       if( pArg && ShellHasFlag(pArg, SHFLG_Echo) ){
13134:         utf8_printf(pArg->out, "%s\n", zStmtSql ? zStmtSql : zSql);
13135:       }
13136: 
13137:       /* Show the EXPLAIN QUERY PLAN if .eqp is on */
13138:       if( pArg && pArg->autoEQP && sqlite3_stmt_isexplain(pStmt)==0 ){
13139:         sqlite3_stmt *pExplain;
13140:         char *zEQP;
13141:         int triggerEQP = 0;
13142:         disable_debug_trace_modes();
13143:         sqlite3_db_config(db, SQLITE_DBCONFIG_TRIGGER_EQP, -1, &triggerEQP);
13144:         if( pArg->autoEQP>=AUTOEQP_trigger ){
13145:           sqlite3_db_config(db, SQLITE_DBCONFIG_TRIGGER_EQP, 1, 0);
13146:         }
13147:         zEQP = sqlite3_mprintf("EXPLAIN QUERY PLAN %s", zStmtSql);
13148:         rc = sqlite3_prepare_v2(db, zEQP, -1, &pExplain, 0);
13149:         if( rc==SQLITE_OK ){
13150:           while( sqlite3_step(pExplain)==SQLITE_ROW ){
13151:             const char *zEQPLine = (const char*)sqlite3_column_text(pExplain,3);
13152:             int iEqpId = sqlite3_column_int(pExplain, 0);
13153:             int iParentId = sqlite3_column_int(pExplain, 1);
13154:             if( zEQPLine==0 ) zEQPLine = "";
13155:             if( zEQPLine[0]=='-' ) eqp_render(pArg);
13156:             eqp_append(pArg, iEqpId, iParentId, zEQPLine);
13157:           }
13158:           eqp_render(pArg);
13159:         }
13160:         sqlite3_finalize(pExplain);
13161:         sqlite3_free(zEQP);
13162:         if( pArg->autoEQP>=AUTOEQP_full ){
13163:           /* Also do an EXPLAIN for ".eqp full" mode */
13164:           zEQP = sqlite3_mprintf("EXPLAIN %s", zStmtSql);
13165:           rc = sqlite3_prepare_v2(db, zEQP, -1, &pExplain, 0);
13166:           if( rc==SQLITE_OK ){
13167:             pArg->cMode = MODE_Explain;
13168:             explain_data_prepare(pArg, pExplain);
13169:             exec_prepared_stmt(pArg, pExplain);
13170:             explain_data_delete(pArg);
13171:           }
13172:           sqlite3_finalize(pExplain);
13173:           sqlite3_free(zEQP);
13174:         }
13175:         if( pArg->autoEQP>=AUTOEQP_trigger && triggerEQP==0 ){
13176:           sqlite3_db_config(db, SQLITE_DBCONFIG_TRIGGER_EQP, 0, 0);
13177:           /* Reprepare pStmt before reactiving trace modes */
13178:           sqlite3_finalize(pStmt);
13179:           sqlite3_prepare_v2(db, zSql, -1, &pStmt, 0);
13180:           if( pArg ) pArg->pStmt = pStmt;
13181:         }
13182:         restore_debug_trace_modes();
13183:       }
13184: 
13185:       if( pArg ){
13186:         pArg->cMode = pArg->mode;
13187:         if( pArg->autoExplain ){
13188:           if( sqlite3_stmt_isexplain(pStmt)==1 ){
13189:             pArg->cMode = MODE_Explain;
13190:           }
13191:           if( sqlite3_stmt_isexplain(pStmt)==2 ){
13192:             pArg->cMode = MODE_EQP;
13193:           }
13194:         }
13195: 
13196:         /* If the shell is currently in ".explain" mode, gather the extra
13197:         ** data required to add indents to the output.*/
13198:         // if( pArg->cMode==MODE_Explain ){
13199:         //   explain_data_prepare(pArg, pStmt);
13200:         // }
13201:       }
13202: 
13203:       bind_prepared_stmt(pArg, pStmt);
13204:       exec_prepared_stmt(pArg, pStmt);
13205:       explain_data_delete(pArg);
13206:       eqp_render(pArg);
13207: 
13208:       /* print usage stats if stats on */
13209:       if( pArg && pArg->statsOn ){
13210:         display_stats(db, pArg, 0);
13211:       }
13212: 
13213:       /* print loop-counters if required */
13214:       if( pArg && pArg->scanstatsOn ){
13215:         display_scanstats(db, pArg);
13216:       }
13217: 
13218:       /* Finalize the statement just executed. If this fails, save a
13219:       ** copy of the error message. Otherwise, set zSql to point to the
13220:       ** next statement to execute. */
13221:       rc2 = sqlite3_finalize(pStmt);
13222:       if( rc!=SQLITE_NOMEM ) rc = rc2;
13223:       if( rc==SQLITE_OK ){
13224:         zSql = zLeftover;
13225:         while( IsSpace(zSql[0]) ) zSql++;
13226:       }else if( pzErrMsg ){
13227:         *pzErrMsg = save_err_msg(db);
13228:       }
13229: 
13230:       /* clear saved stmt handle */
13231:       if( pArg ){
13232:         pArg->pStmt = NULL;
13233:       }
13234:     }
13235:   } /* end while */
13236: 
13237:   return rc;
13238: }
