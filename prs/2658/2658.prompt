14595: /*
14596: ** An object used to read a CSV and other files for import.
14597: */
14598: typedef struct ImportCtx ImportCtx;
14599: struct ImportCtx {
14600:   const char *zFile;  /* Name of the input file */
14601:   FILE *in;           /* Read the CSV text from this input stream */
14602:   int (SQLITE_CDECL *xCloser)(FILE*);      /* Func to close in */
14603:   char *z;            /* Accumulated text for a field */
14604:   int n;              /* Number of bytes in z */
14605:   int nAlloc;         /* Space allocated for z[] */
14606:   int nLine;          /* Current line number */
14607:   int nRow;           /* Number of rows imported */
14608:   int nErr;           /* Number of errors encountered */
14609:   int bNotFirst;      /* True if one or more bytes already read */
14610:   int cTerm;          /* Character that terminated the most recent field */
14611:   int cColSep;        /* The column separator character.  (Usually ",") */
14612:   int cRowSep;        /* The row separator character.  (Usually "\n") */
14613: };
