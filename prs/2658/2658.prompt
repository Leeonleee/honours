14210: /*
14211: ** Make sure the database is open.  If it is not, then open it.  If
14212: ** the database fails to open, print an error message and exit.
14213: */
14214: static void open_db(ShellState *p, int openFlags){
14215:   if( p->db==0 ){
14216:     if( p->openMode==SHELL_OPEN_UNSPEC ){
14217:       if( p->zDbFilename==0 || p->zDbFilename[0]==0 ){
14218:         p->openMode = SHELL_OPEN_NORMAL;
14219:       }else{
14220:         p->openMode = (u8)deduceDatabaseType(p->zDbFilename,
14221:                              (openFlags & OPEN_DB_ZIPFILE)!=0);
14222:       }
14223:     }
14224:     switch( p->openMode ){
14225:       case SHELL_OPEN_APPENDVFS: {
14226:         sqlite3_open_v2(p->zDbFilename, &p->db,
14227:            SQLITE_OPEN_READWRITE|SQLITE_OPEN_CREATE|p->openFlags, "apndvfs");
14228:         break;
14229:       }
14230:       case SHELL_OPEN_HEXDB:
14231:       case SHELL_OPEN_DESERIALIZE: {
14232:         sqlite3_open(0, &p->db);
14233:         break;
14234:       }
14235:       case SHELL_OPEN_ZIPFILE: {
14236:         sqlite3_open(":memory:", &p->db);
14237:         break;
14238:       }
14239:       case SHELL_OPEN_READONLY: {
14240:         sqlite3_open_v2(p->zDbFilename, &p->db,
14241:             SQLITE_OPEN_READONLY|p->openFlags, 0);
14242:         break;
14243:       }
14244:       case SHELL_OPEN_UNSPEC:
14245:       case SHELL_OPEN_NORMAL: {
14246:         sqlite3_open_v2(p->zDbFilename, &p->db,
14247:            SQLITE_OPEN_READWRITE|SQLITE_OPEN_CREATE|p->openFlags, 0);
14248:         break;
14249:       }
14250:     }
14251:     globalDb = p->db;
14252:     if( p->db==0 || SQLITE_OK!=sqlite3_errcode(p->db) ){
14253:       utf8_printf(stderr,"Error: unable to open database \"%s\": %s\n",
14254:           p->zDbFilename, sqlite3_errmsg(p->db));
14255:       if( openFlags & OPEN_DB_KEEPALIVE ){
14256:         sqlite3_open(":memory:", &p->db);
14257:         return;
14258:       }
14259:       exit(1);
14260:     }
14261: #ifndef SQLITE_OMIT_LOAD_EXTENSION
14262:     sqlite3_enable_load_extension(p->db, 1);
14263: #endif
14264:     sqlite3_fileio_init(p->db, 0, 0);
14265:     sqlite3_shathree_init(p->db, 0, 0);
14266:     sqlite3_completion_init(p->db, 0, 0);
14267:     sqlite3_uint_init(p->db, 0, 0);
14268:     sqlite3_decimal_init(p->db, 0, 0);
14269:     sqlite3_ieee_init(p->db, 0, 0);
14270: #if !defined(SQLITE_OMIT_VIRTUALTABLE) && defined(SQLITE_ENABLE_DBPAGE_VTAB)
14271:     sqlite3_dbdata_init(p->db, 0, 0);
14272: #endif
14273: #ifdef SQLITE_HAVE_ZLIB
14274:     sqlite3_zipfile_init(p->db, 0, 0);
14275:     sqlite3_sqlar_init(p->db, 0, 0);
14276: #endif
14277:     sqlite3_create_function(p->db, "shell_add_schema", 3, SQLITE_UTF8, 0,
14278:                             shellAddSchemaName, 0, 0);
14279:     sqlite3_create_function(p->db, "shell_module_schema", 1, SQLITE_UTF8, 0,
14280:                             shellModuleSchema, 0, 0);
14281:     sqlite3_create_function(p->db, "shell_putsnl", 1, SQLITE_UTF8, p,
14282:                             shellPutsFunc, 0, 0);
14283:     sqlite3_create_function(p->db, "shell_escape_crnl", 1, SQLITE_UTF8, 0,
14284:                             shellEscapeCrnl, 0, 0);
14285:     sqlite3_create_function(p->db, "shell_int32", 2, SQLITE_UTF8, 0,
14286:                             shellInt32, 0, 0);
14287:     sqlite3_create_function(p->db, "shell_idquote", 1, SQLITE_UTF8, 0,
14288:                             shellIdQuote, 0, 0);
14289: #ifndef SQLITE_NOHAVE_SYSTEM
14290:     sqlite3_create_function(p->db, "edit", 1, SQLITE_UTF8, 0,
14291:                             editFunc, 0, 0);
14292:     sqlite3_create_function(p->db, "edit", 2, SQLITE_UTF8, 0,
14293:                             editFunc, 0, 0);
14294: #endif
14295:     if( p->openMode==SHELL_OPEN_ZIPFILE ){
14296:       char *zSql = sqlite3_mprintf(
14297:          "CREATE VIRTUAL TABLE zip USING zipfile(%Q);", p->zDbFilename);
14298:       sqlite3_exec(p->db, zSql, 0, 0, 0);
14299:       sqlite3_free(zSql);
14300:     }
14301: #ifdef SQLITE_ENABLE_DESERIALIZE
14302:     else
14303:     if( p->openMode==SHELL_OPEN_DESERIALIZE || p->openMode==SHELL_OPEN_HEXDB ){
14304:       int rc;
14305:       int nData = 0;
14306:       unsigned char *aData;
14307:       if( p->openMode==SHELL_OPEN_DESERIALIZE ){
14308:         aData = (unsigned char*)readFile(p->zDbFilename, &nData);
14309:       }else{
14310:         aData = readHexDb(p, &nData);
14311:         if( aData==0 ){
14312:           return;
14313:         }
14314:       }
14315:       rc = sqlite3_deserialize(p->db, "main", aData, nData, nData,
14316:                    SQLITE_DESERIALIZE_RESIZEABLE |
14317:                    SQLITE_DESERIALIZE_FREEONCLOSE);
14318:       if( rc ){
14319:         utf8_printf(stderr, "Error: sqlite3_deserialize() returns %d\n", rc);
14320:       }
14321:       if( p->szMax>0 ){
14322:         sqlite3_file_control(p->db, "main", SQLITE_FCNTL_SIZE_LIMIT, &p->szMax);
14323:       }
14324:     }
14325: #endif
14326:   }
14327: }
