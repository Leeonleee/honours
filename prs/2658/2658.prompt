12302: #else
12303:   int i, k, n, mx;
12304:   raw_printf(pArg->out, "-------- scanstats --------\n");
12305:   mx = 0;
12306:   for(k=0; k<=mx; k++){
12307:     double rEstLoop = 1.0;
12308:     for(i=n=0; 1; i++){
12309:       sqlite3_stmt *p = pArg->pStmt;
12310:       sqlite3_int64 nLoop, nVisit;
12311:       double rEst;
12312:       int iSid;
12313:       const char *zExplain;
12314:       if( sqlite3_stmt_scanstatus(p, i, SQLITE_SCANSTAT_NLOOP, (void*)&nLoop) ){
12315:         break;
12316:       }
12317:       sqlite3_stmt_scanstatus(p, i, SQLITE_SCANSTAT_SELECTID, (void*)&iSid);
12318:       if( iSid>mx ) mx = iSid;
12319:       if( iSid!=k ) continue;
12320:       if( n==0 ){
12321:         rEstLoop = (double)nLoop;
12322:         if( k>0 ) raw_printf(pArg->out, "-------- subquery %d -------\n", k);
12323:       }
12324:       n++;
12325:       sqlite3_stmt_scanstatus(p, i, SQLITE_SCANSTAT_NVISIT, (void*)&nVisit);
12326:       sqlite3_stmt_scanstatus(p, i, SQLITE_SCANSTAT_EST, (void*)&rEst);
12327:       sqlite3_stmt_scanstatus(p, i, SQLITE_SCANSTAT_EXPLAIN, (void*)&zExplain);
12328:       utf8_printf(pArg->out, "Loop %2d: %s\n", n, zExplain);
12329:       rEstLoop *= rEst;
12330:       raw_printf(pArg->out,
12331:           "         nLoop=%-8lld nRow=%-8lld estRow=%-8lld estRow/Loop=%-8g\n",
12332:           nLoop, nVisit, (sqlite3_int64)(rEstLoop+0.5), rEst
12333:       );
12334:     }
12335:   }
12336:   raw_printf(pArg->out, "---------------------------\n");
12337: #endif
12338: }
