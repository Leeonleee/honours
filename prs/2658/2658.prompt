15045: static int shell_dbinfo_command(ShellState *p, int nArg, char **azArg){
15046:   static const struct { const char *zName; int ofst; } aField[] = {
15047:      { "file change counter:",  24  },
15048:      { "database page count:",  28  },
15049:      { "freelist page count:",  36  },
15050:      { "schema cookie:",        40  },
15051:      { "schema format:",        44  },
15052:      { "default cache size:",   48  },
15053:      { "autovacuum top root:",  52  },
15054:      { "incremental vacuum:",   64  },
15055:      { "text encoding:",        56  },
15056:      { "user version:",         60  },
