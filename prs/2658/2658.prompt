12019:   if( cQuote ) n += n+2;
12020:   z = p->zDestTable = malloc( n+1 );
12021:   if( z==0 ) shell_out_of_memory();
12022:   n = 0;
12023:   if( cQuote ) z[n++] = cQuote;
