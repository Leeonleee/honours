15728: #define AR_SWITCH_DRYRUN     11
15729: 
15730: static int arProcessSwitch(ArCommand *pAr, int eSwitch, const char *zArg){
15731:   switch( eSwitch ){
15732:     case AR_CMD_CREATE:
15733:     case AR_CMD_EXTRACT:
15734:     case AR_CMD_LIST:
15735:     case AR_CMD_UPDATE:
15736:     case AR_CMD_INSERT:
15737:     case AR_CMD_HELP:
15738:       if( pAr->eCmd ){
15739:         return arErrorMsg(pAr, "multiple command options");
15740:       }
15741:       pAr->eCmd = eSwitch;
15742:       break;
15743: 
15744:     case AR_SWITCH_DRYRUN:
15745:       pAr->bDryRun = 1;
15746:       break;
15747:     case AR_SWITCH_VERBOSE:
15748:       pAr->bVerbose = 1;
15749:       break;
15750:     case AR_SWITCH_APPEND:
15751:       pAr->bAppend = 1;
15752:       /* Fall thru into --file */
15753:     case AR_SWITCH_FILE:
15754:       pAr->zFile = zArg;
15755:       break;
15756:     case AR_SWITCH_DIRECTORY:
15757:       pAr->zDir = zArg;
15758:       break;
15759:   }
15760: 
15761:   return SQLITE_OK;
15762: }
