607: time_t LocalFileSystem::GetLastModifiedTime(FileHandle &handle) {
608: 	HANDLE hFile = ((WindowsFileHandle &)handle).fd;
609: 
610: 	// https://docs.microsoft.com/en-us/windows/win32/api/fileapi/nf-fileapi-getfiletime
611: 	FILETIME last_write;
612: 	if (GetFileTime(hFile, nullptr, nullptr, &last_write) == 0) {
613: 		return -1;
614: 	}
615: 
616: 	// https://stackoverflow.com/questions/29266743/what-is-dwlowdatetime-and-dwhighdatetime
617: 	ULARGE_INTEGER ul;
618: 	ul.LowPart = last_write.dwLowDateTime;
619: 	ul.HighPart = last_write.dwHighDateTime;
620: 	int64_t fileTime64 = ul.QuadPart;
621: 
622: 	// fileTime64 contains a 64-bit value representing the number of
623: 	// 100-nanosecond intervals since January 1, 1601 (UTC).
624: 	// https://docs.microsoft.com/en-us/windows/win32/api/minwinbase/ns-minwinbase-filetime
625: 
626: 	// Adapted from: https://stackoverflow.com/questions/6161776/convert-windows-filetime-to-second-in-unix-linux
627: 	const auto WINDOWS_TICK = 10000000;
628: 	const auto SEC_TO_UNIX_EPOCH = 11644473600LL;
629: 	time_t result = (fileTime64 / WINDOWS_TICK - SEC_TO_UNIX_EPOCH);
630: 	return result;
631: }
