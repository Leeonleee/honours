795: static bool HasGlob(const string &str) {
796: 	for (idx_t i = 0; i < str.size(); i++) {
797: 		switch (str[i]) {
798: 		case '*':
799: 		case '?':
800: 		case '[':
801: 			return true;
802: 		default:
803: 			break;
804: 		}
805: 	}
