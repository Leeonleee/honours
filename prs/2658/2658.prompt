663: static void delete_dir_special_snowflake_windows(FileSystem &fs, string directory) {
664: 	if (directory.size() + 3 > MAX_PATH) {
665: 		throw IOException("Pathname too long");
666: 	}
667: 	// create search pattern
668: 	TCHAR szDir[MAX_PATH];
669: 	snprintf(szDir, MAX_PATH, "%s\\*", directory.c_str());
670: 
671: 	WIN32_FIND_DATA ffd;
672: 	HANDLE hFind = FindFirstFile(szDir, &ffd);
673: 	if (hFind == INVALID_HANDLE_VALUE) {
674: 		return;
675: 	}
676: 
677: 	do {
678: 		if (string(ffd.cFileName) == "." || string(ffd.cFileName) == "..") {
679: 			continue;
680: 		}
681: 		if (ffd.dwFileAttributes & FILE_ATTRIBUTE_DIRECTORY) {
682: 			// recurse to zap directory contents
683: 			delete_dir_special_snowflake_windows(fs, fs.JoinPath(directory, ffd.cFileName));
684: 		} else {
685: 			if (strlen(ffd.cFileName) + directory.size() + 1 > MAX_PATH) {
686: 				throw IOException("Pathname too long");
687: 			}
688: 			// create search pattern
689: 			TCHAR del_path[MAX_PATH];
690: 			snprintf(del_path, MAX_PATH, "%s\\%s", directory.c_str(), ffd.cFileName);
691: 			if (!DeleteFileA(del_path)) {
692: 				throw IOException("Failed to delete directory entry");
693: 			}
694: 		}
695: 	} while (FindNextFile(hFind, &ffd) != 0);
696: 
697: 	DWORD dwError = GetLastError();
698: 	if (dwError != ERROR_NO_MORE_FILES) {
699: 		throw IOException("Something went wrong");
700: 	}
701: 	FindClose(hFind);
702: 
703: 	if (!RemoveDirectoryA(directory.c_str())) {
704: 		throw IOException("Failed to delete directory");
705: 	}
706: }
