825: vector<string> LocalFileSystem::Glob(const string &path) {
826: 	if (path.empty()) {
827: 		return vector<string>();
828: 	}
829: 	// first check if the path has a glob at all
830: 	if (!HasGlob(path)) {
831: 		// no glob: return only the file (if it exists)
832: 		vector<string> result;
833: 		if (FileExists(path)) {
834: 			result.push_back(path);
835: 		}
836: 		return result;
837: 	}
838: 	// split up the path into separate chunks
839: 	vector<string> splits;
840: 	idx_t last_pos = 0;
841: 	for (idx_t i = 0; i < path.size(); i++) {
842: 		if (path[i] == '\\' || path[i] == '/') {
843: 			if (i == last_pos) {
844: 				// empty: skip this position
845: 				last_pos = i + 1;
846: 				continue;
847: 			}
848: 			if (splits.empty()) {
849: 				splits.push_back(path.substr(0, i));
850: 			} else {
851: 				splits.push_back(path.substr(last_pos, i - last_pos));
852: 			}
853: 			last_pos = i + 1;
854: 		}
855: 	}
856: 	splits.push_back(path.substr(last_pos, path.size() - last_pos));
857: 	// handle absolute paths
858: 	bool absolute_path = false;
859: 	if (path[0] == '/') {
860: 		// first character is a slash -  unix absolute path
861: 		absolute_path = true;
862: 	} else if (StringUtil::Contains(splits[0], ":")) {
863: 		// first split has a colon -  windows absolute path
864: 		absolute_path = true;
865: 	} else if (splits[0] == "~") {
866: 		// starts with home directory
867: 		auto home_directory = GetHomeDirectory();
868: 		if (!home_directory.empty()) {
869: 			absolute_path = true;
870: 			splits[0] = home_directory;
871: 		}
872: 	}
873: 	vector<string> previous_directories;
874: 	if (absolute_path) {
875: 		// for absolute paths, we don't start by scanning the current directory
876: 		previous_directories.push_back(splits[0]);
877: 	}
878: 	for (idx_t i = absolute_path ? 1 : 0; i < splits.size(); i++) {
879: 		bool is_last_chunk = i + 1 == splits.size();
880: 		bool has_glob = HasGlob(splits[i]);
881: 		// if it's the last chunk we need to find files, otherwise we find directories
882: 		// not the last chunk: gather a list of all directories that match the glob pattern
883: 		vector<string> result;
884: 		if (!has_glob) {
885: 			// no glob, just append as-is
886: 			if (previous_directories.empty()) {
887: 				result.push_back(splits[i]);
888: 			} else {
889: 				for (auto &prev_directory : previous_directories) {
890: 					result.push_back(JoinPath(prev_directory, splits[i]));
891: 				}
892: 			}
893: 		} else {
894: 			if (previous_directories.empty()) {
895: 				// no previous directories: list in the current path
896: 				GlobFiles(*this, ".", splits[i], !is_last_chunk, result, false);
897: 			} else {
898: 				// previous directories
899: 				// we iterate over each of the previous directories, and apply the glob of the current directory
900: 				for (auto &prev_directory : previous_directories) {
901: 					GlobFiles(*this, prev_directory, splits[i], !is_last_chunk, result, true);
902: 				}
903: 			}
904: 		}
905: 		if (is_last_chunk || result.empty()) {
906: 			return result;
907: 		}
908: 		previous_directories = move(result);
909: 	}
910: 	return vector<string>();
911: }
