5863: /*
5864: ** Read and return a 16-bit little-endian unsigned integer from buffer aBuf.
5865: */
5866: static u16 zipfileGetU16(const u8 *aBuf){
5867:   return (aBuf[1] << 8) + aBuf[0];
5868: }
