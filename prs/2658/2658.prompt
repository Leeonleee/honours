314: int RemoveDirectoryRecursive(const char *path) {
315: 	DIR *d = opendir(path);
316: 	idx_t path_len = (idx_t)strlen(path);
317: 	int r = -1;
318: 
319: 	if (d) {
320: 		struct dirent *p;
321: 		r = 0;
322: 		while (!r && (p = readdir(d))) {
323: 			int r2 = -1;
324: 			char *buf;
325: 			idx_t len;
326: 			/* Skip the names "." and ".." as we don't want to recurse on them. */
327: 			if (!strcmp(p->d_name, ".") || !strcmp(p->d_name, "..")) {
328: 				continue;
329: 			}
330: 			len = path_len + (idx_t)strlen(p->d_name) + 2;
331: 			buf = new char[len];
332: 			if (buf) {
333: 				struct stat statbuf;
334: 				snprintf(buf, len, "%s/%s", path, p->d_name);
335: 				if (!stat(buf, &statbuf)) {
336: 					if (S_ISDIR(statbuf.st_mode)) {
337: 						r2 = RemoveDirectoryRecursive(buf);
338: 					} else {
339: 						r2 = unlink(buf);
340: 					}
341: 				}
342: 				delete[] buf;
343: 			}
344: 			r = r2;
345: 		}
346: 		closedir(d);
347: 	}
348: 	if (!r) {
349: 		r = rmdir(path);
350: 	}
351: 	return r;
352: }
