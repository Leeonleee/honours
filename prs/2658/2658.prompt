12685: /*
12686: ** Run a prepared statement and output the result in one of the
12687: ** table-oriented formats: MODE_Column, MODE_Markdown, MODE_Table,
12688: ** or MODE_Box.
12689: **
12690: ** This is different from ordinary exec_prepared_stmt() in that
12691: ** it has to run the entire query and gather the results into memory
12692: ** first, in order to determine column widths, before providing
12693: ** any output.
12694: */
12695: static void exec_prepared_stmt_columnar(
12696:   ShellState *p,                        /* Pointer to ShellState */
12697:   sqlite3_stmt *pStmt                   /* Statment to run */
12698: ){
12699:   sqlite3_int64 nRow = 0;
12700:   int nColumn = 0;
12701:   char **azData = 0;
12702:   sqlite3_int64 nAlloc = 0;
12703:   const char *z;
12704:   int rc;
12705:   sqlite3_int64 i, nData;
12706:   int j, nTotal, w, n;
12707:   const char *colSep = 0;
12708:   const char *rowSep = 0;
12709: 
12710:   rc = sqlite3_step(pStmt);
12711:   if( rc!=SQLITE_ROW ) return;
12712:   nColumn = sqlite3_column_count(pStmt);
12713:   nAlloc = nColumn*4;
12714:   azData = sqlite3_malloc64( nAlloc*sizeof(char*) );
12715:   if( azData==0 ) shell_out_of_memory();
12716:   for(i=0; i<nColumn; i++){
12717:     azData[i] = strdup_handle_newline(p, sqlite3_column_name(pStmt,i));
12718:   }
12719:   do{
12720:     if( (nRow+2)*nColumn >= nAlloc ){
12721:       nAlloc *= 2;
12722:       azData = sqlite3_realloc64(azData, nAlloc*sizeof(char*));
12723:       if( azData==0 ) shell_out_of_memory();
12724:     }
12725:     nRow++;
12726:     for(i=0; i<nColumn; i++){
12727:       z = (const char*)sqlite3_column_text(pStmt,i);
12728:       azData[nRow*nColumn + i] = strdup_handle_newline(p, z);
12729:     }
12730:   }while( (rc = sqlite3_step(pStmt))==SQLITE_ROW );
12731:   if( nColumn>p->nWidth ){
12732:     p->colWidth = realloc(p->colWidth, nColumn*2*sizeof(int));
12733:     if( p->colWidth==0 ) shell_out_of_memory();
12734:     for(i=p->nWidth; i<nColumn; i++) p->colWidth[i] = 0;
12735:     p->nWidth = nColumn;
12736:     p->actualWidth = &p->colWidth[nColumn];
12737:   }
12738:   memset(p->actualWidth, 0, nColumn*sizeof(int));
12739:   for(i=0; i<nColumn; i++){
12740:     w = p->colWidth[i];
12741:     if( w<0 ) w = -w;
12742:     p->actualWidth[i] = w;
12743:   }
12744:   nTotal = nColumn*(nRow+1);
12745:   for(i=0; i<nTotal; i++){
12746:     z = azData[i];
12747:     if( z==0 ) z = p->nullValue;
12748:     n = strlenChar(z);
12749:     j = i%nColumn;
12750:     if( n>p->actualWidth[j] ) p->actualWidth[j] = n;
12751:   }
12752:   if( seenInterrupt ) goto columnar_end;
12753:   switch( p->cMode ){
12754:     case MODE_Column: {
12755:       colSep = "  ";
12756:       rowSep = "\n";
12757:       if( p->showHeader ){
12758:         for(i=0; i<nColumn; i++){
12759:           w = p->actualWidth[i];
12760:           if( p->colWidth[i]<0 ) w = -w;
12761:           utf8_width_print(p->out, w, azData[i]);
12762:           fputs(i==nColumn-1?"\n":"  ", p->out);
12763:         }
12764:         for(i=0; i<nColumn; i++){
12765:           print_dashes(p->out, p->actualWidth[i]);
12766:           fputs(i==nColumn-1?"\n":"  ", p->out);
12767:         }
12768:       }
12769:       break;
12770:     }
12771:     case MODE_Table: {
12772:       colSep = " | ";
12773:       rowSep = " |\n";
12774:       print_row_separator(p, nColumn, "+");
12775:       fputs("| ", p->out);
12776:       for(i=0; i<nColumn; i++){
12777:         w = p->actualWidth[i];
12778:         n = strlenChar(azData[i]);
12779:         utf8_printf(p->out, "%*s%s%*s", (w-n)/2, "", azData[i], (w-n+1)/2, "");
12780:         fputs(i==nColumn-1?" |\n":" | ", p->out);
12781:       }
12782:       print_row_separator(p, nColumn, "+");
12783:       break;
12784:     }
12785:     case MODE_Markdown: {
12786:       colSep = " | ";
12787:       rowSep = " |\n";
12788:       fputs("| ", p->out);
12789:       for(i=0; i<nColumn; i++){
12790:         w = p->actualWidth[i];
12791:         n = strlenChar(azData[i]);
12792:         utf8_printf(p->out, "%*s%s%*s", (w-n)/2, "", azData[i], (w-n+1)/2, "");
12793:         fputs(i==nColumn-1?" |\n":" | ", p->out);
12794:       }
12795:       print_row_separator(p, nColumn, "|");
12796:       break;
12797:     }
12798:     case MODE_Box: {
12799:       colSep = " " BOX_13 " ";
12800:       rowSep = " " BOX_13 "\n";
12801:       print_box_row_separator(p, nColumn, BOX_23, BOX_234, BOX_34);
12802:       utf8_printf(p->out, BOX_13 " ");
12803:       for(i=0; i<nColumn; i++){
12804:         w = p->actualWidth[i];
12805:         n = strlenChar(azData[i]);
12806:         utf8_printf(p->out, "%*s%s%*s%s",
12807:             (w-n)/2, "", azData[i], (w-n+1)/2, "",
12808:             i==nColumn-1?" "BOX_13"\n":" "BOX_13" ");
12809:       }
12810:       print_box_row_separator(p, nColumn, BOX_123, BOX_1234, BOX_134);
12811:       break;
12812:     }
12813:     case MODE_Latex: {
12814:       colSep = " & ";
12815:       rowSep = " \\\\\n";
12816:       fputs("\\begin{tabular}{|", p->out);
12817:       for(i=0; i<nColumn; i++){
12818:         const char *column_type = sqlite3_column_decltype(pStmt, i);
12819:         if (column_type_is_integer(column_type)) {
12820:           fputs("r", p->out);
12821:         } else {
12822:           fputs("l", p->out);
12823:         }
12824:       }
12825:       fputs("|}\n", p->out);
12826:       fputs("\\hline\n", p->out);
12827:       for(i=0; i<nColumn; i++){
12828:         w = p->actualWidth[i];
12829:         n = strlenChar(azData[i]);
12830:         utf8_printf(p->out, "%*s%s%*s", (w-n)/2, "", azData[i], (w-n+1)/2, "");
12831:         fputs(i==nColumn-1? rowSep:colSep, p->out);
12832:       }
12833:       fputs("\\hline\n", p->out);
12834:       break;
12835:     }
12836:   }
12837:   for(i=nColumn, j=0; i<nTotal; i++, j++){
12838:     if( j==0 && p->cMode!=MODE_Column && p->cMode != MODE_Latex ){
12839:       utf8_printf(p->out, "%s", p->cMode==MODE_Box?BOX_13" ":"| ");
12840:     }
12841:     z = azData[i];
12842:     if( z==0 ) z = p->nullValue;
12843:     w = p->actualWidth[j];
12844:     if( p->colWidth[j]<0 ) w = -w;
12845:     utf8_width_print(p->out, w, z);
12846:     if( j==nColumn-1 ){
12847:       utf8_printf(p->out, "%s", rowSep);
12848:       j = -1;
12849:       if( seenInterrupt ) goto columnar_end;
12850:     }else{
12851:       utf8_printf(p->out, "%s", colSep);
12852:     }
12853:   }
12854:   if( p->cMode==MODE_Table ){
12855:     print_row_separator(p, nColumn, "+");
12856:   }else if( p->cMode==MODE_Box ){
12857:     print_box_row_separator(p, nColumn, BOX_12, BOX_124, BOX_14);
12858:   } else if (p->cMode == MODE_Latex) {
12859:     fputs("\\hline\n", p->out);
12860:     fputs("\\end{tabular}\n", p->out);
12861:   }
12862: columnar_end:
12863:   if( seenInterrupt ){
12864:     utf8_printf(p->out, "Interrupt\n");
12865:   }
12866:   nData = (nRow+1)*nColumn;
12867:   for(i=0; i<nData; i++) free(azData[i]);
12868:   sqlite3_free(azData);
12869: }
