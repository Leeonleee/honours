12354: /*
12355: ** If compiled statement pSql appears to be an EXPLAIN statement, allocate
12356: ** and populate the ShellState.aiIndent[] array with the number of
12357: ** spaces each opcode should be indented before it is output.
12358: **
12359: ** The indenting rules are:
12360: **
12361: **     * For each "Next", "Prev", "VNext" or "VPrev" instruction, indent
12362: **       all opcodes that occur between the p2 jump destination and the opcode
12363: **       itself by 2 spaces.
12364: **
12365: **     * For each "Goto", if the jump destination is earlier in the program
12366: **       and ends on one of:
12367: **          Yield  SeekGt  SeekLt  RowSetRead  Rewind
12368: **       or if the P1 parameter is one instead of zero,
12369: **       then indent all opcodes between the earlier instruction
12370: **       and "Goto" by 2 spaces.
12371: */
12372: static void explain_data_prepare(ShellState *p, sqlite3_stmt *pSql){
12373:   const char *zSql;               /* The text of the SQL statement */
12374:   const char *z;                  /* Used to check if this is an EXPLAIN */
12375:   int *abYield = 0;               /* True if op is an OP_Yield */
12376:   int nAlloc = 0;                 /* Allocated size of p->aiIndent[], abYield */
12377:   int iOp;                        /* Index of operation in p->aiIndent[] */
12378: 
12379:   const char *azNext[] = { "Next", "Prev", "VPrev", "VNext", "SorterNext", 0 };
12380:   const char *azYield[] = { "Yield", "SeekLT", "SeekGT", "RowSetRead",
12381:                             "Rewind", 0 };
12382:   const char *azGoto[] = { "Goto", 0 };
12383: 
12384:   /* Try to figure out if this is really an EXPLAIN statement. If this
12385:   ** cannot be verified, return early.  */
12386:   if( sqlite3_column_count(pSql)!=8 ){
12387:     p->cMode = p->mode;
12388:     return;
12389:   }
12390:   zSql = sqlite3_sql(pSql);
12391:   if( zSql==0 ) return;
12392:   for(z=zSql; *z==' ' || *z=='\t' || *z=='\n' || *z=='\f' || *z=='\r'; z++);
12393:   if( sqlite3_strnicmp(z, "explain", 7) ){
12394:     p->cMode = p->mode;
12395:     return;
12396:   }
12397: 
12398:   for(iOp=0; SQLITE_ROW==sqlite3_step(pSql); iOp++){
12399:     int i;
12400:     int iAddr = sqlite3_column_int(pSql, 0);
12401:     const char *zOp = (const char*)sqlite3_column_text(pSql, 1);
12402: 
12403:     /* Set p2 to the P2 field of the current opcode. Then, assuming that
12404:     ** p2 is an instruction address, set variable p2op to the index of that
12405:     ** instruction in the aiIndent[] array. p2 and p2op may be different if
12406:     ** the current instruction is part of a sub-program generated by an
12407:     ** SQL trigger or foreign key.  */
12408:     int p2 = sqlite3_column_int(pSql, 3);
12409:     int p2op = (p2 + (iOp-iAddr));
12410: 
12411:     /* Grow the p->aiIndent array as required */
12412:     if( iOp>=nAlloc ){
12413:       if( iOp==0 ){
12414:         /* Do further verfication that this is explain output.  Abort if
12415:         ** it is not */
12416:         static const char *explainCols[] = {
12417:            "addr", "opcode", "p1", "p2", "p3", "p4", "p5", "comment" };
12418:         int jj;
12419:         for(jj=0; jj<ArraySize(explainCols); jj++){
12420:           if( strcmp(sqlite3_column_name(pSql,jj),explainCols[jj])!=0 ){
12421:             p->cMode = p->mode;
12422:             sqlite3_reset(pSql);
12423:             return;
12424:           }
12425:         }
12426:       }
12427:       nAlloc += 100;
12428:       p->aiIndent = (int*)sqlite3_realloc64(p->aiIndent, nAlloc*sizeof(int));
12429:       if( p->aiIndent==0 ) shell_out_of_memory();
12430:       abYield = (int*)sqlite3_realloc64(abYield, nAlloc*sizeof(int));
12431:       if( abYield==0 ) shell_out_of_memory();
12432:     }
12433:     abYield[iOp] = str_in_array(zOp, azYield);
12434:     p->aiIndent[iOp] = 0;
12435:     p->nIndent = iOp+1;
12436: 
12437:     if( str_in_array(zOp, azNext) ){
12438:       for(i=p2op; i<iOp; i++) p->aiIndent[i] += 2;
12439:     }
12440:     if( str_in_array(zOp, azGoto) && p2op<p->nIndent
12441:      && (abYield[p2op] || sqlite3_column_int(pSql, 2))
12442:     ){
12443:       for(i=p2op; i<iOp; i++) p->aiIndent[i] += 2;
12444:     }
12445:   }
12446: 
12447:   p->iIndent = 0;
12448:   sqlite3_free(abYield);
12449:   sqlite3_reset(pSql);
12450: }
12451: 
12452: /*
12453: ** Free the array allocated by explain_data_prepare().
12454: */
12455: static void explain_data_delete(ShellState *p){
12456:   sqlite3_free(p->aiIndent);
12457:   p->aiIndent = 0;
12458:   p->nIndent = 0;
12459:   p->iIndent = 0;
12460: }
