14404: /*
14405: ** Do C-language style dequoting.
14406: **
14407: **    \a    -> alarm
14408: **    \b    -> backspace
14409: **    \t    -> tab
14410: **    \n    -> newline
14411: **    \v    -> vertical tab
14412: **    \f    -> form feed
14413: **    \r    -> carriage return
14414: **    \s    -> space
14415: **    \"    -> "
14416: **    \'    -> '
14417: **    \\    -> backslash
14418: **    \NNN  -> ascii character NNN in octal
14419: */
14420: static void resolve_backslashes(char *z){
14421:   int i, j;
14422:   char c;
14423:   while( *z && *z!='\\' ) z++;
14424:   for(i=j=0; (c = z[i])!=0; i++, j++){
14425:     if( c=='\\' && z[i+1]!=0 ){
14426:       c = z[++i];
14427:       if( c=='a' ){
14428:         c = '\a';
14429:       }else if( c=='b' ){
14430:         c = '\b';
14431:       }else if( c=='t' ){
14432:         c = '\t';
14433:       }else if( c=='n' ){
14434:         c = '\n';
14435:       }else if( c=='v' ){
14436:         c = '\v';
14437:       }else if( c=='f' ){
14438:         c = '\f';
14439:       }else if( c=='r' ){
14440:         c = '\r';
14441:       }else if( c=='"' ){
14442:         c = '"';
14443:       }else if( c=='\'' ){
14444:         c = '\'';
14445:       }else if( c=='\\' ){
14446:         c = '\\';
14447:       }else if( c>='0' && c<='7' ){
14448:         c -= '0';
14449:         if( z[i+1]>='0' && z[i+1]<='7' ){
14450:           i++;
14451:           c = (c<<3) + z[i] - '0';
14452:           if( z[i+1]>='0' && z[i+1]<='7' ){
14453:             i++;
14454:             c = (c<<3) + z[i] - '0';
14455:           }
14456:         }
14457:       }
14458:     }
14459:     z[j] = c;
14460:   }
14461:   if( j<i ) z[j] = 0;
14462: }
