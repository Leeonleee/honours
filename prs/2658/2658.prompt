5888: /*
5889: ** Write a 32-bit little endiate integer into buffer aBuf.
5890: */
5891: static void zipfilePutU32(u8 *aBuf, u32 val){
5892:   aBuf[0] = val & 0xFF;
5893:   aBuf[1] = (val>>8) & 0xFF;
5894:   aBuf[2] = (val>>16) & 0xFF;
5895:   aBuf[3] = (val>>24) & 0xFF;
5896: }
