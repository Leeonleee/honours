716: bool LocalFileSystem::ListFiles(const string &directory, const std::function<void(string, bool)> &callback) {
717: 	string search_dir = JoinPath(directory, "*");
718: 
719: 	WIN32_FIND_DATA ffd;
720: 	HANDLE hFind = FindFirstFile(search_dir.c_str(), &ffd);
721: 	if (hFind == INVALID_HANDLE_VALUE) {
722: 		return false;
723: 	}
724: 	do {
725: 		string cFileName = string(ffd.cFileName);
726: 		if (cFileName == "." || cFileName == "..") {
727: 			continue;
728: 		}
729: 		callback(cFileName, ffd.dwFileAttributes & FILE_ATTRIBUTE_DIRECTORY);
730: 	} while (FindNextFile(hFind, &ffd) != 0);
731: 
732: 	DWORD dwError = GetLastError();
733: 	if (dwError != ERROR_NO_MORE_FILES) {
734: 		FindClose(hFind);
735: 		return false;
736: 	}
737: 
738: 	FindClose(hFind);
739: 	return true;
740: }
