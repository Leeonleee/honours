13354: ** Toggle the reverse_unordered_selects setting.
13355: */
13356: static void toggleSelectOrder(sqlite3 *db){
13357:   sqlite3_stmt *pStmt = 0;
13358:   int iSetting = 0;
13359:   char zStmt[100];
13360:   sqlite3_prepare_v2(db, "PRAGMA reverse_unordered_selects", -1, &pStmt, 0);
13361:   if( sqlite3_step(pStmt)==SQLITE_ROW ){
13362:     iSetting = sqlite3_column_int(pStmt, 0);
13363:   }
13364:   sqlite3_finalize(pStmt);
13365:   sqlite3_snprintf(sizeof(zStmt), zStmt,
13366:        "PRAGMA reverse_unordered_selects(%d)", !iSetting);
13367:   sqlite3_exec(db, zStmt, 0, 0, 0);
13368: }
