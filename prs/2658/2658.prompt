15547: /*
15548: ** Implementation of ".lint" dot command.
15549: */
15550: static int lintDotCommand(
15551:   ShellState *pState,             /* Current shell tool state */
15552:   char **azArg,                   /* Array of arguments passed to dot command */
15553:   int nArg                        /* Number of entries in azArg[] */
15554: ){
