19915: /*
19916: ** Test to see if a line consists entirely of whitespace.
19917: */
19918: static int _all_whitespace(const char *z){
19919:   for(; *z; z++){
19920:     if( IsSpace(z[0]) ) continue;
19921:     if( *z=='/' && z[1]=='*' ){
19922:       z += 2;
19923:       while( *z && (*z!='*' || z[1]!='/') ){ z++; }
19924:       if( *z==0 ) return 0;
19925:       z++;
19926:       continue;
19927:     }
19928:     if( *z=='-' && z[1]=='-' ){
19929:       z += 2;
19930:       while( *z && *z!='\n' ){ z++; }
19931:       if( *z==0 ) return 1;
19932:       continue;
19933:     }
19934:     return 0;
19935:   }
19936:   return 1;
19937: }
