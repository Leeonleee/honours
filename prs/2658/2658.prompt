13924: /*
13925: ** Try to deduce the type of file for zName based on its content.  Return
13926: ** one of the SHELL_OPEN_* constants.
13927: **
13928: ** If the file does not exist or is empty but its name looks like a ZIP
13929: ** archive and the dfltZip flag is true, then assume it is a ZIP archive.
13930: ** Otherwise, assume an ordinary database regardless of the filename if
13931: ** the type cannot be determined from content.
13932: */
13933: int deduceDatabaseType(const char *zName, int dfltZip){
13934:   FILE *f = fopen(zName, "rb");
13935:   size_t n;
13936:   int rc = SHELL_OPEN_UNSPEC;
13937:   char zBuf[100];
13938:   if( f==0 ){
13939:     if( dfltZip && sqlite3_strlike("%.zip",zName,0)==0 ){
13940:        return SHELL_OPEN_ZIPFILE;
13941:     }else{
13942:        return SHELL_OPEN_NORMAL;
13943:     }
13944:   }
13945:   n = fread(zBuf, 16, 1, f);
13946:   if( n==1 && memcmp(zBuf, "SQLite format 3", 16)==0 ){
13947:     fclose(f);
13948:     return SHELL_OPEN_NORMAL;
13949:   }
13950:   fseek(f, -25, SEEK_END);
13951:   n = fread(zBuf, 25, 1, f);
13952:   if( n==1 && memcmp(zBuf, "Start-Of-SQLite3-", 17)==0 ){
13953:     rc = SHELL_OPEN_APPENDVFS;
13954:   }else{
13955:     fseek(f, -22, SEEK_END);
13956:     n = fread(zBuf, 22, 1, f);
13957:     if( n==1 && zBuf[0]==0x50 && zBuf[1]==0x4b && zBuf[2]==0x05
13958:        && zBuf[3]==0x06 ){
13959:       rc = SHELL_OPEN_ZIPFILE;
13960:     }else if( n==0 && dfltZip && sqlite3_strlike("%.zip",zName,0)==0 ){
13961:       rc = SHELL_OPEN_ZIPFILE;
13962:     }
13963:   }
13964:   fclose(f);
13965:   return rc;
13966: }
