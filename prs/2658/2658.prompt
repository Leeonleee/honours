16354: /*
16355: ** If (*pRc) is not SQLITE_OK when this function is called, it is a no-op.
16356: ** Otherwise, the SQL statement or statements in zSql are executed using
16357: ** database connection db and the error code written to *pRc before
16358: ** this function returns.
16359: */
16360: static void shellExec(sqlite3 *db, int *pRc, const char *zSql){
16361:   int rc = *pRc;
16362:   if( rc==SQLITE_OK ){
16363:     char *zErr = 0;
16364:     rc = sqlite3_exec(db, zSql, 0, 0, &zErr);
16365:     if( rc!=SQLITE_OK ){
16366:       raw_printf(stderr, "SQL error: %s\n", zErr);
16367:     }
16368:     *pRc = rc;
16369:   }
16370: }
