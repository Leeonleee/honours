19956: /*
19957: ** We need a default sqlite3_complete() implementation to use in case
19958: ** the shell is compiled with SQLITE_OMIT_COMPLETE.  The default assumes
19959: ** any arbitrary text is a complete SQL statement.  This is not very
19960: ** user-friendly, but it does seem to work.
19961: */
19962: #ifdef SQLITE_OMIT_COMPLETE
19963: #define sqlite3_complete(x) 1
19964: #endif
19965: 
19966: /*
19967: ** Return true if zSql is a complete SQL statement.  Return false if it
19968: ** ends in the middle of a string literal or C-style comment.
19969: */
19970: static int line_is_complete(char *zSql, int nSql){
19971:   int rc;
19972:   if( zSql==0 ) return 1;
19973:   zSql[nSql] = ';';
19974:   zSql[nSql+1] = 0;
19975:   rc = sqlite3_complete(zSql);
19976:   zSql[nSql] = 0;
19977:   return rc;
19978: }
