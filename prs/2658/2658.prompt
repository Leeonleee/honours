13481: /*
13482: ** Run zQuery.  Use dump_callback() as the callback routine so that
13483: ** the contents of the query are output as SQL statements.
13484: **
13485: ** If we get a SQLITE_CORRUPT error, rerun the query after appending
13486: ** "ORDER BY rowid DESC" to the end.
13487: */
13488: static int run_schema_dump_query(
13489:   ShellState *p,
13490:   const char *zQuery
13491: ){
13492:   int rc;
13493:   char *zErr = 0;
13494:   rc = sqlite3_exec(p->db, zQuery, dump_callback, p, &zErr);
13495:   if( rc==SQLITE_CORRUPT ){
13496:     char *zQ2;
13497:     int len = strlen30(zQuery);
13498:     raw_printf(p->out, "/****** CORRUPTION ERROR *******/\n");
13499:     if( zErr ){
13500:       utf8_printf(p->out, "/****** %s ******/\n", zErr);
13501:       sqlite3_free(zErr);
13502:       zErr = 0;
13503:     }
13504:     zQ2 = malloc( len+100 );
13505:     if( zQ2==0 ) return rc;
13506:     sqlite3_snprintf(len+100, zQ2, "%s ORDER BY rowid DESC", zQuery);
13507:     rc = sqlite3_exec(p->db, zQ2, dump_callback, p, &zErr);
13508:     if( rc ){
13509:       utf8_printf(p->out, "/****** ERROR: %s ******/\n", zErr);
13510:     }else{
13511:       rc = SQLITE_CORRUPT;
13512:     }
13513:     sqlite3_free(zErr);
13514:     free(zQ2);
13515:   } else if( zErr ){
13516:     sqlite3_free(zErr);
13517:     zErr = 0;
13518:   }
13519:   return rc;
13520: }
