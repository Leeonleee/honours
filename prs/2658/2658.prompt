23: //! LogicalOperator is the base class of the logical operators present in the
24: //! logical query tree
25: class LogicalOperator {
26: public:
27: 	explicit LogicalOperator(LogicalOperatorType type) : type(type) {
28: 	}
29: 	LogicalOperator(LogicalOperatorType type, vector<unique_ptr<Expression>> expressions)
30: 	    : type(type), expressions(move(expressions)) {
31: 	}
32: 	virtual ~LogicalOperator() {
33: 	}
34: 
35: 	//! The type of the logical operator
36: 	LogicalOperatorType type;
37: 	//! The set of children of the operator
38: 	vector<unique_ptr<LogicalOperator>> children;
39: 	//! The set of expressions contained within the operator, if any
40: 	vector<unique_ptr<Expression>> expressions;
41: 	//! The types returned by this logical operator. Set by calling LogicalOperator::ResolveTypes.
42: 	vector<LogicalType> types;
43: 	//! Estimated Cardinality
44: 	idx_t estimated_cardinality = 0;
45: 
46: public:
47: 	virtual vector<ColumnBinding> GetColumnBindings() {
48: 		return {ColumnBinding(0, 0)};
49: 	}
50: 	static vector<ColumnBinding> GenerateColumnBindings(idx_t table_idx, idx_t column_count);
51: 	static vector<LogicalType> MapTypes(const vector<LogicalType> &types, const vector<idx_t> &projection_map);
52: 	static vector<ColumnBinding> MapBindings(const vector<ColumnBinding> &types, const vector<idx_t> &projection_map);
53: 
54: 	//! Resolve the types of the logical operator and its children
55: 	void ResolveOperatorTypes();
56: 
57: 	virtual string GetName() const;
58: 	virtual string ParamsToString() const;
59: 	virtual string ToString(idx_t depth = 0) const;
60: 	void Print();
61: 	//! Debug method: verify that the integrity of expressions & child nodes are maintained
62: 	virtual void Verify();
63: 
64: 	void AddChild(unique_ptr<LogicalOperator> child) {
65: 		children.push_back(move(child));
66: 	}
67: 
68: 	virtual idx_t EstimateCardinality(ClientContext &context) {
69: 		// simple estimator, just take the max of the children
70: 		idx_t max_cardinality = 0;
71: 		for (auto &child : children) {
72: 			max_cardinality = MaxValue(child->EstimateCardinality(context), max_cardinality);
73: 		}
74: 		return max_cardinality;
75: 	}
76: 
77: protected:
78: 	//! Resolve types for this specific operator
79: 	virtual void ResolveTypes() = 0;
80: };
