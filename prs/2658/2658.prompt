12295: static void display_scanstats(
12296:   sqlite3 *db,                    /* Database to query */
12297:   ShellState *pArg                /* Pointer to ShellState */
12298: ){
12299: #ifndef SQLITE_ENABLE_STMT_SCANSTATUS
12300:   UNUSED_PARAMETER(db);
