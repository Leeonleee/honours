16243: /*
16244: ** Implementation of ".ar" dot command.
16245: */
16246: static int arDotCommand(
16247:   ShellState *pState,          /* Current shell tool state */
16248:   int fromCmdLine,             /* True if -A command-line option, not .ar cmd */
16249:   char **azArg,                /* Array of arguments passed to dot command */
16250:   int nArg                     /* Number of entries in azArg[] */
16251: ){
16252:   ArCommand cmd;
16253:   int rc;
16254:   memset(&cmd, 0, sizeof(cmd));
16255:   cmd.fromCmdLine = fromCmdLine;
16256:   rc = arParseCommand(azArg, nArg, &cmd);
16257:   if( rc==SQLITE_OK ){
16258:     int eDbType = SHELL_OPEN_UNSPEC;
16259:     cmd.p = pState;
16260:     cmd.db = pState->db;
16261:     if( cmd.zFile ){
16262:       eDbType = deduceDatabaseType(cmd.zFile, 1);
16263:     }else{
16264:       eDbType = pState->openMode;
16265:     }
16266:     if( eDbType==SHELL_OPEN_ZIPFILE ){
16267:       if( cmd.eCmd==AR_CMD_EXTRACT || cmd.eCmd==AR_CMD_LIST ){
16268:         if( cmd.zFile==0 ){
16269:           cmd.zSrcTable = sqlite3_mprintf("zip");
16270:         }else{
16271:           cmd.zSrcTable = sqlite3_mprintf("zipfile(%Q)", cmd.zFile);
16272:         }
16273:       }
16274:       cmd.bZip = 1;
16275:     }else if( cmd.zFile ){
16276:       int flags;
16277:       if( cmd.bAppend ) eDbType = SHELL_OPEN_APPENDVFS;
16278:       if( cmd.eCmd==AR_CMD_CREATE || cmd.eCmd==AR_CMD_INSERT
16279:            || cmd.eCmd==AR_CMD_UPDATE ){
16280:         flags = SQLITE_OPEN_READWRITE|SQLITE_OPEN_CREATE;
16281:       }else{
16282:         flags = SQLITE_OPEN_READONLY;
16283:       }
16284:       cmd.db = 0;
16285:       if( cmd.bDryRun ){
16286:         utf8_printf(pState->out, "-- open database '%s'%s\n", cmd.zFile,
16287:              eDbType==SHELL_OPEN_APPENDVFS ? " using 'apndvfs'" : "");
16288:       }
16289:       rc = sqlite3_open_v2(cmd.zFile, &cmd.db, flags,
16290:              eDbType==SHELL_OPEN_APPENDVFS ? "apndvfs" : 0);
16291:       if( rc!=SQLITE_OK ){
16292:         utf8_printf(stderr, "cannot open file: %s (%s)\n",
16293:             cmd.zFile, sqlite3_errmsg(cmd.db)
16294:         );
16295:         goto end_ar_command;
16296:       }
16297:       sqlite3_fileio_init(cmd.db, 0, 0);
16298:       sqlite3_sqlar_init(cmd.db, 0, 0);
16299:       sqlite3_create_function(cmd.db, "shell_putsnl", 1, SQLITE_UTF8, cmd.p,
16300:                               shellPutsFunc, 0, 0);
16301: 
16302:     }
16303:     if( cmd.zSrcTable==0 && cmd.bZip==0 && cmd.eCmd!=AR_CMD_HELP ){
16304:       if( cmd.eCmd!=AR_CMD_CREATE
16305:        && sqlite3_table_column_metadata(cmd.db,0,"sqlar","name",0,0,0,0,0)
16306:       ){
16307:         utf8_printf(stderr, "database does not contain an 'sqlar' table\n");
16308:         rc = SQLITE_ERROR;
16309:         goto end_ar_command;
16310:       }
16311:       cmd.zSrcTable = sqlite3_mprintf("sqlar");
16312:     }
16313: 
16314:     switch( cmd.eCmd ){
16315:       case AR_CMD_CREATE:
16316:         rc = arCreateOrUpdateCommand(&cmd, 0, 0);
16317:         break;
16318: 
16319:       case AR_CMD_EXTRACT:
16320:         rc = arExtractCommand(&cmd);
16321:         break;
16322: 
16323:       case AR_CMD_LIST:
16324:         rc = arListCommand(&cmd);
16325:         break;
16326: 
16327:       case AR_CMD_HELP:
16328:         arUsage(pState->out);
16329:         break;
16330: 
16331:       case AR_CMD_INSERT:
16332:         rc = arCreateOrUpdateCommand(&cmd, 1, 0);
16333:         break;
16334: 
16335:       default:
16336:         assert( cmd.eCmd==AR_CMD_UPDATE );
16337:         rc = arCreateOrUpdateCommand(&cmd, 1, 1);
16338:         break;
16339:     }
16340:   }
16341: end_ar_command:
16342:   if( cmd.db!=pState->db ){
16343:     close_db(cmd.db);
16344:   }
16345:   sqlite3_free(cmd.zSrcTable);
16346: 
16347:   return rc;
16348: }
