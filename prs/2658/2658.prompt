10151: /*
10152: ** Return the number of bytes of space used by an SQLite value of type
10153: ** eType.
10154: */
10155: static int dbdataValueBytes(int eType){
10156:   switch( eType ){
10157:     case 0: case 8: case 9:
10158:     case 10: case 11:
10159:       return 0;
10160:     case 1:
10161:       return 1;
10162:     case 2:
10163:       return 2;
10164:     case 3:
10165:       return 3;
10166:     case 4:
10167:       return 4;
10168:     case 5:
10169:       return 6;
10170:     case 6:
10171:     case 7:
10172:       return 8;
10173:     default:
10174:       if( eType>0 ){
10175:         return ((eType-12) / 2);
10176:       }
10177:       return 0;
10178:   }
10179: }
