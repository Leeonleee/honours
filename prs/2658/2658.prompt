12871: /*
12872: ** Run a prepared statement
12873: */
12874: static void exec_prepared_stmt(
12875:   ShellState *pArg,                                /* Pointer to ShellState */
12876:   sqlite3_stmt *pStmt                              /* Statment to run */
12877: ){
12878:   int rc;
12879: 
12880:   if( pArg->cMode==MODE_Column
12881:    || pArg->cMode==MODE_Table
12882:    || pArg->cMode==MODE_Box
12883:    || pArg->cMode==MODE_Markdown
12884:    || pArg->cMode==MODE_Latex
12885:   ){
12886:     exec_prepared_stmt_columnar(pArg, pStmt);
12887:     return;
12888:   }
12889: 
12890:   /* perform the first step.  this will tell us if we
12891:   ** have a result set or not and how wide it is.
12892:   */
12893:   rc = sqlite3_step(pStmt);
12894:   /* if we have a result set... */
12895:   if( SQLITE_ROW == rc ){
12896:     /* allocate space for col name ptr, value ptr, and type */
12897:     int nCol = sqlite3_column_count(pStmt);
12898:     void *pData = sqlite3_malloc64(3*nCol*sizeof(const char*) + 1);
12899:     if( !pData ){
12900:       rc = SQLITE_NOMEM;
12901:     }else{
12902:       char **azCols = (char **)pData;      /* Names of result columns */
12903:       char **azVals = &azCols[nCol];       /* Results */
12904:       int *aiTypes = (int *)&azVals[nCol]; /* Result types */
12905:       int i, x;
12906:       assert(sizeof(int) <= sizeof(char *));
12907:       /* save off ptrs to column names */
12908:       for(i=0; i<nCol; i++){
12909:         azCols[i] = (char *)sqlite3_column_name(pStmt, i);
12910:       }
12911:       do{
12912:         if (pArg->cMode!=MODE_Trash) {
12913:           /* extract the data and data types */
12914:           for(i=0; i<nCol; i++){
12915:             aiTypes[i] = x = sqlite3_column_type(pStmt, i);
12916:             if( x==SQLITE_BLOB && pArg && pArg->cMode==MODE_Insert ){
12917:               azVals[i] = "";
12918:             }else{
12919:               azVals[i] = (char*)sqlite3_column_text(pStmt, i);
12920:             }
12921:             if( !azVals[i] && (aiTypes[i]!=SQLITE_NULL) ){
12922:               rc = SQLITE_NOMEM;
12923:               break; /* from for */
12924:             }
12925:           } /* end for */
12926:         }
12927: 
12928:         /* if data and types extracted successfully... */
12929:         if( SQLITE_ROW == rc ){
12930:           /* call the supplied callback with the result row data */
12931:           if( pArg->cMode!=MODE_Trash && shell_callback(pArg, nCol, azVals, azCols, aiTypes) ){
12932:             rc = SQLITE_ABORT;
12933:           }else{
12934:             rc = sqlite3_step(pStmt);
12935:           }
12936:         }
12937:       } while( SQLITE_ROW == rc );
12938:       sqlite3_free(pData);
12939:       if( pArg->cMode==MODE_Json ){
12940:         fputs("]\n", pArg->out);
12941:       }
12942:     }
12943:   }
12944: }
