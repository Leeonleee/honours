16708: /*
16709: ** This function is called to recover data from the database. A script
16710: ** to construct a new database containing all recovered data is output
16711: ** on stream pState->out.
16712: */
16713: static int recoverDatabaseCmd(ShellState *pState, int nArg, char **azArg){
16714:   int rc = SQLITE_OK;
16715:   sqlite3_stmt *pLoop = 0;        /* Loop through all root pages */
16716:   sqlite3_stmt *pPages = 0;       /* Loop through all pages in a group */
16717:   sqlite3_stmt *pCells = 0;       /* Loop through all cells in a page */
16718:   const char *zRecoveryDb = "";   /* Name of "recovery" database */
16719:   const char *zLostAndFound = "lost_and_found";
16720:   int i;
16721:   int nOrphan = -1;
16722:   RecoverTable *pOrphan = 0;
16723: 
16724:   int bFreelist = 1;              /* 0 if --freelist-corrupt is specified */
16725:   int bRowids = 1;                /* 0 if --no-rowids */
16726:   for(i=1; i<nArg; i++){
16727:     char *z = azArg[i];
16728:     int n;
16729:     if( z[0]=='-' && z[1]=='-' ) z++;
16730:     n = strlen30(z);
16731:     if( n<=17 && memcmp("-freelist-corrupt", z, n)==0 ){
16732:       bFreelist = 0;
16733:     }else
16734:     if( n<=12 && memcmp("-recovery-db", z, n)==0 && i<(nArg-1) ){
16735:       i++;
16736:       zRecoveryDb = azArg[i];
16737:     }else
16738:     if( n<=15 && memcmp("-lost-and-found", z, n)==0 && i<(nArg-1) ){
16739:       i++;
16740:       zLostAndFound = azArg[i];
16741:     }else
16742:     if( n<=10 && memcmp("-no-rowids", z, n)==0 ){
16743:       bRowids = 0;
16744:     }
16745:     else{
16746:       utf8_printf(stderr, "unexpected option: %s\n", azArg[i]);
16747:       showHelp(pState->out, azArg[0]);
16748:       return 1;
16749:     }
16750:   }
16751: 
16752:   shellExecPrintf(pState->db, &rc,
16753:     /* Attach an in-memory database named 'recovery'. Create an indexed
16754:     ** cache of the sqlite_dbptr virtual table. */
16755:     "PRAGMA writable_schema = on;"
16756:     "ATTACH %Q AS recovery;"
16757:     "DROP TABLE IF EXISTS recovery.dbptr;"
16758:     "DROP TABLE IF EXISTS recovery.freelist;"
16759:     "DROP TABLE IF EXISTS recovery.map;"
16760:     "DROP TABLE IF EXISTS recovery.schema;"
16761:     "CREATE TABLE recovery.freelist(pgno INTEGER PRIMARY KEY);", zRecoveryDb
16762:   );
16763: 
16764:   if( bFreelist ){
16765:     shellExec(pState->db, &rc,
16766:       "WITH trunk(pgno) AS ("
16767:       "  SELECT shell_int32("
16768:       "      (SELECT data FROM sqlite_dbpage WHERE pgno=1), 8) AS x "
16769:       "      WHERE x>0"
16770:       "    UNION"
16771:       "  SELECT shell_int32("
16772:       "      (SELECT data FROM sqlite_dbpage WHERE pgno=trunk.pgno), 0) AS x "
16773:       "      FROM trunk WHERE x>0"
16774:       "),"
16775:       "freelist(data, n, freepgno) AS ("
16776:       "  SELECT data, min(16384, shell_int32(data, 1)-1), t.pgno "
16777:       "      FROM trunk t, sqlite_dbpage s WHERE s.pgno=t.pgno"
16778:       "    UNION ALL"
16779:       "  SELECT data, n-1, shell_int32(data, 2+n) "
16780:       "      FROM freelist WHERE n>=0"
16781:       ")"
16782:       "REPLACE INTO recovery.freelist SELECT freepgno FROM freelist;"
16783:     );
16784:   }
16785: 
16786:   /* If this is an auto-vacuum database, add all pointer-map pages to
16787:   ** the freelist table. Do this regardless of whether or not
16788:   ** --freelist-corrupt was specified.  */
16789:   shellExec(pState->db, &rc,
16790:     "WITH ptrmap(pgno) AS ("
16791:     "  SELECT 2 WHERE shell_int32("
16792:     "    (SELECT data FROM sqlite_dbpage WHERE pgno=1), 13"
16793:     "  )"
16794:     "    UNION ALL "
16795:     "  SELECT pgno+1+(SELECT page_size FROM pragma_page_size)/5 AS pp "
