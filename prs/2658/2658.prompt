10874: /*
10875: ** SQL function:   edit(VALUE)
10876: **                 edit(VALUE,EDITOR)
10877: **
10878: ** These steps:
10879: **
10880: **     (1) Write VALUE into a temporary file.
10881: **     (2) Run program EDITOR on that temporary file.
10882: **     (3) Read the temporary file back and return its content as the result.
10883: **     (4) Delete the temporary file
10884: **
10885: ** If the EDITOR argument is omitted, use the value in the VISUAL
10886: ** environment variable.  If still there is no EDITOR, through an error.
10887: **
10888: ** Also throw an error if the EDITOR program returns a non-zero exit code.
10889: */
10890: #ifndef SQLITE_NOHAVE_SYSTEM
10891: static void editFunc(
10892:   sqlite3_context *context,
10893:   int argc,
10894:   sqlite3_value **argv
10895: ){
10896:   const char *zEditor;
10897:   char *zTempFile = 0;
10898:   sqlite3 *db;
10899:   char *zCmd = 0;
10900:   int bBin;
10901:   int rc;
10902:   int hasCRNL = 0;
10903:   FILE *f = 0;
10904:   sqlite3_int64 sz;
10905:   sqlite3_int64 x;
10906:   unsigned char *p = 0;
10907: 
10908:   if( argc==2 ){
10909:     zEditor = (const char*)sqlite3_value_text(argv[1]);
10910:   }else{
10911:     zEditor = getenv("VISUAL");
10912:   }
10913:   if( zEditor==0 ){
10914:     sqlite3_result_error(context, "no editor for edit()", -1);
10915:     return;
10916:   }
10917:   if( sqlite3_value_type(argv[0])==SQLITE_NULL ){
10918:     sqlite3_result_error(context, "NULL input to edit()", -1);
10919:     return;
10920:   }
10921:   db = sqlite3_context_db_handle(context);
10922:   zTempFile = 0;
10923:   sqlite3_file_control(db, 0, SQLITE_FCNTL_TEMPFILENAME, &zTempFile);
10924:   if( zTempFile==0 ){
10925:     sqlite3_uint64 r = 0;
10926:     sqlite3_randomness(sizeof(r), &r);
10927:     zTempFile = sqlite3_mprintf("temp%llx", r);
10928:     if( zTempFile==0 ){
10929:       sqlite3_result_error_nomem(context);
10930:       return;
10931:     }
10932:   }
10933:   bBin = sqlite3_value_type(argv[0])==SQLITE_BLOB;
10934:   /* When writing the file to be edited, do \n to \r\n conversions on systems
10935:   ** that want \r\n line endings */
10936:   f = fopen(zTempFile, bBin ? "wb" : "w");
10937:   if( f==0 ){
10938:     sqlite3_result_error(context, "edit() cannot open temp file", -1);
10939:     goto edit_func_end;
10940:   }
10941:   sz = sqlite3_value_bytes(argv[0]);
10942:   if( bBin ){
10943:     x = fwrite(sqlite3_value_blob(argv[0]), 1, (size_t)sz, f);
10944:   }else{
10945:     const char *z = (const char*)sqlite3_value_text(argv[0]);
10946:     /* Remember whether or not the value originally contained \r\n */
10947:     if( z && strstr(z,"\r\n")!=0 ) hasCRNL = 1;
10948:     x = fwrite(sqlite3_value_text(argv[0]), 1, (size_t)sz, f);
10949:   }
10950:   fclose(f);
10951:   f = 0;
10952:   if( x!=sz ){
10953:     sqlite3_result_error(context, "edit() could not write the whole file", -1);
10954:     goto edit_func_end;
10955:   }
10956:   zCmd = sqlite3_mprintf("%s \"%s\"", zEditor, zTempFile);
10957:   if( zCmd==0 ){
10958:     sqlite3_result_error_nomem(context);
10959:     goto edit_func_end;
10960:   }
10961:   rc = system(zCmd);
10962:   sqlite3_free(zCmd);
10963:   if( rc ){
10964:     sqlite3_result_error(context, "EDITOR returned non-zero", -1);
10965:     goto edit_func_end;
10966:   }
10967:   f = fopen(zTempFile, "rb");
10968:   if( f==0 ){
10969:     sqlite3_result_error(context,
10970:       "edit() cannot reopen temp file after edit", -1);
10971:     goto edit_func_end;
10972:   }
10973:   fseek(f, 0, SEEK_END);
10974:   sz = ftell(f);
10975:   rewind(f);
10976:   p = sqlite3_malloc64( sz+1 );
10977:   if( p==0 ){
10978:     sqlite3_result_error_nomem(context);
10979:     goto edit_func_end;
10980:   }
10981:   x = fread(p, 1, (size_t)sz, f);
10982:   fclose(f);
10983:   f = 0;
10984:   if( x!=sz ){
10985:     sqlite3_result_error(context, "could not read back the whole file", -1);
10986:     goto edit_func_end;
10987:   }
10988:   if( bBin ){
10989:     sqlite3_result_blob64(context, p, sz, sqlite3_free);
10990:   }else{
10991:     sqlite3_int64 i, j;
10992:     if( hasCRNL ){
10993:       /* If the original contains \r\n then do no conversions back to \n */
10994:       j = sz;
10995:     }else{
10996:       /* If the file did not originally contain \r\n then convert any new
10997:       ** \r\n back into \n */
10998:       for(i=j=0; i<sz; i++){
10999:         if( p[i]=='\r' && p[i+1]=='\n' ) i++;
11000:         p[j++] = p[i];
11001:       }
11002:       sz = j;
11003:       p[sz] = 0;
11004:     }
11005:     sqlite3_result_text64(context, (const char*)p, sz,
11006:                           sqlite3_free, SQLITE_UTF8);
11007:   }
11008:   p = 0;
11009: 
11010: edit_func_end:
11011:   if( f ) fclose(f);
11012:   unlink(zTempFile);
11013:   sqlite3_free(zTempFile);
11014:   sqlite3_free(p);
11015: }
11016: #endif /* SQLITE_NOHAVE_SYSTEM */
