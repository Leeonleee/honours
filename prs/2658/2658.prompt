15030: /*
15031: ** Convert a 2-byte or 4-byte big-endian integer into a native integer
15032: */
15033: static unsigned int get2byteInt(unsigned char *a){
15034:   return (a[0]<<8) + a[1];
15035: }
15036: static unsigned int get4byteInt(unsigned char *a){
15037:   return (a[0]<<24) + (a[1]<<16) + (a[2]<<8) + a[3];
