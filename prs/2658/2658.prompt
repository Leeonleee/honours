5870: /*
5871: ** Read and return a 32-bit little-endian unsigned integer from buffer aBuf.
5872: */
5873: static u32 zipfileGetU32(const u8 *aBuf){
5874:   return ((u32)(aBuf[3]) << 24)
5875:        + ((u32)(aBuf[2]) << 16)
5876:        + ((u32)(aBuf[1]) <<  8)
5877:        + ((u32)(aBuf[0]) <<  0);
5878: }
