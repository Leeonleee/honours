16410: /*
16411: ** If *pRc is not SQLITE_OK when this function is called, it is a no-op.
16412: ** Otherwise, zFmt is treated as a printf() style string. The result of
16413: ** formatting it along with any trailing arguments is written into a
16414: ** buffer obtained from sqlite3_malloc(), and pointer to which is returned.
16415: ** It is the responsibility of the caller to eventually free this buffer
16416: ** using a call to sqlite3_free().
16417: **
16418: ** If an OOM error occurs, (*pRc) is set to SQLITE_NOMEM and a NULL
16419: ** pointer returned.
16420: */
16421: static char *shellMPrintf(int *pRc, const char *zFmt, ...){
16422:   char *z = 0;
16423:   if( *pRc==SQLITE_OK ){
16424:     va_list ap;
16425:     va_start(ap, zFmt);
16426:     z = sqlite3_vmprintf(zFmt, ap);
16427:     va_end(ap);
16428:     if( z==0 ){
16429:       *pRc = SQLITE_NOMEM;
16430:     }
16431:   }
16432:   return z;
16433: }
