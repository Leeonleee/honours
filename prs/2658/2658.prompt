16466: /*
16467: ** This function is a no-op if (*pRc) is not SQLITE_OK when it is called.
16468: ** Otherwise, it allocates and returns a RecoverTable object based on the
16469: ** final four arguments passed to this function. It is the responsibility
16470: ** of the caller to eventually free the returned object using
16471: ** recoverFreeTable().
16472: */
16473: static RecoverTable *recoverNewTable(
16474:   int *pRc,                       /* IN/OUT: Error code */
16475:   const char *zName,              /* Name of table */
16476:   const char *zSql,               /* CREATE TABLE statement */
16477:   int bIntkey,
16478:   int nCol
16479: ){
16480:   sqlite3 *dbtmp = 0;             /* sqlite3 handle for testing CREATE TABLE */
16481:   int rc = *pRc;
16482:   RecoverTable *pTab = 0;
16483: 
16484:   pTab = (RecoverTable*)shellMalloc(&rc, sizeof(RecoverTable));
16485:   if( rc==SQLITE_OK ){
16486:     int nSqlCol = 0;
16487:     int bSqlIntkey = 0;
16488:     sqlite3_stmt *pStmt = 0;
16489: 
16490:     rc = sqlite3_open("", &dbtmp);
16491:     if( rc==SQLITE_OK ){
16492:       sqlite3_create_function(dbtmp, "shell_idquote", 1, SQLITE_UTF8, 0,
16493:                               shellIdQuote, 0, 0);
16494:     }
16495:     if( rc==SQLITE_OK ){
16496:       rc = sqlite3_exec(dbtmp, "PRAGMA writable_schema = on", 0, 0, 0);
16497:     }
16498:     if( rc==SQLITE_OK ){
16499:       rc = sqlite3_exec(dbtmp, zSql, 0, 0, 0);
16500:       if( rc==SQLITE_ERROR ){
16501:         rc = SQLITE_OK;
16502:         goto finished;
16503:       }
16504:     }
16505:     shellPreparePrintf(dbtmp, &rc, &pStmt,
16506:         "SELECT count(*) FROM pragma_table_info(%Q)", zName
16507:     );
16508:     if( rc==SQLITE_OK && SQLITE_ROW==sqlite3_step(pStmt) ){
16509:       nSqlCol = sqlite3_column_int(pStmt, 0);
16510:     }
16511:     shellFinalize(&rc, pStmt);
16512: 
16513:     if( rc!=SQLITE_OK || nSqlCol<nCol ){
16514:       goto finished;
16515:     }
16516: 
16517:     shellPreparePrintf(dbtmp, &rc, &pStmt,
16518:       "SELECT ("
16519:       "  SELECT substr(data,1,1)==X'0D' FROM sqlite_dbpage WHERE pgno=rootpage"
16520:       ") FROM sqlite_schema WHERE name = %Q", zName
16521:     );
16522:     if( rc==SQLITE_OK && SQLITE_ROW==sqlite3_step(pStmt) ){
16523:       bSqlIntkey = sqlite3_column_int(pStmt, 0);
16524:     }
16525:     shellFinalize(&rc, pStmt);
16526: 
16527:     if( bIntkey==bSqlIntkey ){
16528:       int i;
16529:       const char *zPk = "_rowid_";
16530:       sqlite3_stmt *pPkFinder = 0;
16531: 
16532:       /* If this is an intkey table and there is an INTEGER PRIMARY KEY,
16533:       ** set zPk to the name of the PK column, and pTab->iPk to the index
16534:       ** of the column, where columns are 0-numbered from left to right.
16535:       ** Or, if this is a WITHOUT ROWID table or if there is no IPK column,
16536:       ** leave zPk as "_rowid_" and pTab->iPk at -2.  */
16537:       pTab->iPk = -2;
16538:       if( bIntkey ){
16539:         shellPreparePrintf(dbtmp, &rc, &pPkFinder,
16540:           "SELECT cid, name FROM pragma_table_info(%Q) "
16541:           "  WHERE pk=1 AND type='integer' COLLATE nocase"
16542:           "  AND NOT EXISTS (SELECT cid FROM pragma_table_info(%Q) WHERE pk=2)"
16543:           , zName, zName
16544:         );
16545:         if( rc==SQLITE_OK && SQLITE_ROW==sqlite3_step(pPkFinder) ){
16546:           pTab->iPk = sqlite3_column_int(pPkFinder, 0);
16547:           zPk = (const char*)sqlite3_column_text(pPkFinder, 1);
16548:         }
16549:       }
16550: 
16551:       pTab->zQuoted = shellMPrintf(&rc, "\"%w\"", zName);
16552:       pTab->azlCol = (char**)shellMalloc(&rc, sizeof(char*) * (nSqlCol+1));
16553:       pTab->nCol = nSqlCol;
16554: 
16555:       if( bIntkey ){
16556:         pTab->azlCol[0] = shellMPrintf(&rc, "\"%w\"", zPk);
16557:       }else{
16558:         pTab->azlCol[0] = shellMPrintf(&rc, "");
16559:       }
16560:       i = 1;
16561:       shellPreparePrintf(dbtmp, &rc, &pStmt,
16562:           "SELECT %Q || group_concat(shell_idquote(name), ', ') "
16563:           "  FILTER (WHERE cid!=%d) OVER (ORDER BY %s cid) "
16564:           "FROM pragma_table_info(%Q)",
16565:           bIntkey ? ", " : "", pTab->iPk,
16566:           bIntkey ? "" : "(CASE WHEN pk=0 THEN 1000000 ELSE pk END), ",
16567:           zName
16568:       );
16569:       while( rc==SQLITE_OK && SQLITE_ROW==sqlite3_step(pStmt) ){
16570:         const char *zText = (const char*)sqlite3_column_text(pStmt, 0);
16571:         pTab->azlCol[i] = shellMPrintf(&rc, "%s%s", pTab->azlCol[0], zText);
16572:         i++;
16573:       }
16574:       shellFinalize(&rc, pStmt);
16575: 
16576:       shellFinalize(&rc, pPkFinder);
16577:     }
16578:   }
16579: 
16580:  finished:
16581:   sqlite3_close(dbtmp);
16582:   *pRc = rc;
16583:   if( rc!=SQLITE_OK || (pTab && pTab->zQuoted==0) ){
16584:     recoverFreeTable(pTab);
16585:     pTab = 0;
16586:   }
16587:   return pTab;
16588: }
