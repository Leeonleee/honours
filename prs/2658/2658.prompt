19980: /*
19981: ** Run a single line of SQL.  Return the number of errors.
19982: */
19983: static int runOneSqlLine(ShellState *p, char *zSql, FILE *in, int startline){
19984:   int rc;
19985:   char *zErrMsg = 0;
19986: 
19987:   open_db(p, 0);
19988:   if( ShellHasFlag(p,SHFLG_Backslash) ) resolve_backslashes(zSql);
19989:   if( p->flgProgress & SHELL_PROGRESS_RESET ) p->nProgress = 0;
19990:   BEGIN_TIMER;
19991:   rc = shell_exec(p, zSql, &zErrMsg);
19992:   END_TIMER;
19993:   if( rc || zErrMsg ){
19994:     char zPrefix[100];
19995:     if( in!=0 || !stdin_is_interactive ){
19996:       sqlite3_snprintf(sizeof(zPrefix), zPrefix,
19997:                        "Error: near line %d:", startline);
19998:     }else{
19999:       sqlite3_snprintf(sizeof(zPrefix), zPrefix, "Error:");
20000:     }
20001:     if( zErrMsg!=0 ){
20002:       utf8_printf(stderr, "%s %s\n", zPrefix, zErrMsg);
20003:       sqlite3_free(zErrMsg);
20004:       zErrMsg = 0;
20005:     }else{
20006:       utf8_printf(stderr, "%s %s\n", zPrefix, sqlite3_errmsg(p->db));
20007:     }
20008:     return 1;
20009:   }else if( ShellHasFlag(p, SHFLG_CountChanges) ){
20010:     raw_printf(p->out, "changes: %3d   total_changes: %d\n",
20011:             sqlite3_changes(p->db), sqlite3_total_changes(p->db));
20012:   }
20013:   return 0;
20014: }
