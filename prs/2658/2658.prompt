80: static FileType GetFileTypeInternal(int fd) { // LCOV_EXCL_START
81: 	struct stat s;
82: 	if (fstat(fd, &s) == -1) {
83: 		return FileType::FILE_TYPE_INVALID;
84: 	}
85: 	switch (s.st_mode & S_IFMT) {
86: 	case S_IFBLK:
87: 		return FileType::FILE_TYPE_BLOCKDEV;
88: 	case S_IFCHR:
89: 		return FileType::FILE_TYPE_CHARDEV;
90: 	case S_IFIFO:
91: 		return FileType::FILE_TYPE_FIFO;
92: 	case S_IFDIR:
93: 		return FileType::FILE_TYPE_DIR;
94: 	case S_IFLNK:
95: 		return FileType::FILE_TYPE_LINK;
96: 	case S_IFREG:
97: 		return FileType::FILE_TYPE_REGULAR;
98: 	case S_IFSOCK:
99: 		return FileType::FILE_TYPE_SOCKET;
100: 	default:
101: 		return FileType::FILE_TYPE_INVALID;
102: 	}
103: } // LCOV_EXCL_STOP
