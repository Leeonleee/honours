13840: /*
13841: ** Read the content of file zName into memory obtained from sqlite3_malloc64()
13842: ** and return a pointer to the buffer. The caller is responsible for freeing
13843: ** the memory.
13844: **
13845: ** If parameter pnByte is not NULL, (*pnByte) is set to the number of bytes
13846: ** read.
13847: **
13848: ** For convenience, a nul-terminator byte is always appended to the data read
13849: ** from the file before the buffer is returned. This byte is not included in
13850: ** the final value of (*pnByte), if applicable.
13851: **
13852: ** NULL is returned if any error is encountered. The final value of *pnByte
13853: ** is undefined in this case.
13854: */
13855: static char *readFile(const char *zName, int *pnByte){
13856:   FILE *in = fopen(zName, "rb");
13857:   long nIn;
13858:   size_t nRead;
13859:   char *pBuf;
13860:   if( in==0 ) return 0;
13861:   fseek(in, 0, SEEK_END);
13862:   nIn = ftell(in);
13863:   rewind(in);
13864:   pBuf = sqlite3_malloc64( nIn+1 );
13865:   if( pBuf==0 ){ fclose(in); return 0; }
13866:   nRead = fread(pBuf, nIn, 1, in);
13867:   fclose(in);
13868:   if( nRead!=1 ){
13869:     sqlite3_free(pBuf);
13870:     return 0;
13871:   }
13872:   pBuf[nIn] = 0;
13873:   if( pnByte ) *pnByte = nIn;
13874:   return pBuf;
13875: }
