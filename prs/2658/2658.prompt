15906: /*
15907: ** This function assumes that all arguments within the ArCommand.azArg[]
15908: ** array refer to archive members, as for the --extract or --list commands.
15909: ** It checks that each of them are present. If any specified file is not
15910: ** present in the archive, an error is printed to stderr and an error
15911: ** code returned. Otherwise, if all specified arguments are present in
15912: ** the archive, SQLITE_OK is returned.
15913: **
15914: ** This function strips any trailing '/' characters from each argument.
15915: ** This is consistent with the way the [tar] command seems to work on
15916: ** Linux.
15917: */
15918: static int arCheckEntries(ArCommand *pAr){
15919:   int rc = SQLITE_OK;
15920:   if( pAr->nArg ){
15921:     int i, j;
15922:     sqlite3_stmt *pTest = 0;
15923: 
15924:     shellPreparePrintf(pAr->db, &rc, &pTest,
15925:         "SELECT name FROM %s WHERE name=$name",
15926:         pAr->zSrcTable
15927:     );
15928:     j = sqlite3_bind_parameter_index(pTest, "$name");
15929:     for(i=0; i<pAr->nArg && rc==SQLITE_OK; i++){
15930:       char *z = pAr->azArg[i];
15931:       int n = strlen30(z);
15932:       int bOk = 0;
15933:       while( n>0 && z[n-1]=='/' ) n--;
15934:       z[n] = '\0';
15935:       sqlite3_bind_text(pTest, j, z, -1, SQLITE_STATIC);
15936:       if( SQLITE_ROW==sqlite3_step(pTest) ){
15937:         bOk = 1;
15938:       }
15939:       shellReset(&rc, pTest);
15940:       if( rc==SQLITE_OK && bOk==0 ){
15941:         utf8_printf(stderr, "not found in archive: %s\n", z);
15942:         rc = SQLITE_ERROR;
15943:       }
15944:     }
15945:     shellFinalize(&rc, pTest);
15946:   }
15947:   return rc;
15948: }
