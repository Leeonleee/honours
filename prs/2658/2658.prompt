15146: /*
15147: ** Compare the pattern in zGlob[] against the text in z[].  Return TRUE
15148: ** if they match and FALSE (0) if they do not match.
15149: **
15150: ** Globbing rules:
15151: **
15152: **      '*'       Matches any sequence of zero or more characters.
15153: **
15154: **      '?'       Matches exactly one character.
15155: **
15156: **     [...]      Matches one character from the enclosed list of
15157: **                characters.
15158: **
15159: **     [^...]     Matches one character not in the enclosed list.
15160: **
15161: **      '#'       Matches any sequence of one or more digits with an
15162: **                optional + or - sign in front
15163: **
15164: **      ' '       Any span of whitespace matches any other span of
15165: **                whitespace.
15166: **
15167: ** Extra whitespace at the end of z[] is ignored.
15168: */
15169: static int testcase_glob(const char *zGlob, const char *z){
15170:   int c, c2;
15171:   int invert;
15172:   int seen;
15173: 
15174:   while( (c = (*(zGlob++)))!=0 ){
15175:     if( IsSpace(c) ){
15176:       if( !IsSpace(*z) ) return 0;
15177:       while( IsSpace(*zGlob) ) zGlob++;
15178:       while( IsSpace(*z) ) z++;
15179:     }else if( c=='*' ){
15180:       while( (c=(*(zGlob++))) == '*' || c=='?' ){
15181:         if( c=='?' && (*(z++))==0 ) return 0;
15182:       }
15183:       if( c==0 ){
15184:         return 1;
15185:       }else if( c=='[' ){
15186:         while( *z && testcase_glob(zGlob-1,z)==0 ){
15187:           z++;
15188:         }
15189:         return (*z)!=0;
15190:       }
15191:       while( (c2 = (*(z++)))!=0 ){
15192:         while( c2!=c ){
15193:           c2 = *(z++);
15194:           if( c2==0 ) return 0;
15195:         }
15196:         if( testcase_glob(zGlob,z) ) return 1;
15197:       }
15198:       return 0;
15199:     }else if( c=='?' ){
15200:       if( (*(z++))==0 ) return 0;
15201:     }else if( c=='[' ){
15202:       int prior_c = 0;
15203:       seen = 0;
15204:       invert = 0;
15205:       c = *(z++);
15206:       if( c==0 ) return 0;
15207:       c2 = *(zGlob++);
15208:       if( c2=='^' ){
15209:         invert = 1;
15210:         c2 = *(zGlob++);
15211:       }
15212:       if( c2==']' ){
15213:         if( c==']' ) seen = 1;
15214:         c2 = *(zGlob++);
15215:       }
15216:       while( c2 && c2!=']' ){
15217:         if( c2=='-' && zGlob[0]!=']' && zGlob[0]!=0 && prior_c>0 ){
15218:           c2 = *(zGlob++);
15219:           if( c>=prior_c && c<=c2 ) seen = 1;
15220:           prior_c = 0;
15221:         }else{
15222:           if( c==c2 ){
15223:             seen = 1;
15224:           }
15225:           prior_c = c2;
15226:         }
15227:         c2 = *(zGlob++);
15228:       }
15229:       if( c2==0 || (seen ^ invert)==0 ) return 0;
15230:     }else if( c=='#' ){
15231:       if( (z[0]=='-' || z[0]=='+') && IsDigit(z[1]) ) z++;
15232:       if( !IsDigit(z[0]) ) return 0;
15233:       z++;
15234:       while( IsDigit(z[0]) ){ z++; }
15235:     }else{
15236:       if( c!=(*(z++)) ) return 0;
15237:     }
15238:   }
15239:   while( IsSpace(*z) ){ z++; }
15240:   return *z==0;
15241: }
