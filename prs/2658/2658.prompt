15097:     return 1;
15098:   }
15099:   i = get2byteInt(aHdr+16);
15100:   if( i==1 ) i = 65536;
15101:   utf8_printf(p->out, "%-20s %d\n", "database page size:", i);
15102:   utf8_printf(p->out, "%-20s %d\n", "write format:", aHdr[18]);
15103:   utf8_printf(p->out, "%-20s %d\n", "read format:", aHdr[19]);
15104:   utf8_printf(p->out, "%-20s %d\n", "reserved bytes:", aHdr[20]);
15105:   for(i=0; i<ArraySize(aField); i++){
