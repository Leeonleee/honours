12501:     "CREATE TABLE IF NOT EXISTS temp.sqlite_parameters(\n"
12502:     "  key TEXT PRIMARY KEY,\n"
12503:     "  value ANY\n"
12504:     ") WITHOUT ROWID;",
12505:     0, 0, 0);
12506:   sqlite3_db_config(p->db, SQLITE_DBCONFIG_WRITABLE_SCHEMA, wrSchema, 0);
