13240: /*
13241: ** Release memory previously allocated by tableColumnList().
13242: */
13243: static void freeColumnList(char **azCol){
13244:   int i;
13245:   for(i=1; azCol[i]; i++){
13246:     sqlite3_free(azCol[i]);
13247:   }
13248:   /* azCol[0] is a static string */
13249:   sqlite3_free(azCol);
13250: }
13251: 
13252: /*
13253: ** Return a list of pointers to strings which are the names of all
13254: ** columns in table zTab.   The memory to hold the names is dynamically
13255: ** allocated and must be released by the caller using a subsequent call
13256: ** to freeColumnList().
13257: **
13258: ** The azCol[0] entry is usually NULL.  However, if zTab contains a rowid
13259: ** value that needs to be preserved, then azCol[0] is filled in with the
13260: ** name of the rowid column.
13261: **
13262: ** The first regular column in the table is azCol[1].  The list is terminated
13263: ** by an entry with azCol[i]==0.
13264: */
13265: static char **tableColumnList(ShellState *p, const char *zTab){
13266:   char **azCol = 0;
13267:   sqlite3_stmt *pStmt;
13268:   char *zSql;
13269:   int nCol = 0;
13270:   int nAlloc = 0;
13271:   int nPK = 0;       /* Number of PRIMARY KEY columns seen */
13272:   int isIPK = 0;     /* True if one PRIMARY KEY column of type INTEGER */
13273:   int preserveRowid = ShellHasFlag(p, SHFLG_PreserveRowid);
13274:   int rc;
13275: 
13276:   zSql = sqlite3_mprintf("PRAGMA table_info=%Q", zTab);
13277:   rc = sqlite3_prepare_v2(p->db, zSql, -1, &pStmt, 0);
13278:   sqlite3_free(zSql);
13279:   if( rc ) return 0;
13280:   while( sqlite3_step(pStmt)==SQLITE_ROW ){
13281:     if( nCol>=nAlloc-2 ){
13282:       nAlloc = nAlloc*2 + nCol + 10;
13283:       azCol = sqlite3_realloc(azCol, nAlloc*sizeof(azCol[0]));
13284:       if( azCol==0 ) shell_out_of_memory();
13285:     }
13286:     azCol[++nCol] = sqlite3_mprintf("%s", sqlite3_column_text(pStmt, 1));
13287:     if( sqlite3_column_int(pStmt, 5) ){
13288:       nPK++;
13289:       if( nPK==1
13290:        && sqlite3_stricmp((const char*)sqlite3_column_text(pStmt,2),
13291:                           "INTEGER")==0
13292:       ){
13293:         isIPK = 1;
13294:       }else{
13295:         isIPK = 0;
13296:       }
13297:     }
13298:   }
13299:   sqlite3_finalize(pStmt);
13300:   if( azCol==0 ) return 0;
13301:   azCol[0] = 0;
13302:   azCol[nCol+1] = 0;
13303: 
13304:   /* The decision of whether or not a rowid really needs to be preserved
13305:   ** is tricky.  We never need to preserve a rowid for a WITHOUT ROWID table
13306:   ** or a table with an INTEGER PRIMARY KEY.  We are unable to preserve
13307:   ** rowids on tables where the rowid is inaccessible because there are other
13308:   ** columns in the table named "rowid", "_rowid_", and "oid".
13309:   */
13310:   if( preserveRowid && isIPK ){
13311:     /* If a single PRIMARY KEY column with type INTEGER was seen, then it
13312:     ** might be an alise for the ROWID.  But it might also be a WITHOUT ROWID
13313:     ** table or a INTEGER PRIMARY KEY DESC column, neither of which are
13314:     ** ROWID aliases.  To distinguish these cases, check to see if
13315:     ** there is a "pk" entry in "PRAGMA index_list".  There will be
13316:     ** no "pk" index if the PRIMARY KEY really is an alias for the ROWID.
13317:     */
13318:     zSql = sqlite3_mprintf("SELECT 1 FROM pragma_index_list(%Q)"
13319:                            " WHERE origin='pk'", zTab);
13320:     rc = sqlite3_prepare_v2(p->db, zSql, -1, &pStmt, 0);
13321:     sqlite3_free(zSql);
13322:     if( rc ){
13323:       freeColumnList(azCol);
13324:       return 0;
13325:     }
13326:     rc = sqlite3_step(pStmt);
13327:     sqlite3_finalize(pStmt);
13328:     preserveRowid = rc==SQLITE_ROW;
13329:   }
13330:   if( preserveRowid ){
13331:     /* Only preserve the rowid if we can find a name to use for the
13332:     ** rowid */
13333:     static char *azRowid[] = { "rowid", "_rowid_", "oid" };
13334:     int i, j;
13335:     for(j=0; j<3; j++){
13336:       for(i=1; i<=nCol; i++){
13337:         if( sqlite3_stricmp(azRowid[j],azCol[i])==0 ) break;
13338:       }
13339:       if( i>nCol ){
13340:         /* At this point, we know that azRowid[j] is not the name of any
13341:         ** ordinary column in the table.  Verify that azRowid[j] is a valid
13342:         ** name for the rowid before adding it to azCol[0].  WITHOUT ROWID
13343:         ** tables will fail this last check */
13344:         rc = sqlite3_table_column_metadata(p->db,0,zTab,azRowid[j],0,0,0,0,0);
13345:         if( rc==SQLITE_OK ) azCol[0] = azRowid[j];
13346:         break;
13347:       }
13348:     }
13349:   }
13350:   return azCol;
13351: }
