721: unique_ptr<DuckDBPyRelation> DuckDBPyConnection::ReadJSON(const string &name, const Optional<py::object> &columns,
722:                                                           const Optional<py::object> &sample_size,
723:                                                           const Optional<py::object> &maximum_depth,
724:                                                           const Optional<py::str> &records,
725:                                                           const Optional<py::str> &format) {
726: 	if (!connection) {
727: 		throw ConnectionException("Connection has already been closed");
728: 	}
729: 
730: 	named_parameter_map_t options;
731: 
732: 	if (!py::none().is(columns)) {
733: 		if (!py::isinstance<py::dict>(columns)) {
734: 			throw BinderException("read_json only accepts 'columns' as a dict[str, str]");
735: 		}
736: 		py::dict columns_dict = columns;
737: 		child_list_t<Value> struct_fields;
738: 
739: 		for (auto &kv : columns_dict) {
740: 			auto &column_name = kv.first;
741: 			auto &type = kv.second;
742: 			if (!py::isinstance<py::str>(column_name)) {
743: 				string actual_type = py::str(column_name.get_type());
744: 				throw BinderException("The provided column name must be a str, not of type '%s'", actual_type);
745: 			}
746: 			if (!py::isinstance<py::str>(type)) {
747: 				string actual_type = py::str(column_name.get_type());
748: 				throw BinderException("The provided column type must be a str, not of type '%s'", actual_type);
749: 			}
750: 			struct_fields.emplace_back(py::str(column_name), Value(py::str(type)));
751: 		}
752: 		auto dtype_struct = Value::STRUCT(std::move(struct_fields));
753: 		options["columns"] = std::move(dtype_struct);
754: 	}
755: 
756: 	if (!py::none().is(records)) {
757: 		if (!py::isinstance<py::str>(records)) {
758: 			string actual_type = py::str(records.get_type());
759: 			throw BinderException("read_json only accepts 'records' as a string, not '%s'", actual_type);
760: 		}
761: 		auto records_s = py::reinterpret_borrow<py::str>(records);
762: 		auto records_option = std::string(py::str(records_s));
763: 		options["records"] = Value(records_option);
764: 	}
765: 
766: 	if (!py::none().is(format)) {
767: 		if (!py::isinstance<py::str>(format)) {
768: 			string actual_type = py::str(format.get_type());
769: 			throw BinderException("read_json only accepts 'format' as a string, not '%s'", actual_type);
770: 		}
771: 		auto format_s = py::reinterpret_borrow<py::str>(format);
772: 		auto format_option = std::string(py::str(format_s));
773: 		options["format"] = Value(format_option);
774: 	}
775: 
776: 	if (!py::none().is(sample_size)) {
777: 		if (!py::isinstance<py::int_>(sample_size)) {
778: 			string actual_type = py::str(sample_size.get_type());
779: 			throw BinderException("read_json only accepts 'sample_size' as an integer, not '%s'", actual_type);
780: 		}
781: 		options["sample_size"] = Value::INTEGER(py::int_(sample_size));
782: 	}
783: 
784: 	if (!py::none().is(maximum_depth)) {
785: 		if (!py::isinstance<py::int_>(maximum_depth)) {
786: 			string actual_type = py::str(maximum_depth.get_type());
787: 			throw BinderException("read_json only accepts 'maximum_depth' as an integer, not '%s'", actual_type);
788: 		}
789: 		options["maximum_depth"] = Value::INTEGER(py::int_(maximum_depth));
790: 	}
791: 
792: 	bool auto_detect = false;
793: 	if (!options.count("columns")) {
794: 		options["auto_detect"] = Value::BOOLEAN(true);
795: 		auto_detect = true;
796: 	}
797: 
798: 	auto read_json_relation =
799: 	    make_shared_ptr<ReadJSONRelation>(connection->context, name, std::move(options), auto_detect);
800: 	if (read_json_relation == nullptr) {
801: 		throw BinderException("read_json can only be used when the JSON extension is (statically) loaded");
802: 	}
803: 	return make_uniq<DuckDBPyRelation>(std::move(read_json_relation));
804: }
