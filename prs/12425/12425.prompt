810: unique_ptr<DuckDBPyRelation> DuckDBPyConnection::ReadCSV(
811:     const py::object &name_p, const py::object &header, const py::object &compression, const py::object &sep,
812:     const py::object &delimiter, const py::object &dtype, const py::object &na_values, const py::object &skiprows,
813:     const py::object &quotechar, const py::object &escapechar, const py::object &encoding, const py::object &parallel,
814:     const py::object &date_format, const py::object &timestamp_format, const py::object &sample_size,
815:     const py::object &all_varchar, const py::object &normalize_names, const py::object &filename,
816:     const py::object &null_padding, const py::object &names_p) {
817: 	if (!connection) {
818: 		throw ConnectionException("Connection has already been closed");
819: 	}
820: 	CSVReaderOptions options;
821: 	auto path_like = GetPathLike(name_p);
822: 	auto &name = path_like.files;
823: 	auto file_like_object_wrapper = std::move(path_like.dependency);
824: 	named_parameter_map_t bind_parameters;
825: 
826: 	// First check if the header is explicitly set
827: 	// when false this affects the returned types, so it needs to be known at initialization of the relation
828: 	if (!py::none().is(header)) {
829: 
830: 		bool header_as_int = py::isinstance<py::int_>(header);
831: 		bool header_as_bool = py::isinstance<py::bool_>(header);
832: 
833: 		bool header_value;
834: 		if (header_as_bool) {
835: 			header_value = py::bool_(header);
836: 		} else if (header_as_int) {
837: 			if ((int)py::int_(header) != 0) {
838: 				throw InvalidInputException("read_csv only accepts 0 if 'header' is given as an integer");
839: 			}
840: 			header_value = true;
841: 		} else {
842: 			throw InvalidInputException("read_csv only accepts 'header' as an integer, or a boolean");
843: 		}
844: 		bind_parameters["header"] = Value::BOOLEAN(header_value);
845: 	}
846: 
847: 	if (!py::none().is(compression)) {
848: 		if (!py::isinstance<py::str>(compression)) {
849: 			throw InvalidInputException("read_csv only accepts 'compression' as a string");
850: 		}
851: 		bind_parameters["compression"] = Value(py::str(compression));
852: 	}
853: 
854: 	if (!py::none().is(dtype)) {
855: 		if (py::isinstance<py::dict>(dtype)) {
856: 			child_list_t<Value> struct_fields;
857: 			py::dict dtype_dict = dtype;
858: 			for (auto &kv : dtype_dict) {
859: 				shared_ptr<DuckDBPyType> sql_type;
860: 				if (!py::try_cast(kv.second, sql_type)) {
861: 					throw py::value_error("The types provided to 'dtype' have to be DuckDBPyType");
862: 				}
863: 				struct_fields.emplace_back(py::str(kv.first), Value(sql_type->ToString()));
864: 			}
865: 			auto dtype_struct = Value::STRUCT(std::move(struct_fields));
866: 			bind_parameters["dtypes"] = std::move(dtype_struct);
867: 		} else if (py::isinstance<py::list>(dtype)) {
868: 			vector<Value> list_values;
869: 			py::list dtype_list = dtype;
870: 			for (auto &child : dtype_list) {
871: 				shared_ptr<DuckDBPyType> sql_type;
872: 				if (!py::try_cast(child, sql_type)) {
873: 					throw py::value_error("The types provided to 'dtype' have to be DuckDBPyType");
874: 				}
875: 				list_values.push_back(sql_type->ToString());
876: 			}
877: 			bind_parameters["dtypes"] = Value::LIST(LogicalType::VARCHAR, std::move(list_values));
878: 		} else {
879: 			throw InvalidInputException("read_csv only accepts 'dtype' as a dictionary or a list of strings");
880: 		}
881: 	}
882: 
883: 	bool has_sep = !py::none().is(sep);
884: 	bool has_delimiter = !py::none().is(delimiter);
885: 	if (has_sep && has_delimiter) {
886: 		throw InvalidInputException("read_csv takes either 'delimiter' or 'sep', not both");
887: 	}
888: 	if (has_sep) {
889: 		bind_parameters["delim"] = Value(py::str(sep));
890: 	} else if (has_delimiter) {
891: 		bind_parameters["delim"] = Value(py::str(delimiter));
892: 	}
893: 
894: 	if (!py::none().is(names_p)) {
895: 		if (!py::isinstance<py::list>(names_p)) {
896: 			throw InvalidInputException("read_csv only accepts 'names' as a list of strings");
897: 		}
898: 		vector<Value> names;
899: 		py::list names_list = names_p;
900: 		for (auto &elem : names_list) {
901: 			if (!py::isinstance<py::str>(elem)) {
902: 				throw InvalidInputException("read_csv 'names' list has to consist of only strings");
903: 			}
904: 			names.push_back(Value(std::string(py::str(elem))));
905: 		}
906: 		bind_parameters["names"] = Value::LIST(LogicalType::VARCHAR, std::move(names));
907: 	}
908: 
909: 	if (!py::none().is(na_values)) {
910: 		vector<Value> null_values;
911: 		if (!py::isinstance<py::str>(na_values) && !py::isinstance<py::list>(na_values)) {
912: 			throw InvalidInputException("read_csv only accepts 'na_values' as a string or a list of strings");
913: 		} else if (py::isinstance<py::str>(na_values)) {
914: 			null_values.push_back(Value(py::str(na_values)));
915: 		} else {
916: 			py::list null_list = na_values;
917: 			for (auto &elem : null_list) {
918: 				if (!py::isinstance<py::str>(elem)) {
919: 					throw InvalidInputException("read_csv 'na_values' list has to consist of only strings");
920: 				}
921: 				null_values.push_back(Value(std::string(py::str(elem))));
922: 			}
923: 		}
924: 		bind_parameters["nullstr"] = Value::LIST(LogicalType::VARCHAR, std::move(null_values));
925: 	}
926: 
927: 	if (!py::none().is(skiprows)) {
928: 		if (!py::isinstance<py::int_>(skiprows)) {
929: 			throw InvalidInputException("read_csv only accepts 'skiprows' as an integer");
930: 		}
931: 		bind_parameters["skip"] = Value::INTEGER(py::int_(skiprows));
932: 	}
933: 
934: 	if (!py::none().is(parallel)) {
935: 		if (!py::isinstance<py::bool_>(parallel)) {
936: 			throw InvalidInputException("read_csv only accepts 'parallel' as a boolean");
937: 		}
938: 		bind_parameters["parallel"] = Value::BOOLEAN(py::bool_(parallel));
939: 	}
940: 
941: 	if (!py::none().is(quotechar)) {
942: 		if (!py::isinstance<py::str>(quotechar)) {
943: 			throw InvalidInputException("read_csv only accepts 'quotechar' as a string");
944: 		}
945: 		bind_parameters["quote"] = Value(py::str(quotechar));
946: 	}
947: 
948: 	if (!py::none().is(escapechar)) {
949: 		if (!py::isinstance<py::str>(escapechar)) {
950: 			throw InvalidInputException("read_csv only accepts 'escapechar' as a string");
951: 		}
952: 		bind_parameters["escape"] = Value(py::str(escapechar));
953: 	}
954: 
955: 	if (!py::none().is(encoding)) {
956: 		if (!py::isinstance<py::str>(encoding)) {
957: 			throw InvalidInputException("read_csv only accepts 'encoding' as a string");
958: 		}
959: 		string encoding_str = StringUtil::Lower(py::str(encoding));
960: 		if (encoding_str != "utf8" && encoding_str != "utf-8") {
961: 			throw BinderException("Copy is only supported for UTF-8 encoded files, ENCODING 'UTF-8'");
962: 		}
963: 	}
964: 
965: 	if (!py::none().is(date_format)) {
966: 		if (!py::isinstance<py::str>(date_format)) {
967: 			throw InvalidInputException("read_csv only accepts 'date_format' as a string");
968: 		}
969: 		bind_parameters["dateformat"] = Value(py::str(date_format));
970: 	}
971: 
972: 	if (!py::none().is(timestamp_format)) {
973: 		if (!py::isinstance<py::str>(timestamp_format)) {
974: 			throw InvalidInputException("read_csv only accepts 'timestamp_format' as a string");
975: 		}
976: 		bind_parameters["timestampformat"] = Value(py::str(timestamp_format));
977: 	}
978: 
979: 	if (!py::none().is(sample_size)) {
980: 		if (!py::isinstance<py::int_>(sample_size)) {
981: 			throw InvalidInputException("read_csv only accepts 'sample_size' as an integer");
982: 		}
983: 		bind_parameters["sample_size"] = Value::INTEGER(py::int_(sample_size));
984: 	}
985: 
986: 	if (!py::none().is(all_varchar)) {
987: 		if (!py::isinstance<py::bool_>(all_varchar)) {
988: 			throw InvalidInputException("read_csv only accepts 'all_varchar' as a boolean");
989: 		}
990: 		bind_parameters["all_varchar"] = Value::BOOLEAN(py::bool_(all_varchar));
991: 	}
992: 
993: 	if (!py::none().is(normalize_names)) {
994: 		if (!py::isinstance<py::bool_>(normalize_names)) {
995: 			throw InvalidInputException("read_csv only accepts 'normalize_names' as a boolean");
996: 		}
997: 		bind_parameters["normalize_names"] = Value::BOOLEAN(py::bool_(normalize_names));
998: 	}
999: 
1000: 	if (!py::none().is(filename)) {
1001: 		if (!py::isinstance<py::bool_>(filename)) {
1002: 			throw InvalidInputException("read_csv only accepts 'filename' as a boolean");
1003: 		}
1004: 		bind_parameters["filename"] = Value::BOOLEAN(py::bool_(filename));
1005: 	}
1006: 
1007: 	if (!py::none().is(null_padding)) {
1008: 		if (!py::isinstance<py::bool_>(null_padding)) {
