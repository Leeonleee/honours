1056: unique_ptr<DuckDBPyRelation> DuckDBPyConnection::Table(const string &tname) {
1057: 	if (!connection) {
1058: 		throw ConnectionException("Connection has already been closed");
1059: 	}
1060: 	auto qualified_name = QualifiedName::Parse(tname);
1061: 	if (qualified_name.schema.empty()) {
1062: 		qualified_name.schema = DEFAULT_SCHEMA;
1063: 	}
1064: 	try {
1065: 		return make_uniq<DuckDBPyRelation>(connection->Table(qualified_name.schema, qualified_name.name));
1066: 	} catch (const CatalogException &) {
1067: 		// CatalogException will be of the type '... is not a table'
1068: 		// Not a table in the database, make a query relation that can perform replacement scans
1069: 		auto sql_query = StringUtil::Format("from %s", KeywordHelper::WriteOptionallyQuoted(tname));
1070: 		return RunQuery(py::str(sql_query), tname);
1071: 	}
1072: }
