617: shared_ptr<DuckDBPyConnection> DuckDBPyConnection::Append(const string &name, const PandasDataFrame &value,
618:                                                           bool by_name) {
619: 	RegisterPythonObject("__append_df", value);
620: 	string columns = "";
621: 	if (by_name) {
622: 		auto df_columns = value.attr("columns");
623: 		vector<string> column_names;
624: 		for (auto &column : df_columns) {
625: 			column_names.push_back(std::string(py::str(column)));
626: 		}
627: 		columns += "(";
628: 		for (idx_t i = 0; i < column_names.size(); i++) {
629: 			auto &column = column_names[i];
630: 			if (i != 0) {
631: 				columns += ", ";
632: 			}
633: 			columns += StringUtil::Format("%s", SQLIdentifier(column));
634: 		}
635: 		columns += ")";
636: 	}
637: 
638: 	auto sql_query = StringUtil::Format("INSERT INTO %s %s SELECT * FROM __append_df", SQLIdentifier(name), columns);
639: 	return Execute(py::str(sql_query));
640: }
