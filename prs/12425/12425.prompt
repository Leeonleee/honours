81: void DuckDBPyConnection::DetectEnvironment() {
82: 	// If __main__ does not have a __file__ attribute, we are in interactive mode
83: 	auto main_module = py::module_::import("__main__");
84: 	if (py::hasattr(main_module, "__file__")) {
85: 		return;
86: 	}
87: 	DuckDBPyConnection::environment = PythonEnvironmentType::INTERACTIVE;
88: 	if (!ModuleIsLoaded<IpythonCacheItem>()) {
89: 		return;
90: 	}
91: 
92: 	// Check to see if we are in a Jupyter Notebook
93: 	auto &import_cache_py = *DuckDBPyConnection::ImportCache();
94: 	auto get_ipython = import_cache_py.IPython.get_ipython();
95: 	if (get_ipython.ptr() == nullptr) {
96: 		// Could either not load the IPython module, or it has no 'get_ipython' attribute
97: 		return;
98: 	}
99: 	auto ipython = get_ipython();
100: 	if (!py::hasattr(ipython, "config")) {
101: 		return;
102: 	}
103: 	py::dict ipython_config = ipython.attr("config");
104: 	if (ipython_config.contains("IPKernelApp")) {
105: 		DuckDBPyConnection::environment = PythonEnvironmentType::JUPYTER;
106: 	}
107: 	return;
108: }
109: 
110: bool DuckDBPyConnection::DetectAndGetEnvironment() {
111: 	DuckDBPyConnection::DetectEnvironment();
112: 	return DuckDBPyConnection::IsInteractive();
113: }
114: 
115: bool DuckDBPyConnection::IsJupyter() {
116: 	return DuckDBPyConnection::environment == PythonEnvironmentType::JUPYTER;
117: }
