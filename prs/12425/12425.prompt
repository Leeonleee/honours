584: vector<unique_ptr<SQLStatement>> DuckDBPyConnection::GetStatements(const py::object &query) {
585: 	vector<unique_ptr<SQLStatement>> result;
586: 	if (!connection) {
587: 		throw ConnectionException("Connection has already been closed");
588: 	}
589: 
590: 	shared_ptr<DuckDBPyStatement> statement_obj;
591: 	if (py::try_cast(query, statement_obj)) {
592: 		result.push_back(statement_obj->GetStatement());
593: 		return result;
594: 	}
595: 	if (py::isinstance<py::str>(query)) {
596: 		auto sql_query = std::string(py::str(query));
597: 		return connection->ExtractStatements(sql_query);
598: 	}
599: 	throw InvalidInputException("Please provide either a DuckDBPyStatement or a string representing the query");
600: }
