1680: bool DuckDBPyConnection::IsAcceptedArrowObject(const py::object &object) {
1681: 	if (!ModuleIsLoaded<PyarrowCacheItem>()) {
1682: 		return false;
1683: 	}
1684: 	auto &import_cache_py = *DuckDBPyConnection::ImportCache();
1685: 	if (py::isinstance(object, import_cache_py.pyarrow.Table()) ||
1686: 	    py::isinstance(object, import_cache_py.pyarrow.RecordBatchReader())) {
1687: 		return true;
1688: 	}
1689: 	if (!ModuleIsLoaded<PyarrowDatasetCacheItem>()) {
1690: 		return false;
1691: 	}
1692: 	return (py::isinstance(object, import_cache_py.pyarrow.dataset.Dataset()) ||
1693: 	        py::isinstance(object, import_cache_py.pyarrow.dataset.Scanner()));
1694: }
