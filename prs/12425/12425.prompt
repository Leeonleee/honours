1630: bool IsValidNumpyDimensions(const py::handle &object, int &dim) {
1631: 	// check the dimensions of numpy arrays
1632: 	// should only be called by IsAcceptedNumpyObject
1633: 	auto &import_cache = *DuckDBPyConnection::ImportCache();
1634: 	if (!py::isinstance(object, import_cache.numpy.ndarray())) {
1635: 		return false;
1636: 	}
1637: 	auto shape = (py::cast<py::array>(object)).attr("shape");
1638: 	if (py::len(shape) != 1) {
1639: 		return false;
1640: 	}
1641: 	int cur_dim = (shape.attr("__getitem__")(0)).cast<int>();
1642: 	dim = dim == -1 ? cur_dim : dim;
1643: 	return dim == cur_dim;
1644: }
1645: NumpyObjectType DuckDBPyConnection::IsAcceptedNumpyObject(const py::object &object) {
1646: 	if (!ModuleIsLoaded<NumpyCacheItem>()) {
1647: 		return NumpyObjectType::INVALID;
1648: 	}
1649: 	auto &import_cache = *DuckDBPyConnection::ImportCache();
1650: 	if (py::isinstance(object, import_cache.numpy.ndarray())) {
1651: 		auto len = py::len((py::cast<py::array>(object)).attr("shape"));
1652: 		switch (len) {
1653: 		case 1:
1654: 			return NumpyObjectType::NDARRAY1D;
1655: 		case 2:
1656: 			return NumpyObjectType::NDARRAY2D;
1657: 		default:
1658: 			return NumpyObjectType::INVALID;
1659: 		}
1660: 	} else if (py::isinstance<py::dict>(object)) {
1661: 		int dim = -1;
1662: 		for (auto item : py::cast<py::dict>(object)) {
1663: 			if (!IsValidNumpyDimensions(item.second, dim)) {
1664: 				return NumpyObjectType::INVALID;
1665: 			}
1666: 		}
1667: 		return NumpyObjectType::DICT;
1668: 	} else if (py::isinstance<py::list>(object)) {
1669: 		int dim = -1;
1670: 		for (auto item : py::cast<py::list>(object)) {
1671: 			if (!IsValidNumpyDimensions(item, dim)) {
1672: 				return NumpyObjectType::INVALID;
1673: 			}
1674: 		}
1675: 		return NumpyObjectType::LIST;
1676: 	}
1677: 	return NumpyObjectType::INVALID;
1678: }
