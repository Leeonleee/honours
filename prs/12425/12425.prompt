1439: void CreateNewInstance(DuckDBPyConnection &res, const string &database, DBConfig &config) {
1440: 	// We don't cache unnamed memory instances (i.e., :memory:)
1441: 	bool cache_instance = database != ":memory:" && !database.empty();
1442: 	config.replacement_scans.emplace_back(PythonReplacementScan::Replace);
1443: 	res.database = instance_cache.CreateInstance(database, config, cache_instance);
1444: 	res.connection = make_uniq<Connection>(*res.database);
1445: 	auto &context = *res.connection->context;
1446: 	PandasScanFunction scan_fun;
1447: 	CreateTableFunctionInfo scan_info(scan_fun);
1448: 	MapFunction map_fun;
1449: 	CreateTableFunctionInfo map_info(map_fun);
1450: 	auto &catalog = Catalog::GetSystemCatalog(context);
1451: 	context.transaction.BeginTransaction();
1452: 	catalog.CreateTableFunction(context, &scan_info);
1453: 	catalog.CreateTableFunction(context, &map_info);
1454: 	context.transaction.Commit();
1455: }
