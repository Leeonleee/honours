1191: unique_ptr<DuckDBPyRelation> DuckDBPyConnection::FromArrow(py::object &arrow_object) {
1192: 	if (!connection) {
1193: 		throw ConnectionException("Connection has already been closed");
1194: 	}
1195: 	py::gil_scoped_acquire acquire;
1196: 	string name = "arrow_object_" + StringUtil::GenerateRandomName();
1197: 	if (!IsAcceptedArrowObject(arrow_object)) {
1198: 		auto py_object_type = string(py::str(arrow_object.get_type().attr("__name__")));
1199: 		throw InvalidInputException("Python Object Type %s is not an accepted Arrow Object.", py_object_type);
1200: 	}
1201: 	auto stream_factory =
1202: 	    make_uniq<PythonTableArrowArrayStreamFactory>(arrow_object.ptr(), connection->context->GetClientProperties());
1203: 
1204: 	auto stream_factory_produce = PythonTableArrowArrayStreamFactory::Produce;
1205: 	auto stream_factory_get_schema = PythonTableArrowArrayStreamFactory::GetSchema;
1206: 
1207: 	auto rel = connection
1208: 	               ->TableFunction("arrow_scan", {Value::POINTER(CastPointerToValue(stream_factory.get())),
1209: 	                                              Value::POINTER(CastPointerToValue(stream_factory_produce)),
1210: 	                                              Value::POINTER(CastPointerToValue(stream_factory_get_schema))})
1211: 	               ->Alias(name);
1212: 	auto dependency = make_shared_ptr<ExternalDependency>();
1213: 	auto dependency_item =
1214: 	    PythonDependencyItem::Create(make_uniq<RegisteredArrow>(std::move(stream_factory), arrow_object));
1215: 	dependency->AddDependency("object", std::move(dependency_item));
1216: 	rel->AddExternalDependency(std::move(dependency));
