1474: 
1475: 	if (!DuckDBPyConnection::IsJupyter()) {
1476: 		return;
1477: 	}
1478: 	if (!HasJupyterProgressBarDependencies()) {
1479: 		// Disable progress bar altogether
1480: 		config.system_progress_bar_disable_reason =
1481: 		    "required package 'ipywidgets' is missing, which is needed to render progress bars in Jupyter";
1482: 		config.enable_progress_bar = false;
1483: 		return;
1484: 	}
