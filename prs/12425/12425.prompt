346: shared_ptr<DuckDBPyConnection> DuckDBPyConnection::UnregisterUDF(const string &name) {
347: 	if (!connection) {
348: 		throw ConnectionException("Connection already closed!");
349: 	}
350: 	auto entry = registered_functions.find(name);
351: 	if (entry == registered_functions.end()) {
352: 		// Not registered or already unregistered
353: 		throw InvalidInputException("No function by the name of '%s' was found in the list of registered functions",
354: 		                            name);
355: 	}
356: 
357: 	auto &context = *connection->context;
358: 
359: 	context.RunFunctionInTransaction([&]() {
360: 		// create function
361: 		auto &catalog = Catalog::GetCatalog(context, SYSTEM_CATALOG);
362: 		DropInfo info;
363: 		info.type = CatalogType::SCALAR_FUNCTION_ENTRY;
364: 		info.name = name;
365: 		info.allow_drop_internal = true;
366: 		info.cascade = false;
367: 		info.if_not_found = OnEntryNotFound::THROW_EXCEPTION;
368: 		catalog.DropEntry(context, info);
369: 	});
370: 	registered_functions.erase(entry);
371: 
372: 	return shared_from_this();
373: }
