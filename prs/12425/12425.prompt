155: static unique_ptr<TableRef> ReplaceInternal(ClientContext &context, const string &table_name) {
156: 	py::gil_scoped_acquire acquire;
157: 	// Here we do an exhaustive search on the frame lineage
158: 	auto current_frame = py::module::import("inspect").attr("currentframe")();
159: 	while (hasattr(current_frame, "f_locals")) {
160: 		auto local_dict = py::reinterpret_borrow<py::dict>(current_frame.attr("f_locals"));
161: 		// search local dictionary
162: 		if (local_dict) {
163: 			auto result = TryReplacement(local_dict, table_name, context, current_frame);
164: 			if (result) {
165: 				return result;
166: 			}
167: 		}
168: 		// search global dictionary
169: 		auto global_dict = py::reinterpret_borrow<py::dict>(current_frame.attr("f_globals"));
170: 		if (global_dict) {
171: 			auto result = TryReplacement(global_dict, table_name, context, current_frame);
172: 			if (result) {
173: 				return result;
174: 			}
175: 		}
176: 		current_frame = current_frame.attr("f_back");
177: 	}
178: 	// Not found :(
179: 	return nullptr;
180: }
