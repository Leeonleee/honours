489: unique_ptr<QueryResult> DuckDBPyConnection::ExecuteInternal(vector<unique_ptr<SQLStatement>> statements,
490:                                                             py::object params, bool many) {
491: 	if (!connection) {
492: 		throw ConnectionException("Connection has already been closed");
493: 	}
494: 	if (params.is_none()) {
495: 		params = py::list();
496: 	}
497: 	result = nullptr;
498: 	unique_ptr<PreparedStatement> prep;
499: 	{
500: 		py::gil_scoped_release release;
501: 		unique_lock<std::mutex> lock(py_connection_lock);
502: 
503: 		if (statements.empty()) {
504: 			// no statements to execute
505: 			return nullptr;
506: 		}
507: 		// if there are multiple statements, we directly execute the statements besides the last one
508: 		// we only return the result of the last statement to the user, unless one of the previous statements fails
509: 		for (idx_t i = 0; i + 1 < statements.size(); i++) {
510: 			if (statements[i]->n_param != 0) {
511: 				throw NotImplementedException(
512: 				    "Prepared parameters are only supported for the last statement, please split your query up into "
513: 				    "separate 'execute' calls if you want to use prepared parameters");
514: 			}
515: 			auto pending_query = connection->PendingQuery(std::move(statements[i]), false);
516: 			auto res = CompletePendingQuery(*pending_query);
517: 
518: 			if (res->HasError()) {
519: 				res->ThrowError();
520: 			}
521: 		}
522: 
523: 		prep = connection->Prepare(std::move(statements.back()));
524: 		if (prep->HasError()) {
525: 			prep->error.Throw();
526: 		}
527: 	}
528: 
529: 	// this is a list of a list of parameters in executemany
530: 	py::list params_set;
531: 	if (!many) {
532: 		params_set = py::list(1);
533: 		params_set[0] = params;
534: 	} else {
535: 		if (!py::isinstance<py::list>(params)) {
536: 			throw InvalidInputException("executemany requires a list of parameter sets to be provided");
537: 		}
538: 		auto outer_list = py::list(params);
539: 		if (outer_list.empty()) {
540: 			throw InvalidInputException("executemany requires a non-empty list of parameter sets to be provided");
541: 		}
542: 		params_set = params;
543: 	}
544: 
545: 	// For every entry of the argument list, execute the prepared statement with said arguments
546: 	for (pybind11::handle single_query_params : params_set) {
547: 		case_insensitive_map_t<Value> named_values;
548: 		if (py::isinstance<py::list>(single_query_params) || py::isinstance<py::tuple>(single_query_params)) {
549: 			if (prep->n_param != py::len(single_query_params)) {
550: 				throw InvalidInputException("Prepared statement needs %d parameters, %d given", prep->n_param,
551: 				                            py::len(single_query_params));
552: 			}
553: 			auto unnamed_values = DuckDBPyConnection::TransformPythonParamList(single_query_params);
554: 			for (idx_t i = 0; i < unnamed_values.size(); i++) {
555: 				auto &value = unnamed_values[i];
556: 				auto identifier = std::to_string(i + 1);
557: 				named_values[identifier] = std::move(value);
558: 			}
559: 		} else if (py::isinstance<py::dict>(single_query_params)) {
560: 			auto dict = py::cast<py::dict>(single_query_params);
561: 			named_values = DuckDBPyConnection::TransformPythonParamDict(dict);
562: 		} else {
563: 			throw InvalidInputException("Prepared parameters can only be passed as a list or a dictionary");
564: 		}
565: 		unique_ptr<QueryResult> res;
566: 		{
567: 			py::gil_scoped_release release;
568: 			unique_lock<std::mutex> lock(py_connection_lock);
569: 			auto pending_query = prep->PendingQuery(named_values);
570: 			res = CompletePendingQuery(*pending_query);
571: 
572: 			if (res->HasError()) {
573: 				res->ThrowError();
574: 			}
575: 		}
576: 
577: 		if (!many) {
578: 			return res;
579: 		}
580: 	}
581: 	return nullptr;
582: }
