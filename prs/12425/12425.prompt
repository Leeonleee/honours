431: static std::function<bool(PendingExecutionResult)> FinishedCondition(PendingQueryResult &pending_query) {
432: 	if (pending_query.AllowStreamResult()) {
433: 		return PendingQueryResult::IsFinishedOrBlocked;
434: 	}
435: 	return PendingQueryResult::IsFinished;
436: }
437: 
438: unique_ptr<QueryResult> DuckDBPyConnection::CompletePendingQuery(PendingQueryResult &pending_query) {
439: 	PendingExecutionResult execution_result;
440: 	auto is_finished = FinishedCondition(pending_query);
441: 	do {
442: 		execution_result = pending_query.ExecuteTask();
443: 		{
444: 			py::gil_scoped_acquire gil;
445: 			if (PyErr_CheckSignals() != 0) {
446: 				throw std::runtime_error("Query interrupted");
447: 			}
448: 		}
449: 	} while (!is_finished(execution_result));
450: 	if (execution_result == PendingExecutionResult::EXECUTION_ERROR) {
451: 		pending_query.ThrowError();
452: 	}
453: 	return pending_query.Execute();
454: }
