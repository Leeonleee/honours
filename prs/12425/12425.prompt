1010: 		}
1011: 		bind_parameters["null_padding"] = Value::BOOLEAN(py::bool_(null_padding));
1012: 	}
1013: 
1014: 	// Create the ReadCSV Relation using the 'options'
1015: 
1016: 	auto read_csv_p = connection->ReadCSV(name, std::move(bind_parameters));
1017: 	auto &read_csv = read_csv_p->Cast<ReadCSVRelation>();
1018: 	if (file_like_object_wrapper) {
1019: 		read_csv.AddExternalDependency(std::move(file_like_object_wrapper));
