1114: unique_ptr<DuckDBPyRelation> DuckDBPyConnection::FromDF(const PandasDataFrame &value) {
1115: 	if (!connection) {
1116: 		throw ConnectionException("Connection has already been closed");
1117: 	}
1118: 	string name = "df_" + StringUtil::GenerateRandomName();
1119: 	if (PandasDataFrame::IsPyArrowBacked(value)) {
1120: 		auto table = PandasDataFrame::ToArrowTable(value);
1121: 		return DuckDBPyConnection::FromArrow(table);
1122: 	}
1123: 	auto new_df = PandasScanFunction::PandasReplaceCopiedNames(value);
1124: 	vector<Value> params;
1125: 	params.emplace_back(Value::POINTER(CastPointerToValue(new_df.ptr())));
1126: 	auto rel = connection->TableFunction("pandas_scan", params)->Alias(name);
1127: 	auto dependency = make_shared_ptr<ExternalDependency>();
1128: 	dependency->AddDependency("original", PythonDependencyItem::Create(value));
1129: 	dependency->AddDependency("copy", PythonDependencyItem::Create(new_df));
1130: 	rel->AddExternalDependency(std::move(dependency));
1131: 	return make_uniq<DuckDBPyRelation>(std::move(rel));
1132: }
