642: void DuckDBPyConnection::RegisterArrowObject(const py::object &arrow_object, const string &name) {
643: 	auto stream_factory =
644: 	    make_uniq<PythonTableArrowArrayStreamFactory>(arrow_object.ptr(), connection->context->GetClientProperties());
645: 	auto stream_factory_produce = PythonTableArrowArrayStreamFactory::Produce;
646: 	auto stream_factory_get_schema = PythonTableArrowArrayStreamFactory::GetSchema;
647: 	{
648: 		py::gil_scoped_release release;
649: 		temporary_views[name] =
650: 		    connection
651: 		        ->TableFunction("arrow_scan", {Value::POINTER(CastPointerToValue(stream_factory.get())),
652: 		                                       Value::POINTER(CastPointerToValue(stream_factory_produce)),
653: 		                                       Value::POINTER(CastPointerToValue(stream_factory_get_schema))})
654: 		        ->CreateView(name, true, true);
655: 	}
656: 	vector<shared_ptr<ExternalDependency>> dependencies;
657: 	auto dependency = make_shared_ptr<ExternalDependency>();
658: 	auto dependency_item =
659: 	    PythonDependencyItem::Create(make_uniq<RegisteredArrow>(std::move(stream_factory), arrow_object));
660: 	dependency->AddDependency("object", std::move(dependency_item));
661: 	dependencies.push_back(std::move(dependency));
662: 	connection->context->external_dependencies[name] = std::move(dependencies);
663: }
664: 
665: shared_ptr<DuckDBPyConnection> DuckDBPyConnection::RegisterPythonObject(const string &name,
666:                                                                         const py::object &python_object) {
667: 	if (!connection) {
668: 		throw ConnectionException("Connection has already been closed");
669: 	}
670: 
671: 	if (DuckDBPyConnection::IsPandasDataframe(python_object)) {
672: 		if (PandasDataFrame::IsPyArrowBacked(python_object)) {
673: 			auto arrow_table = PandasDataFrame::ToArrowTable(python_object);
674: 			RegisterArrowObject(arrow_table, name);
675: 		} else {
676: 			auto new_df = PandasScanFunction::PandasReplaceCopiedNames(python_object);
677: 			{
678: 				py::gil_scoped_release release;
679: 				temporary_views[name] =
680: 				    connection->TableFunction("pandas_scan", {Value::POINTER(CastPointerToValue(new_df.ptr()))})
681: 				        ->CreateView(name, true, true);
682: 			}
683: 
684: 			auto dependency = make_shared_ptr<ExternalDependency>();
685: 			dependency->AddDependency("original", PythonDependencyItem::Create(python_object));
686: 			dependency->AddDependency("copy", PythonDependencyItem::Create(std::move(new_df)));
687: 
688: 			vector<shared_ptr<ExternalDependency>> dependencies;
689: 			dependencies.push_back(std::move(dependency));
690: 			connection->context->external_dependencies[name] = std::move(dependencies);
691: 		}
692: 	} else if (IsAcceptedArrowObject(python_object) || IsPolarsDataframe(python_object)) {
693: 		py::object arrow_object;
694: 		if (IsPolarsDataframe(python_object)) {
695: 			if (PolarsDataFrame::IsDataFrame(python_object)) {
696: 				arrow_object = python_object.attr("to_arrow")();
697: 			} else if (PolarsDataFrame::IsLazyFrame(python_object)) {
698: 				py::object materialized = python_object.attr("collect")();
699: 				arrow_object = materialized.attr("to_arrow")();
700: 			} else {
701: 				throw NotImplementedException("Unsupported Polars DF Type");
702: 			}
703: 		} else {
704: 			arrow_object = python_object;
705: 		}
706: 		RegisterArrowObject(arrow_object, name);
707: 	} else if (DuckDBPyRelation::IsRelation(python_object)) {
708: 		auto pyrel = py::cast<DuckDBPyRelation *>(python_object);
709: 		if (!pyrel->CanBeRegisteredBy(*connection)) {
710: 			throw InvalidInputException(
711: 			    "The relation you are attempting to register was not made from this connection");
712: 		}
713: 		pyrel->CreateView(name, true);
714: 	} else {
715: 		auto py_object_type = string(py::str(python_object.get_type().attr("__name__")));
716: 		throw InvalidInputException("Python Object %s not suitable to be registered as a view", py_object_type);
717: 	}
718: 	return shared_from_this();
719: }
