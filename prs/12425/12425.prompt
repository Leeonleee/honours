1514: shared_ptr<DuckDBPyConnection> DuckDBPyConnection::Connect(const string &database, bool read_only,
1515:                                                            const py::dict &config_options) {
1516: 	auto config_dict = TransformPyConfigDict(config_options);
1517: 	if (IsDefaultConnectionString(database, read_only, config_dict)) {
1518: 		return DuckDBPyConnection::DefaultConnection();
1519: 	}
1520: 
1521: 	DBConfig config(read_only);
1522: 	config.AddExtensionOption("pandas_analyze_sample",
1523: 	                          "The maximum number of rows to sample when analyzing a pandas object column.",
1524: 	                          LogicalType::UBIGINT, Value::UBIGINT(1000));
1525: 	if (!DuckDBPyConnection::IsJupyter()) {
1526: 		config_dict["duckdb_api"] = Value("python");
1527: 	} else {
1528: 		config_dict["duckdb_api"] = Value("python jupyter");
1529: 	}
1530: 	config.SetOptionsByName(config_dict);
1531: 
1532: 	auto res = FetchOrCreateInstance(database, config);
1533: 	auto &client_context = *res->connection->context;
1534: 	SetDefaultConfigArguments(client_context);
1535: 	return res;
1536: }
