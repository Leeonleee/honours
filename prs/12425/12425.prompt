456: py::list TransformNamedParameters(const case_insensitive_map_t<idx_t> &named_param_map, const py::dict &params) {
457: 	py::list new_params(params.size());
458: 
459: 	for (auto &item : params) {
460: 		const std::string &item_name = item.first.cast<std::string>();
461: 		auto entry = named_param_map.find(item_name);
462: 		if (entry == named_param_map.end()) {
463: 			throw InvalidInputException(
464: 			    "Named parameters could not be transformed, because query string is missing named parameter '%s'",
465: 			    item_name);
466: 		}
467: 		auto param_idx = entry->second;
468: 		// Add the value of the named parameter to the list
469: 		new_params[param_idx - 1] = item.second;
470: 	}
471: 
472: 	if (named_param_map.size() != params.size()) {
473: 		// One or more named parameters were expected, but not found
474: 		vector<string> missing_params;
475: 		missing_params.reserve(named_param_map.size());
476: 		for (auto &entry : named_param_map) {
477: 			auto &name = entry.first;
478: 			if (!params.contains(name)) {
479: 				missing_params.push_back(name);
480: 			}
481: 		}
482: 		auto message = StringUtil::Join(missing_params, ", ");
483: 		throw InvalidInputException("Not all named parameters have been located, missing: %s", message);
484: 	}
485: 
486: 	return new_params;
487: }
