1025: unique_ptr<DuckDBPyRelation> DuckDBPyConnection::RunQuery(const py::object &query, string alias,
1026:                                                           const py::object &params) {
1027: 	if (!connection) {
1028: 		throw ConnectionException("Connection has already been closed");
1029: 	}
1030: 	if (alias.empty()) {
1031: 		alias = "unnamed_relation_" + StringUtil::GenerateRandomName(16);
1032: 	}
1033: 
1034: 	auto statements = GetStatements(query);
1035: 	if (statements.size() == 1 && statements[0]->type == StatementType::SELECT_STATEMENT && py::none().is(params)) {
1036: 		auto select_statement = unique_ptr_cast<SQLStatement, SelectStatement>(std::move(statements[0]));
1037: 		return make_uniq<DuckDBPyRelation>(connection->RelationFromQuery(std::move(select_statement), alias));
1038: 	}
1039: 
1040: 	auto res = ExecuteInternal(std::move(statements), params);
1041: 	if (!res) {
1042: 		return nullptr;
1043: 	}
1044: 	if (res->properties.return_type != StatementReturnType::QUERY_RESULT) {
1045: 		return nullptr;
1046: 	}
1047: 	if (res->type == QueryResultType::STREAM_RESULT) {
1048: 		auto &stream_result = res->Cast<StreamQueryResult>();
1049: 		res = stream_result.Materialize();
1050: 	}
1051: 	auto &materialized_result = res->Cast<MaterializedQueryResult>();
1052: 	return make_uniq<DuckDBPyRelation>(
1053: 	    make_uniq<MaterializedRelation>(connection->context, materialized_result.TakeCollection(), res->names, alias));
1054: }
