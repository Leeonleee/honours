375: shared_ptr<DuckDBPyConnection>
376: DuckDBPyConnection::RegisterScalarUDF(const string &name, const py::function &udf, const py::object &parameters_p,
377:                                       const shared_ptr<DuckDBPyType> &return_type_p, PythonUDFType type,
378:                                       FunctionNullHandling null_handling, PythonExceptionHandling exception_handling,
379:                                       bool side_effects) {
380: 	if (!connection) {
381: 		throw ConnectionException("Connection already closed!");
382: 	}
383: 	auto &context = *connection->context;
384: 
385: 	if (context.transaction.HasActiveTransaction()) {
386: 		throw InvalidInputException(
387: 		    "This function can not be called with an active transaction!, commit or abort the existing one first");
388: 	}
389: 	if (registered_functions.find(name) != registered_functions.end()) {
390: 		throw NotImplementedException("A function by the name of '%s' is already created, creating multiple "
391: 		                              "functions with the same name is not supported yet, please remove it first",
392: 		                              name);
393: 	}
394: 	auto scalar_function = CreateScalarUDF(name, udf, parameters_p, return_type_p, type == PythonUDFType::ARROW,
395: 	                                       null_handling, exception_handling, side_effects);
396: 	CreateScalarFunctionInfo info(scalar_function);
397: 
398: 	context.RegisterFunction(info);
399: 
400: 	auto dependency = make_uniq<ExternalDependency>();
401: 	dependency->AddDependency("function", PythonDependencyItem::Create(udf));
402: 	registered_functions[name] = std::move(dependency);
403: 
404: 	return shared_from_this();
405: }
