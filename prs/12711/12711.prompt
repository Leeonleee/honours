1696: bool IsValidNumpyDimensions(const py::handle &object, int &dim) {
1697: 	// check the dimensions of numpy arrays
1698: 	// should only be called by IsAcceptedNumpyObject
1699: 	auto &import_cache = *DuckDBPyConnection::ImportCache();
1700: 	if (!py::isinstance(object, import_cache.numpy.ndarray())) {
1701: 		return false;
1702: 	}
1703: 	auto shape = (py::cast<py::array>(object)).attr("shape");
1704: 	if (py::len(shape) != 1) {
1705: 		return false;
1706: 	}
1707: 	int cur_dim = (shape.attr("__getitem__")(0)).cast<int>();
1708: 	dim = dim == -1 ? cur_dim : dim;
1709: 	return dim == cur_dim;
1710: }
1711: NumpyObjectType DuckDBPyConnection::IsAcceptedNumpyObject(const py::object &object) {
1712: 	if (!ModuleIsLoaded<NumpyCacheItem>()) {
1713: 		return NumpyObjectType::INVALID;
1714: 	}
1715: 	auto &import_cache = *DuckDBPyConnection::ImportCache();
1716: 	if (py::isinstance(object, import_cache.numpy.ndarray())) {
1717: 		auto len = py::len((py::cast<py::array>(object)).attr("shape"));
1718: 		switch (len) {
1719: 		case 1:
1720: 			return NumpyObjectType::NDARRAY1D;
1721: 		case 2:
1722: 			return NumpyObjectType::NDARRAY2D;
1723: 		default:
1724: 			return NumpyObjectType::INVALID;
1725: 		}
1726: 	} else if (py::is_dict_like(object)) {
1727: 		int dim = -1;
1728: 		for (auto item : py::cast<py::dict>(object)) {
1729: 			if (!IsValidNumpyDimensions(item.second, dim)) {
1730: 				return NumpyObjectType::INVALID;
1731: 			}
1732: 		}
1733: 		return NumpyObjectType::DICT;
1734: 	} else if (py::is_list_like(object)) {
1735: 		int dim = -1;
1736: 		for (auto item : py::cast<py::list>(object)) {
1737: 			if (!IsValidNumpyDimensions(item, dim)) {
1738: 				return NumpyObjectType::INVALID;
1739: 			}
1740: 		}
1741: 		return NumpyObjectType::LIST;
1742: 	}
1743: 	return NumpyObjectType::INVALID;
1744: }
