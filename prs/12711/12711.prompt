940: 				null_values.push_back(Value(std::string(py::str(elem))));
941: 			}
942: 		}
943: 		bind_parameters["nullstr"] = Value::LIST(LogicalType::VARCHAR, std::move(null_values));
944: 	}
945: 
946: 	if (!py::none().is(skiprows)) {
