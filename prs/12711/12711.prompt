342: bool DuckDBPyConnection::FileSystemIsRegistered(const string &name) {
343: 	auto subsystems = database->GetFileSystem().ListSubSystems();
344: 	return std::find(subsystems.begin(), subsystems.end(), name) != subsystems.end();
345: }
