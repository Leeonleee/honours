1502: void CreateNewInstance(DuckDBPyConnection &res, const string &database, DBConfig &config) {
1503: 	// We don't cache unnamed memory instances (i.e., :memory:)
1504: 	bool cache_instance = database != ":memory:" && !database.empty();
1505: 	config.replacement_scans.emplace_back(PythonReplacementScan::Replace);
1506: 	res.database = instance_cache.CreateInstance(database, config, cache_instance);
1507: 	res.connection = make_uniq<Connection>(*res.database);
1508: 	auto &context = *res.connection->context;
1509: 	PandasScanFunction scan_fun;
1510: 	CreateTableFunctionInfo scan_info(scan_fun);
1511: 	MapFunction map_fun;
1512: 	CreateTableFunctionInfo map_info(map_fun);
1513: 	auto &catalog = Catalog::GetSystemCatalog(context);
1514: 	context.transaction.BeginTransaction();
1515: 	catalog.CreateTableFunction(context, &scan_info);
1516: 	catalog.CreateTableFunction(context, &map_info);
1517: 	context.transaction.Commit();
1518: }
