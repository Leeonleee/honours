125: static unique_ptr<FunctionData> ListZipBind(ClientContext &context, ScalarFunction &bound_function,
126:                                             vector<unique_ptr<Expression>> &arguments) {
127: 	child_list_t<LogicalType> struct_children;
128: 
129: 	// The last argument could be a flag to be set if we want a minimal list or a maximal list
130: 	idx_t size = arguments.size();
131: 	if (size == 0) {
132: 		throw BinderException("Provide at least one argument to " + bound_function.name);
133: 	}
134: 	if (arguments[size - 1]->return_type.id() == LogicalTypeId::BOOLEAN) {
135: 		size--;
136: 	}
137: 
138: 	case_insensitive_set_t struct_names;
139: 	for (idx_t i = 0; i < size; i++) {
140: 		auto &child = arguments[i];
141: 		switch (child->return_type.id()) {
142: 		case LogicalTypeId::LIST:
143: 		case LogicalTypeId::ARRAY:
144: 			child = BoundCastExpression::AddArrayCastToList(context, std::move(child));
145: 			struct_children.push_back(make_pair(string(), ListType::GetChildType(child->return_type)));
146: 			break;
147: 		case LogicalTypeId::SQLNULL:
148: 			struct_children.push_back(make_pair(string(), LogicalTypeId::SQLNULL));
149: 			break;
150: 		case LogicalTypeId::UNKNOWN:
151: 			throw ParameterNotResolvedException();
152: 		default:
153: 			throw BinderException("Parameter type needs to be List");
154: 		}
155: 	}
156: 	bound_function.return_type = LogicalType::LIST(LogicalType::STRUCT(struct_children));
157: 	return make_uniq<VariableReturnBindData>(bound_function.return_type);
158: }
