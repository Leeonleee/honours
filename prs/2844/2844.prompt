1262: 	AddValue(buffer.get() + start, position - start - offset, column, escape_positions);
1263: 	// increase position by 1 and move start to the new position
1264: 	offset = 0;
1265: 	start = ++position;
1266: 	if (position >= buffer_size && !ReadBuffer(start)) {
1267: 		// file ends right after delimiter, go to final state
1268: 		goto final_state;
1269: 	}
1270: 	goto value_start;
1271: add_row : {
1272: 	// check type of newline (\r or \n)
1273: 	bool carriage_return = buffer[position] == '\r';
1274: 	AddValue(buffer.get() + start, position - start - offset, column, escape_positions);
1275: 	finished_chunk = AddRow(insert_chunk, column);
1276: 	// increase position by 1 and move start to the new position
1277: 	offset = 0;
1278: 	start = ++position;
1279: 	if (position >= buffer_size && !ReadBuffer(start)) {
1280: 		// file ends right after delimiter, go to final state
1281: 		goto final_state;
1282: 	}
1283: 	if (carriage_return) {
1284: 		// \r newline, go to special state that parses an optional \n afterwards
1285: 		goto carriage_return;
1286: 	} else {
1287: 		// \n newline, move to value start
1288: 		if (finished_chunk) {
1289: 			return true;
1290: 		}
1291: 		goto value_start;
1292: 	}
1293: }
1294: in_quotes:
1295: 	/* state: in_quotes */
1296: 	// this state parses the remainder of a quoted value
1297: 	position++;
1298: 	do {
1299: 		for (; position < buffer_size; position++) {
1300: 			if (buffer[position] == options.quote[0]) {
1301: 				// quote: move to unquoted state
1302: 				goto unquote;
1303: 			} else if (buffer[position] == options.escape[0]) {
1304: 				// escape: store the escaped position and move to handle_escape state
1305: 				escape_positions.push_back(position - start);
