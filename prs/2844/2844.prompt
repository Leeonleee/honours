988: bool BufferedCSVReader::TryParseComplexCSV(DataChunk &insert_chunk, string &error_message) {
989: 	// used for parsing algorithm
990: 	bool finished_chunk = false;
991: 	idx_t column = 0;
992: 	vector<idx_t> escape_positions;
993: 	uint8_t delimiter_pos = 0, escape_pos = 0, quote_pos = 0;
994: 	idx_t offset = 0;
995: 
996: 	// read values into the buffer (if any)
997: 	if (position >= buffer_size) {
998: 		if (!ReadBuffer(start)) {
999: 			return true;
1000: 		}
1001: 	}
1002: 	// start parsing the first value
1003: 	start = position;
1004: 	goto value_start;
1005: value_start:
1006: 	/* state: value_start */
1007: 	// this state parses the first characters of a value
1008: 	offset = 0;
1009: 	delimiter_pos = 0;
1010: 	quote_pos = 0;
1011: 	do {
1012: 		idx_t count = 0;
1013: 		for (; position < buffer_size; position++) {
1014: 			quote_search.Match(quote_pos, buffer[position]);
1015: 			delimiter_search.Match(delimiter_pos, buffer[position]);
1016: 			count++;
1017: 			if (delimiter_pos == options.delimiter.size()) {
1018: 				// found a delimiter, add the value
1019: 				offset = options.delimiter.size() - 1;
1020: 				goto add_value;
1021: 			} else if (StringUtil::CharacterIsNewline(buffer[position])) {
1022: 				// found a newline, add the row
1023: 				goto add_row;
1024: 			}
1025: 			if (count > quote_pos) {
1026: 				// did not find a quote directly at the start of the value, stop looking for the quote now
1027: 				goto normal;
1028: 			}
1029: 			if (quote_pos == options.quote.size()) {
1030: 				// found a quote, go to quoted loop and skip the initial quote
1031: 				start += options.quote.size();
1032: 				goto in_quotes;
1033: 			}
1034: 		}
1035: 	} while (ReadBuffer(start));
1036: 	// file ends while scanning for quote/delimiter, go to final state
1037: 	goto final_state;
1038: normal:
1039: 	/* state: normal parsing state */
1040: 	// this state parses the remainder of a non-quoted value until we reach a delimiter or newline
1041: 	position++;
1042: 	do {
1043: 		for (; position < buffer_size; position++) {
1044: 			delimiter_search.Match(delimiter_pos, buffer[position]);
1045: 			if (delimiter_pos == options.delimiter.size()) {
1046: 				offset = options.delimiter.size() - 1;
1047: 				goto add_value;
1048: 			} else if (StringUtil::CharacterIsNewline(buffer[position])) {
1049: 				goto add_row;
1050: 			}
1051: 		}
1052: 	} while (ReadBuffer(start));
1053: 	goto final_state;
1054: add_value:
1055: 	AddValue(buffer.get() + start, position - start - offset, column, escape_positions);
1056: 	// increase position by 1 and move start to the new position
1057: 	offset = 0;
1058: 	start = ++position;
1059: 	if (position >= buffer_size && !ReadBuffer(start)) {
1060: 		// file ends right after delimiter, go to final state
1061: 		goto final_state;
1062: 	}
1063: 	goto value_start;
1064: add_row : {
1065: 	// check type of newline (\r or \n)
1066: 	bool carriage_return = buffer[position] == '\r';
1067: 	AddValue(buffer.get() + start, position - start - offset, column, escape_positions);
1068: 	finished_chunk = AddRow(insert_chunk, column);
1069: 	// increase position by 1 and move start to the new position
1070: 	offset = 0;
1071: 	start = ++position;
1072: 	if (position >= buffer_size && !ReadBuffer(start)) {
1073: 		// file ends right after newline, go to final state
1074: 		goto final_state;
1075: 	}
1076: 	if (carriage_return) {
1077: 		// \r newline, go to special state that parses an optional \n afterwards
1078: 		goto carriage_return;
1079: 	} else {
1080: 		// \n newline, move to value start
1081: 		if (finished_chunk) {
1082: 			return true;
1083: 		}
1084: 		goto value_start;
1085: 	}
1086: }
1087: in_quotes:
1088: 	/* state: in_quotes */
1089: 	// this state parses the remainder of a quoted value
1090: 	quote_pos = 0;
1091: 	escape_pos = 0;
1092: 	position++;
1093: 	do {
1094: 		for (; position < buffer_size; position++) {
1095: 			quote_search.Match(quote_pos, buffer[position]);
1096: 			escape_search.Match(escape_pos, buffer[position]);
1097: 			if (quote_pos == options.quote.size()) {
1098: 				goto unquote;
1099: 			} else if (escape_pos == options.escape.size()) {
1100: 				escape_positions.push_back(position - start - (options.escape.size() - 1));
1101: 				goto handle_escape;
1102: 			}
1103: 		}
1104: 	} while (ReadBuffer(start));
1105: 	// still in quoted state at the end of the file, error:
1106: 	error_message = StringUtil::Format("Error in file \"%s\" on line %s: unterminated quotes. (%s)", options.file_path,
1107: 	                                   GetLineNumberStr(linenr, linenr_estimated).c_str(), options.ToString());
1108: 	return false;
1109: unquote:
1110: 	/* state: unquote */
1111: 	// this state handles the state directly after we unquote
1112: 	// in this state we expect either another quote (entering the quoted state again, and escaping the quote)
1113: 	// or a delimiter/newline, ending the current value and moving on to the next value
1114: 	delimiter_pos = 0;
1115: 	quote_pos = 0;
1116: 	position++;
1117: 	if (position >= buffer_size && !ReadBuffer(start)) {
1118: 		// file ends right after unquote, go to final state
1119: 		offset = options.quote.size();
1120: 		goto final_state;
1121: 	}
1122: 	if (StringUtil::CharacterIsNewline(buffer[position])) {
1123: 		// quote followed by newline, add row
1124: 		offset = options.quote.size();
1125: 		goto add_row;
1126: 	}
1127: 	do {
1128: 		idx_t count = 0;
1129: 		for (; position < buffer_size; position++) {
1130: 			quote_search.Match(quote_pos, buffer[position]);
1131: 			delimiter_search.Match(delimiter_pos, buffer[position]);
1132: 			count++;
1133: 			if (count > delimiter_pos && count > quote_pos) {
1134: 				error_message = StringUtil::Format(
1135: 				    "Error in file \"%s\" on line %s: quote should be followed by end of value, end "
1136: 				    "of row or another quote. (%s)",
1137: 				    options.file_path, GetLineNumberStr(linenr, linenr_estimated).c_str(), options.ToString());
1138: 				return false;
1139: 			}
1140: 			if (delimiter_pos == options.delimiter.size()) {
1141: 				// quote followed by delimiter, add value
1142: 				offset = options.quote.size() + options.delimiter.size() - 1;
1143: 				goto add_value;
1144: 			} else if (quote_pos == options.quote.size() &&
1145: 			           (options.escape.empty() || options.escape == options.quote)) {
1146: 				// quote followed by quote, go back to quoted state and add to escape
1147: 				escape_positions.push_back(position - start - (options.quote.size() - 1));
1148: 				goto in_quotes;
1149: 			}
1150: 		}
1151: 	} while (ReadBuffer(start));
1152: 	error_message = StringUtil::Format(
1153: 	    "Error in file \"%s\" on line %s: quote should be followed by end of value, end of row or another quote. (%s)",
1154: 	    options.file_path, GetLineNumberStr(linenr, linenr_estimated).c_str(), options.ToString());
1155: 	return false;
1156: handle_escape:
1157: 	escape_pos = 0;
1158: 	quote_pos = 0;
1159: 	position++;
1160: 	do {
1161: 		idx_t count = 0;
1162: 		for (; position < buffer_size; position++) {
1163: 			quote_search.Match(quote_pos, buffer[position]);
1164: 			escape_search.Match(escape_pos, buffer[position]);
1165: 			count++;
1166: 			if (count > escape_pos && count > quote_pos) {
1167: 				error_message = StringUtil::Format(
1168: 				    "Error in file \"%s\" on line %s: neither QUOTE nor ESCAPE is proceeded by ESCAPE. (%s)",
1169: 				    options.file_path, GetLineNumberStr(linenr, linenr_estimated).c_str(), options.ToString());
1170: 				return false;
1171: 			}
1172: 			if (quote_pos == options.quote.size() || escape_pos == options.escape.size()) {
1173: 				// found quote or escape: move back to quoted state
1174: 				goto in_quotes;
1175: 			}
1176: 		}
1177: 	} while (ReadBuffer(start));
1178: 	error_message =
1179: 	    StringUtil::Format("Error in file \"%s\" on line %s: neither QUOTE nor ESCAPE is proceeded by ESCAPE. (%s)",
1180: 	                       options.file_path, GetLineNumberStr(linenr, linenr_estimated).c_str(), options.ToString());
1181: 	return false;
1182: carriage_return:
1183: 	/* state: carriage_return */
1184: 	// this stage optionally skips a newline (\n) character, which allows \r\n to be interpreted as a single line
1185: 	if (buffer[position] == '\n') {
1186: 		// newline after carriage return: skip
1187: 		start = ++position;
1188: 		if (position >= buffer_size && !ReadBuffer(start)) {
1189: 			// file ends right after newline, go to final state
1190: 			goto final_state;
1191: 		}
1192: 	}
1193: 	if (finished_chunk) {
1194: 		return true;
1195: 	}
1196: 	goto value_start;
1197: final_state:
1198: 	if (finished_chunk) {
1199: 		return true;
1200: 	}
1201: 	if (column > 0 || position > start) {
1202: 		// remaining values to be added to the chunk
1203: 		AddValue(buffer.get() + start, position - start - offset, column, escape_positions);
1204: 		finished_chunk = AddRow(insert_chunk, column);
1205: 	}
1206: 	// final stage, only reached after parsing the file is finished
1207: 	// flush the parsed chunk and finalize parsing
1208: 	if (mode == ParserMode::PARSING) {
1209: 		Flush(insert_chunk);
1210: 	}
1211: 
1212: 	end_of_file_reached = true;
1213: 	return true;
1214: }
