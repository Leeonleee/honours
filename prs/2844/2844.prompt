198: // Helper function for UTF-8 aware space trimming
199: static string TrimWhitespace(const string &col_name) {
200: 	utf8proc_int32_t codepoint;
201: 	auto str = reinterpret_cast<const utf8proc_uint8_t *>(col_name.c_str());
202: 	idx_t size = col_name.size();
203: 	// Find the first character that is not left trimmed
204: 	idx_t begin = 0;
205: 	while (begin < size) {
206: 		auto bytes = utf8proc_iterate(str + begin, size - begin, &codepoint);
207: 		D_ASSERT(bytes > 0);
208: 		if (utf8proc_category(codepoint) != UTF8PROC_CATEGORY_ZS) {
209: 			break;
210: 		}
211: 		begin += bytes;
212: 	}
213: 
214: 	// Find the last character that is not right trimmed
215: 	idx_t end;
216: 	end = begin;
217: 	for (auto next = begin; next < col_name.size();) {
218: 		auto bytes = utf8proc_iterate(str + next, size - next, &codepoint);
219: 		D_ASSERT(bytes > 0);
220: 		next += bytes;
221: 		if (utf8proc_category(codepoint) != UTF8PROC_CATEGORY_ZS) {
222: 			end = next;
223: 		}
224: 	}
225: 
226: 	// return the trimmed string
227: 	return col_name.substr(begin, end - begin);
228: }
