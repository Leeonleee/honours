478: bool BufferedCSVReader::TryCastVector(Vector &parse_chunk_col, idx_t size, const LogicalType &sql_type) {
479: 	// try vector-cast from string to sql_type
480: 	Vector dummy_result(sql_type);
481: 	if (options.has_format[LogicalTypeId::DATE] && sql_type == LogicalTypeId::DATE) {
482: 		// use the date format to cast the chunk
483: 		string error_message;
484: 		return TryCastDateVector(options, parse_chunk_col, dummy_result, size, error_message);
485: 	} else if (options.has_format[LogicalTypeId::TIMESTAMP] && sql_type == LogicalTypeId::TIMESTAMP) {
486: 		// use the timestamp format to cast the chunk
487: 		string error_message;
488: 		return TryCastTimestampVector(options, parse_chunk_col, dummy_result, size, error_message);
489: 	} else {
490: 		// target type is not varchar: perform a cast
491: 		string error_message;
492: 		return VectorOperations::TryCast(parse_chunk_col, dummy_result, size, &error_message, true);
493: 	}
494: }
