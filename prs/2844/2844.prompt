929: vector<LogicalType> BufferedCSVReader::SniffCSV(const vector<LogicalType> &requested_types) {
930: 	for (auto &type : requested_types) {
931: 		// auto detect for blobs not supported: there may be invalid UTF-8 in the file
932: 		if (type.id() == LogicalTypeId::BLOB) {
933: 			return requested_types;
934: 		}
935: 	}
936: 
937: 	// #######
938: 	// ### dialect detection
939: 	// #######
940: 	BufferedCSVReaderOptions original_options = options;
941: 	vector<BufferedCSVReaderOptions> info_candidates;
942: 	idx_t best_num_cols = 0;
943: 
944: 	DetectDialect(requested_types, original_options, info_candidates, best_num_cols);
945: 
946: 	// if no dialect candidate was found, then file was most likely empty and we throw an exception
947: 	if (info_candidates.empty()) {
948: 		throw InvalidInputException(
949: 		    "Error in file \"%s\": CSV options could not be auto-detected. Consider setting parser options manually.",
950: 		    options.file_path);
951: 	}
952: 
953: 	// #######
954: 	// ### type detection (initial)
955: 	// #######
956: 	// type candidates, ordered by descending specificity (~ from high to low)
957: 	vector<LogicalType> type_candidates = {
958: 	    LogicalType::VARCHAR, LogicalType::TIMESTAMP,
959: 	    LogicalType::DATE,    LogicalType::TIME,
960: 	    LogicalType::DOUBLE,  /* LogicalType::FLOAT,*/ LogicalType::BIGINT,
961: 	    LogicalType::INTEGER, /*LogicalType::SMALLINT, LogicalType::TINYINT,*/ LogicalType::BOOLEAN,
962: 	    LogicalType::SQLNULL};
963: 	// format template candidates, ordered by descending specificity (~ from high to low)
964: 	std::map<LogicalTypeId, vector<const char *>> format_template_candidates = {
965: 	    {LogicalTypeId::DATE, {"%m-%d-%Y", "%m-%d-%y", "%d-%m-%Y", "%d-%m-%y", "%Y-%m-%d", "%y-%m-%d"}},
966: 	    {LogicalTypeId::TIMESTAMP,
967: 	     {"%Y-%m-%d %H:%M:%S.%f", "%m-%d-%Y %I:%M:%S %p", "%m-%d-%y %I:%M:%S %p", "%d-%m-%Y %H:%M:%S",
968: 	      "%d-%m-%y %H:%M:%S", "%Y-%m-%d %H:%M:%S", "%y-%m-%d %H:%M:%S"}},
969: 	};
970: 	vector<vector<LogicalType>> best_sql_types_candidates;
971: 	map<LogicalTypeId, vector<string>> best_format_candidates;
972: 	DataChunk best_header_row;
973: 	DetectCandidateTypes(type_candidates, format_template_candidates, info_candidates, original_options, best_num_cols,
974: 	                     best_sql_types_candidates, best_format_candidates, best_header_row);
975: 
976: 	// #######
977: 	// ### header detection
978: 	// #######
979: 	options.num_cols = best_num_cols;
980: 	DetectHeader(best_sql_types_candidates, best_header_row);
981: 
982: 	// #######
983: 	// ### type detection (refining)
984: 	// #######
985: 	return RefineTypeDetection(type_candidates, requested_types, best_sql_types_candidates, best_format_candidates);
986: }
