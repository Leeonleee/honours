186: static void ReadCSVFunction(ClientContext &context, const FunctionData *bind_data_p,
187:                             FunctionOperatorData *operator_state, DataChunk *input, DataChunk &output) {
188: 	auto &bind_data = (ReadCSVData &)*bind_data_p;
189: 	auto &data = (ReadCSVOperatorData &)*operator_state;
190: 	do {
191: 		data.csv_reader->ParseCSV(output);
192: 		bind_data.bytes_read = data.csv_reader->bytes_in_chunk;
193: 		if (output.size() == 0 && data.file_index < bind_data.files.size()) {
194: 			// exhausted this file, but we have more files we can read
195: 			// open the next file and increment the counter
196: 			bind_data.options.file_path = bind_data.files[data.file_index];
