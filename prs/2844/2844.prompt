230: static string NormalizeColumnName(const string &col_name) {
231: 	// normalize UTF8 characters to NFKD
232: 	auto nfkd = utf8proc_NFKD((const utf8proc_uint8_t *)col_name.c_str(), col_name.size());
233: 	const string col_name_nfkd = string((const char *)nfkd, strlen((const char *)nfkd));
234: 	free(nfkd);
235: 
236: 	// only keep ASCII characters 0-9 a-z A-Z and replace spaces with regular whitespace
237: 	string col_name_ascii = "";
238: 	for (idx_t i = 0; i < col_name_nfkd.size(); i++) {
239: 		if (col_name_nfkd[i] == '_' || (col_name_nfkd[i] >= '0' && col_name_nfkd[i] <= '9') ||
240: 		    (col_name_nfkd[i] >= 'A' && col_name_nfkd[i] <= 'Z') ||
241: 		    (col_name_nfkd[i] >= 'a' && col_name_nfkd[i] <= 'z')) {
242: 			col_name_ascii += col_name_nfkd[i];
243: 		} else if (StringUtil::CharacterIsSpace(col_name_nfkd[i])) {
244: 			col_name_ascii += " ";
245: 		}
246: 	}
247: 
248: 	// trim whitespace and replace remaining whitespace by _
249: 	string col_name_trimmed = TrimWhitespace(col_name_ascii);
250: 	string col_name_cleaned = "";
251: 	bool in_whitespace = false;
252: 	for (idx_t i = 0; i < col_name_trimmed.size(); i++) {
253: 		if (col_name_trimmed[i] == ' ') {
254: 			if (!in_whitespace) {
255: 				col_name_cleaned += "_";
256: 				in_whitespace = true;
257: 			}
258: 		} else {
259: 			col_name_cleaned += col_name_trimmed[i];
260: 			in_whitespace = false;
261: 		}
262: 	}
263: 
264: 	// don't leave string empty; if not empty, make lowercase
265: 	if (col_name_cleaned.empty()) {
266: 		col_name_cleaned = "_";
267: 	} else {
268: 		col_name_cleaned = StringUtil::Lower(col_name_cleaned);
269: 	}
270: 
271: 	// prepend _ if name starts with a digit or is a reserved keyword
272: 	if (KeywordHelper::IsKeyword(col_name_cleaned) || (col_name_cleaned[0] >= '0' && col_name_cleaned[0] <= '9')) {
273: 		col_name_cleaned = "_" + col_name_cleaned;
274: 	}
275: 	return col_name_cleaned;
276: }
