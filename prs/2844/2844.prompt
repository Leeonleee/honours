496: enum class QuoteRule : uint8_t { QUOTES_RFC = 0, QUOTES_OTHER = 1, NO_QUOTES = 2 };
497: 
498: void BufferedCSVReader::DetectDialect(const vector<LogicalType> &requested_types,
499:                                       BufferedCSVReaderOptions &original_options,
500:                                       vector<BufferedCSVReaderOptions> &info_candidates, idx_t &best_num_cols) {
501: 	// set up the candidates we consider for delimiter and quote rules based on user input
502: 	vector<string> delim_candidates;
503: 	vector<QuoteRule> quoterule_candidates;
504: 	vector<vector<string>> quote_candidates_map;
505: 	vector<vector<string>> escape_candidates_map = {{""}, {"\\"}, {""}};
506: 
507: 	if (options.has_delimiter) {
508: 		// user provided a delimiter: use that delimiter
509: 		delim_candidates = {options.delimiter};
510: 	} else {
511: 		// no delimiter provided: try standard/common delimiters
512: 		delim_candidates = {",", "|", ";", "\t"};
513: 	}
514: 	if (options.has_quote) {
515: 		// user provided quote: use that quote rule
516: 		quote_candidates_map = {{options.quote}, {options.quote}, {options.quote}};
517: 	} else {
518: 		// no quote rule provided: use standard/common quotes
519: 		quote_candidates_map = {{"\""}, {"\"", "'"}, {""}};
520: 	}
521: 	if (options.has_escape) {
522: 		// user provided escape: use that escape rule
523: 		if (options.escape.empty()) {
524: 			quoterule_candidates = {QuoteRule::QUOTES_RFC};
525: 		} else {
526: 			quoterule_candidates = {QuoteRule::QUOTES_OTHER};
527: 		}
528: 		escape_candidates_map[static_cast<uint8_t>(quoterule_candidates[0])] = {options.escape};
529: 	} else {
530: 		// no escape provided: try standard/common escapes
531: 		quoterule_candidates = {QuoteRule::QUOTES_RFC, QuoteRule::QUOTES_OTHER, QuoteRule::NO_QUOTES};
532: 	}
533: 
534: 	idx_t best_consistent_rows = 0;
535: 	for (auto quoterule : quoterule_candidates) {
536: 		const auto &quote_candidates = quote_candidates_map[static_cast<uint8_t>(quoterule)];
537: 		for (const auto &quote : quote_candidates) {
538: 			for (const auto &delim : delim_candidates) {
539: 				const auto &escape_candidates = escape_candidates_map[static_cast<uint8_t>(quoterule)];
540: 				for (const auto &escape : escape_candidates) {
541: 					BufferedCSVReaderOptions sniff_info = original_options;
542: 					sniff_info.delimiter = delim;
543: 					sniff_info.quote = quote;
544: 					sniff_info.escape = escape;
545: 
546: 					options = sniff_info;
547: 					PrepareComplexParser();
548: 
549: 					JumpToBeginning(original_options.skip_rows);
550: 					sniffed_column_counts.clear();
551: 					if (!TryParseCSV(ParserMode::SNIFFING_DIALECT)) {
552: 						continue;
553: 					}
554: 
555: 					idx_t start_row = original_options.skip_rows;
556: 					idx_t consistent_rows = 0;
557: 					idx_t num_cols = 0;
558: 
559: 					for (idx_t row = 0; row < sniffed_column_counts.size(); row++) {
560: 						if (sniffed_column_counts[row] == num_cols) {
561: 							consistent_rows++;
562: 						} else {
563: 							num_cols = sniffed_column_counts[row];
564: 							start_row = row + original_options.skip_rows;
565: 							consistent_rows = 1;
566: 						}
567: 					}
568: 
569: 					// some logic
570: 					bool more_values = (consistent_rows > best_consistent_rows && num_cols >= best_num_cols);
571: 					bool single_column_before = best_num_cols < 2 && num_cols > best_num_cols;
572: 					bool rows_consistent =
573: 					    start_row + consistent_rows - original_options.skip_rows == sniffed_column_counts.size();
574: 					bool more_than_one_row = (consistent_rows > 1);
575: 					bool more_than_one_column = (num_cols > 1);
576: 					bool start_good = !info_candidates.empty() && (start_row <= info_candidates.front().skip_rows);
577: 
578: 					if (!requested_types.empty() && requested_types.size() != num_cols) {
579: 						continue;
580: 					} else if ((more_values || single_column_before) && rows_consistent) {
581: 						sniff_info.skip_rows = start_row;
582: 						sniff_info.num_cols = num_cols;
583: 						best_consistent_rows = consistent_rows;
584: 						best_num_cols = num_cols;
585: 
586: 						info_candidates.clear();
587: 						info_candidates.push_back(sniff_info);
588: 					} else if (more_than_one_row && more_than_one_column && start_good && rows_consistent) {
589: 						bool same_quote_is_candidate = false;
590: 						for (auto &info_candidate : info_candidates) {
591: 							if (quote.compare(info_candidate.quote) == 0) {
592: 								same_quote_is_candidate = true;
593: 							}
594: 						}
595: 						if (!same_quote_is_candidate) {
596: 							sniff_info.skip_rows = start_row;
597: 							sniff_info.num_cols = num_cols;
598: 							info_candidates.push_back(sniff_info);
599: 						}
600: 					}
601: 				}
602: 			}
603: 		}
604: 	}
605: }
606: 
607: void BufferedCSVReader::DetectCandidateTypes(const vector<LogicalType> &type_candidates,
608:                                              const map<LogicalTypeId, vector<const char *>> &format_template_candidates,
609:                                              const vector<BufferedCSVReaderOptions> &info_candidates,
610:                                              BufferedCSVReaderOptions &original_options, idx_t best_num_cols,
611:                                              vector<vector<LogicalType>> &best_sql_types_candidates,
612:                                              std::map<LogicalTypeId, vector<string>> &best_format_candidates,
613:                                              DataChunk &best_header_row) {
614: 	BufferedCSVReaderOptions best_options;
615: 	idx_t min_varchar_cols = best_num_cols + 1;
616: 
617: 	// check which info candidate leads to minimum amount of non-varchar columns...
618: 	for (const auto &t : format_template_candidates) {
619: 		best_format_candidates[t.first].clear();
620: 	}
621: 	for (auto &info_candidate : info_candidates) {
622: 		options = info_candidate;
623: 		vector<vector<LogicalType>> info_sql_types_candidates(options.num_cols, type_candidates);
624: 		std::map<LogicalTypeId, bool> has_format_candidates;
625: 		std::map<LogicalTypeId, vector<string>> format_candidates;
626: 		for (const auto &t : format_template_candidates) {
627: 			has_format_candidates[t.first] = false;
628: 			format_candidates[t.first].clear();
629: 		}
630: 
631: 		// set all sql_types to VARCHAR so we can do datatype detection based on VARCHAR values
632: 		sql_types.clear();
633: 		sql_types.assign(options.num_cols, LogicalType::VARCHAR);
634: 
635: 		// jump to beginning and skip potential header
636: 		JumpToBeginning(options.skip_rows, true);
637: 		DataChunk header_row;
638: 		header_row.Initialize(sql_types);
639: 		parse_chunk.Copy(header_row);
640: 
641: 		if (header_row.size() == 0) {
642: 			continue;
643: 		}
644: 
645: 		// init parse chunk and read csv with info candidate
646: 		InitParseChunk(sql_types.size());
647: 		ParseCSV(ParserMode::SNIFFING_DATATYPES);
648: 		for (idx_t row_idx = 0; row_idx <= parse_chunk.size(); row_idx++) {
649: 			bool is_header_row = row_idx == 0;
650: 			idx_t row = row_idx - 1;
651: 			for (idx_t col = 0; col < parse_chunk.ColumnCount(); col++) {
652: 				auto &col_type_candidates = info_sql_types_candidates[col];
653: 				while (col_type_candidates.size() > 1) {
654: 					const auto &sql_type = col_type_candidates.back();
655: 					// try cast from string to sql_type
656: 					Value dummy_val;
657: 					if (is_header_row) {
658: 						dummy_val = header_row.GetValue(col, 0);
659: 					} else {
660: 						dummy_val = parse_chunk.GetValue(col, row);
661: 					}
662: 					// try formatting for date types if the user did not specify one and it starts with numeric values.
663: 					string separator;
664: 					if (has_format_candidates.count(sql_type.id()) && !original_options.has_format[sql_type.id()] &&
665: 					    StartsWithNumericDate(separator, dummy_val.str_value)) {
666: 						// generate date format candidates the first time through
667: 						auto &type_format_candidates = format_candidates[sql_type.id()];
668: 						const auto had_format_candidates = has_format_candidates[sql_type.id()];
669: 						if (!has_format_candidates[sql_type.id()]) {
670: 							has_format_candidates[sql_type.id()] = true;
671: 							// order by preference
672: 							auto entry = format_template_candidates.find(sql_type.id());
673: 							if (entry != format_template_candidates.end()) {
674: 								const auto &format_template_list = entry->second;
675: 								for (const auto &t : format_template_list) {
676: 									const auto format_string = GenerateDateFormat(separator, t);
677: 									// don't parse ISO 8601
678: 									if (format_string.find("%Y-%m-%d") == string::npos) {
679: 										type_format_candidates.emplace_back(format_string);
680: 									}
681: 								}
682: 							}
683: 							//	initialise the first candidate
684: 							options.has_format[sql_type.id()] = true;
685: 							//	all formats are constructed to be valid
686: 							SetDateFormat(type_format_candidates.back(), sql_type.id());
687: 						}
688: 						// check all formats and keep the first one that works
689: 						StrpTimeFormat::ParseResult result;
690: 						auto save_format_candidates = type_format_candidates;
691: 						while (!type_format_candidates.empty()) {
692: 							//	avoid using exceptions for flow control...
693: 							auto &current_format = options.date_format[sql_type.id()];
694: 							if (current_format.Parse(dummy_val.str_value, result)) {
695: 								break;
696: 							}
697: 							//	doesn't work - move to the next one
698: 							type_format_candidates.pop_back();
699: 							options.has_format[sql_type.id()] = (!type_format_candidates.empty());
700: 							if (!type_format_candidates.empty()) {
701: 								SetDateFormat(type_format_candidates.back(), sql_type.id());
702: 							}
703: 						}
704: 						//	if none match, then this is not a value of type sql_type,
705: 						if (type_format_candidates.empty()) {
706: 							//	so restore the candidates that did work.
707: 							//	or throw them out if they were generated by this value.
708: 							if (had_format_candidates) {
709: 								type_format_candidates.swap(save_format_candidates);
710: 								if (!type_format_candidates.empty()) {
711: 									SetDateFormat(type_format_candidates.back(), sql_type.id());
712: 								}
713: 							} else {
714: 								has_format_candidates[sql_type.id()] = false;
715: 							}
716: 						}
717: 					}
718: 					// try cast from string to sql_type
719: 					if (TryCastValue(dummy_val, sql_type)) {
720: 						break;
721: 					} else {
722: 						col_type_candidates.pop_back();
723: 					}
724: 				}
725: 			}
726: 			// reset type detection, because first row could be header,
727: 			// but only do it if csv has more than one line (including header)
728: 			if (parse_chunk.size() > 0 && is_header_row) {
729: 				info_sql_types_candidates = vector<vector<LogicalType>>(options.num_cols, type_candidates);
730: 				for (auto &f : format_candidates) {
731: 					f.second.clear();
732: 				}
733: 				for (auto &h : has_format_candidates) {
734: 					h.second = false;
735: 				}
736: 			}
737: 		}
738: 
739: 		idx_t varchar_cols = 0;
740: 		for (idx_t col = 0; col < parse_chunk.ColumnCount(); col++) {
741: 			auto &col_type_candidates = info_sql_types_candidates[col];
742: 			// check number of varchar columns
743: 			const auto &col_type = col_type_candidates.back();
744: 			if (col_type == LogicalType::VARCHAR) {
745: 				varchar_cols++;
746: 			}
747: 		}
748: 
749: 		// it's good if the dialect creates more non-varchar columns, but only if we sacrifice < 30% of best_num_cols.
750: 		if (varchar_cols < min_varchar_cols && parse_chunk.ColumnCount() > (best_num_cols * 0.7)) {
751: 			// we have a new best_options candidate
752: 			best_options = info_candidate;
753: 			min_varchar_cols = varchar_cols;
754: 			best_sql_types_candidates = info_sql_types_candidates;
755: 			best_format_candidates = format_candidates;
756: 			best_header_row.Destroy();
757: 			auto header_row_types = header_row.GetTypes();
758: 			best_header_row.Initialize(header_row_types);
759: 			header_row.Copy(best_header_row);
760: 		}
761: 	}
762: 
763: 	options = best_options;
764: 	for (const auto &best : best_format_candidates) {
765: 		if (!best.second.empty()) {
766: 			SetDateFormat(best.second.back(), best.first);
767: 		}
768: 	}
769: }
