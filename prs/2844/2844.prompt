771: void BufferedCSVReader::DetectHeader(const vector<vector<LogicalType>> &best_sql_types_candidates,
772:                                      const DataChunk &best_header_row) {
773: 	// information for header detection
774: 	bool first_row_consistent = true;
775: 	bool first_row_nulls = false;
776: 
777: 	// check if header row is all null and/or consistent with detected column data types
778: 	first_row_nulls = true;
779: 	for (idx_t col = 0; col < best_sql_types_candidates.size(); col++) {
780: 		auto dummy_val = best_header_row.GetValue(col, 0);
781: 		if (!dummy_val.is_null) {
782: 			first_row_nulls = false;
783: 		}
784: 
785: 		// try cast to sql_type of column
786: 		const auto &sql_type = best_sql_types_candidates[col].back();
787: 		if (!TryCastValue(dummy_val, sql_type)) {
788: 			first_row_consistent = false;
789: 		}
790: 	}
791: 
792: 	// update parser info, and read, generate & set col_names based on previous findings
793: 	if (((!first_row_consistent || first_row_nulls) && !options.has_header) || (options.has_header && options.header)) {
794: 		options.header = true;
795: 		unordered_map<string, idx_t> name_collision_count;
796: 		// get header names from CSV
797: 		for (idx_t col = 0; col < options.num_cols; col++) {
798: 			const auto &val = best_header_row.GetValue(col, 0);
799: 			string col_name = val.ToString();
800: 
801: 			// generate name if field is empty
802: 			if (col_name.empty() || val.is_null) {
803: 				col_name = GenerateColumnName(options.num_cols, col);
804: 			}
805: 
806: 			// normalize names or at least trim whitespace
807: 			if (options.normalize_names) {
808: 				col_name = NormalizeColumnName(col_name);
809: 			} else {
810: 				col_name = TrimWhitespace(col_name);
811: 			}
812: 
813: 			// avoid duplicate header names
814: 			const string col_name_raw = col_name;
815: 			while (name_collision_count.find(col_name) != name_collision_count.end()) {
816: 				name_collision_count[col_name] += 1;
817: 				col_name = col_name + "_" + to_string(name_collision_count[col_name]);
818: 			}
819: 
820: 			col_names.push_back(col_name);
821: 			name_collision_count[col_name] = 0;
822: 		}
823: 
824: 	} else {
825: 		options.header = false;
826: 		for (idx_t col = 0; col < options.num_cols; col++) {
827: 			string column_name = GenerateColumnName(options.num_cols, col);
828: 			col_names.push_back(column_name);
829: 		}
830: 	}
831: }
