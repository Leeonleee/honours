1580: void BufferedCSVReader::Flush(DataChunk &insert_chunk) {
1581: 	if (parse_chunk.size() == 0) {
1582: 		return;
1583: 	}
1584: 	// convert the columns in the parsed chunk to the types of the table
1585: 	insert_chunk.SetCardinality(parse_chunk);
1586: 	for (idx_t col_idx = 0; col_idx < sql_types.size(); col_idx++) {
1587: 		if (sql_types[col_idx].id() == LogicalTypeId::VARCHAR) {
1588: 			// target type is varchar: no need to convert
1589: 			// just test that all strings are valid utf-8 strings
1590: 			auto parse_data = FlatVector::GetData<string_t>(parse_chunk.data[col_idx]);
1591: 			for (idx_t i = 0; i < parse_chunk.size(); i++) {
1592: 				if (!FlatVector::IsNull(parse_chunk.data[col_idx], i)) {
1593: 					auto s = parse_data[i];
1594: 					auto utf_type = Utf8Proc::Analyze(s.GetDataUnsafe(), s.GetSize());
1595: 					if (utf_type == UnicodeType::INVALID) {
1596: 						string col_name = to_string(col_idx);
1597: 						if (col_idx < col_names.size()) {
1598: 							col_name = "\"" + col_names[col_idx] + "\"";
1599: 						}
1600: 						throw InvalidInputException("Error in file \"%s\" between line %llu and %llu in column \"%s\": "
1601: 						                            "file is not valid UTF8. Parser options: %s",
1602: 						                            options.file_path, linenr - parse_chunk.size(), linenr, col_name,
1603: 						                            options.ToString());
1604: 					}
1605: 				}
1606: 			}
1607: 			insert_chunk.data[col_idx].Reference(parse_chunk.data[col_idx]);
1608: 		} else {
1609: 			string error_message;
1610: 			bool success;
1611: 			if (options.has_format[LogicalTypeId::DATE] && sql_types[col_idx].id() == LogicalTypeId::DATE) {
1612: 				// use the date format to cast the chunk
1613: 				success = TryCastDateVector(options, parse_chunk.data[col_idx], insert_chunk.data[col_idx],
1614: 				                            parse_chunk.size(), error_message);
1615: 			} else if (options.has_format[LogicalTypeId::TIMESTAMP] &&
1616: 			           sql_types[col_idx].id() == LogicalTypeId::TIMESTAMP) {
1617: 				// use the date format to cast the chunk
1618: 				success = TryCastTimestampVector(options, parse_chunk.data[col_idx], insert_chunk.data[col_idx],
1619: 				                                 parse_chunk.size(), error_message);
1620: 			} else {
1621: 				// target type is not varchar: perform a cast
1622: 				success = VectorOperations::TryCast(parse_chunk.data[col_idx], insert_chunk.data[col_idx],
1623: 				                                    parse_chunk.size(), &error_message);
1624: 			}
1625: 			if (!success) {
1626: 				string col_name = to_string(col_idx);
1627: 				if (col_idx < col_names.size()) {
1628: 					col_name = "\"" + col_names[col_idx] + "\"";
1629: 				}
1630: 
1631: 				if (options.auto_detect) {
1632: 					throw InvalidInputException("%s in column %s, between line %llu and %llu. Parser "
1633: 					                            "options: %s. Consider either increasing the sample size "
1634: 					                            "(SAMPLE_SIZE=X [X rows] or SAMPLE_SIZE=-1 [all rows]), "
1635: 					                            "or skipping column conversion (ALL_VARCHAR=1)",
1636: 					                            error_message, col_name, linenr - parse_chunk.size() + 1, linenr,
1637: 					                            options.ToString());
1638: 				} else {
1639: 					throw InvalidInputException("%s between line %llu and %llu in column %s. Parser options: %s ",
1640: 					                            error_message, linenr - parse_chunk.size(), linenr, col_name,
1641: 					                            options.ToString());
1642: 				}
1643: 			}
1644: 		}
1645: 	}
1646: 	parse_chunk.Reset();
1647: }
