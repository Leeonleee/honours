48: static bool StartsWithNumericDate(string &separator, const string_t &value) {
49: 	auto begin = value.GetDataUnsafe();
50: 	auto end = begin + value.GetSize();
51: 
52: 	//	StrpTimeFormat::Parse will skip whitespace, so we can too
53: 	auto field1 = std::find_if_not(begin, end, StringUtil::CharacterIsSpace);
54: 	if (field1 == end) {
55: 		return false;
56: 	}
57: 
58: 	//	first numeric field must start immediately
59: 	if (!StringUtil::CharacterIsDigit(*field1)) {
60: 		return false;
61: 	}
62: 	auto literal1 = std::find_if_not(field1, end, StringUtil::CharacterIsDigit);
63: 	if (literal1 == end) {
64: 		return false;
65: 	}
66: 
67: 	//	second numeric field must exist
68: 	auto field2 = std::find_if(literal1, end, StringUtil::CharacterIsDigit);
69: 	if (field2 == end) {
70: 		return false;
71: 	}
72: 	auto literal2 = std::find_if_not(field2, end, StringUtil::CharacterIsDigit);
73: 	if (literal2 == end) {
74: 		return false;
75: 	}
76: 
77: 	//	third numeric field must exist
78: 	auto field3 = std::find_if(literal2, end, StringUtil::CharacterIsDigit);
79: 	if (field3 == end) {
80: 		return false;
81: 	}
82: 
83: 	//	second literal must match first
84: 	if (((field3 - literal2) != (field2 - literal1)) || strncmp(literal1, literal2, (field2 - literal1)) != 0) {
85: 		return false;
86: 	}
87: 
88: 	//	copy the literal as the separator, escaping percent signs
89: 	separator.clear();
90: 	while (literal1 < field2) {
91: 		const auto literal_char = *literal1++;
92: 		if (literal_char == '%') {
93: 			separator.push_back(literal_char);
94: 		}
95: 		separator.push_back(literal_char);
96: 	}
97: 
98: 	return true;
99: }
