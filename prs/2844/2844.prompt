1317: 	// or a delimiter/newline, ending the current value and moving on to the next value
1318: 	position++;
1319: 	if (position >= buffer_size && !ReadBuffer(start)) {
1320: 		// file ends right after unquote, go to final state
1321: 		offset = 1;
1322: 		goto final_state;
1323: 	}
1324: 	if (buffer[position] == options.quote[0] && (options.escape.empty() || options.escape[0] == options.quote[0])) {
1325: 		// escaped quote, return to quoted state and store escape position
1326: 		escape_positions.push_back(position - start);
