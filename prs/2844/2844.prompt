1329: 		// delimiter, add value
1330: 		offset = 1;
1331: 		goto add_value;
1332: 	} else if (StringUtil::CharacterIsNewline(buffer[position])) {
1333: 		offset = 1;
1334: 		goto add_row;
1335: 	} else {
1336: 		error_message = StringUtil::Format(
1337: 		    "Error in file \"%s\" on line %s: quote should be followed by end of value, end of "
1338: 		    "row or another quote. (%s)",
1339: 		    options.file_path, GetLineNumberStr(linenr, linenr_estimated).c_str(), options.ToString());
1340: 		return false;
1341: 	}
1342: handle_escape:
1343: 	/* state: handle_escape */
1344: 	// escape should be followed by a quote or another escape character
1345: 	position++;
