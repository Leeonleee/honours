1397: bool BufferedCSVReader::ReadBuffer(idx_t &start) {
1398: 	auto old_buffer = move(buffer);
1399: 
1400: 	// the remaining part of the last buffer
1401: 	idx_t remaining = buffer_size - start;
1402: 	idx_t buffer_read_size = INITIAL_BUFFER_SIZE;
1403: 	while (remaining > buffer_read_size) {
1404: 		buffer_read_size *= 2;
1405: 	}
1406: 	if (remaining + buffer_read_size > MAXIMUM_CSV_LINE_SIZE) {
1407: 		throw InvalidInputException("Maximum line size of %llu bytes exceeded!", MAXIMUM_CSV_LINE_SIZE);
1408: 	}
1409: 	buffer = unique_ptr<char[]>(new char[buffer_read_size + remaining + 1]);
1410: 	buffer_size = remaining + buffer_read_size;
1411: 	if (remaining > 0) {
1412: 		// remaining from last buffer: copy it here
1413: 		memcpy(buffer.get(), old_buffer.get() + start, remaining);
1414: 	}
1415: 	idx_t read_count = file_handle->Read(buffer.get() + remaining, buffer_read_size);
1416: 
1417: 	bytes_in_chunk += read_count;
1418: 	buffer_size = remaining + read_count;
1419: 	buffer[buffer_size] = '\0';
1420: 	if (old_buffer) {
1421: 		cached_buffers.push_back(move(old_buffer));
1422: 	}
1423: 	start = 0;
1424: 	position = remaining;
1425: 	if (!bom_checked) {
1426: 		bom_checked = true;
1427: 		if (read_count >= 3 && buffer[0] == '\xEF' && buffer[1] == '\xBB' && buffer[2] == '\xBF') {
1428: 			position += 3;
1429: 		}
1430: 	}
1431: 
1432: 	return read_count > 0;
1433: }
