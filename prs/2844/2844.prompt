258: 	// remove any compression
259: 	if (StringUtil::EndsWith(lower_name, ".gz")) {
260: 		lower_name = lower_name.substr(0, lower_name.size() - 3);
261: 	} else if (StringUtil::EndsWith(lower_name, ".zst")) {
262: 		lower_name = lower_name.substr(0, lower_name.size() - 4);
263: 	}
264: 	if (!StringUtil::EndsWith(lower_name, ".csv") && !StringUtil::EndsWith(lower_name, ".tsv")) {
265: 		return nullptr;
266: 	}
267: 	auto table_function = make_unique<TableFunctionRef>();
268: 	vector<unique_ptr<ParsedExpression>> children;
269: 	children.push_back(make_unique<ConstantExpression>(Value(table_name)));
270: 	table_function->function = make_unique<FunctionExpression>("read_csv_auto", move(children));
271: 	return table_function;
