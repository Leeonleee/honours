1225: 		if (!ReadBuffer(start)) {
1226: 			return true;
1227: 		}
1228: 	}
1229: 	// start parsing the first value
1230: 	goto value_start;
1231: value_start:
1232: 	offset = 0;
1233: 	/* state: value_start */
1234: 	// this state parses the first character of a value
1235: 	if (buffer[position] == options.quote[0]) {
