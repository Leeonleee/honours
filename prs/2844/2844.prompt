1347: 		error_message = StringUtil::Format(
1348: 		    "Error in file \"%s\" on line %s: neither QUOTE nor ESCAPE is proceeded by ESCAPE. (%s)", options.file_path,
1349: 		    GetLineNumberStr(linenr, linenr_estimated).c_str(), options.ToString());
1350: 		return false;
1351: 	}
1352: 	if (buffer[position] != options.quote[0] && buffer[position] != options.escape[0]) {
1353: 		error_message = StringUtil::Format(
1354: 		    "Error in file \"%s\" on line %s: neither QUOTE nor ESCAPE is proceeded by ESCAPE. (%s)", options.file_path,
1355: 		    GetLineNumberStr(linenr, linenr_estimated).c_str(), options.ToString());
1356: 		return false;
1357: 	}
1358: 	// escape was followed by quote or escape, go back to quoted state
1359: 	goto in_quotes;
1360: carriage_return:
1361: 	/* state: carriage_return */
1362: 	// this stage optionally skips a newline (\n) character, which allows \r\n to be interpreted as a single line
1363: 	if (buffer[position] == '\n') {
1364: 		// newline after carriage return: skip
1365: 		// increase position by 1 and move start to the new position
1366: 		start = ++position;
1367: 		if (position >= buffer_size && !ReadBuffer(start)) {
1368: 			// file ends right after delimiter, go to final state
1369: 			goto final_state;
1370: 		}
1371: 	}
1372: 	if (finished_chunk) {
1373: 		return true;
1374: 	}
1375: 	goto value_start;
1376: final_state:
1377: 	if (finished_chunk) {
1378: 		return true;
1379: 	}
1380: 
1381: 	if (column > 0 || position > start) {
1382: 		// remaining values to be added to the chunk
1383: 		AddValue(buffer.get() + start, position - start - offset, column, escape_positions);
1384: 		finished_chunk = AddRow(insert_chunk, column);
1385: 	}
1386: 
1387: 	// final stage, only reached after parsing the file is finished
1388: 	// flush the parsed chunk and finalize parsing
1389: 	if (mode == ParserMode::PARSING) {
1390: 		Flush(insert_chunk);
1391: 	}
1392: 
1393: 	end_of_file_reached = true;
1394: 	return true;
1395: }
