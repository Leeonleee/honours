451: template <class OP, class T>
452: static bool TemplatedTryCastDateVector(BufferedCSVReaderOptions &options, Vector &input_vector, Vector &result_vector,
453:                                        idx_t count, string &error_message) {
454: 	D_ASSERT(input_vector.GetType().id() == LogicalTypeId::VARCHAR);
455: 	bool all_converted = true;
456: 	UnaryExecutor::Execute<string_t, T>(input_vector, result_vector, count, [&](string_t input) {
457: 		T result;
458: 		if (!OP::Operation(options, input, result, error_message)) {
459: 			all_converted = false;
460: 		}
461: 		return result;
462: 	});
463: 	return all_converted;
464: }
