1477: void BufferedCSVReader::AddValue(char *str_val, idx_t length, idx_t &column, vector<idx_t> &escape_positions) {
1478: 	if (length == 0 && column == 0) {
1479: 		row_empty = true;
1480: 	} else {
1481: 		row_empty = false;
1482: 	}
1483: 
1484: 	if (!sql_types.empty() && column == sql_types.size() && length == 0) {
1485: 		// skip a single trailing delimiter in last column
1486: 		return;
1487: 	}
1488: 	if (mode == ParserMode::SNIFFING_DIALECT) {
1489: 		column++;
1490: 		return;
1491: 	}
1492: 	if (column >= sql_types.size()) {
1493: 		throw InvalidInputException("Error on line %s: expected %lld values per row, but got more. (%s)",
1494: 		                            GetLineNumberStr(linenr, linenr_estimated).c_str(), sql_types.size(),
1495: 		                            options.ToString());
1496: 	}
1497: 
1498: 	// insert the line number into the chunk
1499: 	idx_t row_entry = parse_chunk.size();
1500: 
1501: 	str_val[length] = '\0';
1502: 
1503: 	// test against null string
1504: 	if (!options.force_not_null[column] && strcmp(options.null_str.c_str(), str_val) == 0) {
1505: 		FlatVector::SetNull(parse_chunk.data[column], row_entry, true);
1506: 	} else {
1507: 		auto &v = parse_chunk.data[column];
1508: 		auto parse_data = FlatVector::GetData<string_t>(v);
1509: 		if (!escape_positions.empty()) {
1510: 			// remove escape characters (if any)
1511: 			string old_val = str_val;
1512: 			string new_val = "";
1513: 			idx_t prev_pos = 0;
1514: 			for (idx_t i = 0; i < escape_positions.size(); i++) {
1515: 				idx_t next_pos = escape_positions[i];
1516: 				new_val += old_val.substr(prev_pos, next_pos - prev_pos);
1517: 
1518: 				if (options.escape.empty() || options.escape == options.quote) {
1519: 					prev_pos = next_pos + options.quote.size();
1520: 				} else {
1521: 					prev_pos = next_pos + options.escape.size();
1522: 				}
1523: 			}
1524: 			new_val += old_val.substr(prev_pos, old_val.size() - prev_pos);
1525: 			escape_positions.clear();
1526: 			parse_data[row_entry] = StringVector::AddStringOrBlob(v, string_t(new_val));
1527: 		} else {
1528: 			parse_data[row_entry] = string_t(str_val, length);
1529: 		}
1530: 	}
1531: 
1532: 	// move to the next column
1533: 	column++;
1534: }
