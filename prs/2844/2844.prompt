341: bool BufferedCSVReader::JumpToNextSample() {
342: 	// get bytes contained in the previously read chunk
343: 	idx_t remaining_bytes_in_buffer = buffer_size - start;
344: 	bytes_in_chunk -= remaining_bytes_in_buffer;
345: 	if (remaining_bytes_in_buffer == 0) {
346: 		return false;
347: 	}
348: 
349: 	// assess if it makes sense to jump, based on size of the first chunk relative to size of the entire file
350: 	if (sample_chunk_idx == 0) {
351: 		idx_t bytes_first_chunk = bytes_in_chunk;
352: 		double chunks_fit = (file_size / (double)bytes_first_chunk);
353: 		jumping_samples = chunks_fit >= options.sample_chunks;
354: 
355: 		// jump back to the beginning
356: 		JumpToBeginning(options.skip_rows, options.header);
357: 		sample_chunk_idx++;
358: 		return true;
359: 	}
360: 
361: 	if (end_of_file_reached || sample_chunk_idx >= options.sample_chunks) {
362: 		return false;
363: 	}
364: 
365: 	// if we deal with any other sources than plaintext files, jumping_samples can be tricky. In that case
366: 	// we just read x continuous chunks from the stream TODO: make jumps possible for zipfiles.
367: 	if (!plain_file_source || !jumping_samples) {
368: 		sample_chunk_idx++;
369: 		return true;
370: 	}
371: 
372: 	// update average bytes per line
373: 	double bytes_per_line = bytes_in_chunk / (double)options.sample_chunk_size;
374: 	bytes_per_line_avg = ((bytes_per_line_avg * (sample_chunk_idx)) + bytes_per_line) / (sample_chunk_idx + 1);
375: 
376: 	// if none of the previous conditions were met, we can jump
377: 	idx_t partition_size = (idx_t)round(file_size / (double)options.sample_chunks);
378: 
379: 	// calculate offset to end of the current partition
380: 	int64_t offset = partition_size - bytes_in_chunk - remaining_bytes_in_buffer;
381: 	auto current_pos = file_handle->SeekPosition();
382: 
383: 	if (current_pos + offset < file_size) {
384: 		// set position in stream and clear failure bits
385: 		file_handle->Seek(current_pos + offset);
386: 
387: 		// estimate linenr
388: 		linenr += (idx_t)round((offset + remaining_bytes_in_buffer) / bytes_per_line_avg);
389: 		linenr_estimated = true;
390: 	} else {
391: 		// seek backwards from the end in last chunk and hope to catch the end of the file
392: 		// TODO: actually it would be good to make sure that the end of file is being reached, because
393: 		// messy end-lines are quite common. For this case, however, we first need a skip_end detection anyways.
394: 		file_handle->Seek(file_size - bytes_in_chunk);
395: 
396: 		// estimate linenr
397: 		linenr = (idx_t)round((file_size - bytes_in_chunk) / bytes_per_line_avg);
398: 		linenr_estimated = true;
399: 	}
400: 
401: 	// reset buffers and parse chunk
402: 	ResetBuffer();
403: 
404: 	// seek beginning of next line
405: 	// FIXME: if this jump ends up in a quoted linebreak, we will have a problem
406: 	string read_line = file_handle->ReadLine();
407: 	linenr++;
408: 
409: 	sample_chunk_idx++;
410: 
411: 	return true;
412: }
