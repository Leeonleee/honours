833: vector<LogicalType> BufferedCSVReader::RefineTypeDetection(const vector<LogicalType> &type_candidates,
834:                                                            const vector<LogicalType> &requested_types,
835:                                                            vector<vector<LogicalType>> &best_sql_types_candidates,
836:                                                            map<LogicalTypeId, vector<string>> &best_format_candidates) {
837: 	// for the type refine we set the SQL types to VARCHAR for all columns
838: 	sql_types.clear();
839: 	sql_types.assign(options.num_cols, LogicalType::VARCHAR);
840: 
841: 	vector<LogicalType> detected_types;
842: 
843: 	// if data types were provided, exit here if number of columns does not match
844: 	if (!requested_types.empty()) {
845: 		if (requested_types.size() != options.num_cols) {
846: 			throw InvalidInputException(
847: 			    "Error while determining column types: found %lld columns but expected %d. (%s)", options.num_cols,
848: 			    requested_types.size(), options.ToString());
849: 		} else {
850: 			detected_types = requested_types;
851: 		}
852: 	} else if (options.all_varchar) {
853: 		// return all types varchar
854: 		detected_types = sql_types;
855: 	} else {
856: 		// jump through the rest of the file and continue to refine the sql type guess
857: 		while (JumpToNextSample()) {
858: 			InitParseChunk(sql_types.size());
859: 			// if jump ends up a bad line, we just skip this chunk
860: 			if (!TryParseCSV(ParserMode::SNIFFING_DATATYPES)) {
861: 				continue;
862: 			}
863: 			for (idx_t col = 0; col < parse_chunk.ColumnCount(); col++) {
864: 				vector<LogicalType> &col_type_candidates = best_sql_types_candidates[col];
865: 				while (col_type_candidates.size() > 1) {
866: 					const auto &sql_type = col_type_candidates.back();
867: 					//	narrow down the date formats
868: 					if (best_format_candidates.count(sql_type.id())) {
869: 						auto &best_type_format_candidates = best_format_candidates[sql_type.id()];
870: 						auto save_format_candidates = best_type_format_candidates;
871: 						while (!best_type_format_candidates.empty()) {
872: 							if (TryCastVector(parse_chunk.data[col], parse_chunk.size(), sql_type)) {
873: 								break;
874: 							}
875: 							//	doesn't work - move to the next one
876: 							best_type_format_candidates.pop_back();
877: 							options.has_format[sql_type.id()] = (!best_type_format_candidates.empty());
878: 							if (!best_type_format_candidates.empty()) {
879: 								SetDateFormat(best_type_format_candidates.back(), sql_type.id());
880: 							}
881: 						}
882: 						//	if none match, then this is not a column of type sql_type,
883: 						if (best_type_format_candidates.empty()) {
884: 							//	so restore the candidates that did work.
885: 							best_type_format_candidates.swap(save_format_candidates);
886: 							if (!best_type_format_candidates.empty()) {
887: 								SetDateFormat(best_type_format_candidates.back(), sql_type.id());
888: 							}
889: 						}
890: 					}
891: 
892: 					if (TryCastVector(parse_chunk.data[col], parse_chunk.size(), sql_type)) {
893: 						break;
894: 					} else {
895: 						col_type_candidates.pop_back();
896: 					}
897: 				}
898: 			}
899: 
900: 			if (!jumping_samples) {
901: 				if ((sample_chunk_idx)*options.sample_chunk_size <= options.buffer_size) {
902: 					// cache parse chunk
903: 					// create a new chunk and fill it with the remainder
904: 					auto chunk = make_unique<DataChunk>();
905: 					auto parse_chunk_types = parse_chunk.GetTypes();
906: 					chunk->Move(parse_chunk);
907: 					cached_chunks.push(move(chunk));
908: 				} else {
909: 					while (!cached_chunks.empty()) {
910: 						cached_chunks.pop();
911: 					}
912: 				}
913: 			}
914: 		}
915: 
916: 		// set sql types
917: 		for (auto &best_sql_types_candidate : best_sql_types_candidates) {
918: 			LogicalType d_type = best_sql_types_candidate.back();
919: 			if (best_sql_types_candidate.size() == type_candidates.size()) {
920: 				d_type = LogicalType::VARCHAR;
921: 			}
922: 			detected_types.push_back(d_type);
923: 		}
924: 	}
925: 
926: 	return detected_types;
927: }
