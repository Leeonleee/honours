1536: bool BufferedCSVReader::AddRow(DataChunk &insert_chunk, idx_t &column) {
1537: 	linenr++;
1538: 
1539: 	if (row_empty) {
1540: 		row_empty = false;
1541: 		if (sql_types.size() != 1) {
1542: 			column = 0;
1543: 			return false;
1544: 		}
1545: 	}
1546: 
1547: 	if (column < sql_types.size() && mode != ParserMode::SNIFFING_DIALECT) {
1548: 		throw InvalidInputException("Error on line %s: expected %lld values per row, but got %d. (%s)",
1549: 		                            GetLineNumberStr(linenr, linenr_estimated).c_str(), sql_types.size(), column,
1550: 		                            options.ToString());
1551: 	}
1552: 
1553: 	if (mode == ParserMode::SNIFFING_DIALECT) {
1554: 		sniffed_column_counts.push_back(column);
1555: 
1556: 		if (sniffed_column_counts.size() == options.sample_chunk_size) {
1557: 			return true;
1558: 		}
1559: 	} else {
1560: 		parse_chunk.SetCardinality(parse_chunk.size() + 1);
1561: 	}
1562: 
1563: 	if (mode == ParserMode::PARSING_HEADER) {
1564: 		return true;
1565: 	}
1566: 
1567: 	if (mode == ParserMode::SNIFFING_DATATYPES && parse_chunk.size() == options.sample_chunk_size) {
1568: 		return true;
1569: 	}
1570: 
1571: 	if (mode == ParserMode::PARSING && parse_chunk.size() == STANDARD_VECTOR_SIZE) {
1572: 		Flush(insert_chunk);
1573: 		return true;
1574: 	}
1575: 
1576: 	column = 0;
1577: 	return false;
1578: }
