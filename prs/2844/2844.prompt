1435: void BufferedCSVReader::ParseCSV(DataChunk &insert_chunk) {
1436: 	// if no auto-detect or auto-detect with jumping samples, we have nothing cached and start from the beginning
1437: 	if (cached_chunks.empty()) {
1438: 		cached_buffers.clear();
1439: 	} else {
1440: 		auto &chunk = cached_chunks.front();
1441: 		parse_chunk.Move(*chunk);
1442: 		cached_chunks.pop();
1443: 		Flush(insert_chunk);
1444: 		return;
1445: 	}
1446: 
1447: 	string error_message;
1448: 	if (!TryParseCSV(ParserMode::PARSING, insert_chunk, error_message)) {
1449: 		throw InvalidInputException(error_message);
1450: 	}
1451: }
