421: bool BufferedCSVReader::TryCastValue(const Value &value, const LogicalType &sql_type) {
422: 	if (options.has_format[LogicalTypeId::DATE] && sql_type.id() == LogicalTypeId::DATE) {
423: 		date_t result;
424: 		string error_message;
425: 		return options.date_format[LogicalTypeId::DATE].TryParseDate(string_t(value.str_value), result, error_message);
426: 	} else if (options.has_format[LogicalTypeId::TIMESTAMP] && sql_type.id() == LogicalTypeId::TIMESTAMP) {
427: 		timestamp_t result;
428: 		string error_message;
429: 		return options.date_format[LogicalTypeId::TIMESTAMP].TryParseTimestamp(string_t(value.str_value), result,
430: 		                                                                       error_message);
431: 	} else {
432: 		Value new_value;
433: 		string error_message;
434: 		return value.TryCastAs(sql_type, new_value, &error_message, true);
435: 	}
436: }
