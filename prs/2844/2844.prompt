116: TextSearchShiftArray::TextSearchShiftArray(string search_term) : length(search_term.size()) {
117: 	if (length > 255) {
118: 		throw Exception("Size of delimiter/quote/escape in CSV reader is limited to 255 bytes");
119: 	}
120: 	// initialize the shifts array
121: 	shifts = unique_ptr<uint8_t[]>(new uint8_t[length * 255]);
122: 	memset(shifts.get(), 0, length * 255 * sizeof(uint8_t));
123: 	// iterate over each of the characters in the array
124: 	for (idx_t main_idx = 0; main_idx < length; main_idx++) {
125: 		uint8_t current_char = (uint8_t)search_term[main_idx];
126: 		// now move over all the remaining positions
127: 		for (idx_t i = main_idx; i < length; i++) {
128: 			bool is_match = true;
129: 			// check if the prefix matches at this position
130: 			// if it does, we move to this position after encountering the current character
131: 			for (idx_t j = 0; j < main_idx; j++) {
132: 				if (search_term[i - main_idx + j] != search_term[j]) {
133: 					is_match = false;
134: 				}
135: 			}
136: 			if (!is_match) {
137: 				continue;
138: 			}
139: 			shifts[i * 255 + current_char] = main_idx + 1;
140: 		}
141: 	}
142: }
