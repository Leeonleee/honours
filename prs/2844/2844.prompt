1237: 		// move to in_quotes state
1238: 		start = position + 1;
1239: 		goto in_quotes;
1240: 	} else {
1241: 		// no quote, move to normal parsing state
1242: 		start = position;
1243: 		goto normal;
1244: 	}
1245: normal:
1246: 	/* state: normal parsing state */
