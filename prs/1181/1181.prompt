19877: /*
19878: ** Return TRUE if a semicolon occurs anywhere in the first N characters
19879: ** of string z[].
19880: */
19881: static int line_contains_semicolon(const char *z, int N){
19882:   int i;
19883:   for(i=0; i<N; i++){  if( z[i]==';' ) return 1; }
19884:   return 0;
