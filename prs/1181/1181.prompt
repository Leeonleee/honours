12617: char *strdup_handle_newline(ShellState *p, const char *z) {
12618:   if (!z) {
12619:     return 0;
12620:   }
12621:   if (p->cMode != MODE_Box) {
12622:     return strdup(z);
12623:   }
12624:   int max_size = 80;
12625:   char *result = malloc(max_size * 2 + 10);
12626:   char *t = result;
12627:   int count = 0;
12628:   int interrupted = 0;
12629:   const char *s;
12630:   for(s = z; *s; s++, t++) {
12631:     if (*s == '\n') {
12632:       *t = '\\';
12633:       t++;
12634:       *t = 'n';
12635:     } else {
12636:       *t = *s;
12637:     }
12638:     count++;
12639:     if (count >= max_size && ((*s & 0xc0) != 0x80)) {
12640:       interrupted = 1;
12641:       break;
12642:     }
12643:   }
12644:   if (interrupted) {
12645:     *t++ = '.';
12646:     *t++ = '.';
12647:     *t++ = '.';
12648:   }
12649:   *t = '\0';
12650:   return result;
12651: }
