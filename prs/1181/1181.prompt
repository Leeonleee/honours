583:       if( n==aw ){
584:         do{ i++; }while( (zUtf[i]&0xc0)==0x80 );
585:         break;
586:       }
587:     }
588:   }
589:   if( n>=aw ){
590:     utf8_printf(pOut, "%.*s", i, zUtf);
591:   }else if( w<0 ){
592:     utf8_printf(pOut, "%*s%s", aw-n, "", zUtf);
593:   }else{
594:     utf8_printf(pOut, "%s%*s", zUtf, aw-n, "");
595:   }
596: }
