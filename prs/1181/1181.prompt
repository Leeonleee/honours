8179: static char *expertDequote(const char *zIn){
8180:   int n = STRLEN(zIn);
8181:   char *zRet = sqlite3_malloc(n);
8182: 
8183:   assert( zIn[0]=='\'' );
8184:   assert( zIn[n-1]=='\'' );
8185: 
8186:   if( zRet ){
8187:     int iOut = 0;
8188:     int iIn = 0;
8189:     for(iIn=1; iIn<(n-1); iIn++){
8190:       if( zIn[iIn]=='\'' ){
8191:         assert( zIn[iIn+1]=='\'' );
8192:         iIn++;
8193:       }
8194:       zRet[iOut++] = zIn[iIn];
8195:     }
8196:     zRet[iOut] = '\0';
8197:   }
8198: 
8199:   return zRet;
8200: }
