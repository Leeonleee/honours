11300:     for(i=0; z[i]; i++){
11301:       if( needCsvQuote[((unsigned char*)z)[i]]
11302:          || (z[i]==p->colSeparator[0] &&
11303:              (nSep==1 || memcmp(z, p->colSeparator, nSep)==0)) ){
11304:         i = 0;
