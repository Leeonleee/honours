8124:   sqlite3 *db,                    /* Database handle to compile against */
8125:   sqlite3_stmt **ppStmt,          /* OUT: Compiled SQL statement */
8126:   char **pzErrmsg,                /* OUT: sqlite3_malloc()ed error message */
