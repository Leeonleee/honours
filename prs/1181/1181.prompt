14551: /* Clean up resourced used by an ImportCtx */
14552: static void import_cleanup(ImportCtx *p){
14553:   if( p->in!=0 && p->xCloser!=0 ){
14554:     p->xCloser(p->in);
14555:     p->in = 0;
14556:   }
14557:   sqlite3_free(p->z);
14558:   p->z = 0;
14559: }
