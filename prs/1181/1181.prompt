6211: /*
6212: ** Advance an ZipfileCsr to its next row of output.
6213: */
6214: static int zipfileNext(sqlite3_vtab_cursor *cur){
6215:   ZipfileCsr *pCsr = (ZipfileCsr*)cur;
6216:   int rc = SQLITE_OK;
