2831:         fsdirSetErrmsg(pCur, "cannot stat file: %s", pCur->zPath);
2832:         return SQLITE_ERROR;
2833:       }
2834:       return SQLITE_OK;
2835:     }
2836:     closedir(pLvl->pDir);
2837:     sqlite3_free(pLvl->zDir);
2838:     pLvl->pDir = 0;
2839:     pLvl->zDir = 0;
2840:     pCur->iLvl--;
