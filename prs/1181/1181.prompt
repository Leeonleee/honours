6230:         pCsr->iNextOff += (int)p->cds.nExtra + p->cds.nFile + p->cds.nComment;
6231:       }
6232:       pCsr->pCurrent = p;
6233:     }
6234:   }else{
6235:     if( !pCsr->bNoop ){
6236:       pCsr->pCurrent = pCsr->pCurrent->pNext;
6237:     }
6238:     if( pCsr->pCurrent==0 ){
6239:       pCsr->bEof = 1;
6240:     }
6241:   }
6242: 
6243:   pCsr->bNoop = 0;
6244:   return rc;
6245: }
