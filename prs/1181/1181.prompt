12948:         raw_printf(out, "%s\n", zIdx);
12949:         raw_printf(out, "%s\n", zEQP);
12950:       }
12951:     }
12952:   }
12953:   sqlite3_expert_destroy(p);
12954:   pState->expert.pExpert = 0;
12955:   return rc;
12956: }
