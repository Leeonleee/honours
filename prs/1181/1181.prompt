15023:   }
15024:   sqlite3_bind_text(pStmt, 1, zDb, -1, SQLITE_STATIC);
15025:   if( sqlite3_step(pStmt)==SQLITE_ROW
15026:    && sqlite3_column_bytes(pStmt,0)>100
15027:   ){
