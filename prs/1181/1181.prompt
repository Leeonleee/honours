19887: /*
19888: ** Test to see if a line consists entirely of whitespace.
19889: */
19890: static int _all_whitespace(const char *z){
19891:   for(; *z; z++){
19892:     if( IsSpace(z[0]) ) continue;
19893:     if( *z=='/' && z[1]=='*' ){
19894:       z += 2;
19895:       while( *z && (*z!='*' || z[1]!='/') ){ z++; }
19896:       if( *z==0 ) return 0;
19897:       z++;
19898:       continue;
19899:     }
19900:     if( *z=='-' && z[1]=='-' ){
19901:       z += 2;
19902:       while( *z && *z!='\n' ){ z++; }
19903:       if( *z==0 ) return 1;
19904:       continue;
19905:     }
19906:     return 0;
19907:   }
19908:   return 1;
19909: }
