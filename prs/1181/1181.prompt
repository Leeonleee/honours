14340: /*
14341: ** Do C-language style dequoting.
14342: **
14343: **    \a    -> alarm
14344: **    \b    -> backspace
14345: **    \t    -> tab
14346: **    \n    -> newline
14347: **    \v    -> vertical tab
14348: **    \f    -> form feed
14349: **    \r    -> carriage return
14350: **    \s    -> space
14351: **    \"    -> "
14352: **    \'    -> '
14353: **    \\    -> backslash
14354: **    \NNN  -> ascii character NNN in octal
14355: */
14356: static void resolve_backslashes(char *z){
14357:   int i, j;
14358:   char c;
14359:   while( *z && *z!='\\' ) z++;
14360:   for(i=j=0; (c = z[i])!=0; i++, j++){
14361:     if( c=='\\' && z[i+1]!=0 ){
14362:       c = z[++i];
14363:       if( c=='a' ){
14364:         c = '\a';
14365:       }else if( c=='b' ){
14366:         c = '\b';
14367:       }else if( c=='t' ){
14368:         c = '\t';
14369:       }else if( c=='n' ){
14370:         c = '\n';
14371:       }else if( c=='v' ){
14372:         c = '\v';
14373:       }else if( c=='f' ){
14374:         c = '\f';
14375:       }else if( c=='r' ){
14376:         c = '\r';
14377:       }else if( c=='"' ){
14378:         c = '"';
14379:       }else if( c=='\'' ){
14380:         c = '\'';
14381:       }else if( c=='\\' ){
14382:         c = '\\';
14383:       }else if( c>='0' && c<='7' ){
14384:         c -= '0';
14385:         if( z[i+1]>='0' && z[i+1]<='7' ){
14386:           i++;
14387:           c = (c<<3) + z[i] - '0';
14388:           if( z[i+1]>='0' && z[i+1]<='7' ){
14389:             i++;
14390:             c = (c<<3) + z[i] - '0';
14391:           }
14392:         }
14393:       }
14394:     }
14395:     z[j] = c;
14396:   }
14397:   if( j<i ) z[j] = 0;
14398: }
