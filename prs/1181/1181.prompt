8142:   sqlite3_stmt **ppStmt,          /* OUT: Compiled SQL statement */
8143:   char **pzErrmsg,                /* OUT: sqlite3_malloc()ed error message */
8144:   const char *zFmt,               /* printf() format of SQL statement */
