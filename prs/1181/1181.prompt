9765:     idxStatementFree(p->pStatement, 0);
9766:     idxTableFree(p->pTable);
9767:     idxWriteFree(p->pWrite);
9768:     idxHashClear(&p->hIdx);
9769:     sqlite3_free(p->zCandidates);
9770:     sqlite3_free(p);
9771:   }
9772: }
