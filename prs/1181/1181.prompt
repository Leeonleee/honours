15067:   sqlite3_free(zSchemaTab);
15068:   sqlite3_file_control(p->db, zDb, SQLITE_FCNTL_DATA_VERSION, &iDataVersion);
15069:   utf8_printf(p->out, "%-20s %u\n", "data version", iDataVersion);
15070:   return 0;
