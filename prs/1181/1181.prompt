14924:       const char *zXdgOpenCmd =
14925: #if defined(_WIN32)
14926:       "start";
14927: #elif defined(__APPLE__)
14928:       "open";
14929: #else
14930:       "xdg-open";
14931: #endif
14932:       char *zCmd;
