11309:       char *zQuoted = sqlite3_mprintf("\"%w\"", z);
11310:       utf8_printf(out, "%s", zQuoted);
11311:       sqlite3_free(zQuoted);
11312:     }else{
11313:       utf8_printf(out, "%s", z);
11314:     }
11315:   }
