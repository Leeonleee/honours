912: /*
913: ** Construct a fake object name and column list to describe the structure
914: ** of the view, virtual table, or table valued function zSchema.zName.
915: */
916: static char *shellFakeSchema(
917:   sqlite3 *db,            /* The database connection containing the vtab */
918:   const char *zSchema,    /* Schema of the database holding the vtab */
919:   const char *zName       /* The name of the virtual table */
920: ){
921:   sqlite3_stmt *pStmt = 0;
922:   char *zSql;
923:   ShellText s;
924:   char cQuote;
925:   char *zDiv = "(";
926:   int nRow = 0;
927: 
928:   zSql = sqlite3_mprintf("PRAGMA \"%w\".table_info=%Q;",
929:                          zSchema ? zSchema : "main", zName);
930:   sqlite3_prepare_v2(db, zSql, -1, &pStmt, 0);
931:   sqlite3_free(zSql);
932:   initText(&s);
933:   if( zSchema ){
934:     cQuote = quoteChar(zSchema);
935:     if( cQuote && sqlite3_stricmp(zSchema,"temp")==0 ) cQuote = 0;
936:     appendText(&s, zSchema, cQuote);
937:     appendText(&s, ".", 0);
938:   }
939:   cQuote = quoteChar(zName);
940:   appendText(&s, zName, cQuote);
941:   while( sqlite3_step(pStmt)==SQLITE_ROW ){
942:     const char *zCol = (const char*)sqlite3_column_text(pStmt, 1);
943:     nRow++;
944:     appendText(&s, zDiv, 0);
945:     zDiv = ",";
946:     cQuote = quoteChar(zCol);
947:     appendText(&s, zCol, cQuote);
948:   }
949:   appendText(&s, ")", 0);
950:   sqlite3_finalize(pStmt);
951:   if( nRow==0 ){
952:     freeText(&s);
953:     s.z = 0;
954:   }
955:   return s.z;
956: }
