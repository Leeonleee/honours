3024: /* completion_vtab is a subclass of sqlite3_vtab which will
3025: ** serve as the underlying representation of a completion virtual table
3026: */
3027: typedef struct completion_vtab completion_vtab;
3028: struct completion_vtab {
3029:   sqlite3_vtab base;  /* Base class - must be first */
3030:   sqlite3 *db;        /* Database connection for this completion vtab */
3031: };
3032: 
3033: /* completion_cursor is a subclass of sqlite3_vtab_cursor which will
3034: ** serve as the underlying representation of a cursor that scans
3035: ** over rows of the result
3036: */
3037: typedef struct completion_cursor completion_cursor;
3038: struct completion_cursor {
3039:   sqlite3_vtab_cursor base;  /* Base class - must be first */
3040:   sqlite3 *db;               /* Database connection for this cursor */
3041:   int nPrefix, nLine;        /* Number of bytes in zPrefix and zLine */
3042:   char *zPrefix;             /* The prefix for the word we want to complete */
3043:   char *zLine;               /* The whole that we want to complete */
3044:   const char *zCurrentRow;   /* Current output row */
3045:   int szRow;                 /* Length of the zCurrentRow string */
3046:   sqlite3_stmt *pStmt;       /* Current statement */
3047:   sqlite3_int64 iRowid;      /* The rowid */
3048:   int ePhase;                /* Current phase */
3049:   int j;                     /* inter-phase counter */
3050: };
3051: 
3052: /* Values for ePhase:
3053: */
3054: #define COMPLETION_FIRST_PHASE   1
3055: #define COMPLETION_KEYWORDS      1
3056: #define COMPLETION_PRAGMAS       2
3057: #define COMPLETION_FUNCTIONS     3
3058: #define COMPLETION_COLLATIONS    4
3059: #define COMPLETION_INDEXES       5
3060: #define COMPLETION_TRIGGERS      6
3061: #define COMPLETION_DATABASES     7
3062: #define COMPLETION_TABLES        8    /* Also VIEWs and TRIGGERs */
3063: #define COMPLETION_COLUMNS       9
3064: #define COMPLETION_MODULES       10
3065: #define COMPLETION_EOF           11
3066: 
3067: /*
3068: ** The completionConnect() method is invoked to create a new
3069: ** completion_vtab that describes the completion virtual table.
3070: **
3071: ** Think of this routine as the constructor for completion_vtab objects.
3072: **
3073: ** All this routine needs to do is:
3074: **
3075: **    (1) Allocate the completion_vtab object and initialize all fields.
3076: **
3077: **    (2) Tell SQLite (via the sqlite3_declare_vtab() interface) what the
3078: **        result set of queries against completion will look like.
3079: */
3080: static int completionConnect(
3081:   sqlite3 *db,
3082:   void *pAux,
3083:   int argc, const char *const*argv,
3084:   sqlite3_vtab **ppVtab,
3085:   char **pzErr
3086: ){
3087:   completion_vtab *pNew;
3088:   int rc;
3089: 
3090:   (void)(pAux);    /* Unused parameter */
3091:   (void)(argc);    /* Unused parameter */
3092:   (void)(argv);    /* Unused parameter */
3093:   (void)(pzErr);   /* Unused parameter */
3094: 
3095: /* Column numbers */
3096: #define COMPLETION_COLUMN_CANDIDATE 0  /* Suggested completion of the input */
3097: #define COMPLETION_COLUMN_PREFIX    1  /* Prefix of the word to be completed */
3098: #define COMPLETION_COLUMN_WHOLELINE 2  /* Entire line seen so far */
3099: #define COMPLETION_COLUMN_PHASE     3  /* ePhase - used for debugging only */
3100: 
3101:   sqlite3_vtab_config(db, SQLITE_VTAB_INNOCUOUS);
3102:   rc = sqlite3_declare_vtab(db,
3103:       "CREATE TABLE x("
3104:       "  candidate TEXT,"
3105:       "  prefix TEXT HIDDEN,"
3106:       "  wholeline TEXT HIDDEN,"
3107:       "  phase INT HIDDEN"        /* Used for debugging only */
3108:       ")");
3109:   if( rc==SQLITE_OK ){
3110:     pNew = sqlite3_malloc( sizeof(*pNew) );
3111:     *ppVtab = (sqlite3_vtab*)pNew;
3112:     if( pNew==0 ) return SQLITE_NOMEM;
3113:     memset(pNew, 0, sizeof(*pNew));
3114:     pNew->db = db;
3115:   }
3116:   return rc;
3117: }
