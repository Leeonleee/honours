6105: /*
6106: ** Buffer aIn (size nIn bytes) contains compressed data. Uncompressed, the
6107: ** size is nOut bytes. This function uncompresses the data and sets the
6108: ** return value in context pCtx to the result (a blob).
6109: **
6110: ** If an error occurs, an error code is left in pCtx instead.
6111: */
6112: static void zipfileInflate(
6113:   sqlite3_context *pCtx,          /* Store result here */
6114:   const u8 *aIn,                  /* Compressed data */
6115:   int nIn,                        /* Size of buffer aIn[] in bytes */
6116:   int nOut                        /* Expected output size */
6117: ){
6118:   u8 *aRes = sqlite3_malloc(nOut);
6119:   if( aRes==0 ){
6120:     sqlite3_result_error_nomem(pCtx);
6121:   }else{
6122:     int err;
6123:     z_stream str;
6124:     memset(&str, 0, sizeof(str));
6125: 
6126:     str.next_in = (Byte*)aIn;
6127:     str.avail_in = nIn;
6128:     str.next_out = (Byte*)aRes;
6129:     str.avail_out = nOut;
6130: 
6131:     err = inflateInit2(&str, -15);
6132:     if( err!=Z_OK ){
6133:       zipfileCtxErrorMsg(pCtx, "inflateInit2() failed (%d)", err);
6134:     }else{
6135:       err = inflate(&str, Z_NO_FLUSH);
6136:       if( err!=Z_STREAM_END ){
6137:         zipfileCtxErrorMsg(pCtx, "inflate() failed (%d)", err);
6138:       }else{
6139:         sqlite3_result_blob(pCtx, aRes, nOut, zipfileFree);
6140:         aRes = 0;
6141:       }
6142:     }
6143:     sqlite3_free(aRes);
6144:     inflateEnd(&str);
6145:   }
6146: }
6147: 
6148: /*
6149: ** Buffer aIn (size nIn bytes) contains uncompressed data. This function
6150: ** compresses it and sets (*ppOut) to point to a buffer containing the
6151: ** compressed data. The caller is responsible for eventually calling
6152: ** sqlite3_free() to release buffer (*ppOut). Before returning, (*pnOut)
6153: ** is set to the size of buffer (*ppOut) in bytes.
6154: **
6155: ** If no error occurs, SQLITE_OK is returned. Otherwise, an SQLite error
6156: ** code is returned and an error message left in virtual-table handle
6157: ** pTab. The values of (*ppOut) and (*pnOut) are left unchanged in this
6158: ** case.
6159: */
6160: static int zipfileDeflate(
6161:   const u8 *aIn, int nIn,         /* Input */
6162:   u8 **ppOut, int *pnOut,         /* Output */
6163:   char **pzErr                    /* OUT: Error message */
6164: ){
6165:   int rc = SQLITE_OK;
6166:   sqlite3_int64 nAlloc;
6167:   z_stream str;
6168:   u8 *aOut;
6169: 
6170:   memset(&str, 0, sizeof(str));
6171:   str.next_in = (Bytef*)aIn;
6172:   str.avail_in = nIn;
6173:   deflateInit2(&str, 9, Z_DEFLATED, -15, 8, Z_DEFAULT_STRATEGY);
6174: 
6175:   nAlloc = deflateBound(&str, nIn);
6176:   aOut = (u8*)sqlite3_malloc64(nAlloc);
6177:   if( aOut==0 ){
6178:     rc = SQLITE_NOMEM;
6179:   }else{
6180:     int res;
6181:     str.next_out = aOut;
6182:     str.avail_out = nAlloc;
6183:     res = deflate(&str, Z_FINISH);
6184:     if( res==Z_STREAM_END ){
6185:       *ppOut = aOut;
6186:       *pnOut = (int)str.total_out;
6187:     }else{
6188:       sqlite3_free(aOut);
6189:       *pzErr = sqlite3_mprintf("zipfile: deflate() error");
6190:       rc = SQLITE_ERROR;
6191:     }
6192:     deflateEnd(&str);
6193:   }
6194: 
6195:   return rc;
6196: }
