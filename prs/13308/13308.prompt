10005: /*
10006: ** Return the number of bytes of space used by an SQLite value of type
10007: ** eType.
10008: */
10009: static int dbdataValueBytes(int eType){
10010:   switch( eType ){
10011:     case 0: case 8: case 9:
10012:     case 10: case 11:
10013:       return 0;
10014:     case 1:
10015:       return 1;
10016:     case 2:
10017:       return 2;
10018:     case 3:
10019:       return 3;
10020:     case 4:
10021:       return 4;
10022:     case 5:
10023:       return 6;
10024:     case 6:
10025:     case 7:
10026:       return 8;
10027:     default:
10028:       if( eType>0 ){
10029:         return ((eType-12) / 2);
10030:       }
10031:       return 0;
10032:   }
10033: }
