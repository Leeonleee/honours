14392: static int sql_trace_callback(
14393:   unsigned mType,         /* The trace type */
14394:   void *pArg,             /* The ShellState pointer */
14395:   void *pP,               /* Usually a pointer to sqlite_stmt */
14396:   void *pX                /* Auxiliary output */
14397: ){
