784: /*
785: ** Interpret zArg as an integer value, possibly with suffixes.
786: */
787: static sqlite3_int64 integerValue(const char *zArg){
788:   sqlite3_int64 v = 0;
789:   static const struct { char *zSuffix; int iMult; } aMult[] = {
790:     { "KiB", 1024 },
791:     { "MiB", 1024*1024 },
792:     { "GiB", 1024*1024*1024 },
793:     { "KB",  1000 },
794:     { "MB",  1000000 },
795:     { "GB",  1000000000 },
796:     { "K",   1000 },
797:     { "M",   1000000 },
798:     { "G",   1000000000 },
799:   };
800:   int i;
801:   int isNeg = 0;
802:   if( zArg[0]=='-' ){
803:     isNeg = 1;
804:     zArg++;
805:   }else if( zArg[0]=='+' ){
806:     zArg++;
807:   }
808:   if( zArg[0]=='0' && zArg[1]=='x' ){
809:     int x;
810:     zArg += 2;
811:     while( (x = hexDigitValue(zArg[0]))>=0 ){
812:       v = (v<<4) + x;
813:       zArg++;
814:     }
815:   }else{
816:     while( IsDigit(zArg[0]) ){
817:       v = v*10 + zArg[0] - '0';
818:       zArg++;
819:     }
820:   }
821:   for(i=0; i<ArraySize(aMult); i++){
822:     if( sqlite3_stricmp(aMult[i].zSuffix, zArg)==0 ){
823:       v *= aMult[i].iMult;
824:       break;
825:     }
826:   }
827:   return isNeg? -v : v;
828: }
