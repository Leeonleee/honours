15874: /*
15875: ** This function is called to search the schema recovered from the
15876: ** sqlite_schema table of the (possibly) corrupt database as part
15877: ** of a ".recover" command. Specifically, for a table with root page
15878: ** iRoot and at least nCol columns. Additionally, if bIntkey is 0, the
15879: ** table must be a WITHOUT ROWID table, or if non-zero, not one of
15880: ** those.
15881: **
15882: ** If a table is found, a (RecoverTable*) object is returned. Or, if
15883: ** no such table is found, but bIntkey is false and iRoot is the
15884: ** root page of an index in the recovered schema, then (*pbNoop) is
15885: ** set to true and NULL returned. Or, if there is no such table or
15886: ** index, NULL is returned and (*pbNoop) set to 0, indicating that
15887: ** the caller should write data to the orphans table.
15888: */
15889: static RecoverTable *recoverFindTable(
15890:   ShellState *pState,             /* Shell state object */
15891:   int *pRc,                       /* IN/OUT: Error code */
15892:   int iRoot,                      /* Root page of table */
15893:   int bIntkey,                    /* True for an intkey table */
15894:   int nCol,                       /* Number of columns in table */
15895:   int *pbNoop                     /* OUT: True if iRoot is root of index */
15896: ){
15897:   sqlite3_stmt *pStmt = 0;
15898:   RecoverTable *pRet = 0;
15899:   int bNoop = 0;
15900:   const char *zSql = 0;
15901:   const char *zName = 0;
15902: 
15903:   /* Search the recovered schema for an object with root page iRoot. */
15904:   shellPreparePrintf(pState->db, pRc, &pStmt,
15905:       "SELECT type, name, sql FROM recovery.schema WHERE rootpage=%d", iRoot
15906:   );
15907:   while( *pRc==SQLITE_OK && SQLITE_ROW==sqlite3_step(pStmt) ){
15908:     const char *zType = (const char*)sqlite3_column_text(pStmt, 0);
15909:     if( bIntkey==0 && sqlite3_stricmp(zType, "index")==0 ){
15910:       bNoop = 1;
15911:       break;
15912:     }
15913:     if( sqlite3_stricmp(zType, "table")==0 ){
15914:       zName = (const char*)sqlite3_column_text(pStmt, 1);
15915:       zSql = (const char*)sqlite3_column_text(pStmt, 2);
15916:       pRet = recoverNewTable(pRc, zName, zSql, bIntkey, nCol);
15917:       break;
15918:     }
15919:   }
15920: 
15921:   shellFinalize(pRc, pStmt);
15922:   *pbNoop = bNoop;
15923:   return pRet;
15924: }
15925: 
15926: /*
15927: ** Return a RecoverTable object representing the orphans table.
15928: */
15929: static RecoverTable *recoverOrphanTable(
15930:   ShellState *pState,             /* Shell state object */
15931:   int *pRc,                       /* IN/OUT: Error code */
15932:   const char *zLostAndFound,      /* Base name for orphans table */
15933:   int nCol                        /* Number of user data columns */
15934: ){
15935:   RecoverTable *pTab = 0;
15936:   if( nCol>=0 && *pRc==SQLITE_OK ){
15937:     int i;
15938: 
15939:     /* This block determines the name of the orphan table. The prefered
15940:     ** name is zLostAndFound. But if that clashes with another name
15941:     ** in the recovered schema, try zLostAndFound_0, zLostAndFound_1
15942:     ** and so on until a non-clashing name is found.  */
15943:     int iTab = 0;
15944:     char *zTab = shellMPrintf(pRc, "%s", zLostAndFound);
15945:     sqlite3_stmt *pTest = 0;
15946:     shellPrepare(pState->db, pRc,
15947:         "SELECT 1 FROM recovery.schema WHERE name=?", &pTest
15948:     );
15949:     if( pTest ) sqlite3_bind_text(pTest, 1, zTab, -1, SQLITE_TRANSIENT);
15950:     while( *pRc==SQLITE_OK && SQLITE_ROW==sqlite3_step(pTest) ){
15951:       shellReset(pRc, pTest);
15952:       sqlite3_free(zTab);
15953:       zTab = shellMPrintf(pRc, "%s_%d", zLostAndFound, iTab++);
15954:       sqlite3_bind_text(pTest, 1, zTab, -1, SQLITE_TRANSIENT);
15955:     }
15956:     shellFinalize(pRc, pTest);
15957: 
15958:     pTab = (RecoverTable*)shellMalloc(pRc, sizeof(RecoverTable));
15959:     if( pTab ){
15960:       pTab->zQuoted = shellMPrintf(pRc, "\"%w\"", zTab);
15961:       pTab->nCol = nCol;
15962:       pTab->iPk = -2;
15963:       if( nCol>0 ){
15964:         pTab->azlCol = (char**)shellMalloc(pRc, sizeof(char*) * (nCol+1));
15965:         if( pTab->azlCol ){
15966:           pTab->azlCol[nCol] = shellMPrintf(pRc, "");
15967:           for(i=nCol-1; i>=0; i--){
15968:             pTab->azlCol[i] = shellMPrintf(pRc, "%s, NULL", pTab->azlCol[i+1]);
15969:           }
15970:         }
15971:       }
15972: 
15973:       if( *pRc!=SQLITE_OK ){
15974:         recoverFreeTable(pTab);
15975:         pTab = 0;
15976:       }else{
15977:         raw_printf(pState->out,
15978:             "CREATE TABLE %s(rootpgno INTEGER, "
15979:             "pgno INTEGER, nfield INTEGER, id INTEGER", pTab->zQuoted
15980:         );
15981:         for(i=0; i<nCol; i++){
15982:           raw_printf(pState->out, ", c%d", i);
15983:         }
15984:         raw_printf(pState->out, ");\n");
15985:       }
15986:     }
15987:     sqlite3_free(zTab);
15988:   }
15989:   return pTab;
15990: }
