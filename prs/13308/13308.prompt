10094: /*
10095: ** Move an sqlite_dbdata or sqlite_dbptr cursor to the next entry.
10096: */
10097: static int dbdataNext(sqlite3_vtab_cursor *pCursor){
10098:   DbdataCursor *pCsr = (DbdataCursor*)pCursor;
10099:   DbdataTable *pTab = (DbdataTable*)pCursor->pVtab;
10100: 
10101:   pCsr->iRowid++;
10102:   while( 1 ){
10103:     int rc;
10104:     int iOff = (pCsr->iPgno==1 ? 100 : 0);
10105:     int bNextPage = 0;
10106: 
10107:     if( pCsr->aPage==0 ){
10108:       while( 1 ){
10109:         if( pCsr->bOnePage==0 && pCsr->iPgno>pCsr->szDb ) return SQLITE_OK;
10110:         rc = dbdataLoadPage(pCsr, pCsr->iPgno, &pCsr->aPage, &pCsr->nPage);
10111:         if( rc!=SQLITE_OK ) return rc;
10112:         if( pCsr->aPage ) break;
10113:         pCsr->iPgno++;
10114:       }
10115:       pCsr->iCell = pTab->bPtr ? -2 : 0;
10116:       pCsr->nCell = get_uint16(&pCsr->aPage[iOff+3]);
10117:     }
10118: 
10119:     if( pTab->bPtr ){
10120:       if( pCsr->aPage[iOff]!=0x02 && pCsr->aPage[iOff]!=0x05 ){
10121:         pCsr->iCell = pCsr->nCell;
10122:       }
10123:       pCsr->iCell++;
10124:       if( pCsr->iCell>=pCsr->nCell ){
10125:         sqlite3_free(pCsr->aPage);
10126:         pCsr->aPage = 0;
10127:         if( pCsr->bOnePage ) return SQLITE_OK;
10128:         pCsr->iPgno++;
10129:       }else{
10130:         return SQLITE_OK;
10131:       }
10132:     }else{
10133:       /* If there is no record loaded, load it now. */
10134:       if( pCsr->pRec==0 ){
10135:         int bHasRowid = 0;
10136:         int nPointer = 0;
10137:         sqlite3_int64 nPayload = 0;
10138:         sqlite3_int64 nHdr = 0;
10139:         int iHdr;
10140:         int U, X;
10141:         int nLocal;
10142: 
10143:         switch( pCsr->aPage[iOff] ){
10144:           case 0x02:
10145:             nPointer = 4;
10146:             break;
10147:           case 0x0a:
10148:             break;
10149:           case 0x0d:
10150:             bHasRowid = 1;
10151:             break;
10152:           default:
10153:             /* This is not a b-tree page with records on it. Continue. */
10154:             pCsr->iCell = pCsr->nCell;
10155:             break;
10156:         }
10157: 
10158:         if( pCsr->iCell>=pCsr->nCell ){
10159:           bNextPage = 1;
10160:         }else{
10161: 
10162:           iOff += 8 + nPointer + pCsr->iCell*2;
10163:           if( iOff>pCsr->nPage ){
10164:             bNextPage = 1;
10165:           }else{
10166:             iOff = get_uint16(&pCsr->aPage[iOff]);
10167:           }
10168: 
10169:           /* For an interior node cell, skip past the child-page number */
10170:           iOff += nPointer;
10171: 
10172:           /* Load the "byte of payload including overflow" field */
10173:           if( bNextPage || iOff>pCsr->nPage ){
10174:             bNextPage = 1;
10175:           }else{
10176:             iOff += dbdataGetVarint(&pCsr->aPage[iOff], &nPayload);
10177:           }
10178: 
10179:           /* If this is a leaf intkey cell, load the rowid */
10180:           if( bHasRowid && !bNextPage && iOff<pCsr->nPage ){
10181:             iOff += dbdataGetVarint(&pCsr->aPage[iOff], &pCsr->iIntkey);
10182:           }
10183: 
10184:           /* Figure out how much data to read from the local page */
10185:           U = pCsr->nPage;
10186:           if( bHasRowid ){
10187:             X = U-35;
10188:           }else{
10189:             X = ((U-12)*64/255)-23;
10190:           }
10191:           if( nPayload<=X ){
10192:             nLocal = nPayload;
10193:           }else{
10194:             int M, K;
10195:             M = ((U-12)*32/255)-23;
10196:             K = M+((nPayload-M)%(U-4));
10197:             if( K<=X ){
10198:               nLocal = K;
10199:             }else{
10200:               nLocal = M;
10201:             }
10202:           }
10203: 
10204:           if( bNextPage || nLocal+iOff>pCsr->nPage ){
10205:             bNextPage = 1;
10206:           }else{
10207: 
10208:             /* Allocate space for payload. And a bit more to catch small buffer
10209:             ** overruns caused by attempting to read a varint or similar from
10210:             ** near the end of a corrupt record.  */
10211:             pCsr->pRec = (u8*)sqlite3_malloc64(nPayload+DBDATA_PADDING_BYTES);
10212:             if( pCsr->pRec==0 ) return SQLITE_NOMEM;
10213:             memset(pCsr->pRec, 0, nPayload+DBDATA_PADDING_BYTES);
10214:             pCsr->nRec = nPayload;
10215: 
10216:             /* Load the nLocal bytes of payload */
10217:             memcpy(pCsr->pRec, &pCsr->aPage[iOff], nLocal);
10218:             iOff += nLocal;
10219: 
10220:             /* Load content from overflow pages */
10221:             if( nPayload>nLocal ){
10222:               sqlite3_int64 nRem = nPayload - nLocal;
10223:               unsigned int pgnoOvfl = get_uint32(&pCsr->aPage[iOff]);
10224:               while( nRem>0 ){
10225:                 u8 *aOvfl = 0;
10226:                 int nOvfl = 0;
10227:                 int nCopy;
10228:                 rc = dbdataLoadPage(pCsr, pgnoOvfl, &aOvfl, &nOvfl);
10229:                 assert( rc!=SQLITE_OK || aOvfl==0 || nOvfl==pCsr->nPage );
10230:                 if( rc!=SQLITE_OK ) return rc;
10231:                 if( aOvfl==0 ) break;
10232: 
10233:                 nCopy = U-4;
10234:                 if( nCopy>nRem ) nCopy = nRem;
10235:                 memcpy(&pCsr->pRec[nPayload-nRem], &aOvfl[4], nCopy);
10236:                 nRem -= nCopy;
10237: 
10238:                 pgnoOvfl = get_uint32(aOvfl);
10239:                 sqlite3_free(aOvfl);
10240:               }
10241:             }
10242: 
10243:             iHdr = dbdataGetVarint(pCsr->pRec, &nHdr);
10244:             pCsr->nHdr = nHdr;
10245:             pCsr->pHdrPtr = &pCsr->pRec[iHdr];
10246:             pCsr->pPtr = &pCsr->pRec[pCsr->nHdr];
10247:             pCsr->iField = (bHasRowid ? -1 : 0);
10248:           }
10249:         }
10250:       }else{
10251:         pCsr->iField++;
10252:         if( pCsr->iField>0 ){
10253:           sqlite3_int64 iType;
10254:           if( pCsr->pHdrPtr>&pCsr->pRec[pCsr->nRec] ){
10255:             bNextPage = 1;
10256:           }else{
10257:             pCsr->pHdrPtr += dbdataGetVarint(pCsr->pHdrPtr, &iType);
10258:             pCsr->pPtr += dbdataValueBytes(iType);
10259:           }
10260:         }
10261:       }
10262: 
10263:       if( bNextPage ){
10264:         sqlite3_free(pCsr->aPage);
10265:         sqlite3_free(pCsr->pRec);
10266:         pCsr->aPage = 0;
10267:         pCsr->pRec = 0;
10268:         if( pCsr->bOnePage ) return SQLITE_OK;
10269:         pCsr->iPgno++;
10270:       }else{
10271:         if( pCsr->iField<0 || pCsr->pHdrPtr<&pCsr->pRec[pCsr->nHdr] ){
10272:           return SQLITE_OK;
10273:         }
10274: 
10275:         /* Advance to the next cell. The next iteration of the loop will load
10276:         ** the record and so on. */
10277:         sqlite3_free(pCsr->pRec);
10278:         pCsr->pRec = 0;
10279:         pCsr->iCell++;
10280:       }
10281:     }
10282:   }
10283: 
10284:   assert( !"can't get here" );
10285:   return SQLITE_OK;
10286: }
