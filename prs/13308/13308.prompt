11341: /* Render a single level of the graph that has iEqpId as its parent.  Called
11342: ** recursively to render sublevels.
11343: */
11344: static void eqp_render_level(ShellState *p, int iEqpId){
11345:   EQPGraphRow *pRow, *pNext;
11346:   int n = strlen30(p->sGraph.zPrefix);
11347:   char *z;
11348:   for(pRow = eqp_next_row(p, iEqpId, 0); pRow; pRow = pNext){
11349:     pNext = eqp_next_row(p, iEqpId, pRow);
11350:     z = pRow->zText;
11351:     utf8_printf(p->out, "%s%s%s\n", p->sGraph.zPrefix,
11352:                 pNext ? "|--" : "`--", z);
11353:     if( n<(int)sizeof(p->sGraph.zPrefix)-7 ){
11354:       memcpy(&p->sGraph.zPrefix[n], pNext ? "|  " : "   ", 4);
11355:       eqp_render_level(p, pRow->iEqpId);
11356:       p->sGraph.zPrefix[n] = 0;
11357:     }
11358:   }
11359: }
