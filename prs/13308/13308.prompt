982: /*
983: ** SQL function:  shell_add_schema(S,X)
984: **
985: ** Add the schema name X to the CREATE statement in S and return the result.
986: ** Examples:
987: **
988: **    CREATE TABLE t1(x)   ->   CREATE TABLE xyz.t1(x);
989: **
990: ** Also works on
991: **
992: **    CREATE INDEX
993: **    CREATE UNIQUE INDEX
994: **    CREATE VIEW
995: **    CREATE TRIGGER
996: **    CREATE VIRTUAL TABLE
997: **
998: ** This UDF is used by the .schema command to insert the schema name of
999: ** attached databases into the middle of the sqlite_schema.sql field.
1000: */
1001: static void shellAddSchemaName(
1002:   sqlite3_context *pCtx,
1003:   int nVal,
1004:   sqlite3_value **apVal
1005: ){
1006:   static const char *aPrefix[] = {
1007:      "TABLE",
1008:      "INDEX",
1009:      "UNIQUE INDEX",
1010:      "VIEW",
1011:      "TRIGGER",
1012:      "VIRTUAL TABLE"
1013:   };
1014:   int i = 0;
1015:   const char *zIn = (const char*)sqlite3_value_text(apVal[0]);
1016:   const char *zSchema = (const char*)sqlite3_value_text(apVal[1]);
1017:   const char *zName = (const char*)sqlite3_value_text(apVal[2]);
1018:   sqlite3 *db = sqlite3_context_db_handle(pCtx);
1019:   UNUSED_PARAMETER(nVal);
1020:   if( zIn!=0 && strncmp(zIn, "CREATE ", 7)==0 ){
1021:     for(i=0; i<(int)(sizeof(aPrefix)/sizeof(aPrefix[0])); i++){
1022:       int n = strlen30(aPrefix[i]);
1023:       if( strncmp(zIn+7, aPrefix[i], n)==0 && zIn[n+7]==' ' ){
1024:         char *z = 0;
1025:         char *zFake = 0;
1026:         if( zSchema ){
1027:           char cQuote = quoteChar(zSchema);
1028:           if( cQuote && sqlite3_stricmp(zSchema,"temp")!=0 ){
1029:             z = sqlite3_mprintf("%.*s \"%w\".%s", n+7, zIn, zSchema, zIn+n+8);
1030:           }else{
1031:             z = sqlite3_mprintf("%.*s %s.%s", n+7, zIn, zSchema, zIn+n+8);
1032:           }
1033:         }
1034:         if( zName
1035:          && aPrefix[i][0]=='V'
1036:          && (zFake = shellFakeSchema(db, zSchema, zName))!=0
1037:         ){
1038:           if( z==0 ){
1039:             z = sqlite3_mprintf("%s\n/* %s */", zIn, zFake);
1040:           }else{
1041:             z = sqlite3_mprintf("%z\n/* %s */", z, zFake);
1042:           }
1043:           free(zFake);
1044:         }
1045:         if( z ){
1046:           sqlite3_result_text(pCtx, z, -1, sqlite3_free);
1047:           return;
1048:         }
1049:       }
1050:     }
1051:   }
1052:   sqlite3_result_value(pCtx, apVal[0]);
1053: }
