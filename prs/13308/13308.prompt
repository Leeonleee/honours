15811:     if( bIntkey==bSqlIntkey ){
15812:       int i;
15813:       const char *zPk = "_rowid_";
15814:       sqlite3_stmt *pPkFinder = 0;
15815: 
15816:       /* If this is an intkey table and there is an INTEGER PRIMARY KEY,
15817:       ** set zPk to the name of the PK column, and pTab->iPk to the index
15818:       ** of the column, where columns are 0-numbered from left to right.
15819:       ** Or, if this is a WITHOUT ROWID table or if there is no IPK column,
15820:       ** leave zPk as "_rowid_" and pTab->iPk at -2.  */
15821:       pTab->iPk = -2;
15822:       if( bIntkey ){
15823:         shellPreparePrintf(dbtmp, &rc, &pPkFinder,
15824:           "SELECT cid, name FROM pragma_table_info(%Q) "
15825:           "  WHERE pk=1 AND type='integer' COLLATE nocase"
15826:           "  AND NOT EXISTS (SELECT cid FROM pragma_table_info(%Q) WHERE pk=2)"
15827:           , zName, zName
15828:         );
15829:         if( rc==SQLITE_OK && SQLITE_ROW==sqlite3_step(pPkFinder) ){
15830:           pTab->iPk = sqlite3_column_int(pPkFinder, 0);
15831:           zPk = (const char*)sqlite3_column_text(pPkFinder, 1);
15832:         }
15833:       }
15834: 
15835:       pTab->zQuoted = shellMPrintf(&rc, "\"%w\"", zName);
15836:       pTab->azlCol = (char**)shellMalloc(&rc, sizeof(char*) * (nSqlCol+1));
15837:       pTab->nCol = nSqlCol;
15838: 
15839:       if( bIntkey ){
15840:         pTab->azlCol[0] = shellMPrintf(&rc, "\"%w\"", zPk);
15841:       }else{
15842:         pTab->azlCol[0] = shellMPrintf(&rc, "");
15843:       }
15844:       i = 1;
15845:       shellPreparePrintf(dbtmp, &rc, &pStmt,
15846:           "SELECT %Q || group_concat(shell_idquote(name), ', ') "
15847:           "  FILTER (WHERE cid!=%d) OVER (ORDER BY %s cid) "
15848:           "FROM pragma_table_info(%Q)",
15849:           bIntkey ? ", " : "", pTab->iPk,
15850:           bIntkey ? "" : "(CASE WHEN pk=0 THEN 1000000 ELSE pk END), ",
15851:           zName
15852:       );
15853:       while( rc==SQLITE_OK && SQLITE_ROW==sqlite3_step(pStmt) ){
15854:         const char *zText = (const char*)sqlite3_column_text(pStmt, 0);
15855:         pTab->azlCol[i] = shellMPrintf(&rc, "%s%s", pTab->azlCol[0], zText);
15856:         i++;
15857:       }
15858:       shellFinalize(&rc, pStmt);
15859: 
15860:       shellFinalize(&rc, pPkFinder);
15861:     }
15862:   }
15863: 
15864:  finished:
15865:   sqlite3_close(dbtmp);
15866:   *pRc = rc;
15867:   if( rc!=SQLITE_OK || (pTab && pTab->zQuoted==0) ){
15868:     recoverFreeTable(pTab);
15869:     pTab = 0;
15870:   }
15871:   return pTab;
15872: }
