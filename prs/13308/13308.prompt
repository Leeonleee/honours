3910:   sqlite3_file *pFile,
3911:   int flags,
3912:   int *pOutFlags
3913: ){
3914:   ApndFile *p;
3915:   sqlite3_file *pSubFile;
3916:   sqlite3_vfs *pSubVfs;
3917:   int rc;
3918:   sqlite3_int64 sz;
3919:   pSubVfs = ORIGVFS(pVfs);
3920:   if( (flags & SQLITE_OPEN_MAIN_DB)==0 ){
3921:     return pSubVfs->xOpen(pSubVfs, zName, pFile, flags, pOutFlags);
3922:   }
3923:   p = (ApndFile*)pFile;
3924:   memset(p, 0, sizeof(*p));
3925:   pSubFile = ORIGFILE(pFile);
3926:   pFile->pMethods = &apnd_io_methods;
3927:   rc = pSubVfs->xOpen(pSubVfs, zName, pSubFile, flags, pOutFlags);
3928:   if( rc ) goto apnd_open_done;
3929:   rc = pSubFile->pMethods->xFileSize(pSubFile, &sz);
3930:   if( rc ){
3931:     pSubFile->pMethods->xClose(pSubFile);
3932:     goto apnd_open_done;
3933:   }
3934:   if( apndIsOrdinaryDatabaseFile(sz, pSubFile) ){
3935:     memmove(pFile, pSubFile, pSubVfs->szOsFile);
3936:     return SQLITE_OK;
3937:   }
3938:   p->iMark = 0;
3939:   p->iPgOne = apndReadMark(sz, pFile);
3940:   if( p->iPgOne>0 ){
3941:     return SQLITE_OK;
3942:   }
3943:   if( (flags & SQLITE_OPEN_CREATE)==0 ){
3944:     pSubFile->pMethods->xClose(pSubFile);
3945:     rc = SQLITE_CANTOPEN;
3946:   }
3947:   p->iPgOne = (sz+0xfff) & ~(sqlite3_int64)0xfff;
3948: apnd_open_done:
3949:   if( rc ) pFile->pMethods = 0;
3950:   return rc;
3951: }
