5799: ** Decode the LFH record in buffer aBuf into (*pLFH). Return SQLITE_ERROR
5800: ** if the record is not well-formed, or SQLITE_OK otherwise.
5801: */
5802: static int zipfileReadLFH(
5803:   u8 *aBuffer,
5804:   ZipfileLFH *pLFH
5805: ){
5806:   u8 *aRead = aBuffer;
5807:   int rc = SQLITE_OK;
5808: 
5809:   u32 sig = zipfileRead32(aRead);
5810:   if( sig!=ZIPFILE_SIGNATURE_LFH ){
5811:     rc = SQLITE_ERROR;
5812:   }else{
5813:     pLFH->iVersionExtract = zipfileRead16(aRead);
5814:     pLFH->flags = zipfileRead16(aRead);
5815:     pLFH->iCompression = zipfileRead16(aRead);
5816:     pLFH->mTime = zipfileRead16(aRead);
5817:     pLFH->mDate = zipfileRead16(aRead);
5818:     pLFH->crc32 = zipfileRead32(aRead);
5819:     pLFH->szCompressed = zipfileRead32(aRead);
5820:     pLFH->szUncompressed = zipfileRead32(aRead);
5821:     pLFH->nFile = zipfileRead16(aRead);
5822:     pLFH->nExtra = zipfileRead16(aRead);
5823:   }
5824:   return rc;
