15312: ** Implementation of .ar "eXtract" command.
15313: */
15314: static int arExtractCommand(ArCommand *pAr){
15315:   const char *zSql1 =
15316:     "SELECT "
15317:     " ($dir || name),"
15318:     " writefile(($dir || name), %s, mode, mtime) "
15319:     "FROM %s WHERE (%s) AND (data IS NULL OR $dirOnly = 0)"
15320:     " AND name NOT GLOB '*..[/\\]*'";
15321: 
15322:   const char *azExtraArg[] = {
15323:     "sqlar_uncompress(data, sz)",
15324:     "data"
15325:   };
15326: 
15327:   sqlite3_stmt *pSql = 0;
15328:   int rc = SQLITE_OK;
15329:   char *zDir = 0;
15330:   char *zWhere = 0;
15331:   int i, j;
15332: 
15333:   /* If arguments are specified, check that they actually exist within
15334:   ** the archive before proceeding. And formulate a WHERE clause to
15335:   ** match them.  */
15336:   rc = arCheckEntries(pAr);
15337:   arWhereClause(&rc, pAr, &zWhere);
15338: 
15339:   if( rc==SQLITE_OK ){
15340:     if( pAr->zDir ){
15341:       zDir = sqlite3_mprintf("%s/", pAr->zDir);
15342:     }else{
15343:       zDir = sqlite3_mprintf("");
15344:     }
15345:     if( zDir==0 ) rc = SQLITE_NOMEM;
15346:   }
15347: 
15348:   shellPreparePrintf(pAr->db, &rc, &pSql, zSql1,
15349:       azExtraArg[pAr->bZip], pAr->zSrcTable, zWhere
15350:   );
15351: 
15352:   if( rc==SQLITE_OK ){
15353:     j = sqlite3_bind_parameter_index(pSql, "$dir");
15354:     sqlite3_bind_text(pSql, j, zDir, -1, SQLITE_STATIC);
15355: 
15356:     /* Run the SELECT statement twice. The first time, writefile() is called
15357:     ** for all archive members that should be extracted. The second time,
15358:     ** only for the directories. This is because the timestamps for
15359:     ** extracted directories must be reset after they are populated (as
15360:     ** populating them changes the timestamp).  */
15361:     for(i=0; i<2; i++){
15362:       j = sqlite3_bind_parameter_index(pSql, "$dirOnly");
15363:       sqlite3_bind_int(pSql, j, i);
15364:       if( pAr->bDryRun ){
15365:         utf8_printf(pAr->p->out, "%s\n", sqlite3_sql(pSql));
15366:       }else{
15367:         while( rc==SQLITE_OK && SQLITE_ROW==sqlite3_step(pSql) ){
15368:           if( i==0 && pAr->bVerbose ){
15369:             utf8_printf(pAr->p->out, "%s\n", sqlite3_column_text(pSql, 0));
15370:           }
15371:         }
15372:       }
15373:       shellReset(&rc, pSql);
15374:     }
15375:     shellFinalize(&rc, pSql);
15376:   }
15377: 
15378:   sqlite3_free(zDir);
15379:   sqlite3_free(zWhere);
15380:   return rc;
15381: }
