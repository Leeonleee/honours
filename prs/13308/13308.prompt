9822: /*
9823: ** This function interprets two types of constraints:
9824: **
9825: **       schema=?
9826: **       pgno=?
9827: **
9828: ** If neither are present, idxNum is set to 0. If schema=? is present,
9829: ** the 0x01 bit in idxNum is set. If pgno=? is present, the 0x02 bit
9830: ** in idxNum is set.
9831: **
9832: ** If both parameters are present, schema is in position 0 and pgno in
9833: ** position 1.
9834: */
9835: static int dbdataBestIndex(sqlite3_vtab *tab, sqlite3_index_info *pIdx){
9836:   DbdataTable *pTab = (DbdataTable*)tab;
9837:   int i;
9838:   int iSchema = -1;
9839:   int iPgno = -1;
9840:   int colSchema = (pTab->bPtr ? DBPTR_COLUMN_SCHEMA : DBDATA_COLUMN_SCHEMA);
9841: 
9842:   for(i=0; i<pIdx->nConstraint; i++){
9843:     struct sqlite3_index_constraint *p = &pIdx->aConstraint[i];
9844:     if( p->op==SQLITE_INDEX_CONSTRAINT_EQ ){
9845:       if( p->iColumn==colSchema ){
9846:         if( p->usable==0 ) return SQLITE_CONSTRAINT;
9847:         iSchema = i;
9848:       }
9849:       if( p->iColumn==DBDATA_COLUMN_PGNO && p->usable ){
9850:         iPgno = i;
9851:       }
9852:     }
9853:   }
9854: 
9855:   if( iSchema>=0 ){
9856:     pIdx->aConstraintUsage[iSchema].argvIndex = 1;
9857:     pIdx->aConstraintUsage[iSchema].omit = 1;
9858:   }
9859:   if( iPgno>=0 ){
9860:     pIdx->aConstraintUsage[iPgno].argvIndex = 1 + (iSchema>=0);
9861:     pIdx->aConstraintUsage[iPgno].omit = 1;
9862:     pIdx->estimatedCost = 100;
9863:     pIdx->estimatedRows =  50;
9864: 
9865:     if( pTab->bPtr==0 && pIdx->nOrderBy && pIdx->aOrderBy[0].desc==0 ){
9866:       int iCol = pIdx->aOrderBy[0].iColumn;
9867:       if( pIdx->nOrderBy==1 ){
9868:         pIdx->orderByConsumed = (iCol==0 || iCol==1);
9869:       }else if( pIdx->nOrderBy==2 && pIdx->aOrderBy[1].desc==0 && iCol==0 ){
9870:         pIdx->orderByConsumed = (pIdx->aOrderBy[1].iColumn==1);
9871:       }
9872:     }
9873: 
9874:   }else{
9875:     pIdx->estimatedCost = 100000000;
9876:     pIdx->estimatedRows = 1000000000;
9877:   }
9878:   pIdx->idxNum = (iSchema>=0 ? 0x01 : 0x00) | (iPgno>=0 ? 0x02 : 0x00);
9879:   return SQLITE_OK;
9880: }
