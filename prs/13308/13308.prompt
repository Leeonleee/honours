1887: /*
1888: ** After all content has been added, invoke SHA3Final() to compute
1889: ** the final hash.  The function returns a pointer to the binary
1890: ** hash value.
1891: */
1892: static unsigned char *SHA3Final(SHA3Context *p){
1893:   unsigned int i;
1894:   if( p->nLoaded==p->nRate-1 ){
1895:     const unsigned char c1 = 0x86;
1896:     SHA3Update(p, &c1, 1);
1897:   }else{
1898:     const unsigned char c2 = 0x06;
1899:     const unsigned char c3 = 0x80;
1900:     SHA3Update(p, &c2, 1);
1901:     p->nLoaded = p->nRate - 1;
1902:     SHA3Update(p, &c3, 1);
1903:   }
1904:   for(i=0; i<p->nRate; i++){
1905:     p->u.x[i+p->nRate] = p->u.x[i^p->ixMask];
1906:   }
1907:   return &p->u.x[p->nRate];
1908: }
