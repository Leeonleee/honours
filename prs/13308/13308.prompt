9053: static void idxSampleFunc(
9054:   sqlite3_context *pCtx,
9055:   int argc,
9056:   sqlite3_value **argv
9057: ){
9058:   struct IdxSampleCtx *p = (struct IdxSampleCtx*)sqlite3_user_data(pCtx);
9059:   int bRet;
9060: 
9061:   (void)argv;
9062:   assert( argc==0 );
9063:   if( p->nRow==0.0 ){
9064:     bRet = 1;
9065:   }else{
9066:     bRet = (p->nRet / p->nRow) <= p->target;
9067:     if( bRet==0 ){
9068:       unsigned short rnd;
9069:       sqlite3_randomness(2, (void*)&rnd);
9070:       bRet = ((int)rnd % 100) <= p->iTarget;
9071:     }
9072:   }
9073: 
9074:   sqlite3_result_int(pCtx, bRet);
9075:   p->nRow += 1.0;
9076:   p->nRet += (double)bRet;
9077: }
