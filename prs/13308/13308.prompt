6965: static int zipfileCommit(sqlite3_vtab *pVtab){
6966:   ZipfileTab *pTab = (ZipfileTab*)pVtab;
6967:   int rc = SQLITE_OK;
6968:   if( pTab->pWriteFd ){
6969:     i64 iOffset = pTab->szCurrent;
6970:     ZipfileEntry *p;
6971:     ZipfileEOCD eocd;
6972:     int nEntry = 0;
6973: 
6974:     /* Write out all entries */
6975:     for(p=pTab->pFirstEntry; rc==SQLITE_OK && p; p=p->pNext){
6976:       int n = zipfileSerializeCDS(p, pTab->aBuffer);
6977:       rc = zipfileAppendData(pTab, pTab->aBuffer, n);
6978:       nEntry++;
6979:     }
6980: 
6981:     /* Write out the EOCD record */
6982:     eocd.iDisk = 0;
6983:     eocd.iFirstDisk = 0;
6984:     eocd.nEntry = (u16)nEntry;
6985:     eocd.nEntryTotal = (u16)nEntry;
6986:     eocd.nSize = (u32)(pTab->szCurrent - iOffset);
6987:     eocd.iOffset = (u32)iOffset;
6988:     rc = zipfileAppendEOCD(pTab, &eocd);
6989: 
6990:     zipfileCleanupTransaction(pTab);
6991:   }
6992:   return rc;
6993: }
