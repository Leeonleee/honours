6295: /*
6296: ** If aBlob is not NULL, then it points to a buffer nBlob bytes in size
6297: ** containing an entire zip archive image. Or, if aBlob is NULL, then pFile
6298: ** is guaranteed to be a file-handle open on a zip file.
6299: **
6300: ** This function attempts to locate the EOCD record within the zip archive
6301: ** and populate *pEOCD with the results of decoding it. SQLITE_OK is
6302: ** returned if successful. Otherwise, an SQLite error code is returned and
6303: ** an English language error message may be left in virtual-table pTab.
6304: */
6305: static int zipfileReadEOCD(
6306:   ZipfileTab *pTab,               /* Return errors here */
6307:   const u8 *aBlob,                /* Pointer to in-memory file image */
6308:   int nBlob,                      /* Size of aBlob[] in bytes */
6309:   FILE *pFile,                    /* Read from this file if aBlob==0 */
6310:   ZipfileEOCD *pEOCD              /* Object to populate */
6311: ){
6312:   u8 *aRead = pTab->aBuffer;      /* Temporary buffer */
6313:   int nRead;                      /* Bytes to read from file */
6314:   int rc = SQLITE_OK;
6315: 
6316:   if( aBlob==0 ){
6317:     i64 iOff;                     /* Offset to read from */
6318:     i64 szFile;                   /* Total size of file in bytes */
6319:     fseek(pFile, 0, SEEK_END);
6320:     szFile = (i64)ftell(pFile);
6321:     if( szFile==0 ){
6322:       memset(pEOCD, 0, sizeof(ZipfileEOCD));
6323:       return SQLITE_OK;
6324:     }
6325:     nRead = (int)(MIN(szFile, ZIPFILE_BUFFER_SIZE));
6326:     iOff = szFile - nRead;
6327:     rc = zipfileReadData(pFile, aRead, nRead, iOff, &pTab->base.zErrMsg);
6328:   }else{
6329:     nRead = (int)(MIN(nBlob, ZIPFILE_BUFFER_SIZE));
6330:     aRead = (u8*)&aBlob[nBlob-nRead];
6331:   }
6332: 
6333:   if( rc==SQLITE_OK ){
6334:     int i;
6335: 
6336:     /* Scan backwards looking for the signature bytes */
6337:     for(i=nRead-20; i>=0; i--){
6338:       if( aRead[i]==0x50 && aRead[i+1]==0x4b
6339:        && aRead[i+2]==0x05 && aRead[i+3]==0x06
6340:       ){
6341:         break;
6342:       }
6343:     }
6344:     if( i<0 ){
6345:       pTab->base.zErrMsg = sqlite3_mprintf(
6346:           "cannot find end of central directory record"
6347:       );
6348:       return SQLITE_ERROR;
6349:     }
6350: 
6351:     aRead += i+4;
6352:     pEOCD->iDisk = zipfileRead16(aRead);
6353:     pEOCD->iFirstDisk = zipfileRead16(aRead);
6354:     pEOCD->nEntry = zipfileRead16(aRead);
6355:     pEOCD->nEntryTotal = zipfileRead16(aRead);
6356:     pEOCD->nSize = zipfileRead32(aRead);
6357:     pEOCD->iOffset = zipfileRead32(aRead);
6358:   }
6359: 
6360:   return rc;
6361: }
