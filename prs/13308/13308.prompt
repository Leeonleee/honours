14026: /* Flags for open_db().
14027: **
14028: ** The default behavior of open_db() is to exit(1) if the database fails to
14029: ** open.  The OPEN_DB_KEEPALIVE flag changes that so that it prints an error
14030: ** but still returns without calling exit.
14031: **
14032: ** The OPEN_DB_ZIPFILE flag causes open_db() to prefer to open files as a
14033: ** ZIP archive if the file does not exist or is empty and its name matches
14034: ** the *.zip pattern.
14035: */
14036: #define OPEN_DB_KEEPALIVE   0x001   /* Return after error if true */
14037: #define OPEN_DB_ZIPFILE     0x002   /* Open as ZIP if name matches *.zip */
14038: /*
14039: ** Make sure the database is open.  If it is not, then open it.  If
14040: ** the database fails to open, print an error message and exit.
14041: */
14042: static void open_db(ShellState *p, int openFlags){
14043:   if( p->db==0 ){
14044:     if( p->openMode==SHELL_OPEN_UNSPEC ){
14045:       if( p->zDbFilename==0 || p->zDbFilename[0]==0 ){
14046:         p->openMode = SHELL_OPEN_NORMAL;
14047:       }else{
14048:         p->openMode = (u8)deduceDatabaseType(p->zDbFilename,
14049:                              (openFlags & OPEN_DB_ZIPFILE)!=0);
14050:       }
14051:     }
14052:     switch( p->openMode ){
14053:       case SHELL_OPEN_APPENDVFS: {
14054:         sqlite3_open_v2(p->zDbFilename, &p->db,
14055:            SQLITE_OPEN_READWRITE|SQLITE_OPEN_CREATE|p->openFlags, "apndvfs");
14056:         break;
14057:       }
14058:       case SHELL_OPEN_HEXDB:
14059:       case SHELL_OPEN_DESERIALIZE: {
14060:         sqlite3_open(0, &p->db);
14061:         break;
14062:       }
14063:       case SHELL_OPEN_ZIPFILE: {
14064:         sqlite3_open(":memory:", &p->db);
14065:         break;
14066:       }
14067:       case SHELL_OPEN_READONLY: {
14068:         sqlite3_open_v2(p->zDbFilename, &p->db,
14069:             SQLITE_OPEN_READONLY|p->openFlags, 0);
14070:         break;
14071:       }
14072:       case SHELL_OPEN_UNSPEC:
14073:       case SHELL_OPEN_NORMAL: {
14074:         sqlite3_open_v2(p->zDbFilename, &p->db,
14075:            SQLITE_OPEN_READWRITE|SQLITE_OPEN_CREATE|p->openFlags, 0);
14076:         break;
14077:       }
14078:     }
14079:     globalDb = p->db;
14080:     if( p->db==0 || SQLITE_OK!=sqlite3_errcode(p->db) ){
14081:       utf8_printf(stderr,"Error: unable to open database \"%s\": %s\n",
14082:           p->zDbFilename, sqlite3_errmsg(p->db));
14083:       if( openFlags & OPEN_DB_KEEPALIVE ){
14084:         sqlite3_open(":memory:", &p->db);
14085:         return;
14086:       }
14087:       exit(1);
14088:     }
14089: #ifndef SQLITE_OMIT_LOAD_EXTENSION
14090:     sqlite3_enable_load_extension(p->db, 1);
14091: #endif
14092:     sqlite3_fileio_init(p->db, 0, 0);
14093:     sqlite3_shathree_init(p->db, 0, 0);
14094:     sqlite3_completion_init(p->db, 0, 0);
14095: #if !defined(SQLITE_OMIT_VIRTUALTABLE) && defined(SQLITE_ENABLE_DBPAGE_VTAB)
14096:     sqlite3_dbdata_init(p->db, 0, 0);
14097: #endif
14098: #ifdef SQLITE_HAVE_ZLIB
14099:     sqlite3_zipfile_init(p->db, 0, 0);
14100:     sqlite3_sqlar_init(p->db, 0, 0);
14101: #endif
14102:     sqlite3_create_function(p->db, "shell_add_schema", 3, SQLITE_UTF8, 0,
14103:                             shellAddSchemaName, 0, 0);
14104:     sqlite3_create_function(p->db, "shell_module_schema", 1, SQLITE_UTF8, 0,
14105:                             shellModuleSchema, 0, 0);
14106:     sqlite3_create_function(p->db, "shell_putsnl", 1, SQLITE_UTF8, p,
14107:                             shellPutsFunc, 0, 0);
14108:     sqlite3_create_function(p->db, "shell_escape_crnl", 1, SQLITE_UTF8, 0,
14109:                             shellEscapeCrnl, 0, 0);
14110:     sqlite3_create_function(p->db, "shell_idquote", 1, SQLITE_UTF8, 0,
14111:                             shellIdQuote, 0, 0);
14112: #ifndef SQLITE_NOHAVE_SYSTEM
14113:     sqlite3_create_function(p->db, "edit", 1, SQLITE_UTF8, 0,
14114:                             editFunc, 0, 0);
