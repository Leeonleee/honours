11361: /*
11362: ** Display and reset the EXPLAIN QUERY PLAN data
11363: */
11364: static void eqp_render(ShellState *p){
11365:   EQPGraphRow *pRow = p->sGraph.pRow;
11366:   if( pRow ){
11367:     if( pRow->zText[0]=='-' ){
11368:       if( pRow->pNext==0 ){
11369:         eqp_reset(p);
11370:         return;
11371:       }
11372:       utf8_printf(p->out, "%s\n", pRow->zText+3);
11373:       p->sGraph.pRow = pRow->pNext;
11374:       sqlite3_free(pRow);
11375:     }else{
11376:       utf8_printf(p->out, "QUERY PLAN\n");
11377:     }
11378:     p->sGraph.zPrefix[0] = 0;
11379:     eqp_render_level(p, 0);
11380:     eqp_reset(p);
11381:   }
