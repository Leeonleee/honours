14619: /*
14620: ** Change the output file back to stdout.
14621: **
14622: ** If the p->doXdgOpen flag is set, that means the output was being
14623: ** redirected to a temporary file named by p->zTempFile.  In that case,
14624: ** launch start/open/xdg-open on that temporary file.
14625: */
14626: static void output_reset(ShellState *p){
14627:   if( p->outfile[0]=='|' ){
14628: #ifndef SQLITE_OMIT_POPEN
14629:     pclose(p->out);
14630: #endif
14631:   }else{
14632:     output_file_close(p->out);
14633: #ifndef SQLITE_NOHAVE_SYSTEM
14634:     if( p->doXdgOpen ){
14635:       const char *zXdgOpenCmd =
14636: #if defined(_WIN32)
14637:       "start";
14638: #elif defined(__APPLE__)
14639:       "open";
14640: #else
14641:       "xdg-open";
14642: #endif
14643:       char *zCmd;
14644:       zCmd = sqlite3_mprintf("%s %s", zXdgOpenCmd, p->zTempFile);
14645:       if( system(zCmd) ){
14646:         utf8_printf(stderr, "Failed: [%s]\n", zCmd);
14647:       }else{
14648:         /* Give the start/open/xdg-open command some time to get
14649:         ** going before we continue, and potential delete the
14650:         ** p->zTempFile data file out from under it */
14651:         sqlite3_sleep(2000);
14652:       }
14653:       sqlite3_free(zCmd);
14654:       outputModePop(p);
14655:       p->doXdgOpen = 0;
14656:     }
14657: #endif /* !defined(SQLITE_NOHAVE_SYSTEM) */
14658:   }
14659:   p->outfile[0] = 0;
14660:   p->out = stdout;
14661: }
