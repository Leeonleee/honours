6623: static int zipfileBegin(sqlite3_vtab *pVtab){
6624:   ZipfileTab *pTab = (ZipfileTab*)pVtab;
6625:   int rc = SQLITE_OK;
6626: 
6627:   assert( pTab->pWriteFd==0 );
6628:   if( pTab->zFile==0 || pTab->zFile[0]==0 ){
6629:     pTab->base.zErrMsg = sqlite3_mprintf("zipfile: missing filename");
6630:     return SQLITE_ERROR;
6631:   }
6632: 
6633:   /* Open a write fd on the file. Also load the entire central directory
6634:   ** structure into memory. During the transaction any new file data is
6635:   ** appended to the archive file, but the central directory is accumulated
6636:   ** in main-memory until the transaction is committed.  */
6637:   pTab->pWriteFd = fopen(pTab->zFile, "ab+");
6638:   if( pTab->pWriteFd==0 ){
6639:     pTab->base.zErrMsg = sqlite3_mprintf(
6640:         "zipfile: failed to open file %s for writing", pTab->zFile
6641:         );
6642:     rc = SQLITE_ERROR;
6643:   }else{
6644:     fseek(pTab->pWriteFd, 0, SEEK_END);
6645:     pTab->szCurrent = pTab->szOrig = (i64)ftell(pTab->pWriteFd);
6646:     rc = zipfileLoadDirectory(pTab, 0, 0);
6647:   }
6648: 
6649:   if( rc!=SQLITE_OK ){
6650:     zipfileCleanupTransaction(pTab);
6651:   }
6652: 
6653:   return rc;
6654: }
