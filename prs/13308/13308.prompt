13121: /*
13122: ** Release memory previously allocated by tableColumnList().
13123: */
13124: static void freeColumnList(char **azCol){
13125:   int i;
13126:   for(i=1; azCol[i]; i++){
13127:     sqlite3_free(azCol[i]);
13128:   }
13129:   /* azCol[0] is a static string */
13130:   sqlite3_free(azCol);
13131: }
13132: 
13133: /*
13134: ** Return a list of pointers to strings which are the names of all
13135: ** columns in table zTab.   The memory to hold the names is dynamically
13136: ** allocated and must be released by the caller using a subsequent call
13137: ** to freeColumnList().
13138: **
13139: ** The azCol[0] entry is usually NULL.  However, if zTab contains a rowid
13140: ** value that needs to be preserved, then azCol[0] is filled in with the
13141: ** name of the rowid column.
13142: **
13143: ** The first regular column in the table is azCol[1].  The list is terminated
13144: ** by an entry with azCol[i]==0.
13145: */
13146: static char **tableColumnList(ShellState *p, const char *zTab){
13147:   char **azCol = 0;
13148:   sqlite3_stmt *pStmt;
13149:   char *zSql;
13150:   int nCol = 0;
13151:   int nAlloc = 0;
13152:   int nPK = 0;       /* Number of PRIMARY KEY columns seen */
13153:   int isIPK = 0;     /* True if one PRIMARY KEY column of type INTEGER */
13154:   int preserveRowid = ShellHasFlag(p, SHFLG_PreserveRowid);
13155:   int rc;
13156: 
13157:   zSql = sqlite3_mprintf("PRAGMA table_info=%Q", zTab);
13158:   rc = sqlite3_prepare_v2(p->db, zSql, -1, &pStmt, 0);
13159:   sqlite3_free(zSql);
13160:   if( rc ) return 0;
13161:   while( sqlite3_step(pStmt)==SQLITE_ROW ){
13162:     if( nCol>=nAlloc-2 ){
13163:       nAlloc = nAlloc*2 + nCol + 10;
13164:       azCol = sqlite3_realloc(azCol, nAlloc*sizeof(azCol[0]));
13165:       if( azCol==0 ) shell_out_of_memory();
13166:     }
13167:     azCol[++nCol] = sqlite3_mprintf("%s", sqlite3_column_text(pStmt, 1));
13168:     if( sqlite3_column_int(pStmt, 5) ){
13169:       nPK++;
13170:       if( nPK==1
13171:        && sqlite3_stricmp((const char*)sqlite3_column_text(pStmt,2),
13172:                           "INTEGER")==0
13173:       ){
13174:         isIPK = 1;
13175:       }else{
13176:         isIPK = 0;
13177:       }
13178:     }
13179:   }
13180:   sqlite3_finalize(pStmt);
13181:   if( azCol==0 ) return 0;
13182:   azCol[0] = 0;
13183:   azCol[nCol+1] = 0;
13184: 
13185:   /* The decision of whether or not a rowid really needs to be preserved
13186:   ** is tricky.  We never need to preserve a rowid for a WITHOUT ROWID table
13187:   ** or a table with an INTEGER PRIMARY KEY.  We are unable to preserve
13188:   ** rowids on tables where the rowid is inaccessible because there are other
13189:   ** columns in the table named "rowid", "_rowid_", and "oid".
13190:   */
13191:   if( preserveRowid && isIPK ){
13192:     /* If a single PRIMARY KEY column with type INTEGER was seen, then it
13193:     ** might be an alise for the ROWID.  But it might also be a WITHOUT ROWID
13194:     ** table or a INTEGER PRIMARY KEY DESC column, neither of which are
13195:     ** ROWID aliases.  To distinguish these cases, check to see if
13196:     ** there is a "pk" entry in "PRAGMA index_list".  There will be
13197:     ** no "pk" index if the PRIMARY KEY really is an alias for the ROWID.
13198:     */
13199:     zSql = sqlite3_mprintf("SELECT 1 FROM pragma_index_list(%Q)"
13200:                            " WHERE origin='pk'", zTab);
13201:     rc = sqlite3_prepare_v2(p->db, zSql, -1, &pStmt, 0);
13202:     sqlite3_free(zSql);
13203:     if( rc ){
13204:       freeColumnList(azCol);
13205:       return 0;
13206:     }
13207:     rc = sqlite3_step(pStmt);
13208:     sqlite3_finalize(pStmt);
13209:     preserveRowid = rc==SQLITE_ROW;
13210:   }
13211:   if( preserveRowid ){
13212:     /* Only preserve the rowid if we can find a name to use for the
13213:     ** rowid */
13214:     static char *azRowid[] = { "rowid", "_rowid_", "oid" };
13215:     int i, j;
13216:     for(j=0; j<3; j++){
13217:       for(i=1; i<=nCol; i++){
13218:         if( sqlite3_stricmp(azRowid[j],azCol[i])==0 ) break;
13219:       }
13220:       if( i>nCol ){
13221:         /* At this point, we know that azRowid[j] is not the name of any
13222:         ** ordinary column in the table.  Verify that azRowid[j] is a valid
13223:         ** name for the rowid before adding it to azCol[0].  WITHOUT ROWID
13224:         ** tables will fail this last check */
13225:         rc = sqlite3_table_column_metadata(p->db,0,zTab,azRowid[j],0,0,0,0,0);
13226:         if( rc==SQLITE_OK ) azCol[0] = azRowid[j];
13227:         break;
13228:       }
13229:     }
13230:   }
13231:   return azCol;
13232: }
