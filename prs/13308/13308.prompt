1246: /*
1247: ** Implementation of the POSIX getenv() function using the Win32 API.
1248: ** This function is not thread-safe.
1249: */
1250: const char *windirent_getenv(
1251:   const char *name
1252: ){
1253:   static char value[32768]; /* Maximum length, per MSDN */
1254:   DWORD dwSize = sizeof(value) / sizeof(char); /* Size in chars */
1255:   DWORD dwRet; /* Value returned by GetEnvironmentVariableA() */
1256: 
1257:   memset(value, 0, sizeof(value));
1258:   dwRet = GetEnvironmentVariableA(name, value, dwSize);
1259:   if( dwRet==0 || dwRet>dwSize ){
1260:     /*
1261:     ** The function call to GetEnvironmentVariableA() failed -OR-
1262:     ** the buffer is not large enough.  Either way, return NULL.
1263:     */
1264:     return 0;
1265:   }else{
1266:     /*
1267:     ** The function call to GetEnvironmentVariableA() succeeded
1268:     ** -AND- the buffer contains the entire value.
1269:     */
1270:     return value;
1271:   }
1272: }
