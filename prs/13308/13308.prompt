11925: /*
11926: ** Execute a query statement that will generate SQL output.  Print
11927: ** the result columns, comma-separated, on a line and then add a
11928: ** semicolon terminator to the end of that line.
11929: **
11930: ** If the number of columns is 1 and that column contains text "--"
11931: ** then write the semicolon on a separate line.  That way, if a
11932: ** "--" comment occurs at the end of the statement, the comment
11933: ** won't consume the semicolon terminator.
11934: */
11935: static int run_table_dump_query(
11936:   ShellState *p,           /* Query context */
11937:   const char *zSelect      /* SELECT statement to extract content */
11938: ){
11939:   sqlite3_stmt *pSelect;
11940:   int rc;
11941:   int nResult;
11942:   int i;
11943:   const char *z;
11944:   rc = sqlite3_prepare_v2(p->db, zSelect, -1, &pSelect, 0);
11945:   if( rc!=SQLITE_OK || !pSelect ){
11946:     utf8_printf(p->out, "/**** ERROR: (%d) %s *****/\n", rc,
11947:                 sqlite3_errmsg(p->db));
11948:     if( (rc&0xff)!=SQLITE_CORRUPT ) p->nErr++;
11949:     return rc;
11950:   }
11951:   rc = sqlite3_step(pSelect);
11952:   nResult = sqlite3_column_count(pSelect);
11953:   while( rc==SQLITE_ROW ){
11954:     z = (const char*)sqlite3_column_text(pSelect, 0);
11955:     utf8_printf(p->out, "%s", z);
11956:     for(i=1; i<nResult; i++){
11957:       utf8_printf(p->out, ",%s", sqlite3_column_text(pSelect, i));
11958:     }
11959:     if( z==0 ) z = "";
11960:     while( z[0] && (z[0]!='-' || z[1]!='-') ) z++;
11961:     if( z[0] ){
11962:       raw_printf(p->out, "\n;\n");
11963:     }else{
11964:       raw_printf(p->out, ";\n");
11965:     }
11966:     rc = sqlite3_step(pSelect);
11967:   }
11968:   rc = sqlite3_finalize(pSelect);
11969:   if( rc!=SQLITE_OK ){
11970:     utf8_printf(p->out, "/**** ERROR: (%d) %s *****/\n", rc,
11971:                 sqlite3_errmsg(p->db));
11972:     if( (rc&0xff)!=SQLITE_CORRUPT ) p->nErr++;
11973:   }
11974:   return rc;
11975: }
