13362: /*
13363: ** Run zQuery.  Use dump_callback() as the callback routine so that
13364: ** the contents of the query are output as SQL statements.
13365: **
13366: ** If we get a SQLITE_CORRUPT error, rerun the query after appending
13367: ** "ORDER BY rowid DESC" to the end.
13368: */
13369: static int run_schema_dump_query(
13370:   ShellState *p,
13371:   const char *zQuery
13372: ){
13373:   int rc;
13374:   char *zErr = 0;
13375:   rc = sqlite3_exec(p->db, zQuery, dump_callback, p, &zErr);
13376:   if( rc==SQLITE_CORRUPT ){
13377:     char *zQ2;
13378:     int len = strlen30(zQuery);
13379:     raw_printf(p->out, "/****** CORRUPTION ERROR *******/\n");
13380:     if( zErr ){
13381:       utf8_printf(p->out, "/****** %s ******/\n", zErr);
13382:       sqlite3_free(zErr);
13383:       zErr = 0;
13384:     }
13385:     zQ2 = malloc( len+100 );
13386:     if( zQ2==0 ) return rc;
13387:     sqlite3_snprintf(len+100, zQ2, "%s ORDER BY rowid DESC", zQuery);
13388:     rc = sqlite3_exec(p->db, zQ2, dump_callback, p, &zErr);
13389:     if( rc ){
13390:       utf8_printf(p->out, "/****** ERROR: %s ******/\n", zErr);
13391:     }else{
13392:       rc = SQLITE_CORRUPT;
13393:     }
13394:     sqlite3_free(zErr);
13395:     free(zQ2);
13396:   } else if( zErr ){
13397:     sqlite3_free(zErr);
13398:     zErr = 0;
13399:   }
13400:   return rc;
13401: }
