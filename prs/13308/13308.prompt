8999:       "SELECT type, name, sql, 2 FROM sqlite_schema "
9000:       "WHERE type = 'trigger'"
9001:       "  AND tbl_name IN(SELECT name FROM sqlite_schema WHERE type = 'view') "
9002:       "ORDER BY 4, 1"
9003:   );
9004:   while( rc==SQLITE_OK && SQLITE_ROW==sqlite3_step(pSchema) ){
9005:     const char *zType = (const char*)sqlite3_column_text(pSchema, 0);
9006:     const char *zName = (const char*)sqlite3_column_text(pSchema, 1);
9007:     const char *zSql = (const char*)sqlite3_column_text(pSchema, 2);
