3277: /*
3278: ** Return values of columns for the row at which the completion_cursor
3279: ** is currently pointing.
3280: */
3281: static int completionColumn(
3282:   sqlite3_vtab_cursor *cur,   /* The cursor */
3283:   sqlite3_context *ctx,       /* First argument to sqlite3_result_...() */
3284:   int i                       /* Which column to return */
3285: ){
3286:   completion_cursor *pCur = (completion_cursor*)cur;
3287:   switch( i ){
3288:     case COMPLETION_COLUMN_CANDIDATE: {
3289:       sqlite3_result_text(ctx, pCur->zCurrentRow, pCur->szRow,SQLITE_TRANSIENT);
3290:       break;
3291:     }
3292:     case COMPLETION_COLUMN_PREFIX: {
3293:       sqlite3_result_text(ctx, pCur->zPrefix, -1, SQLITE_TRANSIENT);
3294:       break;
3295:     }
3296:     case COMPLETION_COLUMN_WHOLELINE: {
3297:       sqlite3_result_text(ctx, pCur->zLine, -1, SQLITE_TRANSIENT);
3298:       break;
3299:     }
3300:     case COMPLETION_COLUMN_PHASE: {
3301:       sqlite3_result_int(ctx, pCur->ePhase);
3302:       break;
3303:     }
3304:   }
3305:   return SQLITE_OK;
3306: }
