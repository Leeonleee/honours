13935: /*
13936: ** Scalar function "shell_escape_crnl" used by the .recover command.
13937: ** The argument passed to this function is the output of built-in
13938: ** function quote(). If the first character of the input is "'",
13939: ** indicating that the value passed to quote() was a text value,
13940: ** then this function searches the input for "\n" and "\r" characters
13941: ** and adds a wrapper similar to the following:
13942: **
13943: **   replace(replace(<input>, '\n', char(10), '\r', char(13));
13944: **
13945: ** Or, if the first character of the input is not "'", then a copy
13946: ** of the input is returned.
13947: */
13948: static void shellEscapeCrnl(
13949:   sqlite3_context *context,
13950:   int argc,
13951:   sqlite3_value **argv
13952: ){
13953:   const char *zText = (const char*)sqlite3_value_text(argv[0]);
13954:   UNUSED_PARAMETER(argc);
13955:   if( zText && zText[0]=='\'' ){
13956:     int nText = sqlite3_value_bytes(argv[0]);
13957:     int i;
13958:     char zBuf1[20];
13959:     char zBuf2[20];
13960:     const char *zNL = 0;
13961:     const char *zCR = 0;
13962:     int nCR = 0;
13963:     int nNL = 0;
13964: 
13965:     for(i=0; zText[i]; i++){
13966:       if( zNL==0 && zText[i]=='\n' ){
13967:         zNL = unused_string(zText, "\\n", "\\012", zBuf1);
13968:         nNL = (int)strlen(zNL);
13969:       }
13970:       if( zCR==0 && zText[i]=='\r' ){
13971:         zCR = unused_string(zText, "\\r", "\\015", zBuf2);
13972:         nCR = (int)strlen(zCR);
13973:       }
13974:     }
13975: 
13976:     if( zNL || zCR ){
13977:       int iOut = 0;
13978:       i64 nMax = (nNL > nCR) ? nNL : nCR;
13979:       i64 nAlloc = nMax * nText + (nMax+64)*2;
13980:       char *zOut = (char*)sqlite3_malloc64(nAlloc);
13981:       if( zOut==0 ){
13982:         sqlite3_result_error_nomem(context);
13983:         return;
13984:       }
13985: 
13986:       if( zNL && zCR ){
13987:         memcpy(&zOut[iOut], "replace(replace(", 16);
13988:         iOut += 16;
13989:       }else{
13990:         memcpy(&zOut[iOut], "replace(", 8);
13991:         iOut += 8;
13992:       }
13993:       for(i=0; zText[i]; i++){
13994:         if( zText[i]=='\n' ){
13995:           memcpy(&zOut[iOut], zNL, nNL);
13996:           iOut += nNL;
13997:         }else if( zText[i]=='\r' ){
13998:           memcpy(&zOut[iOut], zCR, nCR);
13999:           iOut += nCR;
14000:         }else{
14001:           zOut[iOut] = zText[i];
14002:           iOut++;
14003:         }
14004:       }
14005: 
14006:       if( zNL ){
14007:         memcpy(&zOut[iOut], ",'", 2); iOut += 2;
14008:         memcpy(&zOut[iOut], zNL, nNL); iOut += nNL;
14009:         memcpy(&zOut[iOut], "', char(10))", 12); iOut += 12;
14010:       }
14011:       if( zCR ){
14012:         memcpy(&zOut[iOut], ",'", 2); iOut += 2;
14013:         memcpy(&zOut[iOut], zCR, nCR); iOut += nCR;
14014:         memcpy(&zOut[iOut], "', char(13))", 12); iOut += 12;
14015:       }
14016: 
14017:       sqlite3_result_text(context, zOut, iOut, SQLITE_TRANSIENT);
14018:       sqlite3_free(zOut);
14019:       return;
14020:     }
14021:   }
14022: 
14023:   sqlite3_result_value(context, argv[0]);
14024: }
