12483: char *strdup_handle_newline(ShellState *p, const char *z) {
12484:   if (!z) {
12485:     return 0;
12486:   }
12487:   if (p->cMode != MODE_Box) {
12488:     return strdup(z);
12489:   }
12490:   int max_size = 80;
12491:   char *result = malloc(max_size * 2 + 10);
12492:   char *t = result;
12493:   int count = 0;
12494:   int interrupted = 0;
12495:   const char *s;
12496:   for(s = z; *s; s++, t++) {
12497:     if (*s == '\n') {
12498:       *t = '\\';
12499:       t++;
12500:       *t = 'n';
12501:     } else {
12502:       *t = *s;
12503:     }
12504:     count++;
12505:     if (count >= max_size && ((*s & 0xc0) != 0x80)) {
12506:       interrupted = 1;
12507:       break;
12508:     }
12509:   }
12510:   if (interrupted) {
12511:     *t++ = '.';
12512:     *t++ = '.';
12513:     *t++ = '.';
12514:   }
12515:   *t = '\0';
12516:   return result;
12517: }
