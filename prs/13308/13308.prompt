6682: static u32 zipfileGetTime(sqlite3_value *pVal){
6683:   if( pVal==0 || sqlite3_value_type(pVal)==SQLITE_NULL ){
6684:     return zipfileTime();
6685:   }
6686:   return (u32)sqlite3_value_int64(pVal);
