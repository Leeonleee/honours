8581: static int idxCreateFromCons(
8582:   sqlite3expert *p,
8583:   IdxScan *pScan,
8584:   IdxConstraint *pEq,
8585:   IdxConstraint *pTail
8586: ){
8587:   sqlite3 *dbm = p->dbm;
8588:   int rc = SQLITE_OK;
8589:   if( (pEq || pTail) && 0==idxFindCompatible(&rc, dbm, pScan, pEq, pTail) ){
8590:     IdxTable *pTab = pScan->pTab;
8591:     char *zCols = 0;
8592:     char *zIdx = 0;
8593:     IdxConstraint *pCons;
8594:     unsigned int h = 0;
8595:     const char *zFmt;
8596: 
8597:     for(pCons=pEq; pCons; pCons=pCons->pLink){
8598:       zCols = idxAppendColDefn(&rc, zCols, pTab, pCons);
8599:     }
8600:     for(pCons=pTail; pCons; pCons=pCons->pLink){
8601:       zCols = idxAppendColDefn(&rc, zCols, pTab, pCons);
8602:     }
8603: 
8604:     if( rc==SQLITE_OK ){
8605:       /* Hash the list of columns to come up with a name for the index */
8606:       const char *zTable = pScan->pTab->zName;
8607:       char *zName;                /* Index name */
8608:       int i;
8609:       for(i=0; zCols[i]; i++){
8610:         h += ((h<<3) + zCols[i]);
8611:       }
8612:       zName = sqlite3_mprintf("%s_idx_%08x", zTable, h);
8613:       if( zName==0 ){
8614:         rc = SQLITE_NOMEM;
8615:       }else{
8616:         if( idxIdentifierRequiresQuotes(zTable) ){
8617:           zFmt = "CREATE INDEX '%q' ON %Q(%s)";
8618:         }else{
8619:           zFmt = "CREATE INDEX %s ON %s(%s)";
8620:         }
8621:         zIdx = sqlite3_mprintf(zFmt, zName, zTable, zCols);
8622:         if( !zIdx ){
8623:           rc = SQLITE_NOMEM;
8624:         }else{
8625:           rc = sqlite3_exec(dbm, zIdx, 0, 0, p->pzErrmsg);
8626:           idxHashAdd(&rc, &p->hIdx, zName, zIdx);
8627:         }
8628:         sqlite3_free(zName);
8629:         sqlite3_free(zIdx);
8630:       }
8631:     }
8632: 
8633:     sqlite3_free(zCols);
8634:   }
8635:   return rc;
8636: }
