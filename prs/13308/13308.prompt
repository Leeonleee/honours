3159: /*
3160: ** Advance a completion_cursor to its next row of output.
3161: **
3162: ** The ->ePhase, ->j, and ->pStmt fields of the completion_cursor object
3163: ** record the current state of the scan.  This routine sets ->zCurrentRow
3164: ** to the current row of output and then returns.  If no more rows remain,
3165: ** then ->ePhase is set to COMPLETION_EOF which will signal the virtual
3166: ** table that has reached the end of its scan.
3167: **
3168: ** The current implementation just lists potential identifiers and
3169: ** keywords and filters them by zPrefix.  Future enhancements should
3170: ** take zLine into account to try to restrict the set of identifiers and
3171: ** keywords based on what would be legal at the current point of input.
3172: */
3173: static int completionNext(sqlite3_vtab_cursor *cur){
3174:   completion_cursor *pCur = (completion_cursor*)cur;
3175:   int eNextPhase = 0;  /* Next phase to try if current phase reaches end */
3176:   int iCol = -1;       /* If >=0, step pCur->pStmt and use the i-th column */
3177:   pCur->iRowid++;
3178:   while( pCur->ePhase!=COMPLETION_EOF ){
3179:     switch( pCur->ePhase ){
3180:       case COMPLETION_KEYWORDS: {
3181:         if( pCur->j >= sqlite3_keyword_count() ){
3182:           pCur->zCurrentRow = 0;
3183:           pCur->ePhase = COMPLETION_DATABASES;
3184:         }else{
3185:           sqlite3_keyword_name(pCur->j++, &pCur->zCurrentRow, &pCur->szRow);
3186:         }
3187:         iCol = -1;
3188:         break;
3189:       }
3190:       case COMPLETION_DATABASES: {
3191:         if( pCur->pStmt==0 ){
3192:           sqlite3_prepare_v2(pCur->db, "PRAGMA database_list", -1,
3193:                              &pCur->pStmt, 0);
3194:         }
3195:         iCol = 1;
3196:         eNextPhase = COMPLETION_TABLES;
3197:         break;
3198:       }
3199:       case COMPLETION_TABLES: {
3200:         if( pCur->pStmt==0 ){
3201:           sqlite3_stmt *pS2;
3202:           char *zSql = 0;
3203:           const char *zSep = "";
3204:           sqlite3_prepare_v2(pCur->db, "PRAGMA database_list", -1, &pS2, 0);
3205:           while( sqlite3_step(pS2)==SQLITE_ROW ){
3206:             const char *zDb = (const char*)sqlite3_column_text(pS2, 1);
3207:             zSql = sqlite3_mprintf(
3208:                "%z%s"
3209:                "SELECT name FROM \"%w\".sqlite_schema",
3210:                zSql, zSep, zDb
3211:             );
3212:             if( zSql==0 ) return SQLITE_NOMEM;
3213:             zSep = " UNION ";
3214:           }
3215:           sqlite3_finalize(pS2);
3216:           sqlite3_prepare_v2(pCur->db, zSql, -1, &pCur->pStmt, 0);
3217:           sqlite3_free(zSql);
3218:         }
3219:         iCol = 0;
3220:         eNextPhase = COMPLETION_COLUMNS;
3221:         break;
3222:       }
3223:       case COMPLETION_COLUMNS: {
3224:         if( pCur->pStmt==0 ){
3225:           sqlite3_stmt *pS2;
3226:           char *zSql = 0;
3227:           const char *zSep = "";
3228:           sqlite3_prepare_v2(pCur->db, "PRAGMA database_list", -1, &pS2, 0);
3229:           while( sqlite3_step(pS2)==SQLITE_ROW ){
3230:             const char *zDb = (const char*)sqlite3_column_text(pS2, 1);
3231:             zSql = sqlite3_mprintf(
3232:                "%z%s"
3233:                "SELECT pti.name FROM \"%w\".sqlite_schema AS sm"
3234:                        " JOIN pragma_table_info(sm.name,%Q) AS pti"
3235:                " WHERE sm.type='table'",
3236:                zSql, zSep, zDb, zDb
3237:             );
3238:             if( zSql==0 ) return SQLITE_NOMEM;
3239:             zSep = " UNION ";
3240:           }
3241:           sqlite3_finalize(pS2);
3242:           sqlite3_prepare_v2(pCur->db, zSql, -1, &pCur->pStmt, 0);
3243:           sqlite3_free(zSql);
3244:         }
3245:         iCol = 0;
3246:         eNextPhase = COMPLETION_EOF;
3247:         break;
3248:       }
3249:     }
3250:     if( iCol<0 ){
3251:       /* This case is when the phase presets zCurrentRow */
3252:       if( pCur->zCurrentRow==0 ) continue;
3253:     }else{
3254:       if( sqlite3_step(pCur->pStmt)==SQLITE_ROW ){
3255:         /* Extract the next row of content */
3256:         pCur->zCurrentRow = (const char*)sqlite3_column_text(pCur->pStmt, iCol);
3257:         pCur->szRow = sqlite3_column_bytes(pCur->pStmt, iCol);
3258:       }else{
3259:         /* When all rows are finished, advance to the next phase */
3260:         sqlite3_finalize(pCur->pStmt);
3261:         pCur->pStmt = 0;
3262:         pCur->ePhase = eNextPhase;
3263:         continue;
3264:       }
3265:     }
3266:     if( pCur->nPrefix==0 ) break;
3267:     if( pCur->nPrefix<=pCur->szRow
3268:      && sqlite3_strnicmp(pCur->zPrefix, pCur->zCurrentRow, pCur->nPrefix)==0
3269:     ){
3270:       break;
3271:     }
3272:   }
3273: 
3274:   return SQLITE_OK;
3275: }
