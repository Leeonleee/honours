9585: /*
9586: ** Return a component of the report.
9587: */
9588: const char *sqlite3_expert_report(sqlite3expert *p, int iStmt, int eReport){
9589:   const char *zRet = 0;
9590:   IdxStatement *pStmt;
9591: 
9592:   if( p->bRun==0 ) return 0;
9593:   for(pStmt=p->pStatement; pStmt && pStmt->iId!=iStmt; pStmt=pStmt->pNext);
9594:   switch( eReport ){
9595:     case EXPERT_REPORT_SQL:
9596:       if( pStmt ) zRet = pStmt->zSql;
9597:       break;
9598:     case EXPERT_REPORT_INDEXES:
9599:       if( pStmt ) zRet = pStmt->zIdx;
9600:       break;
9601:     case EXPERT_REPORT_PLAN:
9602:       if( pStmt ) zRet = pStmt->zEQP;
9603:       break;
9604:     case EXPERT_REPORT_CANDIDATES:
9605:       zRet = p->zCandidates;
9606:       break;
9607:   }
9608:   return zRet;
9609: }
