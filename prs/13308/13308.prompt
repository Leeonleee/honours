8292: static int idxRegisterVtab(sqlite3expert *p){
8293:   static sqlite3_module expertModule = {
8294:     2,                            /* iVersion */
8295:     expertConnect,                /* xCreate - create a table */
8296:     expertConnect,                /* xConnect - connect to an existing table */
8297:     expertBestIndex,              /* xBestIndex - Determine search strategy */
8298:     expertDisconnect,             /* xDisconnect - Disconnect from a table */
8299:     expertDisconnect,             /* xDestroy - Drop a table */
8300:     expertOpen,                   /* xOpen - open a cursor */
8301:     expertClose,                  /* xClose - close a cursor */
8302:     expertFilter,                 /* xFilter - configure scan constraints */
8303:     expertNext,                   /* xNext - advance a cursor */
8304:     expertEof,                    /* xEof */
8305:     expertColumn,                 /* xColumn - read data */
8306:     expertRowid,                  /* xRowid - read data */
8307:     expertUpdate,                 /* xUpdate - write data */
8308:     0,                            /* xBegin - begin transaction */
8309:     0,                            /* xSync - sync transaction */
8310:     0,                            /* xCommit - commit transaction */
8311:     0,                            /* xRollback - rollback transaction */
8312:     0,                            /* xFindFunction - function overloading */
8313:     0,                            /* xRename - rename the table */
8314:     0,                            /* xSavepoint */
8315:     0,                            /* xRelease */
8316:     0,                            /* xRollbackTo */
8317:     0,                            /* xShadowName */
8318:   };
8319: 
8320:   return sqlite3_create_module(p->dbv, "expert", &expertModule, (void*)p);
8321: }
8322: /*
8323: ** End of virtual table implementation.
8324: *************************************************************************/
8325: /*
8326: ** Finalize SQL statement pStmt. If (*pRc) is SQLITE_OK when this function
8327: ** is called, set it to the return value of sqlite3_finalize() before
8328: ** returning. Otherwise, discard the sqlite3_finalize() return value.
8329: */
8330: static void idxFinalize(int *pRc, sqlite3_stmt *pStmt){
8331:   int rc = sqlite3_finalize(pStmt);
8332:   if( *pRc==SQLITE_OK ) *pRc = rc;
8333: }
