9538: int sqlite3_expert_analyze(sqlite3expert *p, char **pzErr){
9539:   int rc;
9540:   IdxHashEntry *pEntry;
9541: 
9542:   /* Do trigger processing to collect any extra IdxScan structures */
9543:   rc = idxProcessTriggers(p, pzErr);
9544: 
9545:   /* Create candidate indexes within the in-memory database file */
9546:   if( rc==SQLITE_OK ){
9547:     rc = idxCreateCandidates(p);
9548:   }
9549: 
9550:   /* Generate the stat1 data */
9551:   if( rc==SQLITE_OK ){
9552:     rc = idxPopulateStat1(p, pzErr);
9553:   }
9554: 
9555:   /* Formulate the EXPERT_REPORT_CANDIDATES text */
9556:   for(pEntry=p->hIdx.pFirst; pEntry; pEntry=pEntry->pNext){
9557:     p->zCandidates = idxAppendText(&rc, p->zCandidates,
9558:         "%s;%s%s\n", pEntry->zVal,
9559:         pEntry->zVal2 ? " -- stat1: " : "", pEntry->zVal2
9560:     );
9561:   }
9562: 
9563:   /* Figure out which of the candidate indexes are preferred by the query
9564:   ** planner and report the results to the user.  */
9565:   if( rc==SQLITE_OK ){
9566:     rc = idxFindIndexes(p, pzErr);
9567:   }
9568: 
9569:   if( rc==SQLITE_OK ){
9570:     p->bRun = 1;
9571:   }
9572:   return rc;
9573: }
