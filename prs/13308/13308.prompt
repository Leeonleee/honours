14995: /*
14996: ** Values for ArCommand.eCmd.
14997: */
14998: #define AR_CMD_CREATE       1
14999: #define AR_CMD_UPDATE       2
15000: #define AR_CMD_INSERT       3
15001: #define AR_CMD_EXTRACT      4
15002: #define AR_CMD_LIST         5
15003: #define AR_CMD_HELP         6
15004: 
15005: /*
15006: ** Other (non-command) switches.
15007: */
15008: #define AR_SWITCH_VERBOSE     7
15009: #define AR_SWITCH_FILE        8
15010: #define AR_SWITCH_DIRECTORY   9
15011: #define AR_SWITCH_APPEND     10
15012: #define AR_SWITCH_DRYRUN     11
15013: 
15014: static int arProcessSwitch(ArCommand *pAr, int eSwitch, const char *zArg){
15015:   switch( eSwitch ){
15016:     case AR_CMD_CREATE:
15017:     case AR_CMD_EXTRACT:
15018:     case AR_CMD_LIST:
15019:     case AR_CMD_UPDATE:
15020:     case AR_CMD_INSERT:
15021:     case AR_CMD_HELP:
15022:       if( pAr->eCmd ){
15023:         return arErrorMsg(pAr, "multiple command options");
15024:       }
15025:       pAr->eCmd = eSwitch;
15026:       break;
15027: 
15028:     case AR_SWITCH_DRYRUN:
15029:       pAr->bDryRun = 1;
15030:       break;
15031:     case AR_SWITCH_VERBOSE:
15032:       pAr->bVerbose = 1;
15033:       break;
15034:     case AR_SWITCH_APPEND:
15035:       pAr->bAppend = 1;
15036:       /* Fall thru into --file */
15037:     case AR_SWITCH_FILE:
15038:       pAr->zFile = zArg;
15039:       break;
15040:     case AR_SWITCH_DIRECTORY:
15041:       pAr->zDir = zArg;
15042:       break;
15043:   }
15044: 
15045:   return SQLITE_OK;
15046: }
