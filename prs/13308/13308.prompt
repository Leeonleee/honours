14325: /*
14326: ** Interpret zArg as either an integer or a boolean value.  Return 1 or 0
14327: ** for TRUE and FALSE.  Return the integer value if appropriate.
14328: */
14329: static int booleanValue(const char *zArg){
14330:   int i;
14331:   if( zArg[0]=='0' && zArg[1]=='x' ){
14332:     for(i=2; hexDigitValue(zArg[i])>=0; i++){}
14333:   }else{
14334:     for(i=0; zArg[i]>='0' && zArg[i]<='9'; i++){}
14335:   }
14336:   if( i>0 && zArg[i]==0 ) return (int)(integerValue(zArg) & 0xffffffff);
14337:   if( sqlite3_stricmp(zArg, "on")==0 || sqlite3_stricmp(zArg,"yes")==0 ){
14338:     return 1;
14339:   }
14340:   if( sqlite3_stricmp(zArg, "off")==0 || sqlite3_stricmp(zArg,"no")==0 ){
14341:     return 0;
14342:   }
14343:   utf8_printf(stderr, "ERROR: Not a boolean value: \"%s\". Assuming \"no\".\n",
14344:           zArg);
14345:   return 0;
14346: }
