14922: /* Reset the prepared statement created using shellPreparePrintf().
14923: **
14924: ** This routine is could be marked "static".  But it is not always used,
14925: ** depending on compile-time options.  By omitting the "static", we avoid
14926: ** nuisance compiler warnings about "defined but not used".
14927: */
14928: void shellReset(
14929:   int *pRc,
14930:   sqlite3_stmt *pStmt
14931: ){
14932:   int rc = sqlite3_reset(pStmt);
14933:   if( *pRc==SQLITE_OK ){
14934:     if( rc!=SQLITE_OK ){
14935:       sqlite3 *db = sqlite3_db_handle(pStmt);
14936:       raw_printf(stderr, "SQL error: %s\n", sqlite3_errmsg(db));
14937:     }
14938:     *pRc = rc;
14939:   }
14940: }
