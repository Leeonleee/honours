12246: /*
12247: ** If compiled statement pSql appears to be an EXPLAIN statement, allocate
12248: ** and populate the ShellState.aiIndent[] array with the number of
12249: ** spaces each opcode should be indented before it is output.
12250: **
12251: ** The indenting rules are:
12252: **
12253: **     * For each "Next", "Prev", "VNext" or "VPrev" instruction, indent
12254: **       all opcodes that occur between the p2 jump destination and the opcode
12255: **       itself by 2 spaces.
12256: **
12257: **     * For each "Goto", if the jump destination is earlier in the program
12258: **       and ends on one of:
12259: **          Yield  SeekGt  SeekLt  RowSetRead  Rewind
12260: **       or if the P1 parameter is one instead of zero,
12261: **       then indent all opcodes between the earlier instruction
12262: **       and "Goto" by 2 spaces.
12263: */
12264: static void explain_data_prepare(ShellState *p, sqlite3_stmt *pSql){
12265:   const char *zSql;               /* The text of the SQL statement */
12266:   const char *z;                  /* Used to check if this is an EXPLAIN */
12267:   int *abYield = 0;               /* True if op is an OP_Yield */
12268:   int nAlloc = 0;                 /* Allocated size of p->aiIndent[], abYield */
12269:   int iOp;                        /* Index of operation in p->aiIndent[] */
12270: 
12271:   const char *azNext[] = { "Next", "Prev", "VPrev", "VNext", "SorterNext", 0 };
12272:   const char *azYield[] = { "Yield", "SeekLT", "SeekGT", "RowSetRead",
12273:                             "Rewind", 0 };
12274:   const char *azGoto[] = { "Goto", 0 };
12275: 
12276:   /* Try to figure out if this is really an EXPLAIN statement. If this
12277:   ** cannot be verified, return early.  */
12278:   if( sqlite3_column_count(pSql)!=8 ){
12279:     p->cMode = p->mode;
12280:     return;
12281:   }
12282:   zSql = sqlite3_sql(pSql);
12283:   if( zSql==0 ) return;
12284:   for(z=zSql; *z==' ' || *z=='\t' || *z=='\n' || *z=='\f' || *z=='\r'; z++);
12285:   if( sqlite3_strnicmp(z, "explain", 7) ){
12286:     p->cMode = p->mode;
12287:     return;
12288:   }
12289: 
12290:   for(iOp=0; SQLITE_ROW==sqlite3_step(pSql); iOp++){
12291:     int i;
12292:     int iAddr = sqlite3_column_int(pSql, 0);
12293:     const char *zOp = (const char*)sqlite3_column_text(pSql, 1);
12294: 
12295:     /* Set p2 to the P2 field of the current opcode. Then, assuming that
12296:     ** p2 is an instruction address, set variable p2op to the index of that
12297:     ** instruction in the aiIndent[] array. p2 and p2op may be different if
12298:     ** the current instruction is part of a sub-program generated by an
12299:     ** SQL trigger or foreign key.  */
12300:     int p2 = sqlite3_column_int(pSql, 3);
12301:     int p2op = (p2 + (iOp-iAddr));
12302: 
12303:     /* Grow the p->aiIndent array as required */
12304:     if( iOp>=nAlloc ){
12305:       if( iOp==0 ){
12306:         /* Do further verfication that this is explain output.  Abort if
12307:         ** it is not */
12308:         static const char *explainCols[] = {
12309:            "addr", "opcode", "p1", "p2", "p3", "p4", "p5", "comment" };
12310:         int jj;
12311:         for(jj=0; jj<ArraySize(explainCols); jj++){
12312:           if( strcmp(sqlite3_column_name(pSql,jj),explainCols[jj])!=0 ){
12313:             p->cMode = p->mode;
12314:             sqlite3_reset(pSql);
12315:             return;
12316:           }
12317:         }
12318:       }
12319:       nAlloc += 100;
12320:       p->aiIndent = (int*)sqlite3_realloc64(p->aiIndent, nAlloc*sizeof(int));
12321:       if( p->aiIndent==0 ) shell_out_of_memory();
12322:       abYield = (int*)sqlite3_realloc64(abYield, nAlloc*sizeof(int));
12323:       if( abYield==0 ) shell_out_of_memory();
12324:     }
12325:     abYield[iOp] = str_in_array(zOp, azYield);
12326:     p->aiIndent[iOp] = 0;
12327:     p->nIndent = iOp+1;
12328: 
12329:     if( str_in_array(zOp, azNext) ){
12330:       for(i=p2op; i<iOp; i++) p->aiIndent[i] += 2;
12331:     }
12332:     if( str_in_array(zOp, azGoto) && p2op<p->nIndent
12333:      && (abYield[p2op] || sqlite3_column_int(pSql, 2))
12334:     ){
12335:       for(i=p2op; i<iOp; i++) p->aiIndent[i] += 2;
12336:     }
12337:   }
12338: 
12339:   p->iIndent = 0;
12340:   sqlite3_free(abYield);
12341:   sqlite3_reset(pSql);
12342: }
