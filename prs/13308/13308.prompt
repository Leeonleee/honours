12958: /*
12959: ** Execute a statement or set of statements.  Print
12960: ** any result rows/columns depending on the current mode
12961: ** set via the supplied callback.
12962: **
12963: ** This is very similar to SQLite's built-in sqlite3_exec()
12964: ** function except it takes a slightly different callback
12965: ** and callback data argument.
12966: */
12967: static int shell_exec(
12968:   ShellState *pArg,                         /* Pointer to ShellState */
12969:   const char *zSql,                         /* SQL to be evaluated */
12970:   char **pzErrMsg                           /* Error msg written here */
12971: ){
12972:   sqlite3_stmt *pStmt = NULL;     /* Statement to execute. */
12973:   int rc = SQLITE_OK;             /* Return Code */
12974:   int rc2;
12975:   const char *zLeftover;          /* Tail of unprocessed SQL */
12976:   sqlite3 *db = pArg->db;
12977: 
12978:   if( pzErrMsg ){
12979:     *pzErrMsg = NULL;
12980:   }
12981: 
12982: #ifndef SQLITE_OMIT_VIRTUALTABLE
12983:   if( pArg->expert.pExpert ){
12984:     rc = expertHandleSQL(pArg, zSql, pzErrMsg);
12985:     return expertFinish(pArg, (rc!=SQLITE_OK), pzErrMsg);
12986:   }
12987: #endif
12988: 
12989:   while( zSql[0] && (SQLITE_OK == rc) ){
12990:     static const char *zStmtSql;
12991:     rc = sqlite3_prepare_v2(db, zSql, -1, &pStmt, &zLeftover);
12992:     if( SQLITE_OK != rc ){
12993:       if( pzErrMsg ){
12994:         *pzErrMsg = save_err_msg(db);
12995:       }
12996:     }else{
12997:       if( !pStmt ){
12998:         /* this happens for a comment or white-space */
12999:         zSql = zLeftover;
13000:         while( IsSpace(zSql[0]) ) zSql++;
13001:         continue;
13002:       }
13003:       zStmtSql = sqlite3_sql(pStmt);
13004:       if( zStmtSql==0 ) zStmtSql = "";
13005:       while( IsSpace(zStmtSql[0]) ) zStmtSql++;
13006: 
13007:       /* save off the prepared statment handle and reset row count */
13008:       if( pArg ){
13009:         pArg->pStmt = pStmt;
13010:         pArg->cnt = 0;
13011:       }
13012: 
13013:       /* echo the sql statement if echo on */
13014:       if( pArg && ShellHasFlag(pArg, SHFLG_Echo) ){
13015:         utf8_printf(pArg->out, "%s\n", zStmtSql ? zStmtSql : zSql);
13016:       }
13017: 
13018:       /* Show the EXPLAIN QUERY PLAN if .eqp is on */
13019:       if( pArg && pArg->autoEQP && sqlite3_stmt_isexplain(pStmt)==0 ){
13020:         sqlite3_stmt *pExplain;
13021:         char *zEQP;
13022:         int triggerEQP = 0;
13023:         disable_debug_trace_modes();
13024:         sqlite3_db_config(db, SQLITE_DBCONFIG_TRIGGER_EQP, -1, &triggerEQP);
13025:         if( pArg->autoEQP>=AUTOEQP_trigger ){
13026:           sqlite3_db_config(db, SQLITE_DBCONFIG_TRIGGER_EQP, 1, 0);
13027:         }
13028:         zEQP = sqlite3_mprintf("EXPLAIN QUERY PLAN %s", zStmtSql);
13029:         rc = sqlite3_prepare_v2(db, zEQP, -1, &pExplain, 0);
13030:         if( rc==SQLITE_OK ){
13031:           while( sqlite3_step(pExplain)==SQLITE_ROW ){
13032:             const char *zEQPLine = (const char*)sqlite3_column_text(pExplain,3);
13033:             int iEqpId = sqlite3_column_int(pExplain, 0);
13034:             int iParentId = sqlite3_column_int(pExplain, 1);
13035:             if( zEQPLine==0 ) zEQPLine = "";
13036:             if( zEQPLine[0]=='-' ) eqp_render(pArg);
13037:             eqp_append(pArg, iEqpId, iParentId, zEQPLine);
13038:           }
13039:           eqp_render(pArg);
13040:         }
13041:         sqlite3_finalize(pExplain);
13042:         sqlite3_free(zEQP);
13043:         if( pArg->autoEQP>=AUTOEQP_full ){
13044:           /* Also do an EXPLAIN for ".eqp full" mode */
13045:           zEQP = sqlite3_mprintf("EXPLAIN %s", zStmtSql);
13046:           rc = sqlite3_prepare_v2(db, zEQP, -1, &pExplain, 0);
13047:           if( rc==SQLITE_OK ){
13048:             pArg->cMode = MODE_Explain;
13049:             explain_data_prepare(pArg, pExplain);
13050:             exec_prepared_stmt(pArg, pExplain);
13051:             explain_data_delete(pArg);
13052:           }
13053:           sqlite3_finalize(pExplain);
13054:           sqlite3_free(zEQP);
13055:         }
13056:         if( pArg->autoEQP>=AUTOEQP_trigger && triggerEQP==0 ){
13057:           sqlite3_db_config(db, SQLITE_DBCONFIG_TRIGGER_EQP, 0, 0);
13058:           /* Reprepare pStmt before reactiving trace modes */
13059:           sqlite3_finalize(pStmt);
13060:           sqlite3_prepare_v2(db, zSql, -1, &pStmt, 0);
13061:           if( pArg ) pArg->pStmt = pStmt;
13062:         }
13063:         restore_debug_trace_modes();
13064:       }
13065: 
13066:       if( pArg ){
13067:         pArg->cMode = pArg->mode;
13068:         if( pArg->autoExplain ){
13069:           if( sqlite3_stmt_isexplain(pStmt)==1 ){
13070:             pArg->cMode = MODE_Explain;
13071:           }
13072:           if( sqlite3_stmt_isexplain(pStmt)==2 ){
13073:             pArg->cMode = MODE_EQP;
13074:           }
13075:         }
13076: 
13077:         /* If the shell is currently in ".explain" mode, gather the extra
13078:         ** data required to add indents to the output.*/
13079:         // if( pArg->cMode==MODE_Explain ){
13080:         //   explain_data_prepare(pArg, pStmt);
13081:         // }
13082:       }
13083: 
13084:       bind_prepared_stmt(pArg, pStmt);
13085:       exec_prepared_stmt(pArg, pStmt);
13086:       explain_data_delete(pArg);
13087:       eqp_render(pArg);
13088: 
13089:       /* print usage stats if stats on */
13090:       if( pArg && pArg->statsOn ){
13091:         display_stats(db, pArg, 0);
13092:       }
13093: 
13094:       /* print loop-counters if required */
13095:       if( pArg && pArg->scanstatsOn ){
13096:         display_scanstats(db, pArg);
13097:       }
13098: 
13099:       /* Finalize the statement just executed. If this fails, save a
13100:       ** copy of the error message. Otherwise, set zSql to point to the
13101:       ** next statement to execute. */
13102:       rc2 = sqlite3_finalize(pStmt);
13103:       if( rc!=SQLITE_NOMEM ) rc = rc2;
13104:       if( rc==SQLITE_OK ){
13105:         zSql = zLeftover;
13106:         while( IsSpace(zSql[0]) ) zSql++;
13107:       }else if( pzErrMsg ){
13108:         *pzErrMsg = save_err_msg(db);
13109:       }
13110: 
13111:       /* clear saved stmt handle */
13112:       if( pArg ){
13113:         pArg->pStmt = NULL;
13114:       }
13115:     }
13116:   } /* end while */
13117: 
13118:   return rc;
13119: }
