14900: /* Finalize the prepared statement created using shellPreparePrintf().
14901: **
14902: ** This routine is could be marked "static".  But it is not always used,
14903: ** depending on compile-time options.  By omitting the "static", we avoid
14904: ** nuisance compiler warnings about "defined but not used".
14905: */
14906: void shellFinalize(
14907:   int *pRc,
14908:   sqlite3_stmt *pStmt
14909: ){
14910:   if( pStmt ){
14911:     sqlite3 *db = sqlite3_db_handle(pStmt);
14912:     int rc = sqlite3_finalize(pStmt);
14913:     if( *pRc==SQLITE_OK ){
14914:       if( rc!=SQLITE_OK ){
14915:         raw_printf(stderr, "SQL error: %s\n", sqlite3_errmsg(db));
14916:       }
14917:       *pRc = rc;
14918:     }
14919:   }
14920: }
