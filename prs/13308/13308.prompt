6570: static int zipfileGetMode(
6571:   sqlite3_value *pVal,
6572:   int bIsDir,                     /* If true, default to directory */
6573:   u32 *pMode,                     /* OUT: Mode value */
6574:   char **pzErr                    /* OUT: Error message */
6575: ){
6576:   const char *z = (const char*)sqlite3_value_text(pVal);
6577:   u32 mode = 0;
6578:   if( z==0 ){
6579:     mode = (bIsDir ? (S_IFDIR + 0755) : (S_IFREG + 0644));
6580:   }else if( z[0]>='0' && z[0]<='9' ){
6581:     mode = (unsigned int)sqlite3_value_int(pVal);
6582:   }else{
6583:     const char zTemplate[11] = "-rwxrwxrwx";
6584:     int i;
6585:     if( strlen(z)!=10 ) goto parse_error;
6586:     switch( z[0] ){
6587:       case '-': mode |= S_IFREG; break;
6588:       case 'd': mode |= S_IFDIR; break;
6589:       case 'l': mode |= S_IFLNK; break;
6590:       default: goto parse_error;
6591:     }
6592:     for(i=1; i<10; i++){
6593:       if( z[i]==zTemplate[i] ) mode |= 1 << (9-i);
6594:       else if( z[i]!='-' ) goto parse_error;
6595:     }
6596:   }
6597:   if( ((mode & S_IFDIR)==0)==bIsDir ){
6598:     /* The "mode" attribute is a directory, but data has been specified.
6599:     ** Or vice-versa - no data but "mode" is a file or symlink.  */
6600:     *pzErr = sqlite3_mprintf("zipfile: mode does not match data");
6601:     return SQLITE_CONSTRAINT;
6602:   }
6603:   *pMode = mode;
6604:   return SQLITE_OK;
6605: 
6606:  parse_error:
6607:   *pzErr = sqlite3_mprintf("zipfile: parse error in mode: %s", z);
6608:   return SQLITE_ERROR;
6609: }
