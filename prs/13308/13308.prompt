15992: /*
15993: ** This function is called to recover data from the database. A script
15994: ** to construct a new database containing all recovered data is output
15995: ** on stream pState->out.
15996: */
15997: static int recoverDatabaseCmd(ShellState *pState, int nArg, char **azArg){
15998:   int rc = SQLITE_OK;
15999:   sqlite3_stmt *pLoop = 0;        /* Loop through all root pages */
16000:   sqlite3_stmt *pPages = 0;       /* Loop through all pages in a group */
16001:   sqlite3_stmt *pCells = 0;       /* Loop through all cells in a page */
16002:   const char *zRecoveryDb = "";   /* Name of "recovery" database */
16003:   const char *zLostAndFound = "lost_and_found";
16004:   int i;
16005:   int nOrphan = -1;
16006:   RecoverTable *pOrphan = 0;
16007: 
16008:   int bFreelist = 1;              /* 0 if --freelist-corrupt is specified */
16009:   int bRowids = 1;                /* 0 if --no-rowids */
16010:   for(i=1; i<nArg; i++){
16011:     char *z = azArg[i];
16012:     int n;
16013:     if( z[0]=='-' && z[1]=='-' ) z++;
16014:     n = strlen30(z);
16015:     if( n<=17 && memcmp("-freelist-corrupt", z, n)==0 ){
16016:       bFreelist = 0;
16017:     }else
16018:     if( n<=12 && memcmp("-recovery-db", z, n)==0 && i<(nArg-1) ){
16019:       i++;
16020:       zRecoveryDb = azArg[i];
16021:     }else
16022:     if( n<=15 && memcmp("-lost-and-found", z, n)==0 && i<(nArg-1) ){
16023:       i++;
16024:       zLostAndFound = azArg[i];
16025:     }else
16026:     if( n<=10 && memcmp("-no-rowids", z, n)==0 ){
16027:       bRowids = 0;
16028:     }
16029:     else{
16030:       utf8_printf(stderr, "unexpected option: %s\n", azArg[i]);
16031:       showHelp(pState->out, azArg[0]);
16032:       return 1;
16033:     }
16034:   }
16035: 
16036:   shellExecPrintf(pState->db, &rc,
16037:     /* Attach an in-memory database named 'recovery'. Create an indexed
16038:     ** cache of the sqlite_dbptr virtual table. */
16039:     "PRAGMA writable_schema = on;"
16040:     "ATTACH %Q AS recovery;"
16041:     "DROP TABLE IF EXISTS recovery.dbptr;"
16042:     "DROP TABLE IF EXISTS recovery.freelist;"
16043:     "DROP TABLE IF EXISTS recovery.map;"
16044:     "DROP TABLE IF EXISTS recovery.schema;"
16045:     "CREATE TABLE recovery.freelist(pgno INTEGER PRIMARY KEY);", zRecoveryDb
16046:   );
16047: 
16048:   if( bFreelist ){
16049:     shellExec(pState->db, &rc,
16050:       "WITH trunk(pgno) AS ("
16051:       "  SELECT shell_int32("
16052:       "      (SELECT data FROM sqlite_dbpage WHERE pgno=1), 8) AS x "
16053:       "      WHERE x>0"
16054:       "    UNION"
16055:       "  SELECT shell_int32("
16056:       "      (SELECT data FROM sqlite_dbpage WHERE pgno=trunk.pgno), 0) AS x "
16057:       "      FROM trunk WHERE x>0"
16058:       "),"
16059:       "freelist(data, n, freepgno) AS ("
16060:       "  SELECT data, min(16384, shell_int32(data, 1)-1), t.pgno "
16061:       "      FROM trunk t, sqlite_dbpage s WHERE s.pgno=t.pgno"
16062:       "    UNION ALL"
16063:       "  SELECT data, n-1, shell_int32(data, 2+n) "
16064:       "      FROM freelist WHERE n>=0"
16065:       ")"
16066:       "REPLACE INTO recovery.freelist SELECT freepgno FROM freelist;"
16067:     );
16068:   }
16069: 
16070:   /* If this is an auto-vacuum database, add all pointer-map pages to
16071:   ** the freelist table. Do this regardless of whether or not
16072:   ** --freelist-corrupt was specified.  */
16073:   shellExec(pState->db, &rc,
16074:     "WITH ptrmap(pgno) AS ("
16075:     "  SELECT 2 WHERE shell_int32("
16076:     "    (SELECT data FROM sqlite_dbpage WHERE pgno=1), 13"
16077:     "  )"
16078:     "    UNION ALL "
16079:     "  SELECT pgno+1+(SELECT page_size FROM pragma_page_size)/5 AS pp "
16080:     "  FROM ptrmap WHERE pp<=(SELECT page_count FROM pragma_page_count)"
16081:     ")"
16082:     "REPLACE INTO recovery.freelist SELECT pgno FROM ptrmap"
16083:   );
16084: 
16085:   shellExec(pState->db, &rc,
16086:     "CREATE TABLE recovery.dbptr("
16087:     "      pgno, child, PRIMARY KEY(child, pgno)"
16088:     ") WITHOUT ROWID;"
16089:     "INSERT OR IGNORE INTO recovery.dbptr(pgno, child) "
16090:     "    SELECT * FROM sqlite_dbptr"
16091:     "      WHERE pgno NOT IN freelist AND child NOT IN freelist;"
16092: 
16093:     /* Delete any pointer to page 1. This ensures that page 1 is considered
16094:     ** a root page, regardless of how corrupt the db is. */
16095:     "DELETE FROM recovery.dbptr WHERE child = 1;"
16096: 
16097:     /* Delete all pointers to any pages that have more than one pointer
16098:     ** to them. Such pages will be treated as root pages when recovering
16099:     ** data.  */
16100:     "DELETE FROM recovery.dbptr WHERE child IN ("
16101:     "  SELECT child FROM recovery.dbptr GROUP BY child HAVING count(*)>1"
16102:     ");"
16103: 
16104:     /* Create the "map" table that will (eventually) contain instructions
16105:     ** for dealing with each page in the db that contains one or more
16106:     ** records. */
16107:     "CREATE TABLE recovery.map("
16108:       "pgno INTEGER PRIMARY KEY, maxlen INT, intkey, root INT"
16109:     ");"
16110: 
16111:     /* Populate table [map]. If there are circular loops of pages in the
16112:     ** database, the following adds all pages in such a loop to the map
16113:     ** as individual root pages. This could be handled better.  */
16114:     "WITH pages(i, maxlen) AS ("
16115:     "  SELECT page_count, ("
16116:     "    SELECT max(field+1) FROM sqlite_dbdata WHERE pgno=page_count"
16117:     "  ) FROM pragma_page_count WHERE page_count>0"
16118:     "    UNION ALL"
16119:     "  SELECT i-1, ("
16120:     "    SELECT max(field+1) FROM sqlite_dbdata WHERE pgno=i-1"
16121:     "  ) FROM pages WHERE i>=2"
16122:     ")"
16123:     "INSERT INTO recovery.map(pgno, maxlen, intkey, root) "
16124:     "  SELECT i, maxlen, NULL, ("
16125:     "    WITH p(orig, pgno, parent) AS ("
16126:     "      SELECT 0, i, (SELECT pgno FROM recovery.dbptr WHERE child=i)"
16127:     "        UNION "
16128:     "      SELECT i, p.parent, "
16129:     "        (SELECT pgno FROM recovery.dbptr WHERE child=p.parent) FROM p"
16130:     "    )"
16131:     "    SELECT pgno FROM p WHERE (parent IS NULL OR pgno = orig)"
16132:     ") "
16133:     "FROM pages WHERE maxlen IS NOT NULL AND i NOT IN freelist;"
16134:     "UPDATE recovery.map AS o SET intkey = ("
16135:     "  SELECT substr(data, 1, 1)==X'0D' FROM sqlite_dbpage WHERE pgno=o.pgno"
16136:     ");"
16137: 
16138:     /* Extract data from page 1 and any linked pages into table
16139:     ** recovery.schema. With the same schema as an sqlite_schema table.  */
16140:     "CREATE TABLE recovery.schema(type, name, tbl_name, rootpage, sql);"
16141:     "INSERT INTO recovery.schema SELECT "
16142:     "  max(CASE WHEN field=0 THEN value ELSE NULL END),"
16143:     "  max(CASE WHEN field=1 THEN value ELSE NULL END),"
16144:     "  max(CASE WHEN field=2 THEN value ELSE NULL END),"
16145:     "  max(CASE WHEN field=3 THEN value ELSE NULL END),"
16146:     "  max(CASE WHEN field=4 THEN value ELSE NULL END)"
16147:     "FROM sqlite_dbdata WHERE pgno IN ("
16148:     "  SELECT pgno FROM recovery.map WHERE root=1"
16149:     ")"
16150:     "GROUP BY pgno, cell;"
16151:     "CREATE INDEX recovery.schema_rootpage ON schema(rootpage);"
16152:   );
16153: 
16154:   /* Open a transaction, then print out all non-virtual, non-"sqlite_%"
16155:   ** CREATE TABLE statements that extracted from the existing schema.  */
16156:   if( rc==SQLITE_OK ){
16157:     sqlite3_stmt *pStmt = 0;
16158:     /* ".recover" might output content in an order which causes immediate
16159:     ** foreign key constraints to be violated. So disable foreign-key
16160:     ** constraint enforcement to prevent problems when running the output
16161:     ** script. */
16162:     raw_printf(pState->out, "PRAGMA foreign_keys=OFF;\n");
16163:     raw_printf(pState->out, "BEGIN;\n");
16164:     raw_printf(pState->out, "PRAGMA writable_schema = on;\n");
16165:     shellPrepare(pState->db, &rc,
16166:         "SELECT sql FROM recovery.schema "
16167:         "WHERE type='table' AND sql LIKE 'create table%'", &pStmt
16168:     );
16169:     while( rc==SQLITE_OK && SQLITE_ROW==sqlite3_step(pStmt) ){
16170:       const char *zCreateTable = (const char*)sqlite3_column_text(pStmt, 0);
16171:       raw_printf(pState->out, "CREATE TABLE IF NOT EXISTS %s;\n",
16172:           &zCreateTable[12]
16173:       );
16174:     }
16175:     shellFinalize(&rc, pStmt);
16176:   }
16177: 
16178:   /* Figure out if an orphan table will be required. And if so, how many
16179:   ** user columns it should contain */
16180:   shellPrepare(pState->db, &rc,
16181:       "SELECT coalesce(max(maxlen), -2) FROM recovery.map WHERE root>1"
16182:       , &pLoop
16183:   );
16184:   if( rc==SQLITE_OK && SQLITE_ROW==sqlite3_step(pLoop) ){
16185:     nOrphan = sqlite3_column_int(pLoop, 0);
16186:   }
16187:   shellFinalize(&rc, pLoop);
16188:   pLoop = 0;
16189: 
16190:   shellPrepare(pState->db, &rc,
16191:       "SELECT pgno FROM recovery.map WHERE root=?", &pPages
16192:   );
16193: 
16194:   shellPrepare(pState->db, &rc,
16195:       "SELECT max(field), group_concat(shell_escape_crnl(quote"
16196:       "(case when (? AND field<0) then NULL else value end)"
16197:       "), ', ')"
16198:       ", min(field) "
16199:       "FROM sqlite_dbdata WHERE pgno = ? AND field != ?"
16200:       "GROUP BY cell", &pCells
16201:   );
16202: 
16203:   /* Loop through each root page. */
16204:   shellPrepare(pState->db, &rc,
16205:       "SELECT root, intkey, max(maxlen) FROM recovery.map"
16206:       " WHERE root>1 GROUP BY root, intkey ORDER BY root=("
16207:       "  SELECT rootpage FROM recovery.schema WHERE name='sqlite_sequence'"
16208:       ")", &pLoop
16209:   );
16210:   while( rc==SQLITE_OK && SQLITE_ROW==sqlite3_step(pLoop) ){
16211:     int iRoot = sqlite3_column_int(pLoop, 0);
16212:     int bIntkey = sqlite3_column_int(pLoop, 1);
16213:     int nCol = sqlite3_column_int(pLoop, 2);
16214:     int bNoop = 0;
16215:     RecoverTable *pTab;
16216: 
16217:     assert( bIntkey==0 || bIntkey==1 );
16218:     pTab = recoverFindTable(pState, &rc, iRoot, bIntkey, nCol, &bNoop);
16219:     if( bNoop || rc ) continue;
16220:     if( pTab==0 ){
16221:       if( pOrphan==0 ){
16222:         pOrphan = recoverOrphanTable(pState, &rc, zLostAndFound, nOrphan);
16223:       }
16224:       pTab = pOrphan;
16225:       if( pTab==0 ) break;
16226:     }
16227: 
16228:     if( 0==sqlite3_stricmp(pTab->zQuoted, "\"sqlite_sequence\"") ){
16229:       raw_printf(pState->out, "DELETE FROM sqlite_sequence;\n");
16230:     }
16231:     sqlite3_bind_int(pPages, 1, iRoot);
16232:     if( bRowids==0 && pTab->iPk<0 ){
16233:       sqlite3_bind_int(pCells, 1, 1);
16234:     }else{
16235:       sqlite3_bind_int(pCells, 1, 0);
16236:     }
16237:     sqlite3_bind_int(pCells, 3, pTab->iPk);
16238: 
16239:     while( rc==SQLITE_OK && SQLITE_ROW==sqlite3_step(pPages) ){
16240:       int iPgno = sqlite3_column_int(pPages, 0);
16241:       sqlite3_bind_int(pCells, 2, iPgno);
16242:       while( rc==SQLITE_OK && SQLITE_ROW==sqlite3_step(pCells) ){
16243:         int nField = sqlite3_column_int(pCells, 0);
16244:         int iMin = sqlite3_column_int(pCells, 2);
16245:         const char *zVal = (const char*)sqlite3_column_text(pCells, 1);
16246: 
16247:         RecoverTable *pTab2 = pTab;
16248:         if( pTab!=pOrphan && (iMin<0)!=bIntkey ){
16249:           if( pOrphan==0 ){
16250:             pOrphan = recoverOrphanTable(pState, &rc, zLostAndFound, nOrphan);
16251:           }
16252:           pTab2 = pOrphan;
16253:           if( pTab2==0 ) break;
16254:         }
16255: 
16256:         nField = nField+1;
16257:         if( pTab2==pOrphan ){
16258:           raw_printf(pState->out,
16259:               "INSERT INTO %s VALUES(%d, %d, %d, %s%s%s);\n",
16260:               pTab2->zQuoted, iRoot, iPgno, nField,
16261:               iMin<0 ? "" : "NULL, ", zVal, pTab2->azlCol[nField]
16262:           );
16263:         }else{
16264:           raw_printf(pState->out, "INSERT INTO %s(%s) VALUES( %s );\n",
16265:               pTab2->zQuoted, pTab2->azlCol[nField], zVal
16266:           );
16267:         }
16268:       }
16269:       shellReset(&rc, pCells);
16270:     }
16271:     shellReset(&rc, pPages);
16272:     if( pTab!=pOrphan ) recoverFreeTable(pTab);
16273:   }
16274:   shellFinalize(&rc, pLoop);
16275:   shellFinalize(&rc, pPages);
16276:   shellFinalize(&rc, pCells);
16277:   recoverFreeTable(pOrphan);
16278: 
16279:   /* The rest of the schema */
16280:   if( rc==SQLITE_OK ){
16281:     sqlite3_stmt *pStmt = 0;
16282:     shellPrepare(pState->db, &rc,
16283:         "SELECT sql, name FROM recovery.schema "
16284:         "WHERE sql NOT LIKE 'create table%'", &pStmt
16285:     );
16286:     while( rc==SQLITE_OK && SQLITE_ROW==sqlite3_step(pStmt) ){
16287:       const char *zSql = (const char*)sqlite3_column_text(pStmt, 0);
16288:       if( sqlite3_strnicmp(zSql, "create virt", 11)==0 ){
16289:         const char *zName = (const char*)sqlite3_column_text(pStmt, 1);
16290:         char *zPrint = shellMPrintf(&rc,
16291:           "INSERT INTO sqlite_schema VALUES('table', %Q, %Q, 0, %Q)",
16292:           zName, zName, zSql
16293:         );
