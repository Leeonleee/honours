9932: /*
9933: ** Utility methods to decode 16 and 32-bit big-endian unsigned integers.
9934: */
9935: static unsigned int get_uint16(unsigned char *a){
9936:   return (a[0]<<8)|a[1];
9937: }
9938: static unsigned int get_uint32(unsigned char *a){
9939:   return ((unsigned int)a[0]<<24)
9940:        | ((unsigned int)a[1]<<16)
9941:        | ((unsigned int)a[2]<<8)
9942:        | ((unsigned int)a[3]);
9943: }
