4570: /*
4571: ** Expand the Decimal so that it has a least nDigit digits and nFrac
4572: ** digits to the right of the decimal point.
4573: */
4574: static void decimal_expand(Decimal *p, int nDigit, int nFrac){
4575:   int nAddSig;
4576:   int nAddFrac;
4577:   if( p==0 ) return;
4578:   nAddFrac = nFrac - p->nFrac;
4579:   nAddSig = (nDigit - p->nDigit) - nAddFrac;
4580:   if( nAddFrac==0 && nAddSig==0 ) return;
4581:   p->a = sqlite3_realloc64(p->a, nDigit+1);
4582:   if( p->a==0 ){
4583:     p->oom = 1;
4584:     return;
4585:   }
4586:   if( nAddSig ){
4587:     memmove(p->a+nAddSig, p->a, p->nDigit);
4588:     memset(p->a, 0, nAddSig);
4589:     p->nDigit += nAddSig;
4590:   }
4591:   if( nAddFrac ){
4592:     memset(p->a+p->nDigit, 0, nAddFrac);
4593:     p->nDigit += nAddFrac;
4594:     p->nFrac += nAddFrac;
4595:   }
4596: }
