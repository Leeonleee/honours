2640: /*
2641: ** Advance an fsdir_cursor to its next row of output.
2642: */
2643: static int fsdirNext(sqlite3_vtab_cursor *cur){
2644:   fsdir_cursor *pCur = (fsdir_cursor*)cur;
2645:   mode_t m = pCur->sStat.st_mode;
2646: 
2647:   pCur->iRowid++;
2648:   if( S_ISDIR(m) ){
2649:     /* Descend into this directory */
2650:     int iNew = pCur->iLvl + 1;
2651:     FsdirLevel *pLvl;
2652:     if( iNew>=pCur->nLvl ){
2653:       int nNew = iNew+1;
2654:       sqlite3_int64 nByte = nNew*sizeof(FsdirLevel);
2655:       FsdirLevel *aNew = (FsdirLevel*)sqlite3_realloc64(pCur->aLvl, nByte);
2656:       if( aNew==0 ) return SQLITE_NOMEM;
2657:       memset(&aNew[pCur->nLvl], 0, sizeof(FsdirLevel)*(nNew-pCur->nLvl));
2658:       pCur->aLvl = aNew;
2659:       pCur->nLvl = nNew;
2660:     }
2661:     pCur->iLvl = iNew;
2662:     pLvl = &pCur->aLvl[iNew];
2663: 
2664:     pLvl->zDir = pCur->zPath;
2665:     pCur->zPath = 0;
2666:     pLvl->pDir = opendir(pLvl->zDir);
2667:     if( pLvl->pDir==0 ){
2668:       fsdirSetErrmsg(pCur, "cannot read directory: %s", pCur->zPath);
2669:       return SQLITE_ERROR;
2670:     }
2671:   }
2672: 
2673:   while( pCur->iLvl>=0 ){
2674:     FsdirLevel *pLvl = &pCur->aLvl[pCur->iLvl];
2675:     struct dirent *pEntry = readdir(pLvl->pDir);
2676:     if( pEntry ){
2677:       if( pEntry->d_name[0]=='.' ){
2678:        if( pEntry->d_name[1]=='.' && pEntry->d_name[2]=='\0' ) continue;
2679:        if( pEntry->d_name[1]=='\0' ) continue;
2680:       }
2681:       sqlite3_free(pCur->zPath);
2682:       pCur->zPath = sqlite3_mprintf("%s/%s", pLvl->zDir, pEntry->d_name);
2683:       if( pCur->zPath==0 ) return SQLITE_NOMEM;
2684:       if( fileLinkStat(pCur->zPath, &pCur->sStat) ){
2685:         fsdirSetErrmsg(pCur, "cannot stat file: %s", pCur->zPath);
2686:         return SQLITE_ERROR;
2687:       }
2688:       return SQLITE_OK;
2689:     }
2690:     closedir(pLvl->pDir);
2691:     sqlite3_free(pLvl->zDir);
2692:     pLvl->pDir = 0;
2693:     pLvl->zDir = 0;
2694:     pCur->iLvl--;
2695:   }
2696: 
2697:   /* EOF */
2698:   sqlite3_free(pCur->zPath);
2699:   pCur->zPath = 0;
2700:   return SQLITE_OK;
2701: }
