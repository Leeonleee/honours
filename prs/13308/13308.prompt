19192: /*
19193: ** Test to see if a line consists entirely of whitespace.
19194: */
19195: static int _all_whitespace(const char *z){
19196:   for(; *z; z++){
19197:     if( IsSpace(z[0]) ) continue;
19198:     if( *z=='/' && z[1]=='*' ){
19199:       z += 2;
19200:       while( *z && (*z!='*' || z[1]!='/') ){ z++; }
19201:       if( *z==0 ) return 0;
19202:       z++;
19203:       continue;
19204:     }
19205:     if( *z=='-' && z[1]=='-' ){
19206:       z += 2;
19207:       while( *z && *z!='\n' ){ z++; }
19208:       if( *z==0 ) return 1;
19209:       continue;
19210:     }
19211:     return 0;
19212:   }
19213:   return 1;
19214: }
