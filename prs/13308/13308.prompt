4507: /*
4508: ** Compare to Decimal objects.  Return negative, 0, or positive if the
4509: ** first object is less than, equal to, or greater than the second.
4510: **
4511: ** Preconditions for this routine:
4512: **
4513: **    pA!=0
4514: **    pA->isNull==0
4515: **    pB!=0
4516: **    pB->isNull==0
4517: */
4518: static int decimal_cmp(const Decimal *pA, const Decimal *pB){
4519:   int nASig, nBSig, rc, n;
4520:   if( pA->sign!=pB->sign ){
4521:     return pA->sign ? -1 : +1;
4522:   }
4523:   if( pA->sign ){
4524:     const Decimal *pTemp = pA;
4525:     pA = pB;
4526:     pB = pTemp;
4527:   }
4528:   nASig = pA->nDigit - pA->nFrac;
4529:   nBSig = pB->nDigit - pB->nFrac;
4530:   if( nASig!=nBSig ){
4531:     return nASig - nBSig;
4532:   }
4533:   n = pA->nDigit;
4534:   if( n>pB->nDigit ) n = pB->nDigit;
4535:   rc = memcmp(pA->a, pB->a, n);
4536:   if( rc==0 ){
4537:     rc = pA->nDigit - pB->nDigit;
4538:   }
4539:   return rc;
4540: }
