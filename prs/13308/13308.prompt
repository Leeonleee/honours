1912: /*
1913: ** Implementation of the sha3(X,SIZE) function.
1914: **
1915: ** Return a BLOB which is the SIZE-bit SHA3 hash of X.  The default
1916: ** size is 256.  If X is a BLOB, it is hashed as is.
1917: ** For all other non-NULL types of input, X is converted into a UTF-8 string
1918: ** and the string is hashed without the trailing 0x00 terminator.  The hash
1919: ** of a NULL value is NULL.
1920: */
1921: static void sha3Func(
1922:   sqlite3_context *context,
1923:   int argc,
1924:   sqlite3_value **argv
1925: ){
1926:   SHA3Context cx;
1927:   int eType = sqlite3_value_type(argv[0]);
1928:   int nByte = sqlite3_value_bytes(argv[0]);
1929:   int iSize;
1930:   if( argc==1 ){
1931:     iSize = 256;
1932:   }else{
1933:     iSize = sqlite3_value_int(argv[1]);
1934:     if( iSize!=224 && iSize!=256 && iSize!=384 && iSize!=512 ){
1935:       sqlite3_result_error(context, "SHA3 size should be one of: 224 256 "
1936:                                     "384 512", -1);
1937:       return;
1938:     }
1939:   }
1940:   if( eType==SQLITE_NULL ) return;
1941:   SHA3Init(&cx, iSize);
1942:   if( eType==SQLITE_BLOB ){
1943:     SHA3Update(&cx, sqlite3_value_blob(argv[0]), nByte);
1944:   }else{
1945:     SHA3Update(&cx, sqlite3_value_text(argv[0]), nByte);
1946:   }
1947:   sqlite3_result_blob(context, SHA3Final(&cx), iSize/8, SQLITE_TRANSIENT);
1948: }
