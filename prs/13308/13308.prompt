3689: /*
3690: ** Add the append-mark onto the end of the file.
3691: */
3692: static int apndWriteMark(ApndFile *p, sqlite3_file *pFile){
3693:   int i;
3694:   unsigned char a[APND_MARK_SIZE];
3695:   memcpy(a, APND_MARK_PREFIX, APND_MARK_PREFIX_SZ);
3696:   for(i=0; i<8; i++){
3697:     a[APND_MARK_PREFIX_SZ+i] = (p->iPgOne >> (56 - i*8)) & 0xff;
3698:   }
3699:   return pFile->pMethods->xWrite(pFile, a, APND_MARK_SIZE, p->iMark);
3700: }
