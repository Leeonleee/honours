6199: /*
6200: ** Return values of columns for the row at which the series_cursor
6201: ** is currently pointing.
6202: */
6203: static int zipfileColumn(
6204:   sqlite3_vtab_cursor *cur,   /* The cursor */
6205:   sqlite3_context *ctx,       /* First argument to sqlite3_result_...() */
6206:   int i                       /* Which column to return */
6207: ){
6208:   ZipfileCsr *pCsr = (ZipfileCsr*)cur;
6209:   ZipfileCDS *pCDS = &pCsr->pCurrent->cds;
6210:   int rc = SQLITE_OK;
6211:   switch( i ){
6212:     case 0:   /* name */
6213:       sqlite3_result_text(ctx, pCDS->zFile, -1, SQLITE_TRANSIENT);
6214:       break;
6215:     case 1:   /* mode */
6216:       /* TODO: Whether or not the following is correct surely depends on
6217:       ** the platform on which the archive was created.  */
6218:       sqlite3_result_int(ctx, pCDS->iExternalAttr >> 16);
6219:       break;
6220:     case 2: { /* mtime */
6221:       sqlite3_result_int64(ctx, pCsr->pCurrent->mUnixTime);
6222:       break;
6223:     }
6224:     case 3: { /* sz */
6225:       if( sqlite3_vtab_nochange(ctx)==0 ){
6226:         sqlite3_result_int64(ctx, pCDS->szUncompressed);
6227:       }
6228:       break;
6229:     }
6230:     case 4:   /* rawdata */
6231:       if( sqlite3_vtab_nochange(ctx) ) break;
6232:     case 5: { /* data */
6233:       if( i==4 || pCDS->iCompression==0 || pCDS->iCompression==8 ){
6234:         int sz = pCDS->szCompressed;
6235:         int szFinal = pCDS->szUncompressed;
6236:         if( szFinal>0 ){
6237:           u8 *aBuf;
6238:           u8 *aFree = 0;
6239:           if( pCsr->pCurrent->aData ){
6240:             aBuf = pCsr->pCurrent->aData;
6241:           }else{
6242:             aBuf = aFree = sqlite3_malloc64(sz);
6243:             if( aBuf==0 ){
6244:               rc = SQLITE_NOMEM;
6245:             }else{
6246:               FILE *pFile = pCsr->pFile;
6247:               if( pFile==0 ){
6248:                 pFile = ((ZipfileTab*)(pCsr->base.pVtab))->pWriteFd;
6249:               }
6250:               rc = zipfileReadData(pFile, aBuf, sz, pCsr->pCurrent->iDataOff,
6251:                   &pCsr->base.pVtab->zErrMsg
6252:               );
6253:             }
6254:           }
6255:           if( rc==SQLITE_OK ){
6256:             if( i==5 && pCDS->iCompression ){
6257:               zipfileInflate(ctx, aBuf, sz, szFinal);
6258:             }else{
6259:               sqlite3_result_blob(ctx, aBuf, sz, SQLITE_TRANSIENT);
6260:             }
6261:           }
6262:           sqlite3_free(aFree);
6263:         }else{
6264:           /* Figure out if this is a directory or a zero-sized file. Consider
6265:           ** it to be a directory either if the mode suggests so, or if
6266:           ** the final character in the name is '/'.  */
6267:           u32 mode = pCDS->iExternalAttr >> 16;
6268:           if( !(mode & S_IFDIR) && pCDS->zFile[pCDS->nFile-1]!='/' ){
6269:             sqlite3_result_blob(ctx, "", 0, SQLITE_STATIC);
6270:           }
6271:         }
6272:       }
6273:       break;
6274:     }
6275:     case 6:   /* method */
6276:       sqlite3_result_int(ctx, pCDS->iCompression);
6277:       break;
6278:     default:  /* z */
6279:       assert( i==7 );
6280:       sqlite3_result_int64(ctx, pCsr->iId);
6281:       break;
6282:   }
6283: 
6284:   return rc;
6285: }
