8650: static int idxCreateFromWhere(
8651:   sqlite3expert *p,
8652:   IdxScan *pScan,                 /* Create indexes for this scan */
8653:   IdxConstraint *pTail            /* range/ORDER BY constraints for inclusion */
8654: ){
8655:   IdxConstraint *p1 = 0;
8656:   IdxConstraint *pCon;
8657:   int rc;
8658: 
8659:   /* Gather up all the == constraints. */
8660:   for(pCon=pScan->pEq; pCon; pCon=pCon->pNext){
8661:     if( !idxFindConstraint(p1, pCon) && !idxFindConstraint(pTail, pCon) ){
8662:       pCon->pLink = p1;
8663:       p1 = pCon;
8664:     }
8665:   }
8666: 
8667:   /* Create an index using the == constraints collected above. And the
8668:   ** range constraint/ORDER BY terms passed in by the caller, if any. */
8669:   rc = idxCreateFromCons(p, pScan, p1, pTail);
8670: 
8671:   /* If no range/ORDER BY passed by the caller, create a version of the
8672:   ** index for each range constraint.  */
8673:   if( pTail==0 ){
8674:     for(pCon=pScan->pRange; rc==SQLITE_OK && pCon; pCon=pCon->pNext){
8675:       assert( pCon->pLink==0 );
8676:       if( !idxFindConstraint(p1, pCon) && !idxFindConstraint(pTail, pCon) ){
8677:         rc = idxCreateFromCons(p, pScan, p1, pCon);
8678:       }
8679:     }
8680:   }
8681: 
8682:   return rc;
8683: }
