6611: /*
6612: ** Both (const char*) arguments point to nul-terminated strings. Argument
6613: ** nB is the value of strlen(zB). This function returns 0 if the strings are
6614: ** identical, ignoring any trailing '/' character in either path.  */
6615: static int zipfileComparePath(const char *zA, const char *zB, int nB){
6616:   int nA = (int)strlen(zA);
6617:   if( nA>0 && zA[nA-1]=='/' ) nA--;
6618:   if( nB>0 && zB[nB-1]=='/' ) nB--;
6619:   if( nA==nB && memcmp(zA, zB, nA)==0 ) return 0;
6620:   return 1;
6621: }
