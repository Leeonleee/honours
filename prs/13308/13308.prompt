12912: 
12913:   assert( pState->expert.pExpert==0 );
12914:   memset(&pState->expert, 0, sizeof(ExpertInfo));
12915: 
12916:   for(i=1; rc==SQLITE_OK && i<nArg; i++){
12917:     char *z = azArg[i];
12918:     int n;
12919:     if( z[0]=='-' && z[1]=='-' ) z++;
12920:     n = strlen30(z);
12921:     if( n>=2 && 0==strncmp(z, "-verbose", n) ){
12922:       pState->expert.bVerbose = 1;
12923:     }
12924:     else if( n>=2 && 0==strncmp(z, "-sample", n) ){
12925:       if( i==(nArg-1) ){
12926:         raw_printf(stderr, "option requires an argument: %s\n", z);
12927:         rc = SQLITE_ERROR;
12928:       }else{
12929:         iSample = (int)integerValue(azArg[++i]);
12930:         if( iSample<0 || iSample>100 ){
12931:           raw_printf(stderr, "value out of range: %s\n", azArg[i]);
12932:           rc = SQLITE_ERROR;
12933:         }
12934:       }
12935:     }
12936:     else{
12937:       raw_printf(stderr, "unknown option: %s\n", z);
12938:       rc = SQLITE_ERROR;
12939:     }
12940:   }
12941: 
12942:   if( rc==SQLITE_OK ){
12943:     pState->expert.pExpert = sqlite3_expert_new(pState->db, &zErr);
12944:     if( pState->expert.pExpert==0 ){
12945:       raw_printf(stderr, "sqlite3_expert_new: %s\n", zErr);
12946:       rc = SQLITE_ERROR;
12947:     }else{
12948:       sqlite3_expert_config(
12949:           pState->expert.pExpert, EXPERT_CONFIG_SAMPLE, iSample
12950:       );
12951:     }
12952:   }
12953: 
12954:   return rc;
12955: }
