7393: /*
7394: ** Implementation of the "sqlar_compress(X)" SQL function.
7395: **
7396: ** If the type of X is SQLITE_BLOB, and compressing that blob using
7397: ** zlib utility function compress() yields a smaller blob, return the
7398: ** compressed blob. Otherwise, return a copy of X.
7399: **
7400: ** SQLar uses the "zlib format" for compressed content.  The zlib format
7401: ** contains a two-byte identification header and a four-byte checksum at
7402: ** the end.  This is different from ZIP which uses the raw deflate format.
7403: **
7404: ** Future enhancements to SQLar might add support for new compression formats.
7405: ** If so, those new formats will be identified by alternative headers in the
7406: ** compressed data.
7407: */
7408: static void sqlarCompressFunc(
7409:   sqlite3_context *context,
7410:   int argc,
7411:   sqlite3_value **argv
7412: ){
7413:   assert( argc==1 );
7414:   if( sqlite3_value_type(argv[0])==SQLITE_BLOB ){
7415:     const Bytef *pData = sqlite3_value_blob(argv[0]);
7416:     uLong nData = sqlite3_value_bytes(argv[0]);
7417:     uLongf nOut = compressBound(nData);
7418:     Bytef *pOut;
7419: 
7420:     pOut = (Bytef*)sqlite3_malloc(nOut);
7421:     if( pOut==0 ){
7422:       sqlite3_result_error_nomem(context);
7423:       return;
7424:     }else{
7425:       if( Z_OK!=compress(pOut, &nOut, pData, nData) ){
7426:         sqlite3_result_error(context, "error in compress()", -1);
7427:       }else if( nOut<nData ){
7428:         sqlite3_result_blob(context, pOut, nOut, SQLITE_TRANSIENT);
7429:       }else{
7430:         sqlite3_result_value(context, argv[0]);
7431:       }
7432:       sqlite3_free(pOut);
7433:     }
7434:   }else{
7435:     sqlite3_result_value(context, argv[0]);
7436:   }
7437: }
7438: 
7439: /*
7440: ** Implementation of the "sqlar_uncompress(X,SZ)" SQL function
7441: **
7442: ** Parameter SZ is interpreted as an integer. If it is less than or
7443: ** equal to zero, then this function returns a copy of X. Or, if
7444: ** SZ is equal to the size of X when interpreted as a blob, also
7445: ** return a copy of X. Otherwise, decompress blob X using zlib
7446: ** utility function uncompress() and return the results (another
7447: ** blob).
7448: */
7449: static void sqlarUncompressFunc(
7450:   sqlite3_context *context,
7451:   int argc,
7452:   sqlite3_value **argv
7453: ){
7454:   uLong nData;
7455:   uLongf sz;
7456: 
7457:   assert( argc==2 );
7458:   sz = sqlite3_value_int(argv[1]);
7459: 
7460:   if( sz<=0 || sz==(nData = sqlite3_value_bytes(argv[0])) ){
7461:     sqlite3_result_value(context, argv[0]);
7462:   }else{
7463:     const Bytef *pData= sqlite3_value_blob(argv[0]);
7464:     Bytef *pOut = sqlite3_malloc(sz);
7465:     if( Z_OK!=uncompress(pOut, &sz, pData, nData) ){
7466:       sqlite3_result_error(context, "error in uncompress()", -1);
7467:     }else{
7468:       sqlite3_result_blob(context, pOut, sz, SQLITE_TRANSIENT);
7469:     }
7470:     sqlite3_free(pOut);
7471:   }
7472: }
