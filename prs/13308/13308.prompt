19472: /*
19473: ** Read input from the file given by sqliterc_override.  Or if that
19474: ** parameter is NULL, take input from ~/.duckdbrc
19475: **
19476: ** Returns the number of errors.
19477: */
19478: static void process_sqliterc(
19479:   ShellState *p,                  /* Configuration data */
19480:   const char *sqliterc_override   /* Name of config file. NULL to use default */
19481: ){
19482:   char *home_dir = NULL;
19483:   const char *sqliterc = sqliterc_override;
19484:   char *zBuf = 0;
19485:   FILE *inSaved = p->in;
19486:   int savedLineno = p->lineno;
19487: 
19488:   if (sqliterc == NULL) {
19489:     home_dir = find_home_dir(0);
19490:     if( home_dir==0 ){
19491:       raw_printf(stderr, "-- warning: cannot find home directory;"
19492:                       " cannot read ~/.duckdbrc\n");
19493:       return;
19494:     }
19495:     zBuf = sqlite3_mprintf("%s/.duckdbrc",home_dir);
19496:     sqliterc = zBuf;
19497:   }
19498:   p->in = fopen(sqliterc,"rb");
19499:   if( p->in ){
19500:     if( stdin_is_interactive ){
19501:       utf8_printf(stderr,"-- Loading resources from %s\n",sqliterc);
19502:     }
19503:     process_input(p);
19504:     fclose(p->in);
19505:   }
19506:   p->in = inSaved;
19507:   p->lineno = savedLineno;
19508:   sqlite3_free(zBuf);
19509: }
