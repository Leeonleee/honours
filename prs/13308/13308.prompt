14213:       const char *line = azHelp[line_idx];
14214:       if (line[0] != '.') {
14215:         continue;
14216:       }
14217: 	  int found_match = 1;
14218:       size_t line_pos;
14219:       for(line_pos = 0; !IsSpace(line[line_pos]) && line[line_pos] && line_pos + 1 < sizeof(zBuf); line_pos++) {
14220:         zBuf[line_pos] = line[line_pos];
14221: 		if (line_pos < nLine && line[line_pos] != zLine[line_pos]) {
14222: 			// only match prefixes for auto-completion, i.e. ".sh" matches ".shell"
14223: 			found_match = 0;
14224: 			break;
14225: 		}
14226:       }
14227:       zBuf[line_pos] = '\0';
14228:       if (found_match && line_pos >= nLine) {
14229:         linenoiseAddCompletion(lc, zBuf);
14230:       }
14231: 	}
14232:     return;
14233:   }
14234:   if(zLine[0]=='#') return;
14235: //  if( i==nLine-1 ) return;
14236:   zSql = sqlite3_mprintf("CALL sql_auto_complete(%Q)", zLine);
14237:   sqlite3 *localDb = NULL;
14238:   if (!globalDb) {
14239:     sqlite3_open(":memory:", &localDb);
14240:     sqlite3_prepare_v2(localDb, zSql, -1, &pStmt, 0);
14241:   } else {
14242:     sqlite3_prepare_v2(globalDb, zSql, -1, &pStmt, 0);
14243:   }
14244:   sqlite3_free(zSql);
14245:   while( sqlite3_step(pStmt)==SQLITE_ROW ){
14246:     const char *zCompletion = (const char*)sqlite3_column_text(pStmt, 0);
14247: 	int nCompletion = sqlite3_column_bytes(pStmt, 0);
14248: 	int iStart = sqlite3_column_int(pStmt, 1);
14249:     if( iStart+nCompletion < sizeof(zBuf)-1 ){
14250: 		if (!copiedSuggestion) {
14251: 			memcpy(zBuf, zLine, iStart);
14252: 			copiedSuggestion = 1;
14253: 		}
14254:       memcpy(zBuf+iStart, zCompletion, nCompletion+1);
14255:       linenoiseAddCompletion(lc, zBuf);
14256:     }
14257:   }
14258:   sqlite3_finalize(pStmt);
14259:   if (localDb) {
14260: 	  sqlite3_close(localDb);
14261:   }
14262: }
