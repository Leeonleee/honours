676: /*
677: ** This routine reads a line of text from FILE in, stores
678: ** the text in memory obtained from malloc() and returns a pointer
679: ** to the text.  NULL is returned at end of file, or if malloc()
680: ** fails.
681: **
682: ** If zLine is not NULL then it is a malloced buffer returned from
683: ** a previous call to this routine that may be reused.
684: */
685: static char *local_getline(char *zLine, FILE *in){
686:   int nLine = zLine==0 ? 0 : 100;
687:   int n = 0;
688: 
689: #if defined(_WIN32) || defined(WIN32)
690:   int is_stdin = stdin_is_interactive && in==stdin;
691:   int is_utf8 = 0;
692:   if (is_stdin && win_utf8_mode) {
693:       if (SetConsoleCP(CP_UTF8)) {
694:           is_utf8 = 1;
695:       }
696:   }
697: #endif
698:   while( 1 ){
699:     if( n+100>nLine ){
700:       nLine = nLine*2 + 100;
701:       zLine = realloc(zLine, nLine);
702:       if( zLine==0 ) shell_out_of_memory();
703:     }
704:     if( fgets(&zLine[n], nLine - n, in)==0 ){
705:       if( n==0 ){
706:         free(zLine);
707:         return 0;
708:       }
709:       zLine[n] = 0;
710:       break;
711:     }
712:     while( zLine[n] ) n++;
713:     if( n>0 && zLine[n-1]=='\n' ){
714:       n--;
715:       if( n>0 && zLine[n-1]=='\r' ) n--;
716:       zLine[n] = 0;
717:       break;
718:     }
719:   }
720: #if defined(_WIN32) || defined(WIN32)
721:   /* For interactive input on Windows systems, translate the
722:   ** multi-byte characterset characters into UTF-8. */
723:   if(is_stdin && !is_utf8){
724:     char *zTrans = sqlite3_win32_mbcs_to_utf8_v2(zLine, 0);
725:     if( zTrans ){
726:       int nTrans = strlen30(zTrans)+1;
727:       if( nTrans>nLine ){
728:         zLine = realloc(zLine, nTrans);
729:         if( zLine==0 ) shell_out_of_memory();
730:       }
731:       memcpy(zLine, zTrans, nTrans);
732:       sqlite3_free(zTrans);
733:     }
734:   }
735: #endif /* defined(_WIN32) || defined(WIN32) */
736:   return zLine;
737: }
