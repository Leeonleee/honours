6916: /*
6917: ** Serialize the CDS structure into buffer aBuf[]. Return the number
6918: ** of bytes written.
6919: */
6920: static int zipfileSerializeCDS(ZipfileEntry *pEntry, u8 *aBuf){
6921:   u8 *a = aBuf;
6922:   ZipfileCDS *pCDS = &pEntry->cds;
6923: 
6924:   if( pEntry->aExtra==0 ){
6925:     pCDS->nExtra = 9;
6926:   }
6927: 
6928:   zipfileWrite32(a, ZIPFILE_SIGNATURE_CDS);
6929:   zipfileWrite16(a, pCDS->iVersionMadeBy);
6930:   zipfileWrite16(a, pCDS->iVersionExtract);
6931:   zipfileWrite16(a, pCDS->flags);
6932:   zipfileWrite16(a, pCDS->iCompression);
6933:   zipfileWrite16(a, pCDS->mTime);
6934:   zipfileWrite16(a, pCDS->mDate);
6935:   zipfileWrite32(a, pCDS->crc32);
6936:   zipfileWrite32(a, pCDS->szCompressed);
6937:   zipfileWrite32(a, pCDS->szUncompressed);
6938:   assert( a==&aBuf[ZIPFILE_CDS_NFILE_OFF] );
6939:   zipfileWrite16(a, pCDS->nFile);
6940:   zipfileWrite16(a, pCDS->nExtra);
6941:   zipfileWrite16(a, pCDS->nComment);
6942:   zipfileWrite16(a, pCDS->iDiskStart);
6943:   zipfileWrite16(a, pCDS->iInternalAttr);
6944:   zipfileWrite32(a, pCDS->iExternalAttr);
6945:   zipfileWrite32(a, pCDS->iOffset);
6946: 
6947:   memcpy(a, pCDS->zFile, pCDS->nFile);
6948:   a += pCDS->nFile;
6949: 
6950:   if( pEntry->aExtra ){
6951:     int n = (int)pCDS->nExtra + (int)pCDS->nComment;
6952:     memcpy(a, pEntry->aExtra, n);
6953:     a += n;
6954:   }else{
6955:     assert( pCDS->nExtra==9 );
6956:     zipfileWrite16(a, ZIPFILE_EXTRA_TIMESTAMP);
6957:     zipfileWrite16(a, 5);
6958:     *a++ = 0x01;
6959:     zipfileWrite32(a, pEntry->mUnixTime);
6960:   }
6961: 
6962:   return a-aBuf;
6963: }
