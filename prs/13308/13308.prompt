3327: /*
3328: ** This method is called to "rewind" the completion_cursor object back
3329: ** to the first row of output.  This method is always called at least
3330: ** once prior to any call to completionColumn() or completionRowid() or
3331: ** completionEof().
3332: */
3333: static int completionFilter(
3334:   sqlite3_vtab_cursor *pVtabCursor,
3335:   int idxNum, const char *idxStr,
3336:   int argc, sqlite3_value **argv
3337: ){
3338:   completion_cursor *pCur = (completion_cursor *)pVtabCursor;
3339:   int iArg = 0;
3340:   (void)(idxStr);   /* Unused parameter */
3341:   (void)(argc);     /* Unused parameter */
3342:   completionCursorReset(pCur);
3343:   if( idxNum & 1 ){
3344:     pCur->nPrefix = sqlite3_value_bytes(argv[iArg]);
3345:     if( pCur->nPrefix>0 ){
3346:       pCur->zPrefix = sqlite3_mprintf("%s", sqlite3_value_text(argv[iArg]));
3347:       if( pCur->zPrefix==0 ) return SQLITE_NOMEM;
3348:     }
3349:     iArg = 1;
3350:   }
3351:   if( idxNum & 2 ){
3352:     pCur->nLine = sqlite3_value_bytes(argv[iArg]);
3353:     if( pCur->nLine>0 ){
3354:       pCur->zLine = sqlite3_mprintf("%s", sqlite3_value_text(argv[iArg]));
3355:       if( pCur->zLine==0 ) return SQLITE_NOMEM;
3356:     }
3357:   }
3358:   if( pCur->zLine!=0 && pCur->zPrefix==0 ){
3359:     int i = pCur->nLine;
3360:     while( i>0 && (isalnum(pCur->zLine[i-1]) || pCur->zLine[i-1]=='_') ){
3361:       i--;
3362:     }
3363:     pCur->nPrefix = pCur->nLine - i;
3364:     if( pCur->nPrefix>0 ){
3365:       pCur->zPrefix = sqlite3_mprintf("%.*s", pCur->nPrefix, pCur->zLine + i);
3366:       if( pCur->zPrefix==0 ) return SQLITE_NOMEM;
3367:     }
3368:   }
3369:   pCur->iRowid = 0;
3370:   pCur->ePhase = COMPLETION_FIRST_PHASE;
3371:   return completionNext(pVtabCursor);
3372: }
