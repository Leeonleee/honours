8638: /*
8639: ** Return true if list pList (linked by IdxConstraint.pLink) contains
8640: ** a constraint compatible with *p. Otherwise return false.
8641: */
8642: static int idxFindConstraint(IdxConstraint *pList, IdxConstraint *p){
8643:   IdxConstraint *pCmp;
8644:   for(pCmp=pList; pCmp; pCmp=pCmp->pLink){
8645:     if( p->iCol==pCmp->iCol ) return 1;
8646:   }
8647:   return 0;
8648: }
