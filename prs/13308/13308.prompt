8107: static int expertBestIndex(sqlite3_vtab *pVtab, sqlite3_index_info *pIdxInfo){
8108:   ExpertVtab *p = (ExpertVtab*)pVtab;
8109:   int rc = SQLITE_OK;
8110:   int n = 0;
8111:   IdxScan *pScan;
8112:   const int opmask =
8113:     SQLITE_INDEX_CONSTRAINT_EQ | SQLITE_INDEX_CONSTRAINT_GT |
8114:     SQLITE_INDEX_CONSTRAINT_LT | SQLITE_INDEX_CONSTRAINT_GE |
8115:     SQLITE_INDEX_CONSTRAINT_LE;
8116: 
8117:   pScan = idxMalloc(&rc, sizeof(IdxScan));
8118:   if( pScan ){
8119:     int i;
8120: 
8121:     /* Link the new scan object into the list */
8122:     pScan->pTab = p->pTab;
8123:     pScan->pNextScan = p->pExpert->pScan;
8124:     p->pExpert->pScan = pScan;
8125: 
8126:     /* Add the constraints to the IdxScan object */
8127:     for(i=0; i<pIdxInfo->nConstraint; i++){
8128:       struct sqlite3_index_constraint *pCons = &pIdxInfo->aConstraint[i];
8129:       if( pCons->usable
8130:        && pCons->iColumn>=0
8131:        && p->pTab->aCol[pCons->iColumn].iPk==0
8132:        && (pCons->op & opmask)
8133:       ){
8134:         IdxConstraint *pNew;
8135:         const char *zColl = sqlite3_vtab_collation(pIdxInfo, i);
8136:         pNew = idxNewConstraint(&rc, zColl);
8137:         if( pNew ){
8138:           pNew->iCol = pCons->iColumn;
8139:           if( pCons->op==SQLITE_INDEX_CONSTRAINT_EQ ){
8140:             pNew->pNext = pScan->pEq;
8141:             pScan->pEq = pNew;
8142:           }else{
8143:             pNew->bRange = 1;
8144:             pNew->pNext = pScan->pRange;
8145:             pScan->pRange = pNew;
8146:           }
8147:         }
8148:         n++;
8149:         pIdxInfo->aConstraintUsage[i].argvIndex = n;
8150:       }
8151:     }
8152: 
8153:     /* Add the ORDER BY to the IdxScan object */
8154:     for(i=pIdxInfo->nOrderBy-1; i>=0; i--){
8155:       int iCol = pIdxInfo->aOrderBy[i].iColumn;
8156:       if( iCol>=0 ){
8157:         IdxConstraint *pNew = idxNewConstraint(&rc, p->pTab->aCol[iCol].zColl);
8158:         if( pNew ){
8159:           pNew->iCol = iCol;
8160:           pNew->bDesc = pIdxInfo->aOrderBy[i].desc;
8161:           pNew->pNext = pScan->pOrder;
8162:           pNew->pLink = pScan->pOrder;
8163:           pScan->pOrder = pNew;
8164:           n++;
8165:         }
8166:       }
8167:     }
8168:   }
8169: 
8170:   pIdxInfo->estimatedCost = 1000000.0 / (n+1);
8171:   return rc;
8172: }
