1322: /*
1323: ** Implementation of the POSIX readdir() function using the MSVCRT.
1324: */
1325: LPDIRENT readdir(
1326:   LPDIR dirp
1327: ){
1328:   struct _finddata_t data;
1329: 
1330:   if( dirp==NULL ) return NULL;
1331: 
1332:   if( dirp->d_first.d_ino==0 ){
1333:     dirp->d_first.d_ino++;
1334:     dirp->d_next.d_ino++;
1335: 
1336:     return &dirp->d_first;
1337:   }
1338: 
1339: next:
1340: 
1341:   memset(&data, 0, sizeof(struct _finddata_t));
1342:   if( _findnext(dirp->d_handle, &data)==-1 ) return NULL;
1343: 
1344:   /* TODO: Remove this block to allow hidden and/or system files. */
1345:   if( is_filtered(data) ) goto next;
1346: 
1347:   dirp->d_next.d_ino++;
1348:   dirp->d_next.d_attributes = data.attrib;
1349:   strncpy(dirp->d_next.d_name, data.name, NAME_MAX);
1350:   dirp->d_next.d_name[NAME_MAX] = '\0';
1351: 
1352:   return &dirp->d_next;
1353: }
