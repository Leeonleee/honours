7991: /*
7992: ** Prepare an SQL statement using the results of a printf() formatting.
7993: */
7994: static int idxPrintfPrepareStmt(
7995:   sqlite3 *db,                    /* Database handle to compile against */
7996:   sqlite3_stmt **ppStmt,          /* OUT: Compiled SQL statement */
7997:   char **pzErrmsg,                /* OUT: sqlite3_malloc()ed error message */
7998:   const char *zFmt,               /* printf() format of SQL statement */
7999:   ...                             /* Trailing printf() arguments */
8000: ){
8001:   va_list ap;
8002:   int rc;
8003:   char *zSql;
8004:   va_start(ap, zFmt);
8005:   zSql = sqlite3_vmprintf(zFmt, ap);
8006:   if( zSql==0 ){
8007:     rc = SQLITE_NOMEM;
8008:   }else{
8009:     rc = idxPrepareStmt(db, ppStmt, pzErrmsg, zSql);
8010:     sqlite3_free(zSql);
8011:   }
8012:   va_end(ap);
8013:   return rc;
8014: }
