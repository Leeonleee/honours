19233: /*
19234: ** We need a default sqlite3_complete() implementation to use in case
19235: ** the shell is compiled with SQLITE_OMIT_COMPLETE.  The default assumes
19236: ** any arbitrary text is a complete SQL statement.  This is not very
19237: ** user-friendly, but it does seem to work.
19238: */
19239: #ifdef SQLITE_OMIT_COMPLETE
19240: #define sqlite3_complete(x) 1
19241: #endif
19242: 
19243: /*
19244: ** Return true if zSql is a complete SQL statement.  Return false if it
19245: ** ends in the middle of a string literal or C-style comment.
19246: */
19247: static int line_is_complete(char *zSql, int nSql){
19248:   int rc;
19249:   if( zSql==0 ) return 1;
19250:   zSql[nSql] = ';';
19251:   zSql[nSql+1] = 0;
19252:   rc = sqlite3_complete(zSql);
19253:   zSql[nSql] = 0;
19254:   return rc;
19255: }
