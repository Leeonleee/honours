9091: /*
9092: ** Implementation of scalar function rem().
9093: */
9094: static void idxRemFunc(
9095:   sqlite3_context *pCtx,
9096:   int argc,
9097:   sqlite3_value **argv
9098: ){
9099:   struct IdxRemCtx *p = (struct IdxRemCtx*)sqlite3_user_data(pCtx);
9100:   struct IdxRemSlot *pSlot;
9101:   int iSlot;
9102:   assert( argc==2 );
9103: 
9104:   iSlot = sqlite3_value_int(argv[0]);
9105:   assert( iSlot<=p->nSlot );
9106:   pSlot = &p->aSlot[iSlot];
9107: 
9108:   switch( pSlot->eType ){
9109:     case SQLITE_NULL:
9110:       /* no-op */
9111:       break;
9112: 
9113:     case SQLITE_INTEGER:
9114:       sqlite3_result_int64(pCtx, pSlot->iVal);
9115:       break;
9116: 
9117:     case SQLITE_FLOAT:
9118:       sqlite3_result_double(pCtx, pSlot->rVal);
9119:       break;
9120: 
9121:     case SQLITE_BLOB:
9122:       sqlite3_result_blob(pCtx, pSlot->z, pSlot->n, SQLITE_TRANSIENT);
9123:       break;
9124: 
9125:     case SQLITE_TEXT:
9126:       sqlite3_result_text(pCtx, pSlot->z, pSlot->n, SQLITE_TRANSIENT);
9127:       break;
9128:   }
9129: 
9130:   pSlot->eType = sqlite3_value_type(argv[1]);
9131:   switch( pSlot->eType ){
9132:     case SQLITE_NULL:
9133:       /* no-op */
9134:       break;
9135: 
9136:     case SQLITE_INTEGER:
9137:       pSlot->iVal = sqlite3_value_int64(argv[1]);
9138:       break;
9139: 
9140:     case SQLITE_FLOAT:
9141:       pSlot->rVal = sqlite3_value_double(argv[1]);
9142:       break;
9143: 
9144:     case SQLITE_BLOB:
9145:     case SQLITE_TEXT: {
9146:       int nByte = sqlite3_value_bytes(argv[1]);
9147:       if( nByte>pSlot->nByte ){
9148:         char *zNew = (char*)sqlite3_realloc(pSlot->z, nByte*2);
9149:         if( zNew==0 ){
9150:           sqlite3_result_error_nomem(pCtx);
9151:           return;
9152:         }
9153:         pSlot->nByte = nByte*2;
9154:         pSlot->z = zNew;
9155:       }
9156:       pSlot->n = nByte;
9157:       if( pSlot->eType==SQLITE_BLOB ){
9158:         memcpy(pSlot->z, sqlite3_value_blob(argv[1]), nByte);
9159:       }else{
9160:         memcpy(pSlot->z, sqlite3_value_text(argv[1]), nByte);
9161:       }
9162:       break;
9163:     }
9164:   }
9165: }
