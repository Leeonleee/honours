9304: /*
9305: ** This function is called as part of sqlite3_expert_analyze(). Candidate
9306: ** indexes have already been created in database sqlite3expert.dbm, this
9307: ** function populates sqlite_stat1 table in the same database.
9308: **
9309: ** The stat1 data is generated by querying the
9310: */
9311: static int idxPopulateStat1(sqlite3expert *p, char **pzErr){
9312:   int rc = SQLITE_OK;
9313:   int nMax =0;
9314:   struct IdxRemCtx *pCtx = 0;
9315:   struct IdxSampleCtx samplectx;
9316:   int i;
9317:   i64 iPrev = -100000;
9318:   sqlite3_stmt *pAllIndex = 0;
9319:   sqlite3_stmt *pIndexXInfo = 0;
9320:   sqlite3_stmt *pWrite = 0;
9321: 
9322:   const char *zAllIndex =
9323:     "SELECT s.rowid, s.name, l.name FROM "
9324:     "  sqlite_schema AS s, "
9325:     "  pragma_index_list(s.name) AS l "
9326:     "WHERE s.type = 'table'";
9327:   const char *zIndexXInfo =
9328:     "SELECT name, coll FROM pragma_index_xinfo(?) WHERE key";
9329:   const char *zWrite = "INSERT INTO sqlite_stat1 VALUES(?, ?, ?)";
9330: 
9331:   /* If iSample==0, no sqlite_stat1 data is required. */
9332:   if( p->iSample==0 ) return SQLITE_OK;
9333: 
9334:   rc = idxLargestIndex(p->dbm, &nMax, pzErr);
9335:   if( nMax<=0 || rc!=SQLITE_OK ) return rc;
9336: 
9337:   rc = sqlite3_exec(p->dbm, "ANALYZE; PRAGMA writable_schema=1", 0, 0, 0);
9338: 
9339:   if( rc==SQLITE_OK ){
9340:     int nByte = sizeof(struct IdxRemCtx) + (sizeof(struct IdxRemSlot) * nMax);
9341:     pCtx = (struct IdxRemCtx*)idxMalloc(&rc, nByte);
9342:   }
9343: 
9344:   if( rc==SQLITE_OK ){
9345:     sqlite3 *dbrem = (p->iSample==100 ? p->db : p->dbv);
9346:     rc = sqlite3_create_function(
9347:         dbrem, "rem", 2, SQLITE_UTF8, (void*)pCtx, idxRemFunc, 0, 0
9348:     );
9349:   }
9350:   if( rc==SQLITE_OK ){
9351:     rc = sqlite3_create_function(
9352:         p->db, "sample", 0, SQLITE_UTF8, (void*)&samplectx, idxSampleFunc, 0, 0
9353:     );
9354:   }
9355: 
9356:   if( rc==SQLITE_OK ){
9357:     pCtx->nSlot = nMax+1;
9358:     rc = idxPrepareStmt(p->dbm, &pAllIndex, pzErr, zAllIndex);
9359:   }
9360:   if( rc==SQLITE_OK ){
9361:     rc = idxPrepareStmt(p->dbm, &pIndexXInfo, pzErr, zIndexXInfo);
9362:   }
9363:   if( rc==SQLITE_OK ){
9364:     rc = idxPrepareStmt(p->dbm, &pWrite, pzErr, zWrite);
9365:   }
9366: 
9367:   while( rc==SQLITE_OK && SQLITE_ROW==sqlite3_step(pAllIndex) ){
9368:     i64 iRowid = sqlite3_column_int64(pAllIndex, 0);
9369:     const char *zTab = (const char*)sqlite3_column_text(pAllIndex, 1);
9370:     const char *zIdx = (const char*)sqlite3_column_text(pAllIndex, 2);
9371:     if( p->iSample<100 && iPrev!=iRowid ){
9372:       samplectx.target = (double)p->iSample / 100.0;
9373:       samplectx.iTarget = p->iSample;
9374:       samplectx.nRow = 0.0;
9375:       samplectx.nRet = 0.0;
9376:       rc = idxBuildSampleTable(p, zTab);
9377:       if( rc!=SQLITE_OK ) break;
9378:     }
9379:     rc = idxPopulateOneStat1(p, pIndexXInfo, pWrite, zTab, zIdx, pzErr);
9380:     iPrev = iRowid;
9381:   }
9382:   if( rc==SQLITE_OK && p->iSample<100 ){
9383:     rc = sqlite3_exec(p->dbv,
9384:         "DROP TABLE IF EXISTS temp." UNIQUE_TABLE_NAME, 0,0,0
9385:     );
9386:   }
9387: 
9388:   idxFinalize(&rc, pAllIndex);
9389:   idxFinalize(&rc, pIndexXInfo);
9390:   idxFinalize(&rc, pWrite);
9391: 
9392:   for(i=0; i<pCtx->nSlot; i++){
9393:     sqlite3_free(pCtx->aSlot[i].z);
9394:   }
9395:   sqlite3_free(pCtx);
9396: 
9397:   if( rc==SQLITE_OK ){
9398:     rc = sqlite3_exec(p->dbm, "ANALYZE sqlite_schema", 0, 0, 0);
9399:   }
9400: 
9401:   sqlite3_exec(p->db, "DROP TABLE IF EXISTS temp."UNIQUE_TABLE_NAME,0,0,0);
9402:   return rc;
9403: }
