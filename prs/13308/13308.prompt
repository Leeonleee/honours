6517: static int zipfileSerializeLFH(ZipfileEntry *pEntry, u8 *aBuf){
6518:   ZipfileCDS *pCds = &pEntry->cds;
6519:   u8 *a = aBuf;
6520: 
6521:   pCds->nExtra = 9;
6522: 
6523:   /* Write the LFH itself */
6524:   zipfileWrite32(a, ZIPFILE_SIGNATURE_LFH);
6525:   zipfileWrite16(a, pCds->iVersionExtract);
6526:   zipfileWrite16(a, pCds->flags);
6527:   zipfileWrite16(a, pCds->iCompression);
6528:   zipfileWrite16(a, pCds->mTime);
6529:   zipfileWrite16(a, pCds->mDate);
6530:   zipfileWrite32(a, pCds->crc32);
6531:   zipfileWrite32(a, pCds->szCompressed);
6532:   zipfileWrite32(a, pCds->szUncompressed);
6533:   zipfileWrite16(a, (u16)pCds->nFile);
6534:   zipfileWrite16(a, pCds->nExtra);
6535:   assert( a==&aBuf[ZIPFILE_LFH_FIXED_SZ] );
6536: 
6537:   /* Add the file name */
6538:   memcpy(a, pCds->zFile, (int)pCds->nFile);
6539:   a += (int)pCds->nFile;
6540: 
6541:   /* The "extra" data */
6542:   zipfileWrite16(a, ZIPFILE_EXTRA_TIMESTAMP);
6543:   zipfileWrite16(a, 5);
6544:   *a++ = 0x01;
6545:   zipfileWrite32(a, pEntry->mUnixTime);
6546: 
6547:   return a-aBuf;
6548: }
