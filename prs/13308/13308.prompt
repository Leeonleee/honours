6896: static int zipfileSerializeEOCD(ZipfileEOCD *p, u8 *aBuf){
6897:   u8 *a = aBuf;
6898:   zipfileWrite32(a, ZIPFILE_SIGNATURE_EOCD);
6899:   zipfileWrite16(a, p->iDisk);
6900:   zipfileWrite16(a, p->iFirstDisk);
6901:   zipfileWrite16(a, p->nEntry);
6902:   zipfileWrite16(a, p->nEntryTotal);
6903:   zipfileWrite32(a, p->nSize);
6904:   zipfileWrite32(a, p->iOffset);
6905:   zipfileWrite16(a, 0);        /* Size of trailing comment in bytes*/
6906: 
6907:   return a-aBuf;
6908: }
6909: 
6910: static int zipfileAppendEOCD(ZipfileTab *pTab, ZipfileEOCD *p){
6911:   int nBuf = zipfileSerializeEOCD(p, pTab->aBuffer);
6912:   assert( nBuf==ZIPFILE_EOCD_FIXED_SZ );
6913:   return zipfileAppendData(pTab, pTab->aBuffer, nBuf);
6914: }
