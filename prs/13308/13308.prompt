9490: /*
9491: ** Add an SQL statement to the analysis.
9492: */
9493: int sqlite3_expert_sql(
9494:   sqlite3expert *p,               /* From sqlite3_expert_new() */
9495:   const char *zSql,               /* SQL statement to add */
9496:   char **pzErr                    /* OUT: Error message (if any) */
9497: ){
9498:   IdxScan *pScanOrig = p->pScan;
9499:   IdxStatement *pStmtOrig = p->pStatement;
9500:   int rc = SQLITE_OK;
9501:   const char *zStmt = zSql;
9502: 
9503:   if( p->bRun ) return SQLITE_MISUSE;
9504: 
9505:   while( rc==SQLITE_OK && zStmt && zStmt[0] ){
9506:     sqlite3_stmt *pStmt = 0;
9507:     rc = sqlite3_prepare_v2(p->dbv, zStmt, -1, &pStmt, &zStmt);
9508:     if( rc==SQLITE_OK ){
9509:       if( pStmt ){
9510:         IdxStatement *pNew;
9511:         const char *z = sqlite3_sql(pStmt);
9512:         int n = STRLEN(z);
9513:         pNew = (IdxStatement*)idxMalloc(&rc, sizeof(IdxStatement) + n+1);
9514:         if( rc==SQLITE_OK ){
9515:           pNew->zSql = (char*)&pNew[1];
9516:           memcpy(pNew->zSql, z, n+1);
9517:           pNew->pNext = p->pStatement;
9518:           if( p->pStatement ) pNew->iId = p->pStatement->iId+1;
9519:           p->pStatement = pNew;
9520:         }
9521:         sqlite3_finalize(pStmt);
9522:       }
9523:     }else{
9524:       idxDatabaseError(p->dbv, pzErr);
9525:     }
9526:   }
9527: 
9528:   if( rc!=SQLITE_OK ){
9529:     idxScanFree(p->pScan, pScanOrig);
9530:     idxStatementFree(p->pStatement, pStmtOrig);
9531:     p->pScan = pScanOrig;
9532:     p->pStatement = pStmtOrig;
9533:   }
9534: 
9535:   return rc;
9536: }
