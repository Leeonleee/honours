11319: /*
11320: ** Free and reset the EXPLAIN QUERY PLAN data that has been collected
11321: ** in p->sGraph.
11322: */
11323: static void eqp_reset(ShellState *p){
11324:   EQPGraphRow *pRow, *pNext;
11325:   for(pRow = p->sGraph.pRow; pRow; pRow = pNext){
11326:     pNext = pRow->pNext;
11327:     sqlite3_free(pRow);
11328:   }
11329:   memset(&p->sGraph, 0, sizeof(p->sGraph));
11330: }
