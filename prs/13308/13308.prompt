5764: /*
5765: ** Decode the CDS record in buffer aBuf into (*pCDS). Return SQLITE_ERROR
5766: ** if the record is not well-formed, or SQLITE_OK otherwise.
5767: */
5768: static int zipfileReadCDS(u8 *aBuf, ZipfileCDS *pCDS){
5769:   u8 *aRead = aBuf;
5770:   u32 sig = zipfileRead32(aRead);
5771:   int rc = SQLITE_OK;
5772:   if( sig!=ZIPFILE_SIGNATURE_CDS ){
5773:     rc = SQLITE_ERROR;
5774:   }else{
5775:     pCDS->iVersionMadeBy = zipfileRead16(aRead);
5776:     pCDS->iVersionExtract = zipfileRead16(aRead);
5777:     pCDS->flags = zipfileRead16(aRead);
5778:     pCDS->iCompression = zipfileRead16(aRead);
5779:     pCDS->mTime = zipfileRead16(aRead);
5780:     pCDS->mDate = zipfileRead16(aRead);
5781:     pCDS->crc32 = zipfileRead32(aRead);
5782:     pCDS->szCompressed = zipfileRead32(aRead);
5783:     pCDS->szUncompressed = zipfileRead32(aRead);
5784:     assert( aRead==&aBuf[ZIPFILE_CDS_NFILE_OFF] );
5785:     pCDS->nFile = zipfileRead16(aRead);
5786:     pCDS->nExtra = zipfileRead16(aRead);
5787:     pCDS->nComment = zipfileRead16(aRead);
5788:     pCDS->iDiskStart = zipfileRead16(aRead);
5789:     pCDS->iInternalAttr = zipfileRead16(aRead);
5790:     pCDS->iExternalAttr = zipfileRead32(aRead);
5791:     pCDS->iOffset = zipfileRead32(aRead);
5792:     assert( aRead==&aBuf[ZIPFILE_CDS_FIXED_SZ] );
5793:   }
5794: 
5795:   return rc;
5796: }
