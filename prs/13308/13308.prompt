7824: /*
7825: ** Allocate and return nByte bytes of zeroed memory using sqlite3_malloc().
7826: ** If the allocation fails, set *pRc to SQLITE_NOMEM and return NULL.
7827: */
7828: static void *idxMalloc(int *pRc, int nByte){
7829:   void *pRet;
7830:   assert( *pRc==SQLITE_OK );
7831:   assert( nByte>0 );
7832:   pRet = sqlite3_malloc(nByte);
7833:   if( pRet ){
7834:     memset(pRet, 0, nByte);
7835:   }else{
7836:     *pRc = SQLITE_NOMEM;
7837:   }
7838:   return pRet;
7839: }
7840: 
7841: /*
7842: ** Initialize an IdxHash hash table.
7843: */
7844: static void idxHashInit(IdxHash *pHash){
7845:   memset(pHash, 0, sizeof(IdxHash));
7846: }
7847: 
7848: /*
7849: ** Reset an IdxHash hash table.
7850: */
7851: static void idxHashClear(IdxHash *pHash){
7852:   int i;
7853:   for(i=0; i<IDX_HASH_SIZE; i++){
7854:     IdxHashEntry *pEntry;
7855:     IdxHashEntry *pNext;
7856:     for(pEntry=pHash->aHash[i]; pEntry; pEntry=pNext){
7857:       pNext = pEntry->pHashNext;
7858:       sqlite3_free(pEntry->zVal2);
7859:       sqlite3_free(pEntry);
7860:     }
7861:   }
7862:   memset(pHash, 0, sizeof(IdxHash));
7863: }
7864: 
7865: /*
7866: ** Return the index of the hash bucket that the string specified by the
7867: ** arguments to this function belongs.
7868: */
7869: static int idxHashString(const char *z, int n){
7870:   unsigned int ret = 0;
7871:   int i;
7872:   for(i=0; i<n; i++){
7873:     ret += (ret<<3) + (unsigned char)(z[i]);
7874:   }
7875:   return (int)(ret % IDX_HASH_SIZE);
7876: }
7877: 
7878: /*
7879: ** If zKey is already present in the hash table, return non-zero and do
7880: ** nothing. Otherwise, add an entry with key zKey and payload string zVal to
7881: ** the hash table passed as the second argument.
7882: */
7883: static int idxHashAdd(
7884:   int *pRc,
7885:   IdxHash *pHash,
7886:   const char *zKey,
7887:   const char *zVal
7888: ){
7889:   int nKey = STRLEN(zKey);
7890:   int iHash = idxHashString(zKey, nKey);
7891:   int nVal = (zVal ? STRLEN(zVal) : 0);
7892:   IdxHashEntry *pEntry;
7893:   assert( iHash>=0 );
7894:   for(pEntry=pHash->aHash[iHash]; pEntry; pEntry=pEntry->pHashNext){
7895:     if( STRLEN(pEntry->zKey)==nKey && 0==memcmp(pEntry->zKey, zKey, nKey) ){
7896:       return 1;
7897:     }
7898:   }
7899:   pEntry = idxMalloc(pRc, sizeof(IdxHashEntry) + nKey+1 + nVal+1);
7900:   if( pEntry ){
7901:     pEntry->zKey = (char*)&pEntry[1];
7902:     memcpy(pEntry->zKey, zKey, nKey);
7903:     if( zVal ){
7904:       pEntry->zVal = &pEntry->zKey[nKey+1];
7905:       memcpy(pEntry->zVal, zVal, nVal);
7906:     }
7907:     pEntry->pHashNext = pHash->aHash[iHash];
7908:     pHash->aHash[iHash] = pEntry;
7909: 
7910:     pEntry->pNext = pHash->pFirst;
7911:     pHash->pFirst = pEntry;
7912:   }
7913:   return 0;
7914: }
7915: 
7916: /*
7917: ** If zKey/nKey is present in the hash table, return a pointer to the
7918: ** hash-entry object.
7919: */
7920: static IdxHashEntry *idxHashFind(IdxHash *pHash, const char *zKey, int nKey){
7921:   int iHash;
7922:   IdxHashEntry *pEntry;
7923:   if( nKey<0 ) nKey = STRLEN(zKey);
7924:   iHash = idxHashString(zKey, nKey);
7925:   assert( iHash>=0 );
7926:   for(pEntry=pHash->aHash[iHash]; pEntry; pEntry=pEntry->pHashNext){
7927:     if( STRLEN(pEntry->zKey)==nKey && 0==memcmp(pEntry->zKey, zKey, nKey) ){
7928:       return pEntry;
7929:     }
7930:   }
7931:   return 0;
7932: }
7933: 
7934: /*
7935: ** If the hash table contains an entry with a key equal to the string
7936: ** passed as the final two arguments to this function, return a pointer
7937: ** to the payload string. Otherwise, if zKey/nKey is not present in the
7938: ** hash table, return NULL.
7939: */
7940: static const char *idxHashSearch(IdxHash *pHash, const char *zKey, int nKey){
7941:   IdxHashEntry *pEntry = idxHashFind(pHash, zKey, nKey);
7942:   if( pEntry ) return pEntry->zVal;
7943:   return 0;
7944: }
7945: 
7946: /*
7947: ** Allocate and return a new IdxConstraint object. Set the IdxConstraint.zColl
7948: ** variable to point to a copy of nul-terminated string zColl.
7949: */
7950: static IdxConstraint *idxNewConstraint(int *pRc, const char *zColl){
7951:   IdxConstraint *pNew;
7952:   int nColl = STRLEN(zColl);
7953: 
7954:   assert( *pRc==SQLITE_OK );
7955:   pNew = (IdxConstraint*)idxMalloc(pRc, sizeof(IdxConstraint) * nColl + 1);
7956:   if( pNew ){
7957:     pNew->zColl = (char*)&pNew[1];
7958:     memcpy(pNew->zColl, zColl, nColl+1);
7959:   }
7960:   return pNew;
7961: }
