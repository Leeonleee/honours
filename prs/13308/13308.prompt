6702: /*
6703: ** xUpdate method.
6704: */
6705: static int zipfileUpdate(
6706:   sqlite3_vtab *pVtab,
6707:   int nVal,
6708:   sqlite3_value **apVal,
6709:   sqlite_int64 *pRowid
6710: ){
6711:   ZipfileTab *pTab = (ZipfileTab*)pVtab;
6712:   int rc = SQLITE_OK;             /* Return Code */
6713:   ZipfileEntry *pNew = 0;         /* New in-memory CDS entry */
6714: 
6715:   u32 mode = 0;                   /* Mode for new entry */
6716:   u32 mTime = 0;                  /* Modification time for new entry */
6717:   i64 sz = 0;                     /* Uncompressed size */
6718:   const char *zPath = 0;          /* Path for new entry */
6719:   int nPath = 0;                  /* strlen(zPath) */
6720:   const u8 *pData = 0;            /* Pointer to buffer containing content */
6721:   int nData = 0;                  /* Size of pData buffer in bytes */
6722:   int iMethod = 0;                /* Compression method for new entry */
6723:   u8 *pFree = 0;                  /* Free this */
6724:   char *zFree = 0;                /* Also free this */
6725:   ZipfileEntry *pOld = 0;
6726:   ZipfileEntry *pOld2 = 0;
6727:   int bUpdate = 0;                /* True for an update that modifies "name" */
6728:   int bIsDir = 0;
6729:   u32 iCrc32 = 0;
6730: 
6731:   if( pTab->pWriteFd==0 ){
6732:     rc = zipfileBegin(pVtab);
6733:     if( rc!=SQLITE_OK ) return rc;
6734:   }
6735: 
6736:   /* If this is a DELETE or UPDATE, find the archive entry to delete. */
6737:   if( sqlite3_value_type(apVal[0])!=SQLITE_NULL ){
6738:     const char *zDelete = (const char*)sqlite3_value_text(apVal[0]);
6739:     int nDelete = (int)strlen(zDelete);
6740:     if( nVal>1 ){
6741:       const char *zUpdate = (const char*)sqlite3_value_text(apVal[1]);
6742:       if( zUpdate && zipfileComparePath(zUpdate, zDelete, nDelete)!=0 ){
6743:         bUpdate = 1;
6744:       }
6745:     }
6746:     for(pOld=pTab->pFirstEntry; 1; pOld=pOld->pNext){
6747:       if( zipfileComparePath(pOld->cds.zFile, zDelete, nDelete)==0 ){
6748:         break;
6749:       }
6750:       assert( pOld->pNext );
6751:     }
6752:   }
6753: 
6754:   if( nVal>1 ){
6755:     /* Check that "sz" and "rawdata" are both NULL: */
6756:     if( sqlite3_value_type(apVal[5])!=SQLITE_NULL ){
6757:       zipfileTableErr(pTab, "sz must be NULL");
6758:       rc = SQLITE_CONSTRAINT;
6759:     }
6760:     if( sqlite3_value_type(apVal[6])!=SQLITE_NULL ){
6761:       zipfileTableErr(pTab, "rawdata must be NULL");
6762:       rc = SQLITE_CONSTRAINT;
6763:     }
6764: 
6765:     if( rc==SQLITE_OK ){
6766:       if( sqlite3_value_type(apVal[7])==SQLITE_NULL ){
6767:         /* data=NULL. A directory */
6768:         bIsDir = 1;
6769:       }else{
6770:         /* Value specified for "data", and possibly "method". This must be
6771:         ** a regular file or a symlink. */
6772:         const u8 *aIn = sqlite3_value_blob(apVal[7]);
6773:         int nIn = sqlite3_value_bytes(apVal[7]);
6774:         int bAuto = sqlite3_value_type(apVal[8])==SQLITE_NULL;
6775: 
6776:         iMethod = sqlite3_value_int(apVal[8]);
6777:         sz = nIn;
6778:         pData = aIn;
6779:         nData = nIn;
6780:         if( iMethod!=0 && iMethod!=8 ){
6781:           zipfileTableErr(pTab, "unknown compression method: %d", iMethod);
6782:           rc = SQLITE_CONSTRAINT;
6783:         }else{
6784:           if( bAuto || iMethod ){
6785:             int nCmp;
6786:             rc = zipfileDeflate(aIn, nIn, &pFree, &nCmp, &pTab->base.zErrMsg);
6787:             if( rc==SQLITE_OK ){
6788:               if( iMethod || nCmp<nIn ){
6789:                 iMethod = 8;
6790:                 pData = pFree;
6791:                 nData = nCmp;
6792:               }
6793:             }
6794:           }
6795:           iCrc32 = crc32(0, aIn, nIn);
6796:         }
6797:       }
6798:     }
6799: 
6800:     if( rc==SQLITE_OK ){
6801:       rc = zipfileGetMode(apVal[3], bIsDir, &mode, &pTab->base.zErrMsg);
6802:     }
6803: 
6804:     if( rc==SQLITE_OK ){
6805:       zPath = (const char*)sqlite3_value_text(apVal[2]);
6806:       if( zPath==0 ) zPath = "";
6807:       nPath = (int)strlen(zPath);
6808:       mTime = zipfileGetTime(apVal[4]);
6809:     }
6810: 
6811:     if( rc==SQLITE_OK && bIsDir ){
6812:       /* For a directory, check that the last character in the path is a
6813:       ** '/'. This appears to be required for compatibility with info-zip
6814:       ** (the unzip command on unix). It does not create directories
6815:       ** otherwise.  */
6816:       if( nPath<=0 || zPath[nPath-1]!='/' ){
6817:         zFree = sqlite3_mprintf("%s/", zPath);
6818:         zPath = (const char*)zFree;
6819:         if( zFree==0 ){
6820:           rc = SQLITE_NOMEM;
6821:           nPath = 0;
6822:         }else{
6823:           nPath = (int)strlen(zPath);
6824:         }
6825:       }
6826:     }
6827: 
6828:     /* Check that we're not inserting a duplicate entry -OR- updating an
6829:     ** entry with a path, thereby making it into a duplicate. */
6830:     if( (pOld==0 || bUpdate) && rc==SQLITE_OK ){
6831:       ZipfileEntry *p;
6832:       for(p=pTab->pFirstEntry; p; p=p->pNext){
6833:         if( zipfileComparePath(p->cds.zFile, zPath, nPath)==0 ){
6834:           switch( sqlite3_vtab_on_conflict(pTab->db) ){
6835:             case SQLITE_IGNORE: {
6836:               goto zipfile_update_done;
6837:             }
6838:             case SQLITE_REPLACE: {
6839:               pOld2 = p;
6840:               break;
6841:             }
6842:             default: {
6843:               zipfileTableErr(pTab, "duplicate name: \"%s\"", zPath);
6844:               rc = SQLITE_CONSTRAINT;
6845:               break;
6846:             }
6847:           }
6848:           break;
6849:         }
6850:       }
6851:     }
6852: 
6853:     if( rc==SQLITE_OK ){
6854:       /* Create the new CDS record. */
6855:       pNew = zipfileNewEntry(zPath);
6856:       if( pNew==0 ){
6857:         rc = SQLITE_NOMEM;
6858:       }else{
6859:         pNew->cds.iVersionMadeBy = ZIPFILE_NEWENTRY_MADEBY;
6860:         pNew->cds.iVersionExtract = ZIPFILE_NEWENTRY_REQUIRED;
6861:         pNew->cds.flags = ZIPFILE_NEWENTRY_FLAGS;
6862:         pNew->cds.iCompression = (u16)iMethod;
6863:         zipfileMtimeToDos(&pNew->cds, mTime);
6864:         pNew->cds.crc32 = iCrc32;
6865:         pNew->cds.szCompressed = nData;
6866:         pNew->cds.szUncompressed = (u32)sz;
6867:         pNew->cds.iExternalAttr = (mode<<16);
6868:         pNew->cds.iOffset = (u32)pTab->szCurrent;
6869:         pNew->cds.nFile = (u16)nPath;
6870:         pNew->mUnixTime = (u32)mTime;
6871:         rc = zipfileAppendEntry(pTab, pNew, pData, nData);
6872:         zipfileAddEntry(pTab, pOld, pNew);
6873:       }
6874:     }
6875:   }
6876: 
6877:   if( rc==SQLITE_OK && (pOld || pOld2) ){
6878:     ZipfileCsr *pCsr;
6879:     for(pCsr=pTab->pCsrList; pCsr; pCsr=pCsr->pCsrNext){
6880:       if( pCsr->pCurrent && (pCsr->pCurrent==pOld || pCsr->pCurrent==pOld2) ){
6881:         pCsr->pCurrent = pCsr->pCurrent->pNext;
6882:         pCsr->bNoop = 1;
6883:       }
6884:     }
6885: 
6886:     zipfileRemoveEntryFromList(pTab, pOld);
6887:     zipfileRemoveEntryFromList(pTab, pOld2);
6888:   }
6889: 
6890: zipfile_update_done:
6891:   sqlite3_free(pFree);
6892:   sqlite3_free(zFree);
6893:   return rc;
6894: }
