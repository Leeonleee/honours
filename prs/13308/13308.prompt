7810:   sqlite3 *dbm;                   /* In-memory db for this analysis */
7811:   sqlite3 *dbv;                   /* Vtab schema for this analysis */
7812:   IdxTable *pTable;               /* List of all IdxTable objects */
7813:   IdxScan *pScan;                 /* List of scan objects */
7814:   IdxWrite *pWrite;               /* List of write objects */
7815:   IdxStatement *pStatement;       /* List of IdxStatement objects */
7816:   int bRun;                       /* True once analysis has run */
7817:   char **pzErrmsg;
7818:   int rc;                         /* Error code from whereinfo hook */
7819:   IdxHash hIdx;                   /* Hash containing all candidate indexes */
7820:   char *zCandidates;              /* For EXPERT_REPORT_CANDIDATES */
