10963: /*
10964: ** Output the given string as a quoted string using SQL quoting conventions.
10965: ** Additionallly , escape the "\n" and "\r" characters so that they do not
10966: ** get corrupted by end-of-line translation facilities in some operating
10967: ** systems.
10968: **
10969: ** This is like output_quoted_string() but with the addition of the \r\n
10970: ** escape mechanism.
10971: */
10972: static void output_quoted_escaped_string(FILE *out, const char *z){
10973:   int i;
10974:   char c;
10975:   setBinaryMode(out, 1);
10976:   for(i=0; (c = z[i])!=0 && c!='\'' && c!='\n' && c!='\r'; i++){}
10977:   if( c==0 ){
10978:     utf8_printf(out,"'%s'",z);
10979:   }else{
10980:     const char *zNL = 0;
10981:     const char *zCR = 0;
10982:     int nNL = 0;
10983:     int nCR = 0;
10984:     char zBuf1[20], zBuf2[20];
10985:     for(i=0; z[i]; i++){
10986:       if( z[i]=='\n' ) nNL++;
10987:       if( z[i]=='\r' ) nCR++;
10988:     }
10989:     if( nNL ){
10990:       raw_printf(out, "replace(");
10991:       zNL = unused_string(z, "\\n", "\\012", zBuf1);
10992:     }
10993:     if( nCR ){
10994:       raw_printf(out, "replace(");
10995:       zCR = unused_string(z, "\\r", "\\015", zBuf2);
10996:     }
10997:     raw_printf(out, "'");
10998:     while( *z ){
10999:       for(i=0; (c = z[i])!=0 && c!='\n' && c!='\r' && c!='\''; i++){}
11000:       if( c=='\'' ) i++;
11001:       if( i ){
11002:         utf8_printf(out, "%.*s", i, z);
11003:         z += i;
11004:       }
11005:       if( c=='\'' ){
11006:         raw_printf(out, "'");
11007:         continue;
11008:       }
11009:       if( c==0 ){
11010:         break;
11011:       }
11012:       z++;
11013:       if( c=='\n' ){
11014:         raw_printf(out, "%s", zNL);
11015:         continue;
11016:       }
11017:       raw_printf(out, "%s", zCR);
11018:     }
11019:     raw_printf(out, "'");
11020:     if( nCR ){
11021:       raw_printf(out, ",'%s',char(13))", zCR);
11022:     }
11023:     if( nNL ){
11024:       raw_printf(out, ",'%s',char(10))", zNL);
11025:     }
11026:   }
11027:   setTextMode(out, 1);
11028: }
