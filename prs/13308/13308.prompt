15527: /*
15528: ** Implementation of ".ar" dot command.
15529: */
15530: static int arDotCommand(
15531:   ShellState *pState,          /* Current shell tool state */
15532:   int fromCmdLine,             /* True if -A command-line option, not .ar cmd */
15533:   char **azArg,                /* Array of arguments passed to dot command */
15534:   int nArg                     /* Number of entries in azArg[] */
15535: ){
15536:   ArCommand cmd;
15537:   int rc;
15538:   memset(&cmd, 0, sizeof(cmd));
15539:   cmd.fromCmdLine = fromCmdLine;
15540:   rc = arParseCommand(azArg, nArg, &cmd);
15541:   if( rc==SQLITE_OK ){
15542:     int eDbType = SHELL_OPEN_UNSPEC;
15543:     cmd.p = pState;
15544:     cmd.db = pState->db;
15545:     if( cmd.zFile ){
15546:       eDbType = deduceDatabaseType(cmd.zFile, 1);
15547:     }else{
15548:       eDbType = pState->openMode;
15549:     }
15550:     if( eDbType==SHELL_OPEN_ZIPFILE ){
15551:       if( cmd.eCmd==AR_CMD_EXTRACT || cmd.eCmd==AR_CMD_LIST ){
15552:         if( cmd.zFile==0 ){
15553:           cmd.zSrcTable = sqlite3_mprintf("zip");
15554:         }else{
15555:           cmd.zSrcTable = sqlite3_mprintf("zipfile(%Q)", cmd.zFile);
15556:         }
15557:       }
15558:       cmd.bZip = 1;
15559:     }else if( cmd.zFile ){
15560:       int flags;
15561:       if( cmd.bAppend ) eDbType = SHELL_OPEN_APPENDVFS;
15562:       if( cmd.eCmd==AR_CMD_CREATE || cmd.eCmd==AR_CMD_INSERT
15563:            || cmd.eCmd==AR_CMD_UPDATE ){
15564:         flags = SQLITE_OPEN_READWRITE|SQLITE_OPEN_CREATE;
15565:       }else{
15566:         flags = SQLITE_OPEN_READONLY;
15567:       }
15568:       cmd.db = 0;
15569:       if( cmd.bDryRun ){
15570:         utf8_printf(pState->out, "-- open database '%s'%s\n", cmd.zFile,
15571:              eDbType==SHELL_OPEN_APPENDVFS ? " using 'apndvfs'" : "");
15572:       }
15573:       rc = sqlite3_open_v2(cmd.zFile, &cmd.db, flags,
15574:              eDbType==SHELL_OPEN_APPENDVFS ? "apndvfs" : 0);
15575:       if( rc!=SQLITE_OK ){
15576:         utf8_printf(stderr, "cannot open file: %s (%s)\n",
15577:             cmd.zFile, sqlite3_errmsg(cmd.db)
15578:         );
15579:         goto end_ar_command;
15580:       }
15581:       sqlite3_fileio_init(cmd.db, 0, 0);
15582:       sqlite3_sqlar_init(cmd.db, 0, 0);
15583:       sqlite3_create_function(cmd.db, "shell_putsnl", 1, SQLITE_UTF8, cmd.p,
15584:                               shellPutsFunc, 0, 0);
15585: 
15586:     }
15587:     if( cmd.zSrcTable==0 && cmd.bZip==0 && cmd.eCmd!=AR_CMD_HELP ){
15588:       if( cmd.eCmd!=AR_CMD_CREATE
15589:        && sqlite3_table_column_metadata(cmd.db,0,"sqlar","name",0,0,0,0,0)
15590:       ){
15591:         utf8_printf(stderr, "database does not contain an 'sqlar' table\n");
15592:         rc = SQLITE_ERROR;
15593:         goto end_ar_command;
15594:       }
15595:       cmd.zSrcTable = sqlite3_mprintf("sqlar");
15596:     }
15597: 
15598:     switch( cmd.eCmd ){
15599:       case AR_CMD_CREATE:
15600:         rc = arCreateOrUpdateCommand(&cmd, 0, 0);
15601:         break;
15602: 
15603:       case AR_CMD_EXTRACT:
15604:         rc = arExtractCommand(&cmd);
15605:         break;
15606: 
15607:       case AR_CMD_LIST:
15608:         rc = arListCommand(&cmd);
15609:         break;
15610: 
15611:       case AR_CMD_HELP:
15612:         arUsage(pState->out);
15613:         break;
15614: 
15615:       case AR_CMD_INSERT:
15616:         rc = arCreateOrUpdateCommand(&cmd, 1, 0);
15617:         break;
15618: 
15619:       default:
15620:         assert( cmd.eCmd==AR_CMD_UPDATE );
15621:         rc = arCreateOrUpdateCommand(&cmd, 1, 1);
15622:         break;
15623:     }
15624:   }
15625: end_ar_command:
15626:   if( cmd.db!=pState->db ){
15627:     close_db(cmd.db);
15628:   }
15629:   sqlite3_free(cmd.zSrcTable);
15630: 
15631:   return rc;
15632: }
15633: /* End of the ".archive" or ".ar" command logic
15634: *******************************************************************************/
