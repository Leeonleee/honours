1819: /*
1820: ** Initialize a new hash.  iSize determines the size of the hash
1821: ** in bits and should be one of 224, 256, 384, or 512.  Or iSize
1822: ** can be zero to use the default hash size of 256 bits.
1823: */
1824: static void SHA3Init(SHA3Context *p, int iSize){
1825:   memset(p, 0, sizeof(*p));
1826:   if( iSize>=128 && iSize<=512 ){
1827:     p->nRate = (1600 - ((iSize + 31)&~31)*2)/8;
1828:   }else{
1829:     p->nRate = (1600 - 2*256)/8;
1830:   }
1831: #if SHA3_BYTEORDER==1234
1832:   /* Known to be little-endian at compile-time. No-op */
1833: #elif SHA3_BYTEORDER==4321
1834:   p->ixMask = 7;  /* Big-endian */
1835: #else
1836:   {
1837:     static unsigned int one = 1;
1838:     if( 1==*(unsigned char*)&one ){
1839:       /* Little endian.  No byte swapping. */
1840:       p->ixMask = 0;
1841:     }else{
1842:       /* Big endian.  Byte swap. */
1843:       p->ixMask = 7;
1844:     }
1845:   }
1846: #endif
1847: }
