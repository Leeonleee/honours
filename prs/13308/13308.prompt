8033: static char *expertDequote(const char *zIn){
8034:   int n = STRLEN(zIn);
8035:   char *zRet = sqlite3_malloc(n);
8036: 
8037:   assert( zIn[0]=='\'' );
8038:   assert( zIn[n-1]=='\'' );
8039: 
8040:   if( zRet ){
8041:     int iOut = 0;
8042:     int iIn = 0;
8043:     for(iIn=1; iIn<(n-1); iIn++){
8044:       if( zIn[iIn]=='\'' ){
8045:         assert( zIn[iIn+1]=='\'' );
8046:         iIn++;
8047:       }
8048:       zRet[iOut++] = zIn[iIn];
8049:     }
8050:     zRet[iOut] = '\0';
8051:   }
8052: 
8053:   return zRet;
8054: }
