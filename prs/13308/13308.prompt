14454: /*
14455: ** An object used to read a CSV and other files for import.
14456: */
14457: typedef struct ImportCtx ImportCtx;
14458: struct ImportCtx {
14459:   const char *zFile;  /* Name of the input file */
14460:   FILE *in;           /* Read the CSV text from this input stream */
14461:   int (SQLITE_CDECL *xCloser)(FILE*);      /* Func to close in */
14462:   char *z;            /* Accumulated text for a field */
14463:   int n;              /* Number of bytes in z */
14464:   int nAlloc;         /* Space allocated for z[] */
14465:   int nLine;          /* Current line number */
14466:   int nRow;           /* Number of rows imported */
14467:   int nErr;           /* Number of errors encountered */
14468:   int bNotFirst;      /* True if one or more bytes already read */
14469:   int cTerm;          /* Character that terminated the most recent field */
14470:   int cColSep;        /* The column separator character.  (Usually ",") */
14471:   int cRowSep;        /* The row separator character.  (Usually "\n") */
14472: };
14473: 
14474: /* Clean up resourced used by an ImportCtx */
14475: static void import_cleanup(ImportCtx *p){
14476:   if( p->in!=0 && p->xCloser!=0 ){
14477:     p->xCloser(p->in);
14478:     p->in = 0;
14479:   }
14480:   sqlite3_free(p->z);
14481:   p->z = 0;
14482: }
14483: 
14484: /* Append a single byte to z[] */
14485: static void import_append_char(ImportCtx *p, int c){
14486:   if( p->n+1>=p->nAlloc ){
14487:     p->nAlloc += p->nAlloc + 100;
14488:     p->z = sqlite3_realloc64(p->z, p->nAlloc);
14489:     if( p->z==0 ) shell_out_of_memory();
14490:   }
14491:   p->z[p->n++] = (char)c;
14492: }
14493: 
14494: /* Read a single field of CSV text.  Compatible with rfc4180 and extended
14495: ** with the option of having a separator other than ",".
14496: **
14497: **   +  Input comes from p->in.
14498: **   +  Store results in p->z of length p->n.  Space to hold p->z comes
14499: **      from sqlite3_malloc64().
14500: **   +  Use p->cSep as the column separator.  The default is ",".
14501: **   +  Use p->rSep as the row separator.  The default is "\n".
14502: **   +  Keep track of the line number in p->nLine.
14503: **   +  Store the character that terminates the field in p->cTerm.  Store
14504: **      EOF on end-of-file.
14505: **   +  Report syntax errors on stderr
14506: */
14507: static char *SQLITE_CDECL csv_read_one_field(ImportCtx *p){
14508:   int c;
14509:   int cSep = p->cColSep;
14510:   int rSep = p->cRowSep;
14511:   p->n = 0;
14512:   c = fgetc(p->in);
14513:   if( c==EOF || seenInterrupt ){
14514:     p->cTerm = EOF;
14515:     return 0;
14516:   }
14517:   if( c=='"' ){
14518:     int pc, ppc;
14519:     int startLine = p->nLine;
14520:     int cQuote = c;
14521:     pc = ppc = 0;
14522:     while( 1 ){
14523:       c = fgetc(p->in);
14524:       if( c==rSep ) p->nLine++;
14525:       if( c==cQuote ){
14526:         if( pc==cQuote ){
14527:           pc = 0;
14528:           continue;
14529:         }
14530:       }
14531:       if( (c==cSep && pc==cQuote)
14532:        || (c==rSep && pc==cQuote)
14533:        || (c==rSep && pc=='\r' && ppc==cQuote)
14534:        || (c==EOF && pc==cQuote)
14535:       ){
14536:         do{ p->n--; }while( p->z[p->n]!=cQuote );
14537:         p->cTerm = c;
14538:         break;
14539:       }
14540:       if( pc==cQuote && c!='\r' ){
14541:         utf8_printf(stderr, "%s:%d: unescaped %c character\n",
14542:                 p->zFile, p->nLine, cQuote);
14543:       }
14544:       if( c==EOF ){
14545:         utf8_printf(stderr, "%s:%d: unterminated %c-quoted field\n",
14546:                 p->zFile, startLine, cQuote);
14547:         p->cTerm = c;
14548:         break;
14549:       }
14550:       import_append_char(p, c);
14551:       ppc = pc;
14552:       pc = c;
14553:     }
14554:   }else{
14555:     /* If this is the first field being parsed and it begins with the
14556:     ** UTF-8 BOM  (0xEF BB BF) then skip the BOM */
14557:     if( (c&0xff)==0xef && p->bNotFirst==0 ){
14558:       import_append_char(p, c);
14559:       c = fgetc(p->in);
14560:       if( (c&0xff)==0xbb ){
14561:         import_append_char(p, c);
14562:         c = fgetc(p->in);
14563:         if( (c&0xff)==0xbf ){
14564:           p->bNotFirst = 1;
14565:           p->n = 0;
14566:           return csv_read_one_field(p);
14567:         }
14568:       }
14569:     }
14570:     while( c!=EOF && c!=cSep && c!=rSep ){
14571:       import_append_char(p, c);
14572:       c = fgetc(p->in);
14573:     }
14574:     if( c==rSep ){
14575:       p->nLine++;
14576:       if( p->n>0 && p->z[p->n-1]=='\r' ) p->n--;
14577:     }
14578:     p->cTerm = c;
14579:   }
14580:   if( p->z ) p->z[p->n] = 0;
14581:   p->bNotFirst = 1;
14582:   return p->z;
14583: }
14584: 
14585: /* Read a single field of ASCII delimited text.
14586: **
14587: **   +  Input comes from p->in.
14588: **   +  Store results in p->z of length p->n.  Space to hold p->z comes
14589: **      from sqlite3_malloc64().
14590: **   +  Use p->cSep as the column separator.  The default is "\x1F".
14591: **   +  Use p->rSep as the row separator.  The default is "\x1E".
14592: **   +  Keep track of the row number in p->nLine.
14593: **   +  Store the character that terminates the field in p->cTerm.  Store
14594: **      EOF on end-of-file.
14595: **   +  Report syntax errors on stderr
14596: */
14597: static char *SQLITE_CDECL ascii_read_one_field(ImportCtx *p){
14598:   int c;
14599:   int cSep = p->cColSep;
14600:   int rSep = p->cRowSep;
14601:   p->n = 0;
14602:   c = fgetc(p->in);
14603:   if( c==EOF || seenInterrupt ){
14604:     p->cTerm = EOF;
14605:     return 0;
14606:   }
14607:   while( c!=EOF && c!=cSep && c!=rSep ){
14608:     import_append_char(p, c);
14609:     c = fgetc(p->in);
14610:   }
14611:   if( c==rSep ){
14612:     p->nLine++;
14613:   }
14614:   p->cTerm = c;
14615:   if( p->z ) p->z[p->n] = 0;
14616:   return p->z;
14617: }
