5498: /*
5499: ** Construct a new ZipfileTab virtual table object.
5500: **
5501: **   argv[0]   -> module name  ("zipfile")
5502: **   argv[1]   -> database name
5503: **   argv[2]   -> table name
5504: **   argv[...] -> "column name" and other module argument fields.
5505: */
5506: static int zipfileConnect(
5507:   sqlite3 *db,
5508:   void *pAux,
5509:   int argc, const char *const*argv,
5510:   sqlite3_vtab **ppVtab,
5511:   char **pzErr
5512: ){
5513:   int nByte = sizeof(ZipfileTab) + ZIPFILE_BUFFER_SIZE;
5514:   int nFile = 0;
5515:   const char *zFile = 0;
5516:   ZipfileTab *pNew = 0;
5517:   int rc;
5518: 
5519:   /* If the table name is not "zipfile", require that the argument be
5520:   ** specified. This stops zipfile tables from being created as:
5521:   **
5522:   **   CREATE VIRTUAL TABLE zzz USING zipfile();
5523:   **
5524:   ** It does not prevent:
5525:   **
5526:   **   CREATE VIRTUAL TABLE zipfile USING zipfile();
5527:   */
5528:   assert( 0==sqlite3_stricmp(argv[0], "zipfile") );
5529:   if( (0!=sqlite3_stricmp(argv[2], "zipfile") && argc<4) || argc>4 ){
5530:     *pzErr = sqlite3_mprintf("zipfile constructor requires one argument");
5531:     return SQLITE_ERROR;
5532:   }
5533: 
5534:   if( argc>3 ){
5535:     zFile = argv[3];
5536:     nFile = (int)strlen(zFile)+1;
5537:   }
5538: 
5539:   rc = sqlite3_declare_vtab(db, ZIPFILE_SCHEMA);
5540:   if( rc==SQLITE_OK ){
5541:     pNew = (ZipfileTab*)sqlite3_malloc64((sqlite3_int64)nByte+nFile);
5542:     if( pNew==0 ) return SQLITE_NOMEM;
5543:     memset(pNew, 0, nByte+nFile);
5544:     pNew->db = db;
5545:     pNew->aBuffer = (u8*)&pNew[1];
5546:     if( zFile ){
5547:       pNew->zFile = (char*)&pNew->aBuffer[ZIPFILE_BUFFER_SIZE];
5548:       memcpy(pNew->zFile, zFile, nFile);
5549:       zipfileDequote(pNew->zFile);
5550:     }
5551:   }
5552:   sqlite3_vtab_config(db, SQLITE_VTAB_DIRECTONLY);
5553:   *ppVtab = (sqlite3_vtab*)pNew;
5554:   return rc;
5555: }
