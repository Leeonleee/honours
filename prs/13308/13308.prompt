4199: /*
4200: ** Compare text in lexicographic order, except strings of digits
4201: ** compare in numeric order.
4202: */
4203: static int uintCollFunc(
4204:   void *notUsed,
4205:   int nKey1, const void *pKey1,
4206:   int nKey2, const void *pKey2
4207: ){
4208:   const unsigned char *zA = (const unsigned char*)pKey1;
4209:   const unsigned char *zB = (const unsigned char*)pKey2;
4210:   int i=0, j=0, x;
4211:   (void)notUsed;
4212:   while( i<nKey1 && j<nKey2 ){
4213:     x = zA[i] - zB[j];
4214:     if( isdigit(zA[i]) ){
4215:       int k;
4216:       if( !isdigit(zB[j]) ) return x;
4217:       while( i<nKey1 && zA[i]=='0' ){ i++; }
4218:       while( j<nKey2 && zB[j]=='0' ){ j++; }
4219:       k = 0;
4220:       while( i+k<nKey1 && isdigit(zA[i+k])
4221:              && j+k<nKey2 && isdigit(zB[j+k]) ){
4222:         k++;
4223:       }
4224:       if( i+k<nKey1 && isdigit(zA[i+k]) ){
4225:         return +1;
4226:       }else if( j+k<nKey2 && isdigit(zB[j+k]) ){
4227:         return -1;
4228:       }else{
4229:         x = memcmp(zA+i, zB+j, k);
4230:         if( x ) return x;
4231:         i += k;
4232:         j += k;
4233:       }
4234:     }else if( x ){
4235:       return x;
4236:     }else{
4237:       i++;
4238:       j++;
4239:     }
4240:   }
4241:   return (nKey1 - i) - (nKey2 - j);
4242: }
