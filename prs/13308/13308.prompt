7974: /*
7975: ** Prepare an SQL statement.
7976: */
7977: static int idxPrepareStmt(
7978:   sqlite3 *db,                    /* Database handle to compile against */
7979:   sqlite3_stmt **ppStmt,          /* OUT: Compiled SQL statement */
7980:   char **pzErrmsg,                /* OUT: sqlite3_malloc()ed error message */
7981:   const char *zSql                /* SQL statement to compile */
7982: ){
7983:   int rc = sqlite3_prepare_v2(db, zSql, -1, ppStmt, 0);
7984:   if( rc!=SQLITE_OK ){
7985:     *ppStmt = 0;
7986:     idxDatabaseError(db, pzErrmsg);
7987:   }
7988:   return rc;
