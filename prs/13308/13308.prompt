2095: /*
2096: ** Set the result stored by context ctx to a blob containing the
2097: ** contents of file zName.  Or, leave the result unchanged (NULL)
2098: ** if the file does not exist or is unreadable.
2099: **
2100: ** If the file exceeds the SQLite blob size limit, through an
2101: ** SQLITE_TOOBIG error.
2102: **
2103: ** Throw an SQLITE_IOERR if there are difficulties pulling the file
2104: ** off of disk.
2105: */
2106: static void readFileContents(sqlite3_context *ctx, const char *zName){
2107:   FILE *in;
2108:   sqlite3_int64 nIn;
2109:   void *pBuf;
2110:   sqlite3 *db;
2111:   int mxBlob;
2112: 
2113:   in = fopen(zName, "rb");
2114:   if( in==0 ){
2115:     /* File does not exist or is unreadable. Leave the result set to NULL. */
2116:     return;
2117:   }
2118:   fseek(in, 0, SEEK_END);
2119:   nIn = ftell(in);
2120:   rewind(in);
2121:   db = sqlite3_context_db_handle(ctx);
2122:   mxBlob = sqlite3_limit(db, SQLITE_LIMIT_LENGTH, -1);
2123:   if( nIn>mxBlob ){
2124:     sqlite3_result_error_code(ctx, SQLITE_TOOBIG);
2125:     fclose(in);
2126:     return;
2127:   }
2128:   pBuf = sqlite3_malloc64( nIn ? nIn : 1 );
2129:   if( pBuf==0 ){
2130:     sqlite3_result_error_nomem(ctx);
2131:     fclose(in);
2132:     return;
2133:   }
2134:   if( nIn==(sqlite3_int64)fread(pBuf, 1, (size_t)nIn, in) ){
2135:     sqlite3_result_blob64(ctx, pBuf, nIn, sqlite3_free);
2136:   }else{
2137:     sqlite3_result_error_code(ctx, SQLITE_IOERR);
2138:     sqlite3_free(pBuf);
2139:   }
2140:   fclose(in);
2141: }
