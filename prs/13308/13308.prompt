5959: /*
5960: ** If aBlob is not NULL, then it is a pointer to a buffer (nBlob bytes in
5961: ** size) containing an entire zip archive image. Or, if aBlob is NULL,
5962: ** then pFile is a file-handle open on a zip file. In either case, this
5963: ** function creates a ZipfileEntry object based on the zip archive entry
5964: ** for which the CDS record is at offset iOff.
5965: **
5966: ** If successful, SQLITE_OK is returned and (*ppEntry) set to point to
5967: ** the new object. Otherwise, an SQLite error code is returned and the
5968: ** final value of (*ppEntry) undefined.
5969: */
5970: static int zipfileGetEntry(
5971:   ZipfileTab *pTab,               /* Store any error message here */
5972:   const u8 *aBlob,                /* Pointer to in-memory file image */
5973:   int nBlob,                      /* Size of aBlob[] in bytes */
5974:   FILE *pFile,                    /* If aBlob==0, read from this file */
5975:   i64 iOff,                       /* Offset of CDS record */
5976:   ZipfileEntry **ppEntry          /* OUT: Pointer to new object */
5977: ){
5978:   u8 *aRead;
5979:   char **pzErr = &pTab->base.zErrMsg;
5980:   int rc = SQLITE_OK;
5981: 
5982:   if( aBlob==0 ){
5983:     aRead = pTab->aBuffer;
5984:     rc = zipfileReadData(pFile, aRead, ZIPFILE_CDS_FIXED_SZ, iOff, pzErr);
5985:   }else{
5986:     aRead = (u8*)&aBlob[iOff];
5987:   }
5988: 
5989:   if( rc==SQLITE_OK ){
5990:     sqlite3_int64 nAlloc;
5991:     ZipfileEntry *pNew;
5992: 
5993:     int nFile = zipfileGetU16(&aRead[ZIPFILE_CDS_NFILE_OFF]);
5994:     int nExtra = zipfileGetU16(&aRead[ZIPFILE_CDS_NFILE_OFF+2]);
5995:     nExtra += zipfileGetU16(&aRead[ZIPFILE_CDS_NFILE_OFF+4]);
5996: 
5997:     nAlloc = sizeof(ZipfileEntry) + nExtra;
5998:     if( aBlob ){
5999:       nAlloc += zipfileGetU32(&aRead[ZIPFILE_CDS_SZCOMPRESSED_OFF]);
6000:     }
6001: 
6002:     pNew = (ZipfileEntry*)sqlite3_malloc64(nAlloc);
6003:     if( pNew==0 ){
6004:       rc = SQLITE_NOMEM;
6005:     }else{
6006:       memset(pNew, 0, sizeof(ZipfileEntry));
6007:       rc = zipfileReadCDS(aRead, &pNew->cds);
6008:       if( rc!=SQLITE_OK ){
6009:         *pzErr = sqlite3_mprintf("failed to read CDS at offset %lld", iOff);
6010:       }else if( aBlob==0 ){
6011:         rc = zipfileReadData(
6012:             pFile, aRead, nExtra+nFile, iOff+ZIPFILE_CDS_FIXED_SZ, pzErr
6013:         );
6014:       }else{
6015:         aRead = (u8*)&aBlob[iOff + ZIPFILE_CDS_FIXED_SZ];
6016:       }
6017:     }
6018: 
6019:     if( rc==SQLITE_OK ){
6020:       u32 *pt = &pNew->mUnixTime;
6021:       pNew->cds.zFile = sqlite3_mprintf("%.*s", nFile, aRead);
6022:       pNew->aExtra = (u8*)&pNew[1];
6023:       memcpy(pNew->aExtra, &aRead[nFile], nExtra);
6024:       if( pNew->cds.zFile==0 ){
6025:         rc = SQLITE_NOMEM;
6026:       }else if( 0==zipfileScanExtra(&aRead[nFile], pNew->cds.nExtra, pt) ){
6027:         pNew->mUnixTime = zipfileMtime(&pNew->cds);
6028:       }
6029:     }
6030: 
6031:     if( rc==SQLITE_OK ){
6032:       static const int szFix = ZIPFILE_LFH_FIXED_SZ;
6033:       ZipfileLFH lfh;
6034:       if( pFile ){
6035:         rc = zipfileReadData(pFile, aRead, szFix, pNew->cds.iOffset, pzErr);
6036:       }else{
6037:         aRead = (u8*)&aBlob[pNew->cds.iOffset];
6038:       }
6039: 
6040:       rc = zipfileReadLFH(aRead, &lfh);
6041:       if( rc==SQLITE_OK ){
6042:         pNew->iDataOff =  pNew->cds.iOffset + ZIPFILE_LFH_FIXED_SZ;
6043:         pNew->iDataOff += lfh.nFile + lfh.nExtra;
6044:         if( aBlob && pNew->cds.szCompressed ){
6045:           pNew->aData = &pNew->aExtra[nExtra];
6046:           memcpy(pNew->aData, &aBlob[pNew->iDataOff], pNew->cds.szCompressed);
6047:         }
6048:       }else{
6049:         *pzErr = sqlite3_mprintf("failed to read LFH at offset %d",
6050:             (int)pNew->cds.iOffset
6051:         );
6052:       }
6053:     }
6054: 
6055:     if( rc!=SQLITE_OK ){
6056:       zipfileEntryFree(pNew);
6057:     }else{
6058:       *ppEntry = pNew;
6059:     }
6060:   }
6061: 
6062:   return rc;
6063: }
