19257: /*
19258: ** Run a single line of SQL.  Return the number of errors.
19259: */
19260: static int runOneSqlLine(ShellState *p, char *zSql, FILE *in, int startline){
19261:   int rc;
19262:   char *zErrMsg = 0;
19263: 
19264:   open_db(p, 0);
19265:   if( ShellHasFlag(p,SHFLG_Backslash) ) resolve_backslashes(zSql);
19266:   if( p->flgProgress & SHELL_PROGRESS_RESET ) p->nProgress = 0;
19267: #ifndef SHELL_USE_LOCAL_GETLINE
19268:   if( zSql && *zSql && *zSql != '\3' ) shell_add_history(zSql);
19269: #endif
19270:   BEGIN_TIMER;
19271:   rc = shell_exec(p, zSql, &zErrMsg);
19272:   END_TIMER;
19273:   if( rc || zErrMsg ){
19274:     if( zErrMsg!=0 ){
19275: 	  printDatabaseError(zErrMsg);
19276:       sqlite3_free(zErrMsg);
19277:       zErrMsg = 0;
19278:     }else{
19279: 	  shellDatabaseError(p->db);
19280:     }
