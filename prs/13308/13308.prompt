3141: ** Reset the completion_cursor.
3142: */
3143: static void completionCursorReset(completion_cursor *pCur){
3144:   sqlite3_free(pCur->zPrefix);   pCur->zPrefix = 0;  pCur->nPrefix = 0;
3145:   sqlite3_free(pCur->zLine);     pCur->zLine = 0;    pCur->nLine = 0;
