11845: /*
11846: ** Generate an appropriate SELFTEST table in the main database.
11847: */
11848: static void createSelftestTable(ShellState *p){
11849:   char *zErrMsg = 0;
11850:   sqlite3_exec(p->db,
11851:     "SAVEPOINT selftest_init;\n"
11852:     "CREATE TABLE IF NOT EXISTS selftest(\n"
11853:     "  tno INTEGER PRIMARY KEY,\n"   /* Test number */
11854:     "  op TEXT,\n"                   /* Operator:  memo run */
11855:     "  cmd TEXT,\n"                  /* Command text */
11856:     "  ans TEXT\n"                   /* Desired answer */
11857:     ");"
11858:     "CREATE TEMP TABLE [_shell$self](op,cmd,ans);\n"
11859:     "INSERT INTO [_shell$self](rowid,op,cmd)\n"
11860:     "  VALUES(coalesce((SELECT (max(tno)+100)/10 FROM selftest),10),\n"
11861:     "         'memo','Tests generated by --init');\n"
11862:     "INSERT INTO [_shell$self]\n"
11863:     "  SELECT 'run',\n"
11864:     "    'SELECT hex(sha3_query(''SELECT type,name,tbl_name,sql "
11865:                                  "FROM sqlite_schema ORDER BY 2'',224))',\n"
11866:     "    hex(sha3_query('SELECT type,name,tbl_name,sql "
11867:                           "FROM sqlite_schema ORDER BY 2',224));\n"
11868:     "INSERT INTO [_shell$self]\n"
11869:     "  SELECT 'run',"
11870:     "    'SELECT hex(sha3_query(''SELECT * FROM \"' ||"
11871:     "        printf('%w',name) || '\" NOT INDEXED'',224))',\n"
11872:     "    hex(sha3_query(printf('SELECT * FROM \"%w\" NOT INDEXED',name),224))\n"
11873:     "  FROM (\n"
11874:     "    SELECT name FROM sqlite_schema\n"
11875:     "     WHERE type='table'\n"
11876:     "       AND name<>'selftest'\n"
11877:     "       AND coalesce(rootpage,0)>0\n"
11878:     "  )\n"
11879:     " ORDER BY name;\n"
11880:     "INSERT INTO [_shell$self]\n"
11881:     "  VALUES('run','PRAGMA integrity_check','ok');\n"
11882:     "INSERT INTO selftest(tno,op,cmd,ans)"
11883:     "  SELECT rowid*10,op,cmd,ans FROM [_shell$self];\n"
11884:     "DROP TABLE [_shell$self];"
11885:     ,0,0,&zErrMsg);
11886:   if( zErrMsg ){
11887:     utf8_printf(stderr, "SELFTEST initialization failure: %s\n", zErrMsg);
11888:     sqlite3_free(zErrMsg);
11889:   }
11890:   sqlite3_exec(p->db, "RELEASE selftest_init",0,0,0);
11891: }
