11332: /* Return the next EXPLAIN QUERY PLAN line with iEqpId that occurs after
11333: ** pOld, or return the first such line if pOld is NULL
11334: */
11335: static EQPGraphRow *eqp_next_row(ShellState *p, int iEqpId, EQPGraphRow *pOld){
11336:   EQPGraphRow *pRow = pOld ? pOld->pNext : p->sGraph.pRow;
11337:   while( pRow && pRow->iParentId!=iEqpId ) pRow = pRow->pNext;
11338:   return pRow;
11339: }
