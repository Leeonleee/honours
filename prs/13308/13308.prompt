19290: /*
19291: ** Read input from *in and process it.  If *in==0 then input
19292: ** is interactive - the user is typing it it.  Otherwise, input
19293: ** is coming from a file or device.  A prompt is issued and history
19294: ** is saved only if input is interactive.  An interrupt signal will
19295: ** cause this routine to exit immediately, unless input is interactive.
19296: **
19297: ** Return the number of errors.
19298: */
19299: static int process_input(ShellState *p){
19300:   char *zLine = 0;          /* A single input line */
19301:   char *zSql = 0;           /* Accumulated SQL text */
19302:   int nLine;                /* Length of current line */
19303:   int nSql = 0;             /* Bytes of zSql[] used */
19304:   int nAlloc = 0;           /* Allocated zSql[] space */
19305:   int nSqlPrior = 0;        /* Bytes of zSql[] used by prior line */
19306:   int rc;                   /* Error code */
19307:   int errCnt = 0;           /* Number of errors seen */
19308:   int startline = 0;        /* Line number for start of current input */
19309:   int numCtrlC = 0;
19310:   p->lineno = 0;
19311:   while( errCnt==0 || !bail_on_error || (p->in==0 && stdin_is_interactive) ){
19312:     fflush(p->out);
19313:     zLine = one_input_line(p->in, zLine, nSql>0);
19314:     if( zLine==0 ){
19315:       /* End of input */
19316:       if( p->in==0 && stdin_is_interactive ) printf("\n");
19317:       break;
19318:     }
19319:     if (*zLine == '\3') {
19320:       // ctrl c: reset sql statement
19321:       if (nSql == 0 && zLine[1] == '\0' && stdin_is_interactive) {
19322:         // if in interactive mode and we press ctrl c twice
19323:         // on an empty line, we exit
19324:         numCtrlC++;
19325:         if (numCtrlC >= 2) {
19326:           break;
19327:         }
19328:       }
19329:       nSql = 0;
19330:       continue;
19331:     } else {
19332:       numCtrlC = 0;
19333:     }
19334:     if( seenInterrupt ){
19335:       if( p->in!=0 ) break;
19336:       seenInterrupt = 0;
19337:     }
19338:     p->lineno++;
19339:     if( nSql==0 && _all_whitespace(zLine) ){
19340:       if( ShellHasFlag(p, SHFLG_Echo) ) printf("%s\n", zLine);
19341:       continue;
19342:     }
19343:     if( zLine && (zLine[0]=='.' || zLine[0]=='#') && nSql==0 ){
19344:       if( ShellHasFlag(p, SHFLG_Echo) ) printf("%s\n", zLine);
19345:       if( zLine[0]=='.' ){
19346: #ifndef SHELL_USE_LOCAL_GETLINE
19347:         if( zLine && *zLine && *zLine != '\3' ) shell_add_history(zLine);
19348: #endif
19349:         rc = do_meta_command(zLine, p);
19350:         if( rc==2 ){ /* exit requested */
19351:           break;
19352:         }else if( rc ){
19353:           errCnt++;
19354:         }
19355:       }
19356:       continue;
19357:     }
19358:     if( line_is_command_terminator(zLine) && line_is_complete(zSql, nSql) ){
19359:       memcpy(zLine,";",2);
19360:     }
19361:     nLine = strlen30(zLine);
19362:     if( nSql+nLine+2>=nAlloc ){
19363:       nAlloc = nSql+nLine+100;
19364:       zSql = realloc(zSql, nAlloc);
19365:       if( zSql==0 ) shell_out_of_memory();
19366:     }
19367:     nSqlPrior = nSql;
19368:     if( nSql==0 ){
19369:       int i;
19370:       for(i=0; zLine[i] && IsSpace(zLine[i]); i++){}
19371:       assert( nAlloc>0 && zSql!=0 );
19372:       memcpy(zSql, zLine+i, nLine+1-i);
19373:       startline = p->lineno;
19374:       nSql = nLine-i;
19375:     }else{
19376:       zSql[nSql++] = '\n';
19377:       memcpy(zSql+nSql, zLine, nLine+1);
19378:       nSql += nLine;
19379:     }
19380:     if( nSql && line_contains_semicolon(&zSql[nSqlPrior], nSql-nSqlPrior)
19381:                 && sqlite3_complete(zSql) ){
19382:       errCnt += runOneSqlLine(p, zSql, p->in, startline);
19383:       nSql = 0;
19384:       if( p->outCount ){
19385:         output_reset(p);
19386:         p->outCount = 0;
19387:       }else{
19388:         clearTempFile(p);
19389:       }
19390:     }else if( nSql && _all_whitespace(zSql) ){
19391:       if( ShellHasFlag(p, SHFLG_Echo) ) printf("%s\n", zSql);
19392:       nSql = 0;
19393:     }
19394:   }
19395:   if( nSql && !_all_whitespace(zSql) ){
19396:     errCnt += runOneSqlLine(p, zSql, p->in, startline);
19397:   }
19398:   free(zSql);
19399:   free(zLine);
19400:   return errCnt>0;
19401: }
