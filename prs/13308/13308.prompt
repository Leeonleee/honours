14197: /*
14198: ** Linenoise completion callback
14199: */
14200: static void linenoise_completion(const char *zLine, linenoiseCompletions *lc){
14201:   int nLine = strlen30(zLine);
14202:   int copiedSuggestion = 0;
14203:   sqlite3_stmt *pStmt = 0;
14204:   char *zSql;
14205:   char zBuf[1000];
14206: 
14207:   if( nLine>sizeof(zBuf)-30 ) return;
