15234: /*
15235: ** Format a WHERE clause that can be used against the "sqlar" table to
15236: ** identify all archive members that match the command arguments held
15237: ** in (*pAr). Leave this WHERE clause in (*pzWhere) before returning.
15238: ** The caller is responsible for eventually calling sqlite3_free() on
15239: ** any non-NULL (*pzWhere) value.
15240: */
15241: static void arWhereClause(
15242:   int *pRc,
15243:   ArCommand *pAr,
15244:   char **pzWhere                  /* OUT: New WHERE clause */
15245: ){
15246:   char *zWhere = 0;
15247:   if( *pRc==SQLITE_OK ){
15248:     if( pAr->nArg==0 ){
15249:       zWhere = sqlite3_mprintf("1");
15250:     }else{
15251:       int i;
15252:       const char *zSep = "";
15253:       for(i=0; i<pAr->nArg; i++){
15254:         const char *z = pAr->azArg[i];
15255:         zWhere = sqlite3_mprintf(
15256:           "%z%s name = '%q' OR substr(name,1,%d) = '%q/'",
15257:           zWhere, zSep, z, strlen30(z)+1, z
15258:         );
15259:         if( zWhere==0 ){
15260:           *pRc = SQLITE_NOMEM;
