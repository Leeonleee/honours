8472: /*
8473: ** This function appends an index column definition suitable for constraint
8474: ** pCons to the string passed as zIn and returns the result.
8475: */
8476: static char *idxAppendColDefn(
8477:   int *pRc,                       /* IN/OUT: Error code */
8478:   char *zIn,                      /* Column defn accumulated so far */
8479:   IdxTable *pTab,                 /* Table index will be created on */
8480:   IdxConstraint *pCons
8481: ){
8482:   char *zRet = zIn;
8483:   IdxColumn *p = &pTab->aCol[pCons->iCol];
8484:   if( zRet ) zRet = idxAppendText(pRc, zRet, ", ");
8485: 
8486:   if( idxIdentifierRequiresQuotes(p->zName) ){
8487:     zRet = idxAppendText(pRc, zRet, "%Q", p->zName);
8488:   }else{
8489:     zRet = idxAppendText(pRc, zRet, "%s", p->zName);
8490:   }
8491: 
8492:   if( sqlite3_stricmp(p->zColl, pCons->zColl) ){
8493:     if( idxIdentifierRequiresQuotes(pCons->zColl) ){
8494:       zRet = idxAppendText(pRc, zRet, " COLLATE %Q", pCons->zColl);
8495:     }else{
8496:       zRet = idxAppendText(pRc, zRet, " COLLATE %s", pCons->zColl);
8497:     }
8498:   }
8499: 
8500:   if( pCons->bDesc ){
8501:     zRet = idxAppendText(pRc, zRet, " DESC");
8502:   }
8503:   return zRet;
8504: }
