13626: /*
13627: ** Output help text.
13628: **
13629: ** zPattern describes the set of commands for which help text is provided.
13630: ** If zPattern is NULL, then show all commands, but only give a one-line
13631: ** description of each.
13632: **
13633: ** Return the number of matches.
13634: */
13635: static int showHelp(FILE *out, const char *zPattern){
13636:   int i = 0;
13637:   int j = 0;
13638:   int n = 0;
13639:   char *zPat;
13640:   if( zPattern==0
13641:    || zPattern[0]=='0'
13642:    || strcmp(zPattern,"-a")==0
13643:    || strcmp(zPattern,"-all")==0
13644:    || strcmp(zPattern,"--all")==0
13645:   ){
13646:     /* Show all commands, but only one line per command */
13647:     if( zPattern==0 ) zPattern = "";
13648:     for(i=0; i<ArraySize(azHelp); i++){
13649:       if( azHelp[i][0]=='.' || zPattern[0] ){
13650:         utf8_printf(out, "%s\n", azHelp[i]);
13651:         n++;
13652:       }
13653:     }
13654:   }else{
13655:     /* Look for commands that for which zPattern is an exact prefix */
13656:     zPat = sqlite3_mprintf(".%s*", zPattern);
13657:     for(i=0; i<ArraySize(azHelp); i++){
13658:       if( sqlite3_strglob(zPat, azHelp[i])==0 ){
13659:         utf8_printf(out, "%s\n", azHelp[i]);
13660:         j = i+1;
13661:         n++;
13662:       }
13663:     }
13664:     sqlite3_free(zPat);
13665:     if( n ){
13666:       if( n==1 ){
13667:         /* when zPattern is a prefix of exactly one command, then include the
13668:         ** details of that command, which should begin at offset j */
13669:         while( j<ArraySize(azHelp)-1 && azHelp[j][0]!='.' ){
13670:           utf8_printf(out, "%s\n", azHelp[j]);
13671:           j++;
13672:         }
13673:       }
13674:       return n;
13675:     }
13676:     /* Look for commands that contain zPattern anywhere.  Show the complete
13677:     ** text of all commands that match. */
13678:     zPat = sqlite3_mprintf("%%%s%%", zPattern);
13679:     for(i=0; i<ArraySize(azHelp); i++){
13680:       if( azHelp[i][0]=='.' ) j = i;
13681:       if( sqlite3_strlike(zPat, azHelp[i], 0)==0 ){
13682:         utf8_printf(out, "%s\n", azHelp[j]);
13683:         while( j<ArraySize(azHelp)-1 && azHelp[j+1][0]!='.' ){
13684:           j++;
13685:           utf8_printf(out, "%s\n", azHelp[j]);
13686:         }
13687:         i = j;
13688:         n++;
13689:       }
13690:     }
13691:     sqlite3_free(zPat);
13692:   }
13693:   return n;
13694: }
