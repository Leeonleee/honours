9187: static int idxPopulateOneStat1(
9188:   sqlite3expert *p,
9189:   sqlite3_stmt *pIndexXInfo,
9190:   sqlite3_stmt *pWriteStat,
9191:   const char *zTab,
9192:   const char *zIdx,
9193:   char **pzErr
9194: ){
9195:   char *zCols = 0;
9196:   char *zOrder = 0;
9197:   char *zQuery = 0;
9198:   int nCol = 0;
9199:   int i;
9200:   sqlite3_stmt *pQuery = 0;
9201:   int *aStat = 0;
9202:   int rc = SQLITE_OK;
9203: 
9204:   assert( p->iSample>0 );
9205: 
9206:   /* Formulate the query text */
9207:   sqlite3_bind_text(pIndexXInfo, 1, zIdx, -1, SQLITE_STATIC);
9208:   while( SQLITE_OK==rc && SQLITE_ROW==sqlite3_step(pIndexXInfo) ){
9209:     const char *zComma = zCols==0 ? "" : ", ";
9210:     const char *zName = (const char*)sqlite3_column_text(pIndexXInfo, 0);
9211:     const char *zColl = (const char*)sqlite3_column_text(pIndexXInfo, 1);
9212:     zCols = idxAppendText(&rc, zCols,
9213:         "%sx.%Q IS rem(%d, x.%Q) COLLATE %s", zComma, zName, nCol, zName, zColl
9214:     );
9215:     zOrder = idxAppendText(&rc, zOrder, "%s%d", zComma, ++nCol);
9216:   }
9217:   sqlite3_reset(pIndexXInfo);
9218:   if( rc==SQLITE_OK ){
9219:     if( p->iSample==100 ){
9220:       zQuery = sqlite3_mprintf(
9221:           "SELECT %s FROM %Q x ORDER BY %s", zCols, zTab, zOrder
9222:       );
9223:     }else{
9224:       zQuery = sqlite3_mprintf(
9225:           "SELECT %s FROM temp."UNIQUE_TABLE_NAME" x ORDER BY %s", zCols, zOrder
9226:       );
9227:     }
9228:   }
9229:   sqlite3_free(zCols);
9230:   sqlite3_free(zOrder);
9231: 
9232:   /* Formulate the query text */
9233:   if( rc==SQLITE_OK ){
9234:     sqlite3 *dbrem = (p->iSample==100 ? p->db : p->dbv);
9235:     rc = idxPrepareStmt(dbrem, &pQuery, pzErr, zQuery);
9236:   }
9237:   sqlite3_free(zQuery);
9238: 
9239:   if( rc==SQLITE_OK ){
9240:     aStat = (int*)idxMalloc(&rc, sizeof(int)*(nCol+1));
9241:   }
9242:   if( rc==SQLITE_OK && SQLITE_ROW==sqlite3_step(pQuery) ){
9243:     IdxHashEntry *pEntry;
9244:     char *zStat = 0;
9245:     for(i=0; i<=nCol; i++) aStat[i] = 1;
9246:     while( rc==SQLITE_OK && SQLITE_ROW==sqlite3_step(pQuery) ){
9247:       aStat[0]++;
9248:       for(i=0; i<nCol; i++){
9249:         if( sqlite3_column_int(pQuery, i)==0 ) break;
9250:       }
9251:       for(/*no-op*/; i<nCol; i++){
9252:         aStat[i+1]++;
9253:       }
9254:     }
9255: 
9256:     if( rc==SQLITE_OK ){
9257:       int s0 = aStat[0];
9258:       zStat = sqlite3_mprintf("%d", s0);
9259:       if( zStat==0 ) rc = SQLITE_NOMEM;
9260:       for(i=1; rc==SQLITE_OK && i<=nCol; i++){
9261:         zStat = idxAppendText(&rc, zStat, " %d", (s0+aStat[i]/2) / aStat[i]);
9262:       }
9263:     }
9264: 
9265:     if( rc==SQLITE_OK ){
9266:       sqlite3_bind_text(pWriteStat, 1, zTab, -1, SQLITE_STATIC);
9267:       sqlite3_bind_text(pWriteStat, 2, zIdx, -1, SQLITE_STATIC);
9268:       sqlite3_bind_text(pWriteStat, 3, zStat, -1, SQLITE_STATIC);
9269:       sqlite3_step(pWriteStat);
9270:       rc = sqlite3_reset(pWriteStat);
9271:     }
9272: 
9273:     pEntry = idxHashFind(&p->hIdx, zIdx, STRLEN(zIdx));
9274:     if( pEntry ){
9275:       assert( pEntry->zVal2==0 );
9276:       pEntry->zVal2 = zStat;
9277:     }else{
9278:       sqlite3_free(zStat);
9279:     }
9280:   }
9281:   sqlite3_free(aStat);
9282:   idxFinalize(&rc, pQuery);
9283: 
9284:   return rc;
9285: }
