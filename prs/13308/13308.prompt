5717: /*
5718: ** Read and return a 16-bit little-endian unsigned integer from buffer aBuf.
5719: */
5720: static u16 zipfileGetU16(const u8 *aBuf){
5721:   return (aBuf[1] << 8) + aBuf[0];
5722: }
5723: 
5724: /*
5725: ** Read and return a 32-bit little-endian unsigned integer from buffer aBuf.
5726: */
5727: static u32 zipfileGetU32(const u8 *aBuf){
5728:   return ((u32)(aBuf[3]) << 24)
5729:        + ((u32)(aBuf[2]) << 16)
5730:        + ((u32)(aBuf[1]) <<  8)
5731:        + ((u32)(aBuf[0]) <<  0);
5732: }
5733: 
5734: /*
5735: ** Write a 16-bit little endiate integer into buffer aBuf.
5736: */
5737: static void zipfilePutU16(u8 *aBuf, u16 val){
5738:   aBuf[0] = val & 0xFF;
5739:   aBuf[1] = (val>>8) & 0xFF;
5740: }
5741: 
5742: /*
5743: ** Write a 32-bit little endiate integer into buffer aBuf.
5744: */
5745: static void zipfilePutU32(u8 *aBuf, u32 val){
5746:   aBuf[0] = val & 0xFF;
5747:   aBuf[1] = (val>>8) & 0xFF;
5748:   aBuf[2] = (val>>16) & 0xFF;
5749:   aBuf[3] = (val>>24) & 0xFF;
5750: }
5751: 
5752: #define zipfileRead32(aBuf) ( aBuf+=4, zipfileGetU32(aBuf-4) )
5753: #define zipfileRead16(aBuf) ( aBuf+=2, zipfileGetU16(aBuf-2) )
5754: 
5755: #define zipfileWrite32(aBuf,val) { zipfilePutU32(aBuf,val); aBuf+=4; }
5756: #define zipfileWrite16(aBuf,val) { zipfilePutU16(aBuf,val); aBuf+=2; }
