14680: /*
14681: ** Compare the pattern in zGlob[] against the text in z[].  Return TRUE
14682: ** if they match and FALSE (0) if they do not match.
14683: **
14684: ** Globbing rules:
14685: **
14686: **      '*'       Matches any sequence of zero or more characters.
14687: **
14688: **      '?'       Matches exactly one character.
14689: **
14690: **     [...]      Matches one character from the enclosed list of
14691: **                characters.
14692: **
14693: **     [^...]     Matches one character not in the enclosed list.
14694: **
14695: **      '#'       Matches any sequence of one or more digits with an
14696: **                optional + or - sign in front
14697: **
14698: **      ' '       Any span of whitespace matches any other span of
14699: **                whitespace.
14700: **
14701: ** Extra whitespace at the end of z[] is ignored.
14702: */
14703: static int testcase_glob(const char *zGlob, const char *z){
14704:   int c, c2;
14705:   int invert;
14706:   int seen;
14707: 
14708:   while( (c = (*(zGlob++)))!=0 ){
14709:     if( IsSpace(c) ){
14710:       if( !IsSpace(*z) ) return 0;
14711:       while( IsSpace(*zGlob) ) zGlob++;
14712:       while( IsSpace(*z) ) z++;
14713:     }else if( c=='*' ){
14714:       while( (c=(*(zGlob++))) == '*' || c=='?' ){
14715:         if( c=='?' && (*(z++))==0 ) return 0;
14716:       }
14717:       if( c==0 ){
14718:         return 1;
14719:       }else if( c=='[' ){
14720:         while( *z && testcase_glob(zGlob-1,z)==0 ){
14721:           z++;
14722:         }
14723:         return (*z)!=0;
14724:       }
14725:       while( (c2 = (*(z++)))!=0 ){
14726:         while( c2!=c ){
14727:           c2 = *(z++);
14728:           if( c2==0 ) return 0;
14729:         }
14730:         if( testcase_glob(zGlob,z) ) return 1;
14731:       }
14732:       return 0;
14733:     }else if( c=='?' ){
14734:       if( (*(z++))==0 ) return 0;
14735:     }else if( c=='[' ){
14736:       int prior_c = 0;
14737:       seen = 0;
14738:       invert = 0;
14739:       c = *(z++);
14740:       if( c==0 ) return 0;
14741:       c2 = *(zGlob++);
14742:       if( c2=='^' ){
14743:         invert = 1;
14744:         c2 = *(zGlob++);
14745:       }
14746:       if( c2==']' ){
14747:         if( c==']' ) seen = 1;
14748:         c2 = *(zGlob++);
14749:       }
14750:       while( c2 && c2!=']' ){
14751:         if( c2=='-' && zGlob[0]!=']' && zGlob[0]!=0 && prior_c>0 ){
14752:           c2 = *(zGlob++);
14753:           if( c>=prior_c && c<=c2 ) seen = 1;
14754:           prior_c = 0;
14755:         }else{
14756:           if( c==c2 ){
14757:             seen = 1;
14758:           }
14759:           prior_c = c2;
14760:         }
14761:         c2 = *(zGlob++);
14762:       }
14763:       if( c2==0 || (seen ^ invert)==0 ) return 0;
14764:     }else if( c=='#' ){
14765:       if( (z[0]=='-' || z[0]=='+') && IsDigit(z[1]) ) z++;
14766:       if( !IsDigit(z[0]) ) return 0;
14767:       z++;
14768:       while( IsDigit(z[0]) ){ z++; }
14769:     }else{
14770:       if( c!=(*(z++)) ) return 0;
14771:     }
14772:   }
14773:   while( IsSpace(*z) ){ z++; }
14774:   return *z==0;
14775: }
