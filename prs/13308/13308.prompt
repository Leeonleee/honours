3870: /*
3871: ** Check to see if the file is an ordinary SQLite database file.
3872: */
3873: static int apndIsOrdinaryDatabaseFile(sqlite3_int64 sz, sqlite3_file *pFile){
3874:   int rc;
3875:   char zHdr[16];
3876:   static const char aSqliteHdr[] = "SQLite format 3";
3877:   if( sz<512 ) return 0;
3878:   rc = pFile->pMethods->xRead(pFile, zHdr, sizeof(zHdr), 0);
3879:   if( rc ) return 0;
