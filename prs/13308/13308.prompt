1400: /*
1401: ** Implementation of the POSIX closedir() function using the MSVCRT.
1402: */
1403: INT closedir(
1404:   LPDIR dirp
1405: ){
1406:   INT result = 0;
1407: 
1408:   if( dirp==NULL ) return EINVAL;
1409: 
1410:   if( dirp->d_handle!=NULL_INTPTR_T && dirp->d_handle!=BAD_INTPTR_T ){
1411:     result = _findclose(dirp->d_handle);
1412:   }
1413: 
1414:   sqlite3_free(dirp);
1415:   return result;
1416: }
