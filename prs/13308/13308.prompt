8685: /*
8686: ** Create candidate indexes in database [dbm] based on the data in
8687: ** linked-list pScan.
8688: */
8689: static int idxCreateCandidates(sqlite3expert *p){
8690:   int rc = SQLITE_OK;
8691:   IdxScan *pIter;
8692: 
8693:   for(pIter=p->pScan; pIter && rc==SQLITE_OK; pIter=pIter->pNextScan){
8694:     rc = idxCreateFromWhere(p, pIter, 0);
8695:     if( rc==SQLITE_OK && pIter->pOrder ){
8696:       rc = idxCreateFromWhere(p, pIter, pIter->pOrder);
8697:     }
8698:   }
8699: 
8700:   return rc;
8701: }
