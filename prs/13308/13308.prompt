13783: /*
13784: ** Try to deduce the type of file for zName based on its content.  Return
13785: ** one of the SHELL_OPEN_* constants.
13786: **
13787: ** If the file does not exist or is empty but its name looks like a ZIP
13788: ** archive and the dfltZip flag is true, then assume it is a ZIP archive.
13789: ** Otherwise, assume an ordinary database regardless of the filename if
13790: ** the type cannot be determined from content.
13791: */
13792: int deduceDatabaseType(const char *zName, int dfltZip){
13793:   FILE *f = fopen(zName, "rb");
13794:   size_t n;
13795:   int rc = SHELL_OPEN_UNSPEC;
13796:   char zBuf[100];
13797:   if( f==0 ){
13798:     if( dfltZip && sqlite3_strlike("%.zip",zName,0)==0 ){
13799:        return SHELL_OPEN_ZIPFILE;
13800:     }else{
13801:        return SHELL_OPEN_NORMAL;
13802:     }
13803:   }
13804:   n = fread(zBuf, 16, 1, f);
13805:   if( n==1 && memcmp(zBuf, "SQLite format 3", 16)==0 ){
13806:     fclose(f);
13807:     return SHELL_OPEN_NORMAL;
13808:   }
13809:   fseek(f, -25, SEEK_END);
13810:   n = fread(zBuf, 25, 1, f);
13811:   if( n==1 && memcmp(zBuf, "Start-Of-SQLite3-", 17)==0 ){
13812:     rc = SHELL_OPEN_APPENDVFS;
13813:   }else{
13814:     fseek(f, -22, SEEK_END);
13815:     n = fread(zBuf, 22, 1, f);
13816:     if( n==1 && zBuf[0]==0x50 && zBuf[1]==0x4b && zBuf[2]==0x05
13817:        && zBuf[3]==0x06 ){
13818:       rc = SHELL_OPEN_ZIPFILE;
13819:     }else if( n==0 && dfltZip && sqlite3_strlike("%.zip",zName,0)==0 ){
13820:       rc = SHELL_OPEN_ZIPFILE;
13821:     }
13822:   }
13823:   fclose(f);
13824:   return rc;
13825: }
