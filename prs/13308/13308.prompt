2431: ** Implementation of the "writefile(W,X[,Y[,Z]]])" SQL function.
2432: ** Refer to header comments at the top of this file for details.
2433: */
2434: static void writefileFunc(
2435:   sqlite3_context *context,
2436:   int argc,
2437:   sqlite3_value **argv
2438: ){
2439:   const char *zFile;
2440:   mode_t mode = 0;
2441:   int res;
2442:   sqlite3_int64 mtime = -1;
2443: 
2444:   if( argc<2 || argc>4 ){
2445:     sqlite3_result_error(context,
