9405: /*
9406: ** Allocate a new sqlite3expert object.
9407: */
9408: sqlite3expert *sqlite3_expert_new(sqlite3 *db, char **pzErrmsg){
9409:   int rc = SQLITE_OK;
9410:   sqlite3expert *pNew;
9411: 
9412:   pNew = (sqlite3expert*)idxMalloc(&rc, sizeof(sqlite3expert));
9413: 
9414:   /* Open two in-memory databases to work with. The "vtab database" (dbv)
9415:   ** will contain a virtual table corresponding to each real table in
9416:   ** the user database schema, and a copy of each view. It is used to
9417:   ** collect information regarding the WHERE, ORDER BY and other clauses
9418:   ** of the user's query.
9419:   */
9420:   if( rc==SQLITE_OK ){
9421:     pNew->db = db;
9422:     pNew->iSample = 100;
9423:     rc = sqlite3_open(":memory:", &pNew->dbv);
9424:   }
9425:   if( rc==SQLITE_OK ){
9426:     rc = sqlite3_open(":memory:", &pNew->dbm);
9427:     if( rc==SQLITE_OK ){
9428:       sqlite3_db_config(pNew->dbm, SQLITE_DBCONFIG_TRIGGER_EQP, 1, (int*)0);
9429:     }
9430:   }
9431: 
9432: 
9433:   /* Copy the entire schema of database [db] into [dbm]. */
9434:   if( rc==SQLITE_OK ){
9435:     sqlite3_stmt *pSql;
9436:     rc = idxPrintfPrepareStmt(pNew->db, &pSql, pzErrmsg,
9437:         "SELECT sql FROM sqlite_schema WHERE name NOT LIKE 'sqlite_%%'"
9438:         " AND sql NOT LIKE 'CREATE VIRTUAL %%'"
9439:     );
9440:     while( rc==SQLITE_OK && SQLITE_ROW==sqlite3_step(pSql) ){
9441:       const char *zSql = (const char*)sqlite3_column_text(pSql, 0);
9442:       rc = sqlite3_exec(pNew->dbm, zSql, 0, 0, pzErrmsg);
9443:     }
9444:     idxFinalize(&rc, pSql);
9445:   }
9446: 
9447:   /* Create the vtab schema */
9448:   if( rc==SQLITE_OK ){
9449:     rc = idxCreateVtabSchema(pNew, pzErrmsg);
9450:   }
9451: 
9452:   /* Register the auth callback with dbv */
9453:   if( rc==SQLITE_OK ){
9454:     sqlite3_set_authorizer(pNew->dbv, idxAuthCallback, (void*)pNew);
9455:   }
9456: 
9457:   /* If an error has occurred, free the new object and reutrn NULL. Otherwise,
9458:   ** return the new sqlite3expert handle.  */
9459:   if( rc!=SQLITE_OK ){
9460:     sqlite3_expert_destroy(pNew);
9461:     pNew = 0;
9462:   }
9463:   return pNew;
9464: }
