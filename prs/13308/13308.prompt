14265: /*
14266: ** Do C-language style dequoting.
14267: **
14268: **    \a    -> alarm
14269: **    \b    -> backspace
14270: **    \t    -> tab
14271: **    \n    -> newline
14272: **    \v    -> vertical tab
14273: **    \f    -> form feed
14274: **    \r    -> carriage return
14275: **    \s    -> space
14276: **    \"    -> "
14277: **    \'    -> '
14278: **    \\    -> backslash
14279: **    \NNN  -> ascii character NNN in octal
14280: */
14281: static void resolve_backslashes(char *z){
14282:   int i, j;
14283:   char c;
14284:   while( *z && *z!='\\' ) z++;
14285:   for(i=j=0; (c = z[i])!=0; i++, j++){
14286:     if( c=='\\' && z[i+1]!=0 ){
14287:       c = z[++i];
14288:       if( c=='a' ){
14289:         c = '\a';
14290:       }else if( c=='b' ){
14291:         c = '\b';
14292:       }else if( c=='t' ){
14293:         c = '\t';
14294:       }else if( c=='n' ){
14295:         c = '\n';
14296:       }else if( c=='v' ){
14297:         c = '\v';
14298:       }else if( c=='f' ){
14299:         c = '\f';
14300:       }else if( c=='r' ){
14301:         c = '\r';
14302:       }else if( c=='"' ){
14303:         c = '"';
14304:       }else if( c=='\'' ){
14305:         c = '\'';
14306:       }else if( c=='\\' ){
14307:         c = '\\';
14308:       }else if( c>='0' && c<='7' ){
14309:         c -= '0';
14310:         if( z[i+1]>='0' && z[i+1]<='7' ){
14311:           i++;
14312:           c = (c<<3) + z[i] - '0';
14313:           if( z[i+1]>='0' && z[i+1]<='7' ){
14314:             i++;
14315:             c = (c<<3) + z[i] - '0';
14316:           }
14317:         }
14318:       }
14319:     }
14320:     z[j] = c;
14321:   }
14322:   if( j<i ) z[j] = 0;
14323: }
