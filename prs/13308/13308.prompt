11030: /*
11031: ** Output the given string as a quoted according to C or TCL quoting rules.
11032: */
11033: static void output_c_string(FILE *out, const char *z){
11034:   unsigned int c;
11035:   fputc('"', out);
11036:   while( (c = *(z++))!=0 ){
11037:     if( c=='\\' ){
11038:       fputc(c, out);
11039:       fputc(c, out);
11040:     }else if( c=='"' ){
11041:       fputc('\\', out);
11042:       fputc('"', out);
11043:     }else if( c=='\t' ){
11044:       fputc('\\', out);
11045:       fputc('t', out);
11046:     }else if( c=='\n' ){
11047:       fputc('\\', out);
11048:       fputc('n', out);
11049:     }else if( c=='\r' ){
11050:       fputc('\\', out);
11051:       fputc('r', out);
11052:     }else if( !isprint(c&0xff) ){
11053:       raw_printf(out, "\\%03o", c&0xff);
11054:     }else{
11055:       fputc(c, out);
11056:     }
11057:   }
11058:   fputc('"', out);
11059: }
