12547: /*
12548: ** Run a prepared statement and output the result in one of the
12549: ** table-oriented formats: MODE_Column, MODE_Markdown, MODE_Table,
12550: ** MODE_Box or MODE_DuckBox
12551: **
12552: ** This is different from ordinary exec_prepared_stmt() in that
12553: ** it has to run the entire query and gather the results into memory
12554: ** first, in order to determine column widths, before providing
12555: ** any output.
12556: */
12557: static void exec_prepared_stmt_columnar(
12558:   ShellState *p,                        /* Pointer to ShellState */
12559:   sqlite3_stmt *pStmt                   /* Statment to run */
12560: ){
12561:   sqlite3_int64 nRow = 0;
12562:   int nColumn = 0;
12563:   char **azData = 0;
12564:   sqlite3_int64 nAlloc = 0;
12565:   const char *z;
12566:   int rc;
12567:   sqlite3_int64 i, nData;
12568:   int j, nTotal, w, n;
12569:   const char *colSep = 0;
12570:   const char *rowSep = 0;
12571: 
12572:   rc = sqlite3_step(pStmt);
12573:   if( rc!=SQLITE_ROW ) return;
12574:   nColumn = sqlite3_column_count(pStmt);
12575:   nAlloc = nColumn*4;
12576:   azData = sqlite3_malloc64( nAlloc*sizeof(char*) );
12577:   if( azData==0 ) shell_out_of_memory();
12578:   for(i=0; i<nColumn; i++){
12579:     azData[i] = strdup_handle_newline(p, sqlite3_column_name(pStmt,i));
12580:   }
12581:   p->colTypes = realloc(p->colTypes, nColumn * sizeof(int));
12582:   for(i=0; i<nColumn; i++){
12583:     p->colTypes[i] = sqlite3_column_type(pStmt, i);
12584:   }
12585:   do{
12586:     if( (nRow+2)*nColumn >= nAlloc ){
12587:       nAlloc *= 2;
12588:       azData = sqlite3_realloc64(azData, nAlloc*sizeof(char*));
12589:       if( azData==0 ) shell_out_of_memory();
12590:     }
12591:     nRow++;
12592:     for(i=0; i<nColumn; i++){
12593:       z = (const char*)sqlite3_column_text(pStmt,i);
12594:       azData[nRow*nColumn + i] = strdup_handle_newline(p, z);
12595:     }
12596:   }while( (rc = sqlite3_step(pStmt))==SQLITE_ROW );
12597:   if( nColumn>p->nWidth ){
12598:     p->colWidth = realloc(p->colWidth, nColumn*2*sizeof(int));
12599:     if( p->colWidth==0 ) shell_out_of_memory();
12600:     for(i=p->nWidth; i<nColumn; i++) p->colWidth[i] = 0;
12601:     p->nWidth = nColumn;
12602:     p->actualWidth = &p->colWidth[nColumn];
12603:   }
12604:   memset(p->actualWidth, 0, nColumn*sizeof(int));
12605:   for(i=0; i<nColumn; i++){
12606:     w = p->colWidth[i];
12607:     if( w<0 ) w = -w;
12608:     p->actualWidth[i] = w;
12609:   }
12610:   nTotal = nColumn*(nRow+1);
12611:   for(i=0; i<nTotal; i++){
12612:     z = azData[i];
12613:     if( z==0 ) z = p->nullValue;
12614:     n = strlenChar(z);
12615:     j = i%nColumn;
12616:     if( n>p->actualWidth[j] ) p->actualWidth[j] = n;
12617:   }
12618:   if( seenInterrupt ) goto columnar_end;
12619:   switch( p->cMode ){
12620:     case MODE_Column: {
12621:       colSep = "  ";
12622:       rowSep = "\n";
12623:       if( p->showHeader ){
12624:         for(i=0; i<nColumn; i++){
12625:           w = p->actualWidth[i];
12626:           if( p->colWidth[i]<0 ) w = -w;
12627:           utf8_width_print(p->out, w, azData[i]);
12628:           fputs(i==nColumn-1?"\n":"  ", p->out);
12629:         }
12630:         for(i=0; i<nColumn; i++){
12631:           print_dashes(p->out, p->actualWidth[i]);
12632:           fputs(i==nColumn-1?"\n":"  ", p->out);
12633:         }
12634:       }
12635:       break;
12636:     }
12637:     case MODE_Table: {
12638:       colSep = " | ";
12639:       rowSep = " |\n";
12640:       print_row_separator(p, nColumn, "+");
12641:       fputs("| ", p->out);
12642:       for(i=0; i<nColumn; i++){
12643:         w = p->actualWidth[i];
12644:         n = strlenChar(azData[i]);
12645:         utf8_printf(p->out, "%*s%s%*s", (w-n)/2, "", azData[i], (w-n+1)/2, "");
12646:         fputs(i==nColumn-1?" |\n":" | ", p->out);
12647:       }
12648:       print_row_separator(p, nColumn, "+");
12649:       break;
12650:     }
12651:     case MODE_Markdown: {
12652:       colSep = " | ";
12653:       rowSep = " |\n";
12654:       fputs("| ", p->out);
12655:       for(i=0; i<nColumn; i++){
12656:         w = p->actualWidth[i];
12657:         n = strlenChar(azData[i]);
12658:         utf8_printf(p->out, "%*s%s%*s", (w-n)/2, "", azData[i], (w-n+1)/2, "");
12659:         fputs(i==nColumn-1?" |\n":" | ", p->out);
12660:       }
12661:       print_markdown_separator(p, nColumn, "|");
12662:       break;
12663:     }
12664:     case MODE_Box: {
12665:       colSep = " " BOX_13 " ";
12666:       rowSep = " " BOX_13 "\n";
12667:       print_box_row_separator(p, nColumn, BOX_23, BOX_234, BOX_34);
12668:       utf8_printf(p->out, BOX_13 " ");
12669:       for(i=0; i<nColumn; i++){
12670:         w = p->actualWidth[i];
12671:         n = strlenChar(azData[i]);
12672:         utf8_printf(p->out, "%*s%s%*s%s",
12673:             (w-n)/2, "", azData[i], (w-n+1)/2, "",
12674:             i==nColumn-1?" "BOX_13"\n":" "BOX_13" ");
12675:       }
12676:       print_box_row_separator(p, nColumn, BOX_123, BOX_1234, BOX_134);
12677:       break;
12678:     }
12679:     case MODE_Latex: {
12680:       colSep = " & ";
12681:       rowSep = " \\\\\n";
12682:       fputs("\\begin{tabular}{|", p->out);
12683:       for(i=0; i<nColumn; i++){
12684:         const char *column_type = sqlite3_column_decltype(pStmt, i);
12685:         if (column_type_is_integer(column_type)) {
12686:           fputs("r", p->out);
12687:         } else {
12688:           fputs("l", p->out);
12689:         }
12690:       }
12691:       fputs("|}\n", p->out);
12692:       fputs("\\hline\n", p->out);
12693:       for(i=0; i<nColumn; i++){
12694:         w = p->actualWidth[i];
12695:         n = strlenChar(azData[i]);
12696:         utf8_printf(p->out, "%*s%s%*s", (w-n)/2, "", azData[i], (w-n+1)/2, "");
12697:         fputs(i==nColumn-1? rowSep:colSep, p->out);
12698:       }
12699:       fputs("\\hline\n", p->out);
12700:       break;
12701:     }
12702:   }
12703:   for(i=nColumn, j=0; i<nTotal; i++, j++){
12704:     if( j==0 && p->cMode!=MODE_Column && p->cMode != MODE_Latex ){
12705:       utf8_printf(p->out, "%s", p->cMode==MODE_Box?BOX_13" ":"| ");
12706:     }
12707:     z = azData[i];
12708:     if( z==0 ) z = p->nullValue;
12709:     w = p->actualWidth[j];
12710:     if( p->colWidth[j]<0 ) w = -w;
12711:     utf8_width_print(p->out, w, z);
12712:     if( j==nColumn-1 ){
12713:       utf8_printf(p->out, "%s", rowSep);
12714:       j = -1;
12715:       if( seenInterrupt ) goto columnar_end;
12716:     }else{
12717:       utf8_printf(p->out, "%s", colSep);
12718:     }
12719:   }
12720:   if( p->cMode==MODE_Table ){
12721:     print_row_separator(p, nColumn, "+");
12722:   }else if( p->cMode==MODE_Box ){
12723:     print_box_row_separator(p, nColumn, BOX_12, BOX_124, BOX_14);
12724:   } else if (p->cMode == MODE_Latex) {
12725:     fputs("\\hline\n", p->out);
12726:     fputs("\\end{tabular}\n", p->out);
12727:   }
12728: columnar_end:
12729:   if( seenInterrupt ){
12730:     utf8_printf(p->out, "Interrupt\n");
12731:   }
12732:   nData = (nRow+1)*nColumn;
12733:   for(i=0; i<nData; i++) free(azData[i]);
12734:   sqlite3_free(azData);
12735: }
