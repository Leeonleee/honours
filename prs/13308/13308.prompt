9022:         zInner = idxAppendText(&rc, 0, "CREATE TABLE x(");
9023:         for(i=0; i<pTab->nCol; i++){
9024:           zInner = idxAppendText(&rc, zInner, "%s%Q COLLATE %s",
9025:               (i==0 ? "" : ", "), pTab->aCol[i].zName, pTab->aCol[i].zColl
9026:           );
9027:         }
