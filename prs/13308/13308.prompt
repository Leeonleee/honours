9990: /*
9991: ** Read a varint.  Put the value in *pVal and return the number of bytes.
9992: */
9993: static int dbdataGetVarint(const u8 *z, sqlite3_int64 *pVal){
9994:   sqlite3_int64 v = 0;
9995:   int i;
9996:   for(i=0; i<8; i++){
9997:     v = (v<<7) + (z[i]&0x7f);
9998:     if( (z[i]&0x80)==0 ){ *pVal = v; return i+1; }
9999:   }
10000:   v = (v<<8) + (z[i]&0xff);
10001:   *pVal = v;
10002:   return 9;
10003: }
