3883: /*
3884: ** Try to read the append-mark off the end of a file.  Return the
3885: ** start of the appended database if the append-mark is present.  If
3886: ** there is no append-mark, return -1;
3887: */
3888: static sqlite3_int64 apndReadMark(sqlite3_int64 sz, sqlite3_file *pFile){
3889:   int rc, i;
3890:   sqlite3_int64 iMark;
3891:   unsigned char a[APND_MARK_SIZE];
3892: 
3893:   if( sz<=APND_MARK_SIZE ) return -1;
3894:   rc = pFile->pMethods->xRead(pFile, a, APND_MARK_SIZE, sz-APND_MARK_SIZE);
3895:   if( rc ) return -1;
3896:   if( memcmp(a, APND_MARK_PREFIX, APND_MARK_PREFIX_SZ)!=0 ) return -1;
3897:   iMark = ((sqlite3_int64)(a[APND_MARK_PREFIX_SZ]&0x7f))<<56;
3898:   for(i=1; i<8; i++){
3899:     iMark += (sqlite3_int64)a[APND_MARK_PREFIX_SZ+i]<<(56-8*i);
3900:   }
3901:   return iMark;
3902: }
