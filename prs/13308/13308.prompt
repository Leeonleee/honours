19403: /*
19404: ** Return a pathname which is the user's home directory.  A
19405: ** 0 return indicates an error of some kind.
19406: */
19407: static char *find_home_dir(int clearFlag){
19408:   static char *home_dir = NULL;
19409:   if( clearFlag ){
19410:     free(home_dir);
19411:     home_dir = 0;
19412:     return 0;
19413:   }
19414:   if( home_dir ) return home_dir;
19415: 
19416: #if !defined(_WIN32) && !defined(WIN32) && !defined(_WIN32_WCE) \
19417:      && !defined(__RTP__) && !defined(_WRS_KERNEL)
19418:   {
19419:     struct passwd *pwent;
19420:     uid_t uid = getuid();
19421:     if( (pwent=getpwuid(uid)) != NULL) {
19422:       home_dir = pwent->pw_dir;
19423:     }
19424:   }
19425: #endif
19426: 
19427: #if defined(_WIN32_WCE)
19428:   /* Windows CE (arm-wince-mingw32ce-gcc) does not provide getenv()
19429:    */
19430:   home_dir = "/";
19431: #else
19432: 
19433: #if defined(_WIN32) || defined(WIN32)
19434:   if (!home_dir) {
19435:     home_dir = getenv("USERPROFILE");
19436:   }
19437: #endif
19438: 
19439:   if (!home_dir) {
19440:     home_dir = getenv("HOME");
19441:   }
19442: 
19443: #if defined(_WIN32) || defined(WIN32)
19444:   if (!home_dir) {
19445:     char *zDrive, *zPath;
19446:     int n;
19447:     zDrive = getenv("HOMEDRIVE");
19448:     zPath = getenv("HOMEPATH");
19449:     if( zDrive && zPath ){
19450:       n = strlen30(zDrive) + strlen30(zPath) + 1;
19451:       home_dir = malloc( n );
19452:       if( home_dir==0 ) return 0;
19453:       sqlite3_snprintf(n, home_dir, "%s%s", zDrive, zPath);
19454:       return home_dir;
19455:     }
19456:     home_dir = "c:\\";
19457:   }
19458: #endif
19459: 
19460: #endif /* !_WIN32_WCE */
19461: 
19462:   if( home_dir ){
19463:     int n = strlen30(home_dir) + 1;
19464:     char *z = malloc( n );
19465:     if( z ) memcpy(z, home_dir, n);
19466:     home_dir = z;
19467:   }
19468: 
19469:   return home_dir;
