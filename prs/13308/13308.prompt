14870: /*
14871: ** Create a prepared statement using printf-style arguments for the SQL.
14872: **
14873: ** This routine is could be marked "static".  But it is not always used,
14874: ** depending on compile-time options.  By omitting the "static", we avoid
14875: ** nuisance compiler warnings about "defined but not used".
14876: */
14877: void shellPreparePrintf(
14878:   sqlite3 *db,
14879:   int *pRc,
14880:   sqlite3_stmt **ppStmt,
14881:   const char *zFmt,
14882:   ...
14883: ){
14884:   *ppStmt = 0;
14885:   if( *pRc==SQLITE_OK ){
14886:     va_list ap;
14887:     char *z;
14888:     va_start(ap, zFmt);
14889:     z = sqlite3_vmprintf(zFmt, ap);
14890:     va_end(ap);
14891:     if( z==0 ){
14892:       *pRc = SQLITE_NOMEM;
14893:     }else{
14894:       shellPrepare(db, pRc, z, ppStmt);
14895:       sqlite3_free(z);
14896:     }
14897:   }
14898: }
