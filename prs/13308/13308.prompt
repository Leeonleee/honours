10035: /*
10036: ** Load a value of type eType from buffer pData and use it to set the
10037: ** result of context object pCtx.
10038: */
10039: static void dbdataValue(
10040:   sqlite3_context *pCtx,
10041:   int eType,
10042:   u8 *pData,
10043:   int nData
10044: ){
10045:   if( eType>=0 && dbdataValueBytes(eType)<=nData ){
10046:     switch( eType ){
10047:       case 0:
10048:       case 10:
10049:       case 11:
10050:         sqlite3_result_null(pCtx);
10051:         break;
10052: 
10053:       case 8:
10054:         sqlite3_result_int(pCtx, 0);
10055:         break;
10056:       case 9:
10057:         sqlite3_result_int(pCtx, 1);
10058:         break;
10059: 
10060:       case 1: case 2: case 3: case 4: case 5: case 6: case 7: {
10061:         sqlite3_uint64 v = (signed char)pData[0];
10062:         pData++;
10063:         switch( eType ){
10064:           case 7:
10065:           case 6:  v = (v<<16) + (pData[0]<<8) + pData[1];  pData += 2;
10066:           case 5:  v = (v<<16) + (pData[0]<<8) + pData[1];  pData += 2;
10067:           case 4:  v = (v<<8) + pData[0];  pData++;
10068:           case 3:  v = (v<<8) + pData[0];  pData++;
10069:           case 2:  v = (v<<8) + pData[0];  pData++;
10070:         }
10071: 
10072:         if( eType==7 ){
10073:           double r;
10074:           memcpy(&r, &v, sizeof(r));
10075:           sqlite3_result_double(pCtx, r);
10076:         }else{
10077:           sqlite3_result_int64(pCtx, (sqlite3_int64)v);
10078:         }
10079:         break;
10080:       }
10081: 
10082:       default: {
10083:         int n = ((eType-12) / 2);
10084:         if( eType % 2 ){
10085:           sqlite3_result_text(pCtx, (const char*)pData, n, SQLITE_TRANSIENT);
10086:         }else{
10087:           sqlite3_result_blob(pCtx, pData, n, SQLITE_TRANSIENT);
10088:         }
10089:       }
10090:     }
10091:   }
10092: }
