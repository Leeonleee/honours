8994:   */
8995:   rc = idxPrepareStmt(p->db, &pSchema, pzErrmsg,
8996:       "SELECT type, name, sql, 1 FROM sqlite_schema "
8997:       "WHERE type IN ('table','view') AND name NOT LIKE 'sqlite_%%' "
