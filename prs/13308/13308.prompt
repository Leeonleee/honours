3706:   sqlite3_file *pFile,
3707:   const void *zBuf,
3708:   int iAmt,
3709:   sqlite_int64 iOfst
3710: ){
3711:   int rc;
3712:   ApndFile *p = (ApndFile *)pFile;
3713:   pFile = ORIGFILE(pFile);
3714:   if( iOfst+iAmt>=APND_MAX_SIZE ) return SQLITE_FULL;
3715:   rc = pFile->pMethods->xWrite(pFile, zBuf, iAmt, iOfst+p->iPgOne);
3716:   if( rc==SQLITE_OK &&  iOfst + iAmt + p->iPgOne > p->iMark ){
3717:     sqlite3_int64 sz = 0;
3718:     rc = pFile->pMethods->xFileSize(pFile, &sz);
3719:     if( rc==SQLITE_OK ){
3720:       p->iMark = sz - APND_MARK_SIZE;
3721:       if( iOfst + iAmt + p->iPgOne > p->iMark ){
3722:         p->iMark = p->iPgOne + iOfst + iAmt;
3723:         rc = apndWriteMark(p, pFile);
3724:       }
3725:     }
3726:   }
3727:   return rc;
3728: }
