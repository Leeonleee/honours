2478: /*
2479: ** SQL function:   lsmode(MODE)
2480: **
2481: ** Given a numberic st_mode from stat(), convert it into a human-readable
2482: ** text string in the style of "ls -l".
2483: */
2484: static void lsModeFunc(
2485:   sqlite3_context *context,
2486:   int argc,
2487:   sqlite3_value **argv
2488: ){
2489:   int i;
2490:   int iMode = sqlite3_value_int(argv[0]);
2491:   char z[16];
2492:   (void)argc;
2493:   if( S_ISLNK(iMode) ){
2494:     z[0] = 'l';
2495:   }else if( S_ISREG(iMode) ){
2496:     z[0] = '-';
2497:   }else if( S_ISDIR(iMode) ){
2498:     z[0] = 'd';
2499:   }else{
2500:     z[0] = '?';
2501:   }
2502:   for(i=0; i<3; i++){
2503:     int m = (iMode >> ((2-i)*3));
2504:     char *a = &z[1 + i*3];
2505:     a[0] = (m & 0x4) ? 'r' : '-';
2506:     a[1] = (m & 0x2) ? 'w' : '-';
2507:     a[2] = (m & 0x1) ? 'x' : '-';
2508:   }
2509:   z[10] = '\0';
2510:   sqlite3_result_text(context, z, -1, SQLITE_TRANSIENT);
