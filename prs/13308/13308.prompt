10736: /*
10737: ** SQL function:   edit(VALUE)
10738: **                 edit(VALUE,EDITOR)
10739: **
10740: ** These steps:
10741: **
10742: **     (1) Write VALUE into a temporary file.
10743: **     (2) Run program EDITOR on that temporary file.
10744: **     (3) Read the temporary file back and return its content as the result.
10745: **     (4) Delete the temporary file
10746: **
10747: ** If the EDITOR argument is omitted, use the value in the VISUAL
10748: ** environment variable.  If still there is no EDITOR, through an error.
10749: **
10750: ** Also throw an error if the EDITOR program returns a non-zero exit code.
10751: */
10752: #ifndef SQLITE_NOHAVE_SYSTEM
10753: static void editFunc(
10754:   sqlite3_context *context,
10755:   int argc,
10756:   sqlite3_value **argv
10757: ){
10758:   const char *zEditor;
10759:   char *zTempFile = 0;
10760:   sqlite3 *db;
10761:   char *zCmd = 0;
10762:   int bBin;
10763:   int rc;
10764:   int hasCRNL = 0;
10765:   FILE *f = 0;
10766:   sqlite3_int64 sz;
10767:   sqlite3_int64 x;
10768:   unsigned char *p = 0;
10769: 
10770:   if( argc==2 ){
10771:     zEditor = (const char*)sqlite3_value_text(argv[1]);
10772:   }else{
10773:     zEditor = getenv("VISUAL");
10774:   }
10775:   if( zEditor==0 ){
10776:     sqlite3_result_error(context, "no editor for edit()", -1);
10777:     return;
10778:   }
10779:   if( sqlite3_value_type(argv[0])==SQLITE_NULL ){
10780:     sqlite3_result_error(context, "NULL input to edit()", -1);
10781:     return;
10782:   }
10783:   db = sqlite3_context_db_handle(context);
10784:   zTempFile = 0;
10785:   sqlite3_file_control(db, 0, SQLITE_FCNTL_TEMPFILENAME, &zTempFile);
10786:   if( zTempFile==0 ){
10787:     sqlite3_uint64 r = 0;
10788:     sqlite3_randomness(sizeof(r), &r);
10789:     zTempFile = sqlite3_mprintf("temp%llx", r);
10790:     if( zTempFile==0 ){
10791:       sqlite3_result_error_nomem(context);
10792:       return;
10793:     }
10794:   }
10795:   bBin = sqlite3_value_type(argv[0])==SQLITE_BLOB;
10796:   /* When writing the file to be edited, do \n to \r\n conversions on systems
10797:   ** that want \r\n line endings */
10798:   f = fopen(zTempFile, bBin ? "wb" : "w");
10799:   if( f==0 ){
10800:     sqlite3_result_error(context, "edit() cannot open temp file", -1);
10801:     goto edit_func_end;
10802:   }
10803:   sz = sqlite3_value_bytes(argv[0]);
10804:   if( bBin ){
10805:     x = fwrite(sqlite3_value_blob(argv[0]), 1, (size_t)sz, f);
10806:   }else{
10807:     const char *z = (const char*)sqlite3_value_text(argv[0]);
10808:     /* Remember whether or not the value originally contained \r\n */
10809:     if( z && strstr(z,"\r\n")!=0 ) hasCRNL = 1;
10810:     x = fwrite(sqlite3_value_text(argv[0]), 1, (size_t)sz, f);
10811:   }
10812:   fclose(f);
10813:   f = 0;
10814:   if( x!=sz ){
10815:     sqlite3_result_error(context, "edit() could not write the whole file", -1);
10816:     goto edit_func_end;
10817:   }
10818:   zCmd = sqlite3_mprintf("%s \"%s\"", zEditor, zTempFile);
10819:   if( zCmd==0 ){
10820:     sqlite3_result_error_nomem(context);
10821:     goto edit_func_end;
10822:   }
10823:   rc = system(zCmd);
10824:   sqlite3_free(zCmd);
10825:   if( rc ){
10826:     sqlite3_result_error(context, "EDITOR returned non-zero", -1);
10827:     goto edit_func_end;
10828:   }
10829:   f = fopen(zTempFile, "rb");
10830:   if( f==0 ){
10831:     sqlite3_result_error(context,
10832:       "edit() cannot reopen temp file after edit", -1);
10833:     goto edit_func_end;
10834:   }
10835:   fseek(f, 0, SEEK_END);
10836:   sz = ftell(f);
10837:   rewind(f);
10838:   p = sqlite3_malloc64( sz+1 );
10839:   if( p==0 ){
10840:     sqlite3_result_error_nomem(context);
10841:     goto edit_func_end;
10842:   }
10843:   x = fread(p, 1, (size_t)sz, f);
10844:   fclose(f);
10845:   f = 0;
10846:   if( x!=sz ){
10847:     sqlite3_result_error(context, "could not read back the whole file", -1);
10848:     goto edit_func_end;
10849:   }
10850:   if( bBin ){
10851:     sqlite3_result_blob64(context, p, sz, sqlite3_free);
10852:   }else{
10853:     sqlite3_int64 i, j;
10854:     if( hasCRNL ){
10855:       /* If the original contains \r\n then do no conversions back to \n */
10856:       j = sz;
10857:     }else{
10858:       /* If the file did not originally contain \r\n then convert any new
10859:       ** \r\n back into \n */
10860:       for(i=j=0; i<sz; i++){
10861:         if( p[i]=='\r' && p[i+1]=='\n' ) i++;
10862:         p[j++] = p[i];
10863:       }
10864:       sz = j;
10865:       p[sz] = 0;
10866:     }
10867:     sqlite3_result_text64(context, (const char*)p, sz,
10868:                           sqlite3_free, SQLITE_UTF8);
10869:   }
10870:   p = 0;
10871: 
10872: edit_func_end:
10873:   if( f ) fclose(f);
10874:   unlink(zTempFile);
10875:   sqlite3_free(zTempFile);
10876:   sqlite3_free(p);
10877: }
10878: #endif /* SQLITE_NOHAVE_SYSTEM */
