8417: /*
8418: ** This function is a no-op if *pRc is set to anything other than
8419: ** SQLITE_OK when it is called.
8420: **
8421: ** If *pRc is initially set to SQLITE_OK, then the text specified by
8422: ** the printf() style arguments is appended to zIn and the result returned
8423: ** in a buffer allocated by sqlite3_malloc(). sqlite3_free() is called on
8424: ** zIn before returning.
8425: */
8426: static char *idxAppendText(int *pRc, char *zIn, const char *zFmt, ...){
8427:   va_list ap;
8428:   char *zAppend = 0;
8429:   char *zRet = 0;
8430:   int nIn = zIn ? STRLEN(zIn) : 0;
8431:   int nAppend = 0;
8432:   va_start(ap, zFmt);
8433:   if( *pRc==SQLITE_OK ){
8434:     zAppend = sqlite3_vmprintf(zFmt, ap);
8435:     if( zAppend ){
8436:       nAppend = STRLEN(zAppend);
8437:       zRet = (char*)sqlite3_malloc(nIn + nAppend + 1);
8438:     }
8439:     if( zAppend && zRet ){
8440:       if( nIn ) memcpy(zRet, zIn, nIn);
8441:       memcpy(&zRet[nIn], zAppend, nAppend+1);
8442:     }else{
8443:       sqlite3_free(zRet);
8444:       zRet = 0;
8445:       *pRc = SQLITE_NOMEM;
8446:     }
8447:     sqlite3_free(zAppend);
8448:     sqlite3_free(zIn);
8449:   }
8450:   va_end(ap);
8451:   return zRet;
8452: }
