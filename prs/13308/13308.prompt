10296: /*
10297: ** Determine the size in pages of database zSchema (where zSchema is
10298: ** "main", "temp" or the name of an attached database) and set
10299: ** pCsr->szDb accordingly. If successful, return SQLITE_OK. Otherwise,
10300: ** an SQLite error code.
10301: */
10302: static int dbdataDbsize(DbdataCursor *pCsr, const char *zSchema){
10303:   DbdataTable *pTab = (DbdataTable*)pCsr->base.pVtab;
10304:   char *zSql = 0;
10305:   int rc, rc2;
10306:   sqlite3_stmt *pStmt = 0;
10307: 
10308:   zSql = sqlite3_mprintf("PRAGMA %Q.page_count", zSchema);
10309:   if( zSql==0 ) return SQLITE_NOMEM;
10310:   rc = sqlite3_prepare_v2(pTab->db, zSql, -1, &pStmt, 0);
10311:   sqlite3_free(zSql);
10312:   if( rc==SQLITE_OK && sqlite3_step(pStmt)==SQLITE_ROW ){
10313:     pCsr->szDb = sqlite3_column_int(pStmt, 0);
10314:   }
10315:   rc2 = sqlite3_finalize(pStmt);
10316:   if( rc==SQLITE_OK ) rc = rc2;
10317:   return rc;
10318: }
