11164:     int i;
11165:     int nSep = strlen30(p->colSeparator);
11166:     for(i=0; z[i]; i++){
11167:       if( needCsvQuote[((unsigned char*)z)[i]]
11168:          || (z[i]==p->colSeparator[0] &&
11169:              (nSep==1 || memcmp(z, p->colSeparator, nSep)==0)) ){
11170:         i = 0;
11171:         break;
11172:       }
11173:     }
11174:     if( i==0 ){
11175:       char *zQuoted = sqlite3_mprintf("\"%w\"", z);
11176:       utf8_printf(out, "%s", zQuoted);
11177:       sqlite3_free(zQuoted);
11178:     }else{
11179:       utf8_printf(out, "%s", z);
11180:     }
11181:   }
11182:   if( bSep ){
11183:     utf8_printf(p->out, "%s", p->colSeparator);
11184:   }
