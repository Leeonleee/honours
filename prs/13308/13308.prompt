12739: /*
12740: ** Run a prepared statement
12741: */
12742: static void exec_prepared_stmt(
12743:   ShellState *pArg,                                /* Pointer to ShellState */
12744:   sqlite3_stmt *pStmt                              /* Statment to run */
12745: ){
12746:   int rc;
12747:   if (pArg->cMode == MODE_DuckBox) {
12748: 	  size_t max_rows = pArg->outfile[0] == '\0' || pArg->outfile[0] == '|' ? pArg->max_rows : (size_t) -1;
12749: 	  size_t max_width = pArg->outfile[0] == '\0' || pArg->outfile[0] == '|' ? pArg->max_width : (size_t) -1;
12750: 	  char *str = sqlite3_print_duckbox(pStmt, max_rows, max_width, pArg->nullValue, pArg->columns);
12751: 	  if (str) {
12752: 		  utf8_printf(pArg->out, "%s", str);
12753: 		  sqlite3_free(str);
12754: 	  }
12755: 	  return;
12756:   }
12757: 
12758:   if( pArg->cMode==MODE_Column
12759:    || pArg->cMode==MODE_Table
12760:    || pArg->cMode==MODE_Box
12761:    || pArg->cMode==MODE_Markdown
12762:    || pArg->cMode==MODE_Latex
12763:   ){
12764:     exec_prepared_stmt_columnar(pArg, pStmt);
12765:     return;
12766:   }
12767: 
12768:   /* perform the first step.  this will tell us if we
12769:   ** have a result set or not and how wide it is.
12770:   */
12771:   rc = sqlite3_step(pStmt);
12772:   /* if we have a result set... */
12773:   if( SQLITE_ROW == rc ){
12774:     /* allocate space for col name ptr, value ptr, and type */
12775:     int nCol = sqlite3_column_count(pStmt);
12776:     void *pData = sqlite3_malloc64(3*nCol*sizeof(const char*) + 1);
12777:     if( !pData ){
12778:       rc = SQLITE_NOMEM;
12779:     }else{
12780:       char **azCols = (char **)pData;      /* Names of result columns */
12781:       char **azVals = &azCols[nCol];       /* Results */
12782:       int *aiTypes = (int *)&azVals[nCol]; /* Result types */
12783:       int i, x;
12784:       assert(sizeof(int) <= sizeof(char *));
12785:       /* save off ptrs to column names */
12786:       for(i=0; i<nCol; i++){
12787:         azCols[i] = (char *)sqlite3_column_name(pStmt, i);
12788:       }
12789:       do{
12790:         if (pArg->cMode!=MODE_Trash) {
12791:           /* extract the data and data types */
12792:           for(i=0; i<nCol; i++){
12793:             aiTypes[i] = x = sqlite3_column_type(pStmt, i);
12794:             if( x==SQLITE_BLOB && pArg && pArg->cMode==MODE_Insert ){
12795:               azVals[i] = "";
12796:             }else{
12797:               azVals[i] = (char*)sqlite3_column_text(pStmt, i);
12798:             }
12799:             if( !azVals[i] && (aiTypes[i]!=SQLITE_NULL) ){
12800:               rc = SQLITE_NOMEM;
12801:               break; /* from for */
12802:             }
12803:           } /* end for */
12804:         }
12805: 
12806:         /* if data and types extracted successfully... */
12807:         if( SQLITE_ROW == rc ){
12808:           /* call the supplied callback with the result row data */
12809:           if( pArg->cMode!=MODE_Trash && shell_callback(pArg, nCol, azVals, azCols, aiTypes) ){
12810:             rc = SQLITE_ABORT;
12811:           }else{
12812:             rc = sqlite3_step(pStmt);
12813:           }
12814:         }
12815:       } while( SQLITE_ROW == rc );
12816:       sqlite3_free(pData);
12817:       if( pArg->cMode==MODE_Json ){
12818:         fputs("]\n", pArg->out);
12819:       }
12820:       if( pArg->cMode==MODE_Jsonlines ){
12821:         fputs("\n", pArg->out);
12822:       }
12823:     }
12824:   }
12825: }
