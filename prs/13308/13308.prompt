13234: /*
13235: ** Toggle the reverse_unordered_selects setting.
13236: */
13237: static void toggleSelectOrder(sqlite3 *db){
13238:   sqlite3_stmt *pStmt = 0;
13239:   int iSetting = 0;
13240:   char zStmt[100];
13241:   sqlite3_prepare_v2(db, "PRAGMA reverse_unordered_selects", -1, &pStmt, 0);
13242:   if( sqlite3_step(pStmt)==SQLITE_ROW ){
13243:     iSetting = sqlite3_column_int(pStmt, 0);
13244:   }
13245:   sqlite3_finalize(pStmt);
13246:   sqlite3_snprintf(sizeof(zStmt), zStmt,
13247:        "PRAGMA reverse_unordered_selects(%d)", !iSetting);
13248:   sqlite3_exec(db, zStmt, 0, 0, 0);
13249: }
