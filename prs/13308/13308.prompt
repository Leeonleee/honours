12827: #ifndef SQLITE_OMIT_VIRTUALTABLE
12828: /*
12829: ** This function is called to process SQL if the previous shell command
12830: ** was ".expert". It passes the SQL in the second argument directly to
12831: ** the sqlite3expert object.
12832: **
12833: ** If successful, SQLITE_OK is returned. Otherwise, an SQLite error
12834: ** code. In this case, (*pzErr) may be set to point to a buffer containing
12835: ** an English language error message. It is the responsibility of the
12836: ** caller to eventually free this buffer using sqlite3_free().
12837: */
12838: static int expertHandleSQL(
12839:   ShellState *pState,
12840:   const char *zSql,
12841:   char **pzErr
12842: ){
12843:   assert( pState->expert.pExpert );
12844:   assert( pzErr==0 || *pzErr==0 );
12845:   return sqlite3_expert_sql(pState->expert.pExpert, zSql, pzErr);
12846: }
12847: 
12848: /*
12849: ** This function is called either to silently clean up the object
12850: ** created by the ".expert" command (if bCancel==1), or to generate a
12851: ** report from it and then clean it up (if bCancel==0).
12852: **
12853: ** If successful, SQLITE_OK is returned. Otherwise, an SQLite error
12854: ** code. In this case, (*pzErr) may be set to point to a buffer containing
12855: ** an English language error message. It is the responsibility of the
12856: ** caller to eventually free this buffer using sqlite3_free().
12857: */
12858: static int expertFinish(
12859:   ShellState *pState,
12860:   int bCancel,
12861:   char **pzErr
12862: ){
12863:   int rc = SQLITE_OK;
12864:   sqlite3expert *p = pState->expert.pExpert;
12865:   assert( p );
12866:   assert( bCancel || pzErr==0 || *pzErr==0 );
12867:   if( bCancel==0 ){
12868:     FILE *out = pState->out;
12869:     int bVerbose = pState->expert.bVerbose;
12870: 
12871:     rc = sqlite3_expert_analyze(p, pzErr);
12872:     if( rc==SQLITE_OK ){
12873:       int nQuery = sqlite3_expert_count(p);
12874:       int i;
12875: 
12876:       if( bVerbose ){
12877:         const char *zCand = sqlite3_expert_report(p,0,EXPERT_REPORT_CANDIDATES);
12878:         raw_printf(out, "-- Candidates -----------------------------\n");
12879:         raw_printf(out, "%s\n", zCand);
12880:       }
12881:       for(i=0; i<nQuery; i++){
12882:         const char *zSql = sqlite3_expert_report(p, i, EXPERT_REPORT_SQL);
12883:         const char *zIdx = sqlite3_expert_report(p, i, EXPERT_REPORT_INDEXES);
12884:         const char *zEQP = sqlite3_expert_report(p, i, EXPERT_REPORT_PLAN);
12885:         if( zIdx==0 ) zIdx = "(no new indexes)\n";
12886:         if( bVerbose ){
12887:           raw_printf(out, "-- Query %d --------------------------------\n",i+1);
12888:           raw_printf(out, "%s\n\n", zSql);
12889:         }
12890:         raw_printf(out, "%s\n", zIdx);
12891:         raw_printf(out, "%s\n", zEQP);
12892:       }
12893:     }
12894:   }
12895:   sqlite3_expert_destroy(p);
12896:   pState->expert.pExpert = 0;
12897:   return rc;
12898: }
