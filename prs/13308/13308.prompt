11061: /*
11062: ** Output the given string as a quoted according to JSON quoting rules.
11063: */
11064: static void output_json_string(FILE *out, const char *z, int n){
11065:   unsigned int c;
11066:   if( n<0 ) n = (int)strlen(z);
11067:   fputc('"', out);
11068:   while( n-- ){
11069:     c = *(z++);
11070:     if( c=='\\' || c=='"' ){
11071:       fputc('\\', out);
11072:       fputc(c, out);
11073:     }else if( c<=0x1f ){
11074:       fputc('\\', out);
11075:       if( c=='\b' ){
11076:         fputc('b', out);
11077:       }else if( c=='\f' ){
11078:         fputc('f', out);
11079:       }else if( c=='\n' ){
11080:         fputc('n', out);
11081:       }else if( c=='\r' ){
11082:         fputc('r', out);
11083:       }else if( c=='\t' ){
11084:         fputc('t', out);
11085:       }else{
11086:          raw_printf(out, "u%04x",c);
