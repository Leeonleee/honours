14408:     zSql = (const char*)pX;
14409:   }else{
14410:     pStmt = (sqlite3_stmt*)pP;
14411:     switch( p->eTraceType ){
14412:       case SHELL_TRACE_EXPANDED: {
14413:         zSql = sqlite3_expanded_sql(pStmt);
14414:         break;
14415:       }
14416: #ifdef SQLITE_ENABLE_NORMALIZE
14417:       case SHELL_TRACE_NORMALIZED: {
14418:         zSql = sqlite3_normalized_sql(pStmt);
14419:         break;
14420:       }
14421: #endif
14422:       default: {
14423:         zSql = sqlite3_sql(pStmt);
14424:         break;
14425:       }
14426:     }
14427:   }
14428:   if( zSql==0 ) return 0;
14429:   nSql = strlen30(zSql);
14430:   while( nSql>0 && zSql[nSql-1]==';' ){ nSql--; }
14431:   switch( mType ){
14432:     case SQLITE_TRACE_ROW:
14433:     case SQLITE_TRACE_STMT: {
14434:       utf8_printf(p->traceOut, "%.*s;\n", nSql, zSql);
14435:       break;
14436:     }
14437:     case SQLITE_TRACE_PROFILE: {
14438:       sqlite3_int64 nNanosec = *(sqlite3_int64*)pX;
14439:       utf8_printf(p->traceOut, "%.*s; -- %lld ns\n", nSql, zSql, nNanosec);
14440:       break;
14441:     }
14442:   }
14443:   return 0;
14444: }
14445: #endif
