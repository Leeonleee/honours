6363: /*
6364: ** Add object pNew to the linked list that begins at ZipfileTab.pFirstEntry
6365: ** and ends with pLastEntry. If argument pBefore is NULL, then pNew is added
6366: ** to the end of the list. Otherwise, it is added to the list immediately
6367: ** before pBefore (which is guaranteed to be a part of said list).
6368: */
6369: static void zipfileAddEntry(
6370:   ZipfileTab *pTab,
6371:   ZipfileEntry *pBefore,
6372:   ZipfileEntry *pNew
6373: ){
6374:   assert( (pTab->pFirstEntry==0)==(pTab->pLastEntry==0) );
6375:   assert( pNew->pNext==0 );
6376:   if( pBefore==0 ){
6377:     if( pTab->pFirstEntry==0 ){
6378:       pTab->pFirstEntry = pTab->pLastEntry = pNew;
6379:     }else{
6380:       assert( pTab->pLastEntry->pNext==0 );
6381:       pTab->pLastEntry->pNext = pNew;
6382:       pTab->pLastEntry = pNew;
6383:     }
6384:   }else{
6385:     ZipfileEntry **pp;
6386:     for(pp=&pTab->pFirstEntry; *pp!=pBefore; pp=&((*pp)->pNext));
6387:     pNew->pNext = pBefore;
6388:     *pp = pNew;
6389:   }
6390: }
