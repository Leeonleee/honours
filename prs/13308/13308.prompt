851: /* zIn is either a pointer to a NULL-terminated string in memory obtained
852: ** from malloc(), or a NULL pointer. The string pointed to by zAppend is
853: ** added to zIn, and the result returned in memory obtained from malloc().
854: ** zIn, if it was not NULL, is freed.
855: **
856: ** If the third argument, quote, is not '\0', then it is used as a
857: ** quote character for zAppend.
858: */
859: static void appendText(ShellText *p, char const *zAppend, char quote){
860:   int len;
861:   int i;
862:   int nAppend = strlen30(zAppend);
863: 
864:   len = nAppend+p->n+1;
865:   if( quote ){
866:     len += 2;
867:     for(i=0; i<nAppend; i++){
868:       if( zAppend[i]==quote ) len++;
869:     }
870:   }
871: 
872:   if( p->n+len>=p->nAlloc ){
873:     p->nAlloc = p->nAlloc*2 + len + 20;
874:     p->z = realloc(p->z, p->nAlloc);
875:     if( p->z==0 ) shell_out_of_memory();
876:   }
877: 
878:   if( quote ){
879:     char *zCsr = p->z+p->n;
880:     *zCsr++ = quote;
881:     for(i=0; i<nAppend; i++){
882:       *zCsr++ = zAppend[i];
883:       if( zAppend[i]==quote ) *zCsr++ = quote;
884:     }
885:     *zCsr++ = quote;
886:     p->n = (int)(zCsr - p->z);
887:     *zCsr = '\0';
888:   }else{
889:     memcpy(p->z+p->n, zAppend, nAppend);
890:     p->n += nAppend;
891:     p->z[p->n] = '\0';
892:   }
893: }
