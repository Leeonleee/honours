8454: /*
8455: ** Return true if zId must be quoted in order to use it as an SQL
8456: ** identifier, or false otherwise.
8457: */
8458: static int idxIdentifierRequiresQuotes(const char *zId){
8459:   int i;
8460:   for(i=0; zId[i]; i++){
8461:     if( !(zId[i]=='_')
8462:      && !(zId[i]>='0' && zId[i]<='9')
8463:      && !(zId[i]>='a' && zId[i]<='z')
8464:      && !(zId[i]>='A' && zId[i]<='Z')
8465:     ){
8466:       return 1;
8467:     }
8468:   }
8469:   return 0;
8470: }
