6413: /*
6414: ** xFilter callback.
6415: */
6416: static int zipfileFilter(
6417:   sqlite3_vtab_cursor *cur,
6418:   int idxNum, const char *idxStr,
6419:   int argc, sqlite3_value **argv
6420: ){
6421:   ZipfileTab *pTab = (ZipfileTab*)cur->pVtab;
6422:   ZipfileCsr *pCsr = (ZipfileCsr*)cur;
6423:   const char *zFile = 0;          /* Zip file to scan */
6424:   int rc = SQLITE_OK;             /* Return Code */
6425:   int bInMemory = 0;              /* True for an in-memory zipfile */
6426: 
6427:   zipfileResetCursor(pCsr);
6428: 
6429:   if( pTab->zFile ){
6430:     zFile = pTab->zFile;
6431:   }else if( idxNum==0 ){
6432:     zipfileCursorErr(pCsr, "zipfile() function requires an argument");
6433:     return SQLITE_ERROR;
6434:   }else if( sqlite3_value_type(argv[0])==SQLITE_BLOB ){
6435:     const u8 *aBlob = (const u8*)sqlite3_value_blob(argv[0]);
6436:     int nBlob = sqlite3_value_bytes(argv[0]);
6437:     assert( pTab->pFirstEntry==0 );
6438:     rc = zipfileLoadDirectory(pTab, aBlob, nBlob);
6439:     pCsr->pFreeEntry = pTab->pFirstEntry;
6440:     pTab->pFirstEntry = pTab->pLastEntry = 0;
6441:     if( rc!=SQLITE_OK ) return rc;
6442:     bInMemory = 1;
6443:   }else{
6444:     zFile = (const char*)sqlite3_value_text(argv[0]);
6445:   }
6446: 
6447:   if( 0==pTab->pWriteFd && 0==bInMemory ){
6448:     pCsr->pFile = fopen(zFile, "rb");
6449:     if( pCsr->pFile==0 ){
6450:       zipfileCursorErr(pCsr, "cannot open file: %s", zFile);
6451:       rc = SQLITE_ERROR;
6452:     }else{
6453:       rc = zipfileReadEOCD(pTab, 0, 0, pCsr->pFile, &pCsr->eocd);
6454:       if( rc==SQLITE_OK ){
6455:         if( pCsr->eocd.nEntry==0 ){
6456:           pCsr->bEof = 1;
6457:         }else{
6458:           pCsr->iNextOff = pCsr->eocd.iOffset;
6459:           rc = zipfileNext(cur);
6460:         }
6461:       }
6462:     }
6463:   }else{
6464:     pCsr->bNoop = 1;
6465:     pCsr->pCurrent = pCsr->pFreeEntry ? pCsr->pFreeEntry : pTab->pFirstEntry;
6466:     rc = zipfileNext(cur);
6467:   }
6468: 
6469:   return rc;
6470: }
