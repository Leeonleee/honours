10320: /*
10321: ** xFilter method for sqlite_dbdata and sqlite_dbptr.
10322: */
10323: static int dbdataFilter(
10324:   sqlite3_vtab_cursor *pCursor,
10325:   int idxNum, const char *idxStr,
10326:   int argc, sqlite3_value **argv
10327: ){
10328:   DbdataCursor *pCsr = (DbdataCursor*)pCursor;
10329:   DbdataTable *pTab = (DbdataTable*)pCursor->pVtab;
10330:   int rc = SQLITE_OK;
10331:   const char *zSchema = "main";
10332: 
10333:   dbdataResetCursor(pCsr);
10334:   assert( pCsr->iPgno==1 );
10335:   if( idxNum & 0x01 ){
10336:     zSchema = (const char*)sqlite3_value_text(argv[0]);
10337:   }
10338:   if( idxNum & 0x02 ){
10339:     pCsr->iPgno = sqlite3_value_int(argv[(idxNum & 0x01)]);
10340:     pCsr->bOnePage = 1;
10341:   }else{
10342:     pCsr->nPage = dbdataDbsize(pCsr, zSchema);
10343:     rc = dbdataDbsize(pCsr, zSchema);
10344:   }
10345: 
10346:   if( rc==SQLITE_OK ){
10347:     if( pTab->pStmt ){
10348:       pCsr->pStmt = pTab->pStmt;
10349:       pTab->pStmt = 0;
10350:     }else{
10351:       rc = sqlite3_prepare_v2(pTab->db,
10352:           "SELECT data FROM sqlite_dbpage(?) WHERE pgno=?", -1,
10353:           &pCsr->pStmt, 0
10354:       );
10355:     }
10356:   }
10357:   if( rc==SQLITE_OK ){
10358:     rc = sqlite3_bind_text(pCsr->pStmt, 1, zSchema, -1, SQLITE_TRANSIENT);
10359:   }else{
10360:     pTab->base.zErrMsg = sqlite3_mprintf("%s", sqlite3_errmsg(pTab->db));
10361:   }
10362:   if( rc==SQLITE_OK ){
10363:     rc = dbdataNext(pCursor);
10364:   }
10365:   return rc;
10366: }
