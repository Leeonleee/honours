830: /*
831: ** A variable length string to which one can append text.
832: */
833: typedef struct ShellText ShellText;
834: struct ShellText {
835:   char *z;
836:   int n;
837:   int nAlloc;
838: };
839: 
840: /*
841: ** Initialize and destroy a ShellText object
842: */
843: static void initText(ShellText *p){
844:   memset(p, 0, sizeof(*p));
845: }
846: static void freeText(ShellText *p){
847:   free(p->z);
848:   initText(p);
849: }
