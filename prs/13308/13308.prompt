15403: /*
15404: ** Implementation of .ar "create", "insert", and "update" commands.
15405: **
15406: **     create    ->     Create a new SQL archive
15407: **     insert    ->     Insert or reinsert all files listed
15408: **     update    ->     Insert files that have changed or that were not
15409: **                      previously in the archive
15410: **
15411: ** Create the "sqlar" table in the database if it does not already exist.
15412: ** Then add each file in the azFile[] array to the archive. Directories
15413: ** are added recursively. If argument bVerbose is non-zero, a message is
15414: ** printed on stdout for each file archived.
15415: **
15416: ** The create command is the same as update, except that it drops
15417: ** any existing "sqlar" table before beginning.  The "insert" command
15418: ** always overwrites every file named on the command-line, where as
15419: ** "update" only overwrites if the size or mtime or mode has changed.
15420: */
15421: static int arCreateOrUpdateCommand(
15422:   ArCommand *pAr,                 /* Command arguments and options */
15423:   int bUpdate,                    /* true for a --create. */
15424:   int bOnlyIfChanged              /* Only update if file has changed */
15425: ){
15426:   const char *zCreate =
15427:       "CREATE TABLE IF NOT EXISTS sqlar(\n"
15428:       "  name TEXT PRIMARY KEY,  -- name of the file\n"
15429:       "  mode INT,               -- access permissions\n"
15430:       "  mtime INT,              -- last modification time\n"
15431:       "  sz INT,                 -- original file size\n"
15432:       "  data BLOB               -- compressed content\n"
15433:       ")";
15434:   const char *zDrop = "DROP TABLE IF EXISTS sqlar";
15435:   const char *zInsertFmt[2] = {
15436:      "REPLACE INTO %s(name,mode,mtime,sz,data)\n"
15437:      "  SELECT\n"
15438:      "    %s,\n"
15439:      "    mode,\n"
15440:      "    mtime,\n"
15441:      "    CASE substr(lsmode(mode),1,1)\n"
15442:      "      WHEN '-' THEN length(data)\n"
15443:      "      WHEN 'd' THEN 0\n"
15444:      "      ELSE -1 END,\n"
15445:      "    sqlar_compress(data)\n"
15446:      "  FROM fsdir(%Q,%Q) AS disk\n"
15447:      "  WHERE lsmode(mode) NOT LIKE '?%%'%s;"
15448:      ,
15449:      "REPLACE INTO %s(name,mode,mtime,data)\n"
15450:      "  SELECT\n"
15451:      "    %s,\n"
15452:      "    mode,\n"
15453:      "    mtime,\n"
15454:      "    data\n"
15455:      "  FROM fsdir(%Q,%Q) AS disk\n"
15456:      "  WHERE lsmode(mode) NOT LIKE '?%%'%s;"
15457:   };
15458:   int i;                          /* For iterating through azFile[] */
15459:   int rc;                         /* Return code */
15460:   const char *zTab = 0;           /* SQL table into which to insert */
15461:   char *zSql;
15462:   char zTemp[50];
15463:   char *zExists = 0;
15464: 
15465:   arExecSql(pAr, "PRAGMA page_size=512");
15466:   rc = arExecSql(pAr, "SAVEPOINT ar;");
15467:   if( rc!=SQLITE_OK ) return rc;
15468:   zTemp[0] = 0;
15469:   if( pAr->bZip ){
15470:     /* Initialize the zipfile virtual table, if necessary */
15471:     if( pAr->zFile ){
15472:       sqlite3_uint64 r;
15473:       sqlite3_randomness(sizeof(r),&r);
15474:       sqlite3_snprintf(sizeof(zTemp),zTemp,"zip%016llx",r);
15475:       zTab = zTemp;
15476:       zSql = sqlite3_mprintf(
15477:          "CREATE VIRTUAL TABLE temp.%s USING zipfile(%Q)",
15478:          zTab, pAr->zFile
15479:       );
15480:       rc = arExecSql(pAr, zSql);
15481:       sqlite3_free(zSql);
15482:     }else{
15483:       zTab = "zip";
15484:     }
15485:   }else{
15486:     /* Initialize the table for an SQLAR */
15487:     zTab = "sqlar";
15488:     if( bUpdate==0 ){
15489:       rc = arExecSql(pAr, zDrop);
15490:       if( rc!=SQLITE_OK ) goto end_ar_transaction;
15491:     }
15492:     rc = arExecSql(pAr, zCreate);
15493:   }
15494:   if( bOnlyIfChanged ){
15495:     zExists = sqlite3_mprintf(
15496:       " AND NOT EXISTS("
15497:           "SELECT 1 FROM %s AS mem"
15498:           " WHERE mem.name=disk.name"
15499:           " AND mem.mtime=disk.mtime"
15500:           " AND mem.mode=disk.mode)", zTab);
15501:   }else{
15502:     zExists = sqlite3_mprintf("");
15503:   }
15504:   if( zExists==0 ) rc = SQLITE_NOMEM;
15505:   for(i=0; i<pAr->nArg && rc==SQLITE_OK; i++){
15506:     char *zSql2 = sqlite3_mprintf(zInsertFmt[pAr->bZip], zTab,
15507:         pAr->bVerbose ? "shell_putsnl(name)" : "name",
15508:         pAr->azArg[i], pAr->zDir, zExists);
15509:     rc = arExecSql(pAr, zSql2);
15510:     sqlite3_free(zSql2);
15511:   }
15512: end_ar_transaction:
15513:   if( rc!=SQLITE_OK ){
15514:     sqlite3_exec(pAr->db, "ROLLBACK TO ar; RELEASE ar;", 0, 0, 0);
15515:   }else{
15516:     rc = arExecSql(pAr, "RELEASE ar;");
15517:     if( pAr->bZip && pAr->zFile ){
15518:       zSql = sqlite3_mprintf("DROP TABLE %s", zTemp);
15519:       arExecSql(pAr, zSql);
15520:       sqlite3_free(zSql);
15521:     }
15522:   }
15523:   sqlite3_free(zExists);
15524:   return rc;
15525: }
