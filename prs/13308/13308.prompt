773: /*
774: ** Return the value of a hexadecimal digit.  Return -1 if the input
775: ** is not a hex digit.
776: */
777: static int hexDigitValue(char c){
778:   if( c>='0' && c<='9' ) return c - '0';
779:   if( c>='a' && c<='f' ) return c - 'a' + 10;
780:   if( c>='A' && c<='F' ) return c - 'A' + 10;
781:   return -1;
782: }
