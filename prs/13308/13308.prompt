4318: /*
4319: ** Allocate a new Decimal object.  Initialize it to the number given
4320: ** by the input string.
4321: */
4322: static Decimal *decimal_new(
4323:   sqlite3_context *pCtx,
4324:   sqlite3_value *pIn,
4325:   int nAlt,
4326:   const unsigned char *zAlt
4327: ){
4328:   Decimal *p;
4329:   int n, i;
4330:   const unsigned char *zIn;
4331:   int iExp = 0;
4332:   p = sqlite3_malloc( sizeof(*p) );
4333:   if( p==0 ) goto new_no_mem;
4334:   p->sign = 0;
4335:   p->oom = 0;
4336:   p->isInit = 1;
4337:   p->isNull = 0;
4338:   p->nDigit = 0;
4339:   p->nFrac = 0;
4340:   if( zAlt ){
4341:     n = nAlt,
4342:     zIn = zAlt;
4343:   }else{
4344:     if( sqlite3_value_type(pIn)==SQLITE_NULL ){
4345:       p->a = 0;
4346:       p->isNull = 1;
4347:       return p;
4348:     }
4349:     n = sqlite3_value_bytes(pIn);
4350:     zIn = sqlite3_value_text(pIn);
4351:   }
4352:   p->a = sqlite3_malloc64( n+1 );
4353:   if( p->a==0 ) goto new_no_mem;
4354:   for(i=0; isspace(zIn[i]); i++){}
4355:   if( zIn[i]=='-' ){
4356:     p->sign = 1;
4357:     i++;
4358:   }else if( zIn[i]=='+' ){
4359:     i++;
4360:   }
4361:   while( i<n && zIn[i]=='0' ) i++;
4362:   while( i<n ){
4363:     char c = zIn[i];
4364:     if( c>='0' && c<='9' ){
4365:       p->a[p->nDigit++] = c - '0';
4366:     }else if( c=='.' ){
4367:       p->nFrac = p->nDigit + 1;
4368:     }else if( c=='e' || c=='E' ){
4369:       int j = i+1;
4370:       int neg = 0;
4371:       if( j>=n ) break;
4372:       if( zIn[j]=='-' ){
4373:         neg = 1;
4374:         j++;
4375:       }else if( zIn[j]=='+' ){
4376:         j++;
4377:       }
4378:       while( j<n && iExp<1000000 ){
4379:         if( zIn[j]>='0' && zIn[j]<='9' ){
4380:           iExp = iExp*10 + zIn[j] - '0';
4381:         }
4382:         j++;
4383:       }
4384:       if( neg ) iExp = -iExp;
4385:       break;
4386:     }
4387:     i++;
4388:   }
4389:   if( p->nFrac ){
4390:     p->nFrac = p->nDigit - (p->nFrac - 1);
4391:   }
4392:   if( iExp>0 ){
4393:     if( p->nFrac>0 ){
4394:       if( iExp<=p->nFrac ){
4395:         p->nFrac -= iExp;
4396:         iExp = 0;
4397:       }else{
4398:         iExp -= p->nFrac;
4399:         p->nFrac = 0;
4400:       }
4401:     }
4402:     if( iExp>0 ){
4403:       p->a = sqlite3_realloc64(p->a, p->nDigit + iExp + 1 );
4404:       if( p->a==0 ) goto new_no_mem;
4405:       memset(p->a+p->nDigit, 0, iExp);
4406:       p->nDigit += iExp;
4407:     }
4408:   }else if( iExp<0 ){
4409:     int nExtra;
4410:     iExp = -iExp;
4411:     nExtra = p->nDigit - p->nFrac - 1;
4412:     if( nExtra ){
4413:       if( nExtra>=iExp ){
4414:         p->nFrac += iExp;
4415:         iExp  = 0;
4416:       }else{
4417:         iExp -= nExtra;
4418:         p->nFrac = p->nDigit - 1;
4419:       }
4420:     }
4421:     if( iExp>0 ){
4422:       p->a = sqlite3_realloc64(p->a, p->nDigit + iExp + 1 );
4423:       if( p->a==0 ) goto new_no_mem;
4424:       memmove(p->a+iExp, p->a, p->nDigit);
4425:       memset(p->a, 0, iExp);
4426:       p->nDigit += iExp;
4427:       p->nFrac += iExp;
4428:     }
4429:   }
4430:   return p;
