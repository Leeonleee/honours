5868: /*
5869: ** Convert the standard MS-DOS timestamp stored in the mTime and mDate
5870: ** fields of the CDS structure passed as the only argument to a 32-bit
5871: ** UNIX seconds-since-the-epoch timestamp. Return the result.
5872: **
5873: ** "Standard" MS-DOS time format:
5874: **
5875: **   File modification time:
5876: **     Bits 00-04: seconds divided by 2
5877: **     Bits 05-10: minute
5878: **     Bits 11-15: hour
5879: **   File modification date:
5880: **     Bits 00-04: day
5881: **     Bits 05-08: month (1-12)
5882: **     Bits 09-15: years from 1980
5883: **
5884: ** https://msdn.microsoft.com/en-us/library/9kkf9tah.aspx
5885: */
5886: static u32 zipfileMtime(ZipfileCDS *pCDS){
5887:   int Y = (1980 + ((pCDS->mDate >> 9) & 0x7F));
5888:   int M = ((pCDS->mDate >> 5) & 0x0F);
5889:   int D = (pCDS->mDate & 0x1F);
5890:   int B = -13;
5891: 
5892:   int sec = (pCDS->mTime & 0x1F)*2;
5893:   int min = (pCDS->mTime >> 5) & 0x3F;
5894:   int hr = (pCDS->mTime >> 11) & 0x1F;
5895:   i64 JD;
5896: 
5897:   /* JD = INT(365.25 * (Y+4716)) + INT(30.6001 * (M+1)) + D + B - 1524.5 */
5898: 
5899:   /* Calculate the JD in seconds for noon on the day in question */
5900:   if( M<3 ){
5901:     Y = Y-1;
5902:     M = M+12;
5903:   }
5904:   JD = (i64)(24*60*60) * (
5905:       (int)(365.25 * (Y + 4716))
5906:     + (int)(30.6001 * (M + 1))
5907:     + D + B - 1524
5908:   );
5909: 
5910:   /* Correct the JD for the time within the day */
5911:   JD += (hr-12) * 3600 + min * 60 + sec;
5912: 
5913:   /* Convert JD to unix timestamp (the JD epoch is 2440587.5) */
5914:   return (u32)(JD - (i64)(24405875) * 24*60*6);
5915: }
5916: 
5917: /*
5918: ** The opposite of zipfileMtime(). This function populates the mTime and
5919: ** mDate fields of the CDS structure passed as the first argument according
5920: ** to the UNIX timestamp value passed as the second.
5921: */
5922: static void zipfileMtimeToDos(ZipfileCDS *pCds, u32 mUnixTime){
5923:   /* Convert unix timestamp to JD (2440588 is noon on 1/1/1970) */
5924:   i64 JD = (i64)2440588 + mUnixTime / (24*60*60);
5925: 
5926:   int A, B, C, D, E;
5927:   int yr, mon, day;
5928:   int hr, min, sec;
5929: 
5930:   A = (int)((JD - 1867216.25)/36524.25);
5931:   A = (int)(JD + 1 + A - (A/4));
5932:   B = A + 1524;
5933:   C = (int)((B - 122.1)/365.25);
5934:   D = (36525*(C&32767))/100;
5935:   E = (int)((B-D)/30.6001);
5936: 
5937:   day = B - D - (int)(30.6001*E);
5938:   mon = (E<14 ? E-1 : E-13);
5939:   yr = mon>2 ? C-4716 : C-4715;
5940: 
5941:   hr = (mUnixTime % (24*60*60)) / (60*60);
5942:   min = (mUnixTime % (60*60)) / 60;
5943:   sec = (mUnixTime % 60);
5944: 
5945:   if( yr>=1980 ){
5946:     pCds->mDate = (u16)(day + (mon << 5) + ((yr-1980) << 9));
5947:     pCds->mTime = (u16)(sec/2 + (min<<5) + (hr<<11));
5948:   }else{
5949:     pCds->mDate = pCds->mTime = 0;
5950:   }
5951: 
5952:   assert( mUnixTime<315507600
5953:        || mUnixTime==zipfileMtime(pCds)
5954:        || ((mUnixTime % 2) && mUnixTime-1==zipfileMtime(pCds))
5955:        /* || (mUnixTime % 2) */
5956:   );
5957: }
