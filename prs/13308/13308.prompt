7780: /*
7781: ** A hash table for storing strings. With space for a payload string
7782: ** with each entry. Methods are:
7783: **
7784: **   idxHashInit()
7785: **   idxHashClear()
7786: **   idxHashAdd()
7787: **   idxHashSearch()
7788: */
7789: #define IDX_HASH_SIZE 1023
7790: typedef struct IdxHashEntry IdxHashEntry;
7791: typedef struct IdxHash IdxHash;
7792: struct IdxHashEntry {
7793:   char *zKey;                     /* nul-terminated key */
7794:   char *zVal;                     /* nul-terminated value string */
7795:   char *zVal2;                    /* nul-terminated value string 2 */
7796:   IdxHashEntry *pHashNext;        /* Next entry in same hash bucket */
7797:   IdxHashEntry *pNext;            /* Next entry in hash */
7798: };
7799: struct IdxHash {
7800:   IdxHashEntry *pFirst;
7801:   IdxHashEntry *aHash[IDX_HASH_SIZE];
7802: };
