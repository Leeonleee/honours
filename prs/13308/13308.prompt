609: /*
610: ** Determines if a string is a number of not.
611: */
612: static int isNumber(const char *z, int *realnum){
613:   if( *z=='-' || *z=='+' ) z++;
614:   if( !IsDigit(*z) ){
615:     return 0;
616:   }
617:   z++;
618:   if( realnum ) *realnum = 0;
619:   while( IsDigit(*z) ){ z++; }
620:   if( *z=='.' ){
621:     z++;
622:     if( !IsDigit(*z) ) return 0;
623:     while( IsDigit(*z) ){ z++; }
624:     if( realnum ) *realnum = 1;
625:   }
626:   if( *z=='e' || *z=='E' ){
627:     z++;
628:     if( *z=='+' || *z=='-' ) z++;
629:     if( !IsDigit(*z) ) return 0;
630:     while( IsDigit(*z) ){ z++; }
631:     if( realnum ) *realnum = 1;
632:   }
633:   return *z==0;
634: }
