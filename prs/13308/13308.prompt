3374: /*
3375: ** SQLite will invoke this method one or more times while planning a query
3376: ** that uses the completion virtual table.  This routine needs to create
3377: ** a query plan for each invocation and compute an estimated cost for that
3378: ** plan.
3379: **
3380: ** There are two hidden parameters that act as arguments to the table-valued
3381: ** function:  "prefix" and "wholeline".  Bit 0 of idxNum is set if "prefix"
3382: ** is available and bit 1 is set if "wholeline" is available.
3383: */
3384: static int completionBestIndex(
3385:   sqlite3_vtab *tab,
3386:   sqlite3_index_info *pIdxInfo
3387: ){
3388:   int i;                 /* Loop over constraints */
3389:   int idxNum = 0;        /* The query plan bitmask */
3390:   int prefixIdx = -1;    /* Index of the start= constraint, or -1 if none */
3391:   int wholelineIdx = -1; /* Index of the stop= constraint, or -1 if none */
3392:   int nArg = 0;          /* Number of arguments that completeFilter() expects */
3393:   const struct sqlite3_index_constraint *pConstraint;
3394: 
3395:   (void)(tab);    /* Unused parameter */
3396:   pConstraint = pIdxInfo->aConstraint;
3397:   for(i=0; i<pIdxInfo->nConstraint; i++, pConstraint++){
3398:     if( pConstraint->usable==0 ) continue;
3399:     if( pConstraint->op!=SQLITE_INDEX_CONSTRAINT_EQ ) continue;
3400:     switch( pConstraint->iColumn ){
3401:       case COMPLETION_COLUMN_PREFIX:
3402:         prefixIdx = i;
3403:         idxNum |= 1;
3404:         break;
3405:       case COMPLETION_COLUMN_WHOLELINE:
3406:         wholelineIdx = i;
3407:         idxNum |= 2;
3408:         break;
3409:     }
3410:   }
3411:   if( prefixIdx>=0 ){
3412:     pIdxInfo->aConstraintUsage[prefixIdx].argvIndex = ++nArg;
3413:     pIdxInfo->aConstraintUsage[prefixIdx].omit = 1;
3414:   }
3415:   if( wholelineIdx>=0 ){
3416:     pIdxInfo->aConstraintUsage[wholelineIdx].argvIndex = ++nArg;
3417:     pIdxInfo->aConstraintUsage[wholelineIdx].omit = 1;
3418:   }
3419:   pIdxInfo->idxNum = idxNum;
3420:   pIdxInfo->estimatedCost = (double)5000 - 1000*nArg;
3421:   pIdxInfo->estimatedRows = 500 - 100*nArg;
3422:   return SQLITE_OK;
3423: }
