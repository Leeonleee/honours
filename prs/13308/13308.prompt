6392: static int zipfileLoadDirectory(ZipfileTab *pTab, const u8 *aBlob, int nBlob){
6393:   ZipfileEOCD eocd;
6394:   int rc;
6395:   int i;
6396:   i64 iOff;
6397: 
6398:   rc = zipfileReadEOCD(pTab, aBlob, nBlob, pTab->pWriteFd, &eocd);
6399:   iOff = eocd.iOffset;
6400:   for(i=0; rc==SQLITE_OK && i<eocd.nEntry; i++){
6401:     ZipfileEntry *pNew = 0;
6402:     rc = zipfileGetEntry(pTab, aBlob, nBlob, pTab->pWriteFd, iOff, &pNew);
6403: 
6404:     if( rc==SQLITE_OK ){
6405:       zipfileAddEntry(pTab, 0, pNew);
6406:       iOff += ZIPFILE_CDS_FIXED_SZ;
6407:       iOff += (int)pNew->cds.nExtra + pNew->cds.nFile + pNew->cds.nComment;
6408:     }
6409:   }
6410:   return rc;
6411: }
