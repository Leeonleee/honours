4598: /*
4599: ** Add the value pB into pA.
4600: **
4601: ** Both pA and pB might become denormalized by this routine.
4602: */
4603: static void decimal_add(Decimal *pA, Decimal *pB){
4604:   int nSig, nFrac, nDigit;
4605:   int i, rc;
4606:   if( pA==0 ){
4607:     return;
4608:   }
4609:   if( pA->oom || pB==0 || pB->oom ){
4610:     pA->oom = 1;
4611:     return;
4612:   }
4613:   if( pA->isNull || pB->isNull ){
4614:     pA->isNull = 1;
4615:     return;
4616:   }
4617:   nSig = pA->nDigit - pA->nFrac;
4618:   if( nSig && pA->a[0]==0 ) nSig--;
4619:   if( nSig<pB->nDigit-pB->nFrac ){
4620:     nSig = pB->nDigit - pB->nFrac;
4621:   }
4622:   nFrac = pA->nFrac;
4623:   if( nFrac<pB->nFrac ) nFrac = pB->nFrac;
4624:   nDigit = nSig + nFrac + 1;
4625:   decimal_expand(pA, nDigit, nFrac);
4626:   decimal_expand(pB, nDigit, nFrac);
4627:   if( pA->oom || pB->oom ){
4628:     pA->oom = 1;
4629:   }else{
4630:     if( pA->sign==pB->sign ){
4631:       int carry = 0;
4632:       for(i=nDigit-1; i>=0; i--){
4633:         int x = pA->a[i] + pB->a[i] + carry;
4634:         if( x>=10 ){
4635:           carry = 1;
4636:           pA->a[i] = x - 10;
4637:         }else{
4638:           carry = 0;
4639:           pA->a[i] = x;
4640:         }
4641:       }
4642:     }else{
4643:       signed char *aA, *aB;
4644:       int borrow = 0;
4645:       rc = memcmp(pA->a, pB->a, nDigit);
4646:       if( rc<0 ){
4647:         aA = pB->a;
4648:         aB = pA->a;
4649:         pA->sign = !pA->sign;
4650:       }else{
4651:         aA = pA->a;
4652:         aB = pB->a;
4653:       }
4654:       for(i=nDigit-1; i>=0; i--){
4655:         int x = aA[i] - aB[i] - borrow;
4656:         if( x<0 ){
4657:           pA->a[i] = x+10;
4658:           borrow = 1;
4659:         }else{
4660:           pA->a[i] = x;
4661:           borrow = 0;
4662:         }
4663:       }
4664:     }
4665:   }
4666: }
