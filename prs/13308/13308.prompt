15190: /*
15191: ** This function assumes that all arguments within the ArCommand.azArg[]
15192: ** array refer to archive members, as for the --extract or --list commands.
15193: ** It checks that each of them are present. If any specified file is not
15194: ** present in the archive, an error is printed to stderr and an error
15195: ** code returned. Otherwise, if all specified arguments are present in
15196: ** the archive, SQLITE_OK is returned.
15197: **
15198: ** This function strips any trailing '/' characters from each argument.
15199: ** This is consistent with the way the [tar] command seems to work on
15200: ** Linux.
15201: */
15202: static int arCheckEntries(ArCommand *pAr){
15203:   int rc = SQLITE_OK;
15204:   if( pAr->nArg ){
15205:     int i, j;
15206:     sqlite3_stmt *pTest = 0;
15207: 
15208:     shellPreparePrintf(pAr->db, &rc, &pTest,
15209:         "SELECT name FROM %s WHERE name=$name",
15210:         pAr->zSrcTable
15211:     );
15212:     j = sqlite3_bind_parameter_index(pTest, "$name");
15213:     for(i=0; i<pAr->nArg && rc==SQLITE_OK; i++){
15214:       char *z = pAr->azArg[i];
15215:       int n = strlen30(z);
15216:       int bOk = 0;
15217:       while( n>0 && z[n-1]=='/' ) n--;
15218:       z[n] = '\0';
15219:       sqlite3_bind_text(pTest, j, z, -1, SQLITE_STATIC);
15220:       if( SQLITE_ROW==sqlite3_step(pTest) ){
15221:         bOk = 1;
15222:       }
15223:       shellReset(&rc, pTest);
15224:       if( rc==SQLITE_OK && bOk==0 ){
15225:         utf8_printf(stderr, "not found in archive: %s\n", z);
15226:         rc = SQLITE_ERROR;
15227:       }
15228:     }
15229:     shellFinalize(&rc, pTest);
15230:   }
15231:   return rc;
15232: }
