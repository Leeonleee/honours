5479: /*
5480: ** If string zIn is quoted, dequote it in place. Otherwise, if the string
5481: ** is not quoted, do nothing.
5482: */
5483: static void zipfileDequote(char *zIn){
5484:   char q = zIn[0];
5485:   if( q=='"' || q=='\'' || q=='`' || q=='[' ){
5486:     int iIn = 1;
5487:     int iOut = 0;
5488:     if( q=='[' ) q = ']';
5489:     while( ALWAYS(zIn[iIn]) ){
5490:       char c = zIn[iIn++];
5491:       if( c==q && zIn[iIn++]!=q ) break;
5492:       zIn[iOut++] = c;
5493:     }
5494:     zIn[iOut] = '\0';
5495:   }
5496: }
