8335: /*
8336: ** Attempt to allocate an IdxTable structure corresponding to table zTab
8337: ** in the main database of connection db. If successful, set (*ppOut) to
8338: ** point to the new object and return SQLITE_OK. Otherwise, return an
8339: ** SQLite error code and set (*ppOut) to NULL. In this case *pzErrmsg may be
8340: ** set to point to an error string.
8341: **
8342: ** It is the responsibility of the caller to eventually free either the
8343: ** IdxTable object or error message using sqlite3_free().
8344: */
8345: static int idxGetTableInfo(
8346:   sqlite3 *db,                    /* Database connection to read details from */
8347:   const char *zTab,               /* Table name */
8348:   IdxTable **ppOut,               /* OUT: New object (if successful) */
8349:   char **pzErrmsg                 /* OUT: Error message (if not) */
8350: ){
8351:   sqlite3_stmt *p1 = 0;
8352:   int nCol = 0;
8353:   int nTab = STRLEN(zTab);
8354:   int nByte = sizeof(IdxTable) + nTab + 1;
8355:   IdxTable *pNew = 0;
8356:   int rc, rc2;
8357:   char *pCsr = 0;
8358: 
8359:   rc = idxPrintfPrepareStmt(db, &p1, pzErrmsg, "PRAGMA table_info=%Q", zTab);
8360:   while( rc==SQLITE_OK && SQLITE_ROW==sqlite3_step(p1) ){
8361:     const char *zCol = (const char*)sqlite3_column_text(p1, 1);
8362:     nByte += 1 + STRLEN(zCol);
8363:     rc = sqlite3_table_column_metadata(
8364:         db, "main", zTab, zCol, 0, &zCol, 0, 0, 0
8365:     );
8366:     nByte += 1 + STRLEN(zCol);
8367:     nCol++;
8368:   }
8369:   rc2 = sqlite3_reset(p1);
8370:   if( rc==SQLITE_OK ) rc = rc2;
8371: 
8372:   nByte += sizeof(IdxColumn) * nCol;
8373:   if( rc==SQLITE_OK ){
8374:     pNew = idxMalloc(&rc, nByte);
8375:   }
8376:   if( rc==SQLITE_OK ){
8377:     pNew->aCol = (IdxColumn*)&pNew[1];
8378:     pNew->nCol = nCol;
8379:     pCsr = (char*)&pNew->aCol[nCol];
8380:   }
8381: 
8382:   nCol = 0;
8383:   while( rc==SQLITE_OK && SQLITE_ROW==sqlite3_step(p1) ){
8384:     const char *zCol = (const char*)sqlite3_column_text(p1, 1);
8385:     int nCopy = STRLEN(zCol) + 1;
8386:     pNew->aCol[nCol].zName = pCsr;
8387:     pNew->aCol[nCol].iPk = sqlite3_column_int(p1, 5);
8388:     memcpy(pCsr, zCol, nCopy);
8389:     pCsr += nCopy;
8390: 
8391:     rc = sqlite3_table_column_metadata(
8392:         db, "main", zTab, zCol, 0, &zCol, 0, 0, 0
8393:     );
8394:     if( rc==SQLITE_OK ){
8395:       nCopy = STRLEN(zCol) + 1;
8396:       pNew->aCol[nCol].zColl = pCsr;
8397:       memcpy(pCsr, zCol, nCopy);
8398:       pCsr += nCopy;
8399:     }
8400: 
8401:     nCol++;
8402:   }
8403:   idxFinalize(&rc, p1);
8404: 
8405:   if( rc!=SQLITE_OK ){
8406:     sqlite3_free(pNew);
8407:     pNew = 0;
8408:   }else{
8409:     pNew->zName = pCsr;
8410:     memcpy(pNew->zName, zTab, nTab+1);
8411:   }
8412: 
8413:   *ppOut = pNew;
8414:   return rc;
8415: }
