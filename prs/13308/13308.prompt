10928: /*
10929: ** Output the given string as a quoted string using SQL quoting conventions.
10930: **
10931: ** See also: output_quoted_escaped_string()
10932: */
10933: static void output_quoted_string(FILE *out, const char *z){
10934:   int i;
10935:   char c;
10936:   setBinaryMode(out, 1);
10937:   for(i=0; (c = z[i])!=0 && c!='\''; i++){}
10938:   if( c==0 ){
10939:     utf8_printf(out,"'%s'",z);
10940:   }else{
10941:     raw_printf(out, "'");
10942:     while( *z ){
10943:       for(i=0; (c = z[i])!=0 && c!='\''; i++){}
10944:       if( c=='\'' ) i++;
10945:       if( i ){
10946:         utf8_printf(out, "%.*s", i, z);
10947:         z += i;
10948:       }
10949:       if( c=='\'' ){
10950:         raw_printf(out, "'");
10951:         continue;
10952:       }
10953:       if( c==0 ){
10954:         break;
10955:       }
10956:       z++;
10957:     }
10958:     raw_printf(out, "'");
10959:   }
10960:   setTextMode(out, 1);
10961: }
