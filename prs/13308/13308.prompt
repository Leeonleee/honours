11460: /*
11461: ** This is the callback routine that the shell
11462: ** invokes for each row of a query result.
11463: */
11464: static int shell_callback(
11465:   void *pArg,
11466:   int nArg,        /* Number of result columns */
11467:   char **azArg,    /* Text of each result column */
11468:   char **azCol,    /* Column names */
11469:   int *aiType      /* Column types.  Might be NULL */
11470: ){
11471:   int i;
11472:   ShellState *p = (ShellState*)pArg;
11473: 
11474:   if( azArg==0 ) return 0;
11475:   switch( p->cMode ){
11476:     case MODE_Line: {
11477:       int w = 5;
11478:       if( azArg==0 ) break;
11479:       for(i=0; i<nArg; i++){
11480:         int len = strlen30(azCol[i] ? azCol[i] : "");
11481:         if( len>w ) w = len;
11482:       }
11483:       if( p->cnt++>0 ) utf8_printf(p->out, "%s", p->rowSeparator);
11484:       for(i=0; i<nArg; i++){
11485:         utf8_printf(p->out,"%*s = %s%s", w, azCol[i],
11486:                 azArg[i] ? azArg[i] : p->nullValue, p->rowSeparator);
11487:       }
11488:       break;
11489:     }
11490:     case MODE_Explain: {
11491:       if (nArg != 2) {
11492:         break;
11493:       }
11494:       if (strcmp(azArg[0], "logical_plan") == 0
11495:             || strcmp(azArg[0], "logical_opt") == 0
11496:             || strcmp(azArg[0], "physical_plan") == 0) {
11497:         utf8_printf(p->out, "\n┌─────────────────────────────┐\n");
11498:         utf8_printf(p->out, "│┌───────────────────────────┐│\n");
11499:         if (strcmp(azArg[0], "logical_plan") == 0) {
11500:           utf8_printf(p->out, "││ Unoptimized Logical Plan  ││\n");
11501:         } else if (strcmp(azArg[0], "logical_opt") == 0) {
11502:           utf8_printf(p->out, "││  Optimized Logical Plan   ││\n");
11503:         } else if (strcmp(azArg[0], "physical_plan") == 0) {
11504:           utf8_printf(p->out, "││       Physical Plan       ││\n");
11505:         }
11506:         utf8_printf(p->out, "│└───────────────────────────┘│\n");
11507:         utf8_printf(p->out, "└─────────────────────────────┘\n");
11508:       }
11509:       utf8_printf(p->out, "%s", azArg[1]);
11510:       break;
11511:     }
11512:     case MODE_Semi: {   /* .schema and .fullschema output */
11513:       printSchemaLine(p->out, azArg[0], "\n");
11514:       break;
11515:     }
11516:     case MODE_Pretty: {  /* .schema and .fullschema with --indent */
11517:       char *z;
11518:       int j;
11519:       int nParen = 0;
11520:       char cEnd = 0;
11521:       char c;
11522:       int nLine = 0;
11523:       assert( nArg==1 );
11524:       if( azArg[0]==0 ) break;
11525:       if( sqlite3_strlike("CREATE VIEW%", azArg[0], 0)==0
11526:        || sqlite3_strlike("CREATE TRIG%", azArg[0], 0)==0
11527:       ){
11528:         utf8_printf(p->out, "%s;\n", azArg[0]);
11529:         break;
11530:       }
11531:       z = sqlite3_mprintf("%s", azArg[0]);
11532:       j = 0;
11533:       for(i=0; IsSpace(z[i]); i++){}
11534:       for(; (c = z[i])!=0; i++){
11535:         if( IsSpace(c) ){
11536:           if( z[j-1]=='\r' ) z[j-1] = '\n';
11537:           if( IsSpace(z[j-1]) || z[j-1]=='(' ) continue;
11538:         }else if( (c=='(' || c==')') && j>0 && IsSpace(z[j-1]) ){
11539:           j--;
11540:         }
11541:         z[j++] = c;
11542:       }
11543:       while( j>0 && IsSpace(z[j-1]) ){ j--; }
11544:       z[j] = 0;
11545:       if( strlen30(z)>=79 ){
11546:         for(i=j=0; (c = z[i])!=0; i++){ /* Copy from z[i] back to z[j] */
11547:           if( c==cEnd ){
11548:             cEnd = 0;
11549:           }else if( c=='"' || c=='\'' || c=='`' ){
11550:             cEnd = c;
11551:           }else if( c=='[' ){
11552:             cEnd = ']';
11553:           }else if( c=='-' && z[i+1]=='-' ){
11554:             cEnd = '\n';
11555:           }else if( c=='(' ){
11556:             nParen++;
11557:           }else if( c==')' ){
11558:             nParen--;
11559:             if( nLine>0 && nParen==0 && j>0 ){
11560:               printSchemaLineN(p->out, z, j, "\n");
11561:               j = 0;
11562:             }
11563:           }
11564:           z[j++] = c;
11565:           if( nParen==1 && cEnd==0
11566:            && (c=='(' || c=='\n' || (c==',' && !wsToEol(z+i+1)))
11567:           ){
11568:             if( c=='\n' ) j--;
11569:             printSchemaLineN(p->out, z, j, "\n  ");
11570:             j = 0;
11571:             nLine++;
11572:             while( IsSpace(z[i+1]) ){ i++; }
11573:           }
11574:         }
11575:         z[j] = 0;
11576:       }
11577:       printSchemaLine(p->out, z, ";\n");
11578:       sqlite3_free(z);
11579:       break;
11580:     }
11581:     case MODE_List: {
11582:       if( p->cnt++==0 && p->showHeader ){
11583:         for(i=0; i<nArg; i++){
11584:           utf8_printf(p->out,"%s%s",azCol[i],
11585:                   i==nArg-1 ? p->rowSeparator : p->colSeparator);
11586:         }
11587:       }
11588:       if( azArg==0 ) break;
11589:       for(i=0; i<nArg; i++){
11590:         char *z = azArg[i];
11591:         if( z==0 ) z = p->nullValue;
11592:         utf8_printf(p->out, "%s", z);
11593:         if( i<nArg-1 ){
11594:           utf8_printf(p->out, "%s", p->colSeparator);
11595:         }else{
11596:           utf8_printf(p->out, "%s", p->rowSeparator);
11597:         }
11598:       }
11599:       break;
11600:     }
11601:     case MODE_Html: {
11602:       if( p->cnt++==0 && p->showHeader ){
11603:         raw_printf(p->out,"<tr>");
11604:         for(i=0; i<nArg; i++){
11605:           raw_printf(p->out,"<th>");
11606:           output_html_string(p->out, azCol[i]);
