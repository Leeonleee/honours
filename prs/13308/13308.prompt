8773: /*
8774: ** This function is called after candidate indexes have been created. It
8775: ** runs all the queries to see which indexes they prefer, and populates
8776: ** IdxStatement.zIdx and IdxStatement.zEQP with the results.
8777: */
8778: int idxFindIndexes(
8779:   sqlite3expert *p,
8780:   char **pzErr                         /* OUT: Error message (sqlite3_malloc) */
8781: ){
8782:   IdxStatement *pStmt;
8783:   sqlite3 *dbm = p->dbm;
8784:   int rc = SQLITE_OK;
8785: 
8786:   IdxHash hIdx;
8787:   idxHashInit(&hIdx);
8788: 
8789:   for(pStmt=p->pStatement; rc==SQLITE_OK && pStmt; pStmt=pStmt->pNext){
8790:     IdxHashEntry *pEntry;
8791:     sqlite3_stmt *pExplain = 0;
8792:     idxHashClear(&hIdx);
8793:     rc = idxPrintfPrepareStmt(dbm, &pExplain, pzErr,
8794:         "EXPLAIN QUERY PLAN %s", pStmt->zSql
8795:     );
8796:     while( rc==SQLITE_OK && sqlite3_step(pExplain)==SQLITE_ROW ){
8797:       /* int iId = sqlite3_column_int(pExplain, 0); */
8798:       /* int iParent = sqlite3_column_int(pExplain, 1); */
8799:       /* int iNotUsed = sqlite3_column_int(pExplain, 2); */
8800:       const char *zDetail = (const char*)sqlite3_column_text(pExplain, 3);
8801:       int nDetail;
8802:       int i;
8803: 
8804:       if( !zDetail ) continue;
8805:       nDetail = STRLEN(zDetail);
8806: 
8807:       for(i=0; i<nDetail; i++){
8808:         const char *zIdx = 0;
8809:         if( i+13<nDetail && memcmp(&zDetail[i], " USING INDEX ", 13)==0 ){
8810:           zIdx = &zDetail[i+13];
8811:         }else if( i+22<nDetail
8812:             && memcmp(&zDetail[i], " USING COVERING INDEX ", 22)==0
8813:         ){
8814:           zIdx = &zDetail[i+22];
8815:         }
8816:         if( zIdx ){
8817:           const char *zSql;
8818:           int nIdx = 0;
8819:           while( zIdx[nIdx]!='\0' && (zIdx[nIdx]!=' ' || zIdx[nIdx+1]!='(') ){
8820:             nIdx++;
8821:           }
8822:           zSql = idxHashSearch(&p->hIdx, zIdx, nIdx);
8823:           if( zSql ){
8824:             idxHashAdd(&rc, &hIdx, zSql, 0);
8825:             if( rc ) goto find_indexes_out;
8826:           }
8827:           break;
8828:         }
8829:       }
8830: 
8831:       if( zDetail[0]!='-' ){
8832:         pStmt->zEQP = idxAppendText(&rc, pStmt->zEQP, "%s\n", zDetail);
8833:       }
8834:     }
8835: 
8836:     for(pEntry=hIdx.pFirst; pEntry; pEntry=pEntry->pNext){
8837:       pStmt->zIdx = idxAppendText(&rc, pStmt->zIdx, "%s;\n", pEntry->zKey);
8838:     }
8839: 
8840:     idxFinalize(&rc, pExplain);
8841:   }
8842: 
8843:  find_indexes_out:
8844:   idxHashClear(&hIdx);
8845:   return rc;
8846: }
