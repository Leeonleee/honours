9079: struct IdxRemCtx {
9080:   int nSlot;
9081:   struct IdxRemSlot {
9082:     int eType;                    /* SQLITE_NULL, INTEGER, REAL, TEXT, BLOB */
9083:     i64 iVal;                     /* SQLITE_INTEGER value */
9084:     double rVal;                  /* SQLITE_FLOAT value */
9085:     int nByte;                    /* Bytes of space allocated at z */
9086:     int n;                        /* Size of buffer z */
9087:     char *z;                      /* SQLITE_TEXT/BLOB value */
9088:   } aSlot[1];
9089: };
