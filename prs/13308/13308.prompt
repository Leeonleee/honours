8506: /*
8507: ** Search database dbm for an index compatible with the one idxCreateFromCons()
8508: ** would create from arguments pScan, pEq and pTail. If no error occurs and
8509: ** such an index is found, return non-zero. Or, if no such index is found,
8510: ** return zero.
8511: **
8512: ** If an error occurs, set *pRc to an SQLite error code and return zero.
8513: */
8514: static int idxFindCompatible(
8515:   int *pRc,                       /* OUT: Error code */
8516:   sqlite3* dbm,                   /* Database to search */
8517:   IdxScan *pScan,                 /* Scan for table to search for index on */
8518:   IdxConstraint *pEq,             /* List of == constraints */
8519:   IdxConstraint *pTail            /* List of range constraints */
8520: ){
8521:   const char *zTbl = pScan->pTab->zName;
8522:   sqlite3_stmt *pIdxList = 0;
8523:   IdxConstraint *pIter;
8524:   int nEq = 0;                    /* Number of elements in pEq */
8525:   int rc;
8526: 
8527:   /* Count the elements in list pEq */
8528:   for(pIter=pEq; pIter; pIter=pIter->pLink) nEq++;
8529: 
8530:   rc = idxPrintfPrepareStmt(dbm, &pIdxList, 0, "PRAGMA index_list=%Q", zTbl);
8531:   while( rc==SQLITE_OK && sqlite3_step(pIdxList)==SQLITE_ROW ){
8532:     int bMatch = 1;
8533:     IdxConstraint *pT = pTail;
8534:     sqlite3_stmt *pInfo = 0;
8535:     const char *zIdx = (const char*)sqlite3_column_text(pIdxList, 1);
8536: 
8537:     /* Zero the IdxConstraint.bFlag values in the pEq list */
8538:     for(pIter=pEq; pIter; pIter=pIter->pLink) pIter->bFlag = 0;
8539: 
8540:     rc = idxPrintfPrepareStmt(dbm, &pInfo, 0, "PRAGMA index_xInfo=%Q", zIdx);
8541:     while( rc==SQLITE_OK && sqlite3_step(pInfo)==SQLITE_ROW ){
8542:       int iIdx = sqlite3_column_int(pInfo, 0);
8543:       int iCol = sqlite3_column_int(pInfo, 1);
8544:       const char *zColl = (const char*)sqlite3_column_text(pInfo, 4);
8545: 
8546:       if( iIdx<nEq ){
8547:         for(pIter=pEq; pIter; pIter=pIter->pLink){
8548:           if( pIter->bFlag ) continue;
8549:           if( pIter->iCol!=iCol ) continue;
8550:           if( sqlite3_stricmp(pIter->zColl, zColl) ) continue;
8551:           pIter->bFlag = 1;
8552:           break;
8553:         }
8554:         if( pIter==0 ){
8555:           bMatch = 0;
8556:           break;
8557:         }
8558:       }else{
8559:         if( pT ){
8560:           if( pT->iCol!=iCol || sqlite3_stricmp(pT->zColl, zColl) ){
8561:             bMatch = 0;
8562:             break;
8563:           }
8564:           pT = pT->pLink;
8565:         }
8566:       }
8567:     }
8568:     idxFinalize(&rc, pInfo);
8569: 
8570:     if( rc==SQLITE_OK && bMatch ){
8571:       sqlite3_finalize(pIdxList);
8572:       return 1;
8573:     }
8574:   }
8575:   idxFinalize(&rc, pIdxList);
8576: 
8577:   *pRc = rc;
8578:   return 0;
8579: }
