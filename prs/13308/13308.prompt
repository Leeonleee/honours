4787: /*
4788: ** SQL Function:   decimal_mul(X, Y)
4789: **
4790: ** Return the product of X and Y.
4791: **
4792: ** All significant digits after the decimal point are retained.
4793: ** Trailing zeros after the decimal point are omitted as long as
4794: ** the number of digits after the decimal point is no less than
4795: ** either the number of digits in either input.
4796: */
4797: static void decimalMulFunc(
4798:   sqlite3_context *context,
4799:   int argc,
4800:   sqlite3_value **argv
4801: ){
4802:   Decimal *pA = decimal_new(context, argv[0], 0, 0);
4803:   Decimal *pB = decimal_new(context, argv[1], 0, 0);
4804:   signed char *acc = 0;
4805:   int i, j, k;
4806:   int minFrac;
4807:   UNUSED_PARAMETER(argc);
4808:   if( pA==0 || pA->oom || pA->isNull
4809:    || pB==0 || pB->oom || pB->isNull
4810:   ){
4811:     goto mul_end;
4812:   }
4813:   acc = sqlite3_malloc64( pA->nDigit + pB->nDigit + 2 );
4814:   if( acc==0 ){
4815:     sqlite3_result_error_nomem(context);
4816:     goto mul_end;
4817:   }
4818:   memset(acc, 0, pA->nDigit + pB->nDigit + 2);
4819:   minFrac = pA->nFrac;
4820:   if( pB->nFrac<minFrac ) minFrac = pB->nFrac;
4821:   for(i=pA->nDigit-1; i>=0; i--){
4822:     signed char f = pA->a[i];
4823:     int carry = 0, x;
4824:     for(j=pB->nDigit-1, k=i+j+3; j>=0; j--, k--){
4825:       x = acc[k] + f*pB->a[j] + carry;
4826:       acc[k] = x%10;
4827:       carry = x/10;
4828:     }
4829:     x = acc[k] + carry;
4830:     acc[k] = x%10;
4831:     acc[k-1] += x/10;
4832:   }
4833:   sqlite3_free(pA->a);
4834:   pA->a = acc;
4835:   acc = 0;
4836:   pA->nDigit += pB->nDigit + 2;
4837:   pA->nFrac += pB->nFrac;
4838:   pA->sign ^= pB->sign;
4839:   while( pA->nFrac>minFrac && pA->a[pA->nDigit-1]==0 ){
4840:     pA->nFrac--;
4841:     pA->nDigit--;
4842:   }
4843:   decimal_result(context, pA);
4844: 
4845: mul_end:
4846:   sqlite3_free(acc);
4847:   decimal_free(pA);
4848:   decimal_free(pB);
4849: }
