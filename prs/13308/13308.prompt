6656: /*
6657: ** Return the current time as a 32-bit timestamp in UNIX epoch format (like
6658: ** time(2)).
6659: */
6660: static u32 zipfileTime(void){
6661:   sqlite3_vfs *pVfs = sqlite3_vfs_find(0);
6662:   u32 ret;
6663:   if( pVfs->iVersion>=2 && pVfs->xCurrentTimeInt64 ){
6664:     i64 ms;
6665:     pVfs->xCurrentTimeInt64(pVfs, &ms);
6666:     ret = (u32)((ms/1000) - ((i64)24405875 * 8640));
6667:   }else{
6668:     double day;
6669:     pVfs->xCurrentTime(pVfs, &day);
6670:     ret = (u32)((day - 2440587.5) * 86400);
6671:   }
6672:   return ret;
6673: }
