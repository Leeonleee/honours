14816: /*
14817: ** Create a new temp file name with the given suffix.
14818: */
14819: static void newTempFile(ShellState *p, const char *zSuffix){
14820:   clearTempFile(p);
14821:   sqlite3_free(p->zTempFile);
14822:   p->zTempFile = 0;
14823:   if( p->db ){
14824:     sqlite3_file_control(p->db, 0, SQLITE_FCNTL_TEMPFILENAME, &p->zTempFile);
14825:   }
14826:   if( p->zTempFile==0 ){
14827:     /* If p->db is an in-memory database then the TEMPFILENAME file-control
14828:     ** will not work and we will need to fallback to guessing */
14829:     char *zTemp;
14830:     sqlite3_uint64 r;
14831:     sqlite3_randomness(sizeof(r), &r);
14832:     zTemp = getenv("TEMP");
14833:     if( zTemp==0 ) zTemp = getenv("TMP");
14834:     if( zTemp==0 ){
14835: #ifdef _WIN32
14836:       zTemp = "\\tmp";
14837: #else
14838:       zTemp = "/tmp";
14839: #endif
14840:     }
14841:     p->zTempFile = sqlite3_mprintf("%s/temp%llx.%s", zTemp, r, zSuffix);
14842:   }else{
14843:     p->zTempFile = sqlite3_mprintf("%z.%s", p->zTempFile, zSuffix);
14844:   }
14845:   if( p->zTempFile==0 ){
14846:     raw_printf(stderr, "out of memory\n");
14847:     exit(1);
14848:   }
14849: }
