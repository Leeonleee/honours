5300: #define ZIPFILE_NEWENTRY_MADEBY   ((3<<8) + 30)
5301: #define ZIPFILE_NEWENTRY_REQUIRED 20
5302: #define ZIPFILE_NEWENTRY_FLAGS    0x800
