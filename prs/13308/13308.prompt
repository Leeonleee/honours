13251: /*
13252: ** This is a different callback routine used for dumping the database.
13253: ** Each row received by this callback consists of a table name,
13254: ** the table type ("index" or "table") and SQL to create the table.
13255: ** This routine should print text sufficient to recreate the table.
13256: */
13257: static int dump_callback(void *pArg, int nArg, char **azArg, char **azNotUsed){
13258:   int rc;
13259:   const char *zTable;
13260:   const char *zType;
13261:   const char *zSql;
13262:   ShellState *p = (ShellState *)pArg;
13263: 
13264:   UNUSED_PARAMETER(azNotUsed);
13265:   if( nArg!=3 || azArg==0 ) return 0;
13266:   zTable = azArg[0];
13267:   zType = azArg[1];
13268:   zSql = azArg[2];
13269: 
13270:   if( strcmp(zTable, "sqlite_sequence")==0 ){
13271:     raw_printf(p->out, "DELETE FROM sqlite_sequence;\n");
13272:   }else if( sqlite3_strglob("sqlite_stat?", zTable)==0 ){
13273:     raw_printf(p->out, "ANALYZE sqlite_schema;\n");
13274:   }else if( strncmp(zTable, "sqlite_", 7)==0 ){
13275:     return 0;
13276:   }else if( strncmp(zSql, "CREATE VIRTUAL TABLE", 20)==0 ){
13277:     char *zIns;
13278:     if( !p->writableSchema ){
13279:       raw_printf(p->out, "PRAGMA writable_schema=ON;\n");
13280:       p->writableSchema = 1;
13281:     }
13282:     zIns = sqlite3_mprintf(
13283:        "INSERT INTO sqlite_schema(type,name,tbl_name,rootpage,sql)"
13284:        "VALUES('table','%q','%q',0,'%q');",
13285:        zTable, zTable, zSql);
13286:     utf8_printf(p->out, "%s\n", zIns);
13287:     sqlite3_free(zIns);
13288:     return 0;
13289:   }else{
13290:     printSchemaLine(p->out, zSql, ";\n");
13291:   }
13292: 
13293:   if( strcmp(zType, "table")==0 ){
13294:     ShellText sSelect;
13295:     ShellText sTable;
13296:     char **azCol;
13297:     int i;
13298:     char *savedDestTable;
13299:     int savedMode;
13300: 
13301:     azCol = tableColumnList(p, zTable);
13302:     if( azCol==0 ){
13303:       p->nErr++;
13304:       return 0;
13305:     }
13306: 
13307:     /* Always quote the table name, even if it appears to be pure ascii,
13308:     ** in case it is a keyword. Ex:  INSERT INTO "table" ... */
13309:     initText(&sTable);
13310:     appendText(&sTable, zTable, quoteChar(zTable));
13311:     /* If preserving the rowid, add a column list after the table name.
13312:     ** In other words:  "INSERT INTO tab(rowid,a,b,c,...) VALUES(...)"
13313:     ** instead of the usual "INSERT INTO tab VALUES(...)".
13314:     */
13315:     if( azCol[0] ){
13316:       appendText(&sTable, "(", 0);
13317:       appendText(&sTable, azCol[0], 0);
13318:       for(i=1; azCol[i]; i++){
13319:         appendText(&sTable, ",", 0);
13320:         appendText(&sTable, azCol[i], quoteChar(azCol[i]));
13321:       }
13322:       appendText(&sTable, ")", 0);
13323:     }
13324: 
13325:     /* Build an appropriate SELECT statement */
13326:     initText(&sSelect);
13327:     appendText(&sSelect, "SELECT ", 0);
13328:     if( azCol[0] ){
13329:       appendText(&sSelect, azCol[0], 0);
13330:       appendText(&sSelect, ",", 0);
13331:     }
13332:     for(i=1; azCol[i]; i++){
13333:       appendText(&sSelect, azCol[i], quoteChar(azCol[i]));
13334:       if( azCol[i+1] ){
13335:         appendText(&sSelect, ",", 0);
13336:       }
13337:     }
13338:     freeColumnList(azCol);
13339:     appendText(&sSelect, " FROM ", 0);
13340:     appendText(&sSelect, zTable, quoteChar(zTable));
13341: 
13342:     savedDestTable = p->zDestTable;
13343:     savedMode = p->mode;
13344:     p->zDestTable = sTable.z;
13345:     p->mode = p->cMode = MODE_Insert;
13346:     rc = shell_exec(p, sSelect.z, 0);
13347:     if( (rc&0xff)==SQLITE_CORRUPT ){
13348:       raw_printf(p->out, "/****** CORRUPTION ERROR *******/\n");
13349:       toggleSelectOrder(p->db);
13350:       shell_exec(p, sSelect.z, 0);
13351:       toggleSelectOrder(p->db);
13352:     }
13353:     p->zDestTable = savedDestTable;
13354:     p->mode = savedMode;
13355:     freeText(&sTable);
13356:     freeText(&sSelect);
13357:     if( rc ) p->nErr++;
13358:   }
13359:   return 0;
13360: }
