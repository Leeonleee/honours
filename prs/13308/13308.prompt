1849: /*
1850: ** Make consecutive calls to the SHA3Update function to add new content
1851: ** to the hash
1852: */
1853: static void SHA3Update(
1854:   SHA3Context *p,
1855:   const unsigned char *aData,
1856:   unsigned int nData
1857: ){
1858:   unsigned int i = 0;
1859: #if SHA3_BYTEORDER==1234
1860:   if( (p->nLoaded % 8)==0 && ((aData - (const unsigned char*)0)&7)==0 ){
1861:     for(; i+7<nData; i+=8){
1862:       p->u.s[p->nLoaded/8] ^= *(u64*)&aData[i];
1863:       p->nLoaded += 8;
1864:       if( p->nLoaded>=p->nRate ){
1865:         KeccakF1600Step(p);
1866:         p->nLoaded = 0;
1867:       }
1868:     }
1869:   }
1870: #endif
1871:   for(; i<nData; i++){
1872: #if SHA3_BYTEORDER==1234
1873:     p->u.x[p->nLoaded] ^= aData[i];
1874: #elif SHA3_BYTEORDER==4321
1875:     p->u.x[p->nLoaded^0x07] ^= aData[i];
1876: #else
1877:     p->u.x[p->nLoaded^p->ixMask] ^= aData[i];
1878: #endif
1879:     p->nLoaded++;
1880:     if( p->nLoaded==p->nRate ){
1881:       KeccakF1600Step(p);
1882:       p->nLoaded = 0;
1883:     }
1884:   }
1885: }
