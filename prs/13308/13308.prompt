12418: /*
12419: ** UTF8 box-drawing characters.  Imagine box lines like this:
12420: **
12421: **           1
12422: **           |
12423: **       4 --+-- 2
12424: **           |
12425: **           3
12426: **
12427: ** Each box characters has between 2 and 4 of the lines leading from
12428: ** the center.  The characters are here identified by the numbers of
12429: ** their corresponding lines.
12430: */
12431: #define BOX_24   "\342\224\200"  /* U+2500 --- */
12432: #define BOX_13   "\342\224\202"  /* U+2502  |  */
12433: #define BOX_23   "\342\224\214"  /* U+250c  ,- */
12434: #define BOX_34   "\342\224\220"  /* U+2510 -,  */
12435: #define BOX_12   "\342\224\224"  /* U+2514  '- */
12436: #define BOX_14   "\342\224\230"  /* U+2518 -'  */
12437: #define BOX_123  "\342\224\234"  /* U+251c  |- */
12438: #define BOX_134  "\342\224\244"  /* U+2524 -|  */
12439: #define BOX_234  "\342\224\254"  /* U+252c -,- */
12440: #define BOX_124  "\342\224\264"  /* U+2534 -'- */
12441: #define BOX_1234 "\342\224\274"  /* U+253c -|- */
12442: 
12443: /* Draw horizontal line N characters long using unicode box
12444: ** characters
12445: */
12446: static void print_box_line(FILE *out, int N){
12447:   const char zDash[] =
12448:       BOX_24 BOX_24 BOX_24 BOX_24 BOX_24 BOX_24 BOX_24 BOX_24 BOX_24 BOX_24
12449:       BOX_24 BOX_24 BOX_24 BOX_24 BOX_24 BOX_24 BOX_24 BOX_24 BOX_24 BOX_24;
12450:   const int nDash = sizeof(zDash) - 1;
12451:   N *= 3;
12452:   while( N>nDash ){
12453:     utf8_printf(out, zDash);
12454:     N -= nDash;
12455:   }
12456:   utf8_printf(out, "%.*s", N, zDash);
12457: }
