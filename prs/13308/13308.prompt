1355: /*
1356: ** Implementation of the POSIX readdir_r() function using the MSVCRT.
1357: */
1358: INT readdir_r(
1359:   LPDIR dirp,
1360:   LPDIRENT entry,
1361:   LPDIRENT *result
1362: ){
1363:   struct _finddata_t data;
1364: 
1365:   if( dirp==NULL ) return EBADF;
1366: 
1367:   if( dirp->d_first.d_ino==0 ){
1368:     dirp->d_first.d_ino++;
1369:     dirp->d_next.d_ino++;
1370: 
1371:     entry->d_ino = dirp->d_first.d_ino;
1372:     entry->d_attributes = dirp->d_first.d_attributes;
1373:     strncpy(entry->d_name, dirp->d_first.d_name, NAME_MAX);
1374:     entry->d_name[NAME_MAX] = '\0';
1375: 
1376:     *result = entry;
1377:     return 0;
1378:   }
1379: 
1380: next:
1381: 
1382:   memset(&data, 0, sizeof(struct _finddata_t));
1383:   if( _findnext(dirp->d_handle, &data)==-1 ){
1384:     *result = NULL;
1385:     return ENOENT;
1386:   }
1387: 
1388:   /* TODO: Remove this block to allow hidden and/or system files. */
1389:   if( is_filtered(data) ) goto next;
1390: 
1391:   entry->d_ino = (ino_t)-1; /* not available */
1392:   entry->d_attributes = data.attrib;
1393:   strncpy(entry->d_name, data.name, NAME_MAX);
1394:   entry->d_name[NAME_MAX] = '\0';
1395: 
1396:   *result = entry;
1397:   return 0;
1398: }
