9945: /*
9946: ** Load page pgno from the database via the sqlite_dbpage virtual table.
9947: ** If successful, set (*ppPage) to point to a buffer containing the page
9948: ** data, (*pnPage) to the size of that buffer in bytes and return
9949: ** SQLITE_OK. In this case it is the responsibility of the caller to
9950: ** eventually free the buffer using sqlite3_free().
9951: **
9952: ** Or, if an error occurs, set both (*ppPage) and (*pnPage) to 0 and
9953: ** return an SQLite error code.
9954: */
9955: static int dbdataLoadPage(
9956:   DbdataCursor *pCsr,             /* Cursor object */
9957:   unsigned int pgno,              /* Page number of page to load */
9958:   u8 **ppPage,                    /* OUT: pointer to page buffer */
9959:   int *pnPage                     /* OUT: Size of (*ppPage) in bytes */
9960: ){
9961:   int rc2;
9962:   int rc = SQLITE_OK;
9963:   sqlite3_stmt *pStmt = pCsr->pStmt;
9964: 
9965:   *ppPage = 0;
9966:   *pnPage = 0;
9967:   sqlite3_bind_int64(pStmt, 2, pgno);
9968:   if( SQLITE_ROW==sqlite3_step(pStmt) ){
9969:     int nCopy = sqlite3_column_bytes(pStmt, 0);
9970:     if( nCopy>0 ){
9971:       u8 *pPage;
9972:       pPage = (u8*)sqlite3_malloc64(nCopy + DBDATA_PADDING_BYTES);
9973:       if( pPage==0 ){
9974:         rc = SQLITE_NOMEM;
9975:       }else{
9976:         const u8 *pCopy = sqlite3_column_blob(pStmt, 0);
9977:         memcpy(pPage, pCopy, nCopy);
9978:         memset(&pPage[nCopy], 0, DBDATA_PADDING_BYTES);
9979:       }
9980:       *ppPage = pPage;
9981:       *pnPage = nCopy;
9982:     }
9983:   }
9984:   rc2 = sqlite3_reset(pStmt);
9985:   if( rc==SQLITE_OK ) rc = rc2;
9986: 
9987:   return rc;
9988: }
