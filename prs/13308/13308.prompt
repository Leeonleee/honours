5009:     if( sqlite3_value_type(argv[0])==SQLITE_BLOB
5010:      && sqlite3_value_bytes(argv[0])==sizeof(r)
5011:     ){
5012:       const unsigned char *x = sqlite3_value_blob(argv[0]);
5013:       unsigned int i;
5014:       sqlite3_uint64 v = 0;
5015:       for(i=0; i<sizeof(r); i++){
5016:         v = (v<<8) | x[i];
5017:       }
5018:       memcpy(&r, &v, sizeof(r));
5019:     }else{
5020:       r = sqlite3_value_double(argv[0]);
5021:     }
5022:     if( r<0.0 ){
5023:       isNeg = 1;
5024:       r = -r;
5025:     }else{
5026:       isNeg = 0;
5027:     }
5028:     memcpy(&a,&r,sizeof(a));
5029:     if( a==0 ){
5030:       e = 0;
5031:       m = 0;
5032:     }else{
5033:       e = a>>52;
5034:       m = a & ((((sqlite3_int64)1)<<52)-1);
5035:       if( e==0 ){
5036:         m <<= 1;
5037:       }else{
5038:         m |= ((sqlite3_int64)1)<<52;
5039:       }
5040:       while( e<1075 && m>0 && (m&1)==0 ){
5041:         m >>= 1;
5042:         e++;
5043:       }
5044:       if( isNeg ) m = -m;
5045:     }
5046:     switch( *(int*)sqlite3_user_data(context) ){
5047:       case 0:
5048:         sqlite3_snprintf(sizeof(zResult), zResult, "ieee754(%lld,%d)",
5049:                          m, e-1075);
5050:         sqlite3_result_text(context, zResult, -1, SQLITE_TRANSIENT);
5051:         break;
5052:       case 1:
5053:         sqlite3_result_int64(context, m);
5054:         break;
5055:       case 2:
5056:         sqlite3_result_int(context, e-1075);
5057:         break;
5058:     }
5059:   }else{
5060:     sqlite3_int64 m, e, a;
5061:     double r;
5062:     int isNeg = 0;
5063:     m = sqlite3_value_int64(argv[0]);
5064:     e = sqlite3_value_int64(argv[1]);
5065:     if( m<0 ){
5066:       isNeg = 1;
5067:       m = -m;
5068:       if( m<0 ) return;
5069:     }else if( m==0 && e>-1000 && e<1000 ){
5070:       sqlite3_result_double(context, 0.0);
5071:       return;
5072:     }
5073:     while( (m>>32)&0xffe00000 ){
5074:       m >>= 1;
5075:       e++;
5076:     }
5077:     while( m!=0 && ((m>>32)&0xfff00000)==0 ){
5078:       m <<= 1;
5079:       e--;
5080:     }
5081:     e += 1075;
5082:     if( e<=0 ){
5083:       /* Subnormal */
5084:       m >>= 1-e;
5085:       e = 0;
5086:     }else if( e>0x7ff ){
5087:       e = 0x7ff;
5088:     }
5089:     a = m & ((((sqlite3_int64)1)<<52)-1);
5090:     a |= e<<52;
5091:     if( isNeg ) a |= ((sqlite3_uint64)1)<<63;
5092:     memcpy(&r, &a, sizeof(r));
5093:     sqlite3_result_double(context, r);
5094:   }
5095: }
