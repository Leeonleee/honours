574: /*
575: ** Output string zUtf to stream pOut as w characters.  If w is negative,
576: ** then right-justify the text.  W is the width in UTF-8 characters, not
577: ** in bytes.  This is different from the %*.*s specification in printf
578: ** since with %*.*s the width is measured in bytes, not characters.
579: */
580: static void utf8_width_print(FILE *pOut, int w, const char *zUtf){
581:   int i;
582:   int n;
583:   int aw = w<0 ? -w : w;
584:   char zBuf[1000];
585:   if( aw>(int)sizeof(zBuf)/3 ) aw = (int)sizeof(zBuf)/3;
586: #ifdef HAVE_LINENOISE
587:   i = linenoiseGetRenderPosition(zUtf, strlen(zUtf), aw, &n);
588:   if (i < 0)
589: #endif
590:   for(i=n=0; zUtf[i]; i++){
591:     if( (zUtf[i]&0xc0)!=0x80 ){
592:       n++;
593:       if( n==aw ){
594:         do{ i++; }while( (zUtf[i]&0xc0)==0x80 );
595:         break;
596:       }
597:     }
598:   }
599:   if( n>=aw ){
600:     utf8_printf(pOut, "%.*s", i, zUtf);
601:   }else if( w<0 ){
602:     utf8_printf(pOut, "%*s%s", aw-n, "", zUtf);
603:   }else{
604:     utf8_printf(pOut, "%s%*s", zUtf, aw-n, "");
605:   }
606: }
