1274: /*
1275: ** Implementation of the POSIX opendir() function using the MSVCRT.
1276: */
1277: LPDIR opendir(
1278:   const char *dirname
1279: ){
1280:   struct _finddata_t data;
1281:   LPDIR dirp = (LPDIR)sqlite3_malloc(sizeof(DIR));
1282:   SIZE_T namesize = sizeof(data.name) / sizeof(data.name[0]);
1283: 
1284:   if( dirp==NULL ) return NULL;
1285:   memset(dirp, 0, sizeof(DIR));
1286: 
1287:   /* TODO: Remove this if Unix-style root paths are not used. */
1288:   if( sqlite3_stricmp(dirname, "/")==0 ){
1289:     dirname = windirent_getenv("SystemDrive");
1290:   }
1291: 
1292:   memset(&data, 0, sizeof(struct _finddata_t));
1293:   _snprintf(data.name, namesize, "%s\\*", dirname);
1294:   dirp->d_handle = _findfirst(data.name, &data);
1295: 
1296:   if( dirp->d_handle==BAD_INTPTR_T ){
1297:     closedir(dirp);
1298:     return NULL;
1299:   }
1300: 
1301:   /* TODO: Remove this block to allow hidden and/or system files. */
1302:   if( is_filtered(data) ){
1303: next:
1304: 
1305:     memset(&data, 0, sizeof(struct _finddata_t));
1306:     if( _findnext(dirp->d_handle, &data)==-1 ){
1307:       closedir(dirp);
1308:       return NULL;
1309:     }
1310: 
1311:     /* TODO: Remove this block to allow hidden and/or system files. */
1312:     if( is_filtered(data) ) goto next;
1313:   }
1314: 
1315:   dirp->d_first.d_attributes = data.attrib;
1316:   strncpy(dirp->d_first.d_name, data.name, NAME_MAX);
1317:   dirp->d_first.d_name[NAME_MAX] = '\0';
1318: 
1319:   return dirp;
1320: }
