5567: /*
5568: ** Release resources that should be freed at the end of a write
5569: ** transaction.
5570: */
5571: static void zipfileCleanupTransaction(ZipfileTab *pTab){
5572:   ZipfileEntry *pEntry;
5573:   ZipfileEntry *pNext;
5574: 
5575:   if( pTab->pWriteFd ){
5576:     fclose(pTab->pWriteFd);
5577:     pTab->pWriteFd = 0;
5578:   }
5579:   for(pEntry=pTab->pFirstEntry; pEntry; pEntry=pNext){
5580:     pNext = pEntry->pNext;
5581:     zipfileEntryFree(pEntry);
5582:   }
5583:   pTab->pFirstEntry = 0;
5584:   pTab->pLastEntry = 0;
5585:   pTab->szCurrent = 0;
5586:   pTab->szOrig = 0;
5587: }
