10907: /*
10908: ** Find a string that is not found anywhere in z[].  Return a pointer
10909: ** to that string.
10910: **
10911: ** Try to use zA and zB first.  If both of those are already found in z[]
10912: ** then make up some string and store it in the buffer zBuf.
10913: */
10914: static const char *unused_string(
10915:   const char *z,                    /* Result must not appear anywhere in z */
10916:   const char *zA, const char *zB,   /* Try these first */
10917:   char *zBuf                        /* Space to store a generated string */
10918: ){
10919:   unsigned i = 0;
10920:   if( strstr(z, zA)==0 ) return zA;
10921:   if( strstr(z, zB)==0 ) return zB;
10922:   do{
10923:     sqlite3_snprintf(20,zBuf,"(%s%u)", zA, i++);
10924:   }while( strstr(z,zBuf)!=0 );
10925:   return zBuf;
10926: }
