15048: /*
15049: ** Parse the command line for an ".ar" command. The results are written into
15050: ** structure (*pAr). SQLITE_OK is returned if the command line is parsed
15051: ** successfully, otherwise an error message is written to stderr and
15052: ** SQLITE_ERROR returned.
15053: */
15054: static int arParseCommand(
15055:   char **azArg,                   /* Array of arguments passed to dot command */
15056:   int nArg,                       /* Number of entries in azArg[] */
15057:   ArCommand *pAr                  /* Populate this object */
15058: ){
15059:   struct ArSwitch {
15060:     const char *zLong;
15061:     char cShort;
15062:     u8 eSwitch;
15063:     u8 bArg;
15064:   } aSwitch[] = {
15065:     { "create",    'c', AR_CMD_CREATE,       0 },
15066:     { "extract",   'x', AR_CMD_EXTRACT,      0 },
15067:     { "insert",    'i', AR_CMD_INSERT,       0 },
15068:     { "list",      't', AR_CMD_LIST,         0 },
15069:     { "update",    'u', AR_CMD_UPDATE,       0 },
15070:     { "help",      'h', AR_CMD_HELP,         0 },
15071:     { "verbose",   'v', AR_SWITCH_VERBOSE,   0 },
15072:     { "file",      'f', AR_SWITCH_FILE,      1 },
15073:     { "append",    'a', AR_SWITCH_APPEND,    1 },
15074:     { "directory", 'C', AR_SWITCH_DIRECTORY, 1 },
15075:     { "dryrun",    'n', AR_SWITCH_DRYRUN,    0 },
15076:   };
15077:   int nSwitch = sizeof(aSwitch) / sizeof(struct ArSwitch);
15078:   struct ArSwitch *pEnd = &aSwitch[nSwitch];
15079: 
15080:   if( nArg<=1 ){
15081:     utf8_printf(stderr, "Wrong number of arguments.  Usage:\n");
15082:     return arUsage(stderr);
15083:   }else{
15084:     char *z = azArg[1];
15085:     if( z[0]!='-' ){
15086:       /* Traditional style [tar] invocation */
15087:       int i;
15088:       int iArg = 2;
15089:       for(i=0; z[i]; i++){
15090:         const char *zArg = 0;
15091:         struct ArSwitch *pOpt;
15092:         for(pOpt=&aSwitch[0]; pOpt<pEnd; pOpt++){
15093:           if( z[i]==pOpt->cShort ) break;
15094:         }
15095:         if( pOpt==pEnd ){
15096:           return arErrorMsg(pAr, "unrecognized option: %c", z[i]);
15097:         }
15098:         if( pOpt->bArg ){
15099:           if( iArg>=nArg ){
15100:             return arErrorMsg(pAr, "option requires an argument: %c",z[i]);
15101:           }
15102:           zArg = azArg[iArg++];
15103:         }
15104:         if( arProcessSwitch(pAr, pOpt->eSwitch, zArg) ) return SQLITE_ERROR;
15105:       }
15106:       pAr->nArg = nArg-iArg;
15107:       if( pAr->nArg>0 ){
15108:         pAr->azArg = &azArg[iArg];
15109:       }
15110:     }else{
15111:       /* Non-traditional invocation */
15112:       int iArg;
15113:       for(iArg=1; iArg<nArg; iArg++){
15114:         int n;
15115:         z = azArg[iArg];
15116:         if( z[0]!='-' ){
15117:           /* All remaining command line words are command arguments. */
15118:           pAr->azArg = &azArg[iArg];
15119:           pAr->nArg = nArg-iArg;
15120:           break;
15121:         }
15122:         n = strlen30(z);
15123: 
15124:         if( z[1]!='-' ){
15125:           int i;
15126:           /* One or more short options */
15127:           for(i=1; i<n; i++){
15128:             const char *zArg = 0;
15129:             struct ArSwitch *pOpt;
15130:             for(pOpt=&aSwitch[0]; pOpt<pEnd; pOpt++){
15131:               if( z[i]==pOpt->cShort ) break;
15132:             }
15133:             if( pOpt==pEnd ){
15134:               return arErrorMsg(pAr, "unrecognized option: %c", z[i]);
15135:             }
15136:             if( pOpt->bArg ){
15137:               if( i<(n-1) ){
15138:                 zArg = &z[i+1];
15139:                 i = n;
15140:               }else{
15141:                 if( iArg>=(nArg-1) ){
15142:                   return arErrorMsg(pAr, "option requires an argument: %c",
15143:                                     z[i]);
15144:                 }
15145:                 zArg = azArg[++iArg];
15146:               }
15147:             }
15148:             if( arProcessSwitch(pAr, pOpt->eSwitch, zArg) ) return SQLITE_ERROR;
15149:           }
15150:         }else if( z[2]=='\0' ){
15151:           /* A -- option, indicating that all remaining command line words
15152:           ** are command arguments.  */
15153:           pAr->azArg = &azArg[iArg+1];
15154:           pAr->nArg = nArg-iArg-1;
15155:           break;
15156:         }else{
15157:           /* A long option */
15158:           const char *zArg = 0;             /* Argument for option, if any */
15159:           struct ArSwitch *pMatch = 0;      /* Matching option */
15160:           struct ArSwitch *pOpt;            /* Iterator */
15161:           for(pOpt=&aSwitch[0]; pOpt<pEnd; pOpt++){
15162:             const char *zLong = pOpt->zLong;
15163:             if( (n-2)<=strlen30(zLong) && 0==memcmp(&z[2], zLong, n-2) ){
15164:               if( pMatch ){
15165:                 return arErrorMsg(pAr, "ambiguous option: %s",z);
15166:               }else{
15167:                 pMatch = pOpt;
15168:               }
15169:             }
15170:           }
15171: 
15172:           if( pMatch==0 ){
15173:             return arErrorMsg(pAr, "unrecognized option: %s", z);
15174:           }
15175:           if( pMatch->bArg ){
15176:             if( iArg>=(nArg-1) ){
15177:               return arErrorMsg(pAr, "option requires an argument: %s", z);
15178:             }
15179:             zArg = azArg[++iArg];
15180:           }
15181:           if( arProcessSwitch(pAr, pMatch->eSwitch, zArg) ) return SQLITE_ERROR;
15182:         }
15183:       }
15184:     }
15185:   }
15186: 
15187:   return SQLITE_OK;
15188: }
