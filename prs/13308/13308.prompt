6065: /*
6066: ** Advance an ZipfileCsr to its next row of output.
6067: */
6068: static int zipfileNext(sqlite3_vtab_cursor *cur){
6069:   ZipfileCsr *pCsr = (ZipfileCsr*)cur;
6070:   int rc = SQLITE_OK;
6071: 
6072:   if( pCsr->pFile ){
6073:     i64 iEof = pCsr->eocd.iOffset + pCsr->eocd.nSize;
6074:     zipfileEntryFree(pCsr->pCurrent);
6075:     pCsr->pCurrent = 0;
6076:     if( pCsr->iNextOff>=iEof ){
6077:       pCsr->bEof = 1;
6078:     }else{
6079:       ZipfileEntry *p = 0;
6080:       ZipfileTab *pTab = (ZipfileTab*)(cur->pVtab);
6081:       rc = zipfileGetEntry(pTab, 0, 0, pCsr->pFile, pCsr->iNextOff, &p);
6082:       if( rc==SQLITE_OK ){
6083:         pCsr->iNextOff += ZIPFILE_CDS_FIXED_SZ;
6084:         pCsr->iNextOff += (int)p->cds.nExtra + p->cds.nFile + p->cds.nComment;
6085:       }
6086:       pCsr->pCurrent = p;
6087:     }
6088:   }else{
6089:     if( !pCsr->bNoop ){
6090:       pCsr->pCurrent = pCsr->pCurrent->pNext;
6091:     }
6092:     if( pCsr->pCurrent==0 ){
6093:       pCsr->bEof = 1;
6094:     }
6095:   }
6096: 
6097:   pCsr->bNoop = 0;
6098:   return rc;
6099: }
