7282: /*
7283: ** xFinalize() callback for zipfile aggregate function.
7284: */
7285: void zipfileFinal(sqlite3_context *pCtx){
7286:   ZipfileCtx *p;
7287:   ZipfileEOCD eocd;
7288:   sqlite3_int64 nZip;
7289:   u8 *aZip;
7290: 
7291:   p = (ZipfileCtx*)sqlite3_aggregate_context(pCtx, sizeof(ZipfileCtx));
7292:   if( p==0 ) return;
7293:   if( p->nEntry>0 ){
7294:     memset(&eocd, 0, sizeof(eocd));
7295:     eocd.nEntry = (u16)p->nEntry;
7296:     eocd.nEntryTotal = (u16)p->nEntry;
7297:     eocd.nSize = p->cds.n;
7298:     eocd.iOffset = p->body.n;
7299: 
7300:     nZip = p->body.n + p->cds.n + ZIPFILE_EOCD_FIXED_SZ;
7301:     aZip = (u8*)sqlite3_malloc64(nZip);
7302:     if( aZip==0 ){
7303:       sqlite3_result_error_nomem(pCtx);
7304:     }else{
7305:       memcpy(aZip, p->body.a, p->body.n);
7306:       memcpy(&aZip[p->body.n], p->cds.a, p->cds.n);
7307:       zipfileSerializeEOCD(&eocd, &aZip[p->body.n + p->cds.n]);
7308:       sqlite3_result_blob(pCtx, aZip, (int)nZip, zipfileFree);
7309:     }
7310:   }
7311: 
7312:   sqlite3_free(p->body.a);
7313:   sqlite3_free(p->cds.a);
7314: }
