8056: /*
8057: ** This function is the implementation of both the xConnect and xCreate
8058: ** methods of the r-tree virtual table.
8059: **
8060: **   argv[0]   -> module name
8061: **   argv[1]   -> database name
8062: **   argv[2]   -> table name
8063: **   argv[...] -> column names...
8064: */
8065: static int expertConnect(
8066:   sqlite3 *db,
8067:   void *pAux,
8068:   int argc, const char *const*argv,
8069:   sqlite3_vtab **ppVtab,
8070:   char **pzErr
8071: ){
8072:   sqlite3expert *pExpert = (sqlite3expert*)pAux;
8073:   ExpertVtab *p = 0;
8074:   int rc;
8075: 
8076:   if( argc!=4 ){
8077:     *pzErr = sqlite3_mprintf("internal error!");
8078:     rc = SQLITE_ERROR;
8079:   }else{
8080:     char *zCreateTable = expertDequote(argv[3]);
8081:     if( zCreateTable ){
8082:       rc = sqlite3_declare_vtab(db, zCreateTable);
8083:       if( rc==SQLITE_OK ){
8084:         p = idxMalloc(&rc, sizeof(ExpertVtab));
8085:       }
8086:       if( rc==SQLITE_OK ){
8087:         p->pExpert = pExpert;
8088:         p->pTab = pExpert->pTable;
8089:         assert( sqlite3_stricmp(p->pTab->zName, argv[2])==0 );
8090:       }
8091:       sqlite3_free(zCreateTable);
8092:     }else{
8093:       rc = SQLITE_NOMEM;
8094:     }
8095:   }
8096: 
8097:   *ppVtab = (sqlite3_vtab*)p;
8098:   return rc;
8099: }
