7102: /*
7103: ** xStep() callback for the zipfile() aggregate. This can be called in
7104: ** any of the following ways:
7105: **
7106: **   SELECT zipfile(name,data) ...
7107: **   SELECT zipfile(name,mode,mtime,data) ...
7108: **   SELECT zipfile(name,mode,mtime,data,method) ...
7109: */
7110: void zipfileStep(sqlite3_context *pCtx, int nVal, sqlite3_value **apVal){
7111:   ZipfileCtx *p;                  /* Aggregate function context */
7112:   ZipfileEntry e;                 /* New entry to add to zip archive */
7113: 
7114:   sqlite3_value *pName = 0;
7115:   sqlite3_value *pMode = 0;
7116:   sqlite3_value *pMtime = 0;
7117:   sqlite3_value *pData = 0;
7118:   sqlite3_value *pMethod = 0;
7119: 
7120:   int bIsDir = 0;
7121:   u32 mode;
7122:   int rc = SQLITE_OK;
7123:   char *zErr = 0;
7124: 
7125:   int iMethod = -1;               /* Compression method to use (0 or 8) */
7126: 
7127:   const u8 *aData = 0;            /* Possibly compressed data for new entry */
7128:   int nData = 0;                  /* Size of aData[] in bytes */
7129:   int szUncompressed = 0;         /* Size of data before compression */
7130:   u8 *aFree = 0;                  /* Free this before returning */
7131:   u32 iCrc32 = 0;                 /* crc32 of uncompressed data */
7132: 
7133:   char *zName = 0;                /* Path (name) of new entry */
7134:   int nName = 0;                  /* Size of zName in bytes */
7135:   char *zFree = 0;                /* Free this before returning */
7136:   int nByte;
7137: 
7138:   memset(&e, 0, sizeof(e));
7139:   p = (ZipfileCtx*)sqlite3_aggregate_context(pCtx, sizeof(ZipfileCtx));
7140:   if( p==0 ) return;
7141: 
7142:   /* Martial the arguments into stack variables */
7143:   if( nVal!=2 && nVal!=4 && nVal!=5 ){
7144:     zErr = sqlite3_mprintf("wrong number of arguments to function zipfile()");
7145:     rc = SQLITE_ERROR;
7146:     goto zipfile_step_out;
7147:   }
7148:   pName = apVal[0];
7149:   if( nVal==2 ){
7150:     pData = apVal[1];
7151:   }else{
7152:     pMode = apVal[1];
7153:     pMtime = apVal[2];
7154:     pData = apVal[3];
7155:     if( nVal==5 ){
7156:       pMethod = apVal[4];
7157:     }
7158:   }
7159: 
7160:   /* Check that the 'name' parameter looks ok. */
7161:   zName = (char*)sqlite3_value_text(pName);
7162:   nName = sqlite3_value_bytes(pName);
7163:   if( zName==0 ){
7164:     zErr = sqlite3_mprintf("first argument to zipfile() must be non-NULL");
7165:     rc = SQLITE_ERROR;
7166:     goto zipfile_step_out;
7167:   }
7168: 
7169:   /* Inspect the 'method' parameter. This must be either 0 (store), 8 (use
7170:   ** deflate compression) or NULL (choose automatically).  */
7171:   if( pMethod && SQLITE_NULL!=sqlite3_value_type(pMethod) ){
7172:     iMethod = (int)sqlite3_value_int64(pMethod);
7173:     if( iMethod!=0 && iMethod!=8 ){
7174:       zErr = sqlite3_mprintf("illegal method value: %d", iMethod);
7175:       rc = SQLITE_ERROR;
7176:       goto zipfile_step_out;
7177:     }
7178:   }
7179: 
7180:   /* Now inspect the data. If this is NULL, then the new entry must be a
7181:   ** directory.  Otherwise, figure out whether or not the data should
7182:   ** be deflated or simply stored in the zip archive. */
7183:   if( sqlite3_value_type(pData)==SQLITE_NULL ){
7184:     bIsDir = 1;
7185:     iMethod = 0;
7186:   }else{
7187:     aData = sqlite3_value_blob(pData);
7188:     szUncompressed = nData = sqlite3_value_bytes(pData);
7189:     iCrc32 = crc32(0, aData, nData);
7190:     if( iMethod<0 || iMethod==8 ){
7191:       int nOut = 0;
7192:       rc = zipfileDeflate(aData, nData, &aFree, &nOut, &zErr);
7193:       if( rc!=SQLITE_OK ){
7194:         goto zipfile_step_out;
7195:       }
7196:       if( iMethod==8 || nOut<nData ){
7197:         aData = aFree;
7198:         nData = nOut;
7199:         iMethod = 8;
7200:       }else{
7201:         iMethod = 0;
7202:       }
7203:     }
7204:   }
7205: 
7206:   /* Decode the "mode" argument. */
7207:   rc = zipfileGetMode(pMode, bIsDir, &mode, &zErr);
7208:   if( rc ) goto zipfile_step_out;
7209: 
7210:   /* Decode the "mtime" argument. */
7211:   e.mUnixTime = zipfileGetTime(pMtime);
7212: 
7213:   /* If this is a directory entry, ensure that there is exactly one '/'
7214:   ** at the end of the path. Or, if this is not a directory and the path
7215:   ** ends in '/' it is an error. */
7216:   if( bIsDir==0 ){
7217:     if( nName>0 && zName[nName-1]=='/' ){
7218:       zErr = sqlite3_mprintf("non-directory name must not end with /");
7219:       rc = SQLITE_ERROR;
7220:       goto zipfile_step_out;
7221:     }
7222:   }else{
7223:     if( nName==0 || zName[nName-1]!='/' ){
7224:       zName = zFree = sqlite3_mprintf("%s/", zName);
7225:       if( zName==0 ){
7226:         rc = SQLITE_NOMEM;
7227:         goto zipfile_step_out;
7228:       }
7229:       nName = (int)strlen(zName);
7230:     }else{
7231:       while( nName>1 && zName[nName-2]=='/' ) nName--;
7232:     }
7233:   }
7234: 
7235:   /* Assemble the ZipfileEntry object for the new zip archive entry */
7236:   e.cds.iVersionMadeBy = ZIPFILE_NEWENTRY_MADEBY;
7237:   e.cds.iVersionExtract = ZIPFILE_NEWENTRY_REQUIRED;
7238:   e.cds.flags = ZIPFILE_NEWENTRY_FLAGS;
7239:   e.cds.iCompression = (u16)iMethod;
7240:   zipfileMtimeToDos(&e.cds, (u32)e.mUnixTime);
7241:   e.cds.crc32 = iCrc32;
7242:   e.cds.szCompressed = nData;
7243:   e.cds.szUncompressed = szUncompressed;
7244:   e.cds.iExternalAttr = (mode<<16);
7245:   e.cds.iOffset = p->body.n;
7246:   e.cds.nFile = (u16)nName;
7247:   e.cds.zFile = zName;
7248: 
7249:   /* Append the LFH to the body of the new archive */
7250:   nByte = ZIPFILE_LFH_FIXED_SZ + e.cds.nFile + 9;
7251:   if( (rc = zipfileBufferGrow(&p->body, nByte)) ) goto zipfile_step_out;
7252:   p->body.n += zipfileSerializeLFH(&e, &p->body.a[p->body.n]);
7253: 
7254:   /* Append the data to the body of the new archive */
7255:   if( nData>0 ){
7256:     if( (rc = zipfileBufferGrow(&p->body, nData)) ) goto zipfile_step_out;
7257:     memcpy(&p->body.a[p->body.n], aData, nData);
7258:     p->body.n += nData;
7259:   }
7260: 
7261:   /* Append the CDS record to the directory of the new archive */
7262:   nByte = ZIPFILE_CDS_FIXED_SZ + e.cds.nFile + 9;
7263:   if( (rc = zipfileBufferGrow(&p->cds, nByte)) ) goto zipfile_step_out;
7264:   p->cds.n += zipfileSerializeCDS(&e, &p->cds.a[p->cds.n]);
7265: 
7266:   /* Increment the count of entries in the archive */
7267:   p->nEntry++;
7268: 
7269:  zipfile_step_out:
7270:   sqlite3_free(aFree);
7271:   sqlite3_free(zFree);
7272:   if( rc ){
7273:     if( zErr ){
7274:       sqlite3_result_error(pCtx, zErr, -1);
7275:     }else{
7276:       sqlite3_result_error_code(pCtx, rc);
7277:     }
7278:   }
7279:   sqlite3_free(zErr);
7280: }
