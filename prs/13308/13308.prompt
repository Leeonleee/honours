2835: /*
2836: ** SQLite will invoke this method one or more times while planning a query
2837: ** that uses the generate_series virtual table.  This routine needs to create
2838: ** a query plan for each invocation and compute an estimated cost for that
2839: ** plan.
2840: **
2841: ** In this implementation idxNum is used to represent the
2842: ** query plan.  idxStr is unused.
2843: **
2844: ** The query plan is represented by values of idxNum:
2845: **
2846: **  (1)  The path value is supplied by argv[0]
2847: **  (2)  Path is in argv[0] and dir is in argv[1]
2848: */
2849: static int fsdirBestIndex(
2850:   sqlite3_vtab *tab,
2851:   sqlite3_index_info *pIdxInfo
2852: ){
2853:   int i;                 /* Loop over constraints */
2854:   int idxPath = -1;      /* Index in pIdxInfo->aConstraint of PATH= */
2855:   int idxDir = -1;       /* Index in pIdxInfo->aConstraint of DIR= */
2856:   int seenPath = 0;      /* True if an unusable PATH= constraint is seen */
2857:   int seenDir = 0;       /* True if an unusable DIR= constraint is seen */
2858:   const struct sqlite3_index_constraint *pConstraint;
2859: 
2860:   (void)tab;
2861:   pConstraint = pIdxInfo->aConstraint;
2862:   for(i=0; i<pIdxInfo->nConstraint; i++, pConstraint++){
2863:     if( pConstraint->op!=SQLITE_INDEX_CONSTRAINT_EQ ) continue;
2864:     switch( pConstraint->iColumn ){
2865:       case FSDIR_COLUMN_PATH: {
2866:         if( pConstraint->usable ){
2867:           idxPath = i;
2868:           seenPath = 0;
2869:         }else if( idxPath<0 ){
2870:           seenPath = 1;
2871:         }
2872:         break;
2873:       }
2874:       case FSDIR_COLUMN_DIR: {
2875:         if( pConstraint->usable ){
2876:           idxDir = i;
2877:           seenDir = 0;
2878:         }else if( idxDir<0 ){
2879:           seenDir = 1;
2880:         }
2881:         break;
2882:       }
2883:     }
2884:   }
2885:   if( seenPath || seenDir ){
2886:     /* If input parameters are unusable, disallow this plan */
2887:     return SQLITE_CONSTRAINT;
2888:   }
2889: 
2890:   if( idxPath<0 ){
2891:     pIdxInfo->idxNum = 0;
2892:     /* The pIdxInfo->estimatedCost should have been initialized to a huge
2893:     ** number.  Leave it unchanged. */
2894:     pIdxInfo->estimatedRows = 0x7fffffff;
2895:   }else{
2896:     pIdxInfo->aConstraintUsage[idxPath].omit = 1;
2897:     pIdxInfo->aConstraintUsage[idxPath].argvIndex = 1;
2898:     if( idxDir>=0 ){
2899:       pIdxInfo->aConstraintUsage[idxDir].omit = 1;
2900:       pIdxInfo->aConstraintUsage[idxDir].argvIndex = 2;
2901:       pIdxInfo->idxNum = 2;
2902:       pIdxInfo->estimatedCost = 10.0;
2903:     }else{
2904:       pIdxInfo->idxNum = 1;
2905:       pIdxInfo->estimatedCost = 100.0;
2906:     }
2907:   }
2908: 
2909:   return SQLITE_OK;
2910: }
