19182: /*
19183: ** Return TRUE if a semicolon occurs anywhere in the first N characters
19184: ** of string z[].
19185: */
19186: static int line_contains_semicolon(const char *z, int N){
19187:   int i;
19188:   for(i=0; i<N; i++){  if( z[i]==';' ) return 1; }
19189:   return 0;
