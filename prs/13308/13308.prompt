10368: /*
10369: ** Return a column for the sqlite_dbdata or sqlite_dbptr table.
10370: */
10371: static int dbdataColumn(
10372:   sqlite3_vtab_cursor *pCursor,
10373:   sqlite3_context *ctx,
10374:   int i
10375: ){
10376:   DbdataCursor *pCsr = (DbdataCursor*)pCursor;
10377:   DbdataTable *pTab = (DbdataTable*)pCursor->pVtab;
10378:   if( pTab->bPtr ){
10379:     switch( i ){
10380:       case DBPTR_COLUMN_PGNO:
10381:         sqlite3_result_int64(ctx, pCsr->iPgno);
10382:         break;
10383:       case DBPTR_COLUMN_CHILD: {
10384:         int iOff = pCsr->iPgno==1 ? 100 : 0;
10385:         if( pCsr->iCell<0 ){
10386:           iOff += 8;
10387:         }else{
10388:           iOff += 12 + pCsr->iCell*2;
10389:           if( iOff>pCsr->nPage ) return SQLITE_OK;
10390:           iOff = get_uint16(&pCsr->aPage[iOff]);
10391:         }
10392:         if( iOff<=pCsr->nPage ){
10393:           sqlite3_result_int64(ctx, get_uint32(&pCsr->aPage[iOff]));
10394:         }
10395:         break;
10396:       }
10397:     }
10398:   }else{
10399:     switch( i ){
10400:       case DBDATA_COLUMN_PGNO:
10401:         sqlite3_result_int64(ctx, pCsr->iPgno);
10402:         break;
10403:       case DBDATA_COLUMN_CELL:
10404:         sqlite3_result_int(ctx, pCsr->iCell);
10405:         break;
10406:       case DBDATA_COLUMN_FIELD:
10407:         sqlite3_result_int(ctx, pCsr->iField);
10408:         break;
10409:       case DBDATA_COLUMN_VALUE: {
10410:         if( pCsr->iField<0 ){
10411:           sqlite3_result_int64(ctx, pCsr->iIntkey);
10412:         }else{
10413:           sqlite3_int64 iType;
10414:           dbdataGetVarint(pCsr->pHdrPtr, &iType);
10415:           dbdataValue(
10416:               ctx, iType, pCsr->pPtr, &pCsr->pRec[pCsr->nRec] - pCsr->pPtr
10417:           );
10418:         }
10419:         break;
10420:       }
10421:     }
10422:   }
10423:   return SQLITE_OK;
10424: }
