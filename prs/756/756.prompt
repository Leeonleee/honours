84: 	auto &result_mask = FlatVector::Nullmask(result);
85: 	for (idx_t i = 0; i < count; i++) {
86: 		auto val = source_data[i];
87: 		if (RTYPE::IsNull(val)) {
88: 			result_mask[i] = true;
89: 		} else {
90: 			result_data[i] = RTYPE::Convert(val);
91: 		}
92: 	}
93: }
