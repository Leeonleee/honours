242: void ShiftRight(unsigned char *ar, int size, int shift) {
243: 	int carry = 0;
244: 	while (shift--) {
245: 		for (int i = size - 1; i >= 0; --i) {
246: 			int next = (ar[i] & 1) ? 0x80 : 0;
247: 			ar[i] = carry | (ar[i] >> 1);
248: 			carry = next;
249: 		}
250: 	}
251: }
