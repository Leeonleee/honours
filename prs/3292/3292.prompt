277: void BaseAppender::FlushChunk() {
278: 	if (chunk->size() == 0) {
279: 		return;
280: 	}
281: 	collection.Append(move(chunk));
282: 	InitializeChunk();
283: 	if (collection.ChunkCount() >= FLUSH_COUNT) {
284: 		Flush();
285: 	}
286: }
