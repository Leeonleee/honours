310: 	str[0] = toupper(str[0]);
311: 	for (idx_t i = 0; i < str.size(); i++) {
312: 		if (str[i] == '_') {
313: 			str[i] = ' ';
314: 			if (i + 1 < str.size()) {
315: 				str[i + 1] = toupper(str[i + 1]);
316: 			}
317: 		}
318: 	}
319: 	return str;
