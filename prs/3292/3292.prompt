42: static void VarintEncode(uint32_t val, Serializer &ser) {
43: 	do {
44: 		uint8_t byte = val & 127;
45: 		val >>= 7;
46: 		if (val != 0) {
47: 			byte |= 128;
48: 		}
49: 		ser.Write<uint8_t>(byte);
50: 	} while (val != 0);
51: }
52: 
53: static uint8_t GetVarintSize(uint32_t val) {
54: 	uint8_t res = 0;
55: 	do {
56: 		uint8_t byte = val & 127;
57: 		val >>= 7;
58: 		if (val != 0) {
59: 			byte |= 128;
60: 		}
61: 		res++;
62: 	} while (val != 0);
63: 	return res;
64: }
