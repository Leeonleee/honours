1136: //===--------------------------------------------------------------------===//
1137: class EnumWriterPageState : public ColumnWriterPageState {
1138: public:
1139: 	explicit EnumWriterPageState(uint32_t bit_width) : encoder(bit_width), written_value(false) {
1140: 	}
1141: 
1142: 	RleBpEncoder encoder;
