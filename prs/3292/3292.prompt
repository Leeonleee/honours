744: 		if (right.upper == 0 && right.lower == 0) {
745: 			mask.SetInvalid(idx);
746: 			return left;
747: 		} else {
748: 			return OP::template Operation<LEFT_TYPE, RIGHT_TYPE, RESULT_TYPE>(left, right);
