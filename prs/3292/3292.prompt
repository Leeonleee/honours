106: 		if (state->count <= 1) {
107: 			mask.SetInvalid(idx);
108: 		} else {
109: 			target[idx] = sqrt(state->dsquared / (state->count - 1));
110: 			if (!Value::DoubleIsValid(target[idx])) {
