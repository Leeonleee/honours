83: hugeint_t PositiveHugeintLeftShift(hugeint_t lhs, uint32_t amount) {
84: 	D_ASSERT(amount > 0 && amount < 64);
85: 	hugeint_t result;
86: 	result.lower = lhs.lower << amount;
87: 	result.upper = (lhs.upper << amount) + (lhs.lower >> (64 - amount));
88: 	return result;
89: }
