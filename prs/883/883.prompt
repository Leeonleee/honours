7873:     void FatalConditionHandler::handleSignal( int sig ) {
7874:         char const * name = "<unknown signal>";
7875:         for (auto const& def : signalDefs) {
7876:             if (sig == def.id) {
7877:                 name = def.name;
7878:                 break;
7879:             }
7880:         }
7881:         reset();
7882:         reportFatal(name);
7883:         raise( sig );
7884:     }
