437: idx_t QueryProfiler::GetDepth(QueryProfiler::TreeNode &node) {
438: 	idx_t depth = 0;
439: 	for (auto &child : node.children) {
440: 		depth = max(depth, GetDepth(*child));
441: 	}
442: 	return depth + 1;
443: }
