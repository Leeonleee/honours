11500: 
11501:     ReusableStringStream rss;
11502:     rss << std::setprecision( precision )
11503:         << std::fixed
11504:         << value;
11505:     std::string d = rss.str();
11506:     std::size_t i = d.find_last_not_of( '0' );
11507:     if( i != std::string::npos && i != d.size()-1 ) {
11508:         if( d[i] == '.' )
11509:             i++;
11510:         d = d.substr( 0, i+1 );
11511:     }
11512:     return d;
11513: }
