707: 				AppendColumnSegment<int, bool, RBooleanType>(data_ptr, v, this_count);
708: 				break;
709: 			}
710: 			case RType::INTEGER: {
711: 				auto data_ptr = INTEGER_POINTER(coldata) + data.position;
