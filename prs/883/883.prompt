276: 	auto ms = impala_ns / 1000000; // nanoseconds
277: 	auto ms_per_day = (int64_t)60 * 60 * 24 * 1000;
278: 	date_t date = Date::EpochToDate(ms / 1000);
279: 	dtime_t time = (dtime_t)(ms % ms_per_day);
