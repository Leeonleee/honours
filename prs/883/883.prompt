6610:     inline auto normaliseOpt( std::string const &optName ) -> std::string {
6611: #ifdef CATCH_PLATFORM_WINDOWS
6612:         if( optName[0] == '/' )
6613:             return "-" + optName.substr( 1 );
6614:         else
6615: #endif
6616:             return optName;
6617:     }
