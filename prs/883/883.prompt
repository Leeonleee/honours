209: string Pipeline::ToString() const {
210: 	string str = PhysicalOperatorToString(sink->type);
211: 	auto node = this->child;
212: 	while (node) {
213: 		str = PhysicalOperatorToString(node->type) + " -> " + str;
214: 		node = node->children[0].get();
215: 	}
216: 	return str;
217: }
