1509:         template<typename T>
1510:         auto operator <= ( T const& lhs ) -> ExprLhs<T const&> {
1511:             return ExprLhs<T const&>{ lhs };
1512:         }
1513: 
1514:         auto operator <=( bool value ) -> ExprLhs<bool> {
1515:             return ExprLhs<bool>{ value };
1516:         }
1517:     };
