629: struct DataFrameScanFunctionData : public TableFunctionData {
630: 	DataFrameScanFunctionData(SEXP df, idx_t row_count, vector<RType> rtypes)
631: 	    : df(df), row_count(row_count), rtypes(rtypes), position(0) {
632: 	}
