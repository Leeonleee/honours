1059: static void VarintEncode(uint32_t val, Serializer &ser) {
1060: 	do {
1061: 		uint8_t byte = val & 127;
1062: 		val >>= 7;
1063: 		if (val != 0) {
1064: 			byte |= 128;
1065: 		}
1066: 		ser.Write<uint8_t>(byte);
1067: 	} while (val != 0);
1068: }
1069: 
1070: static uint8_t GetVarintSize(uint32_t val) {
1071: 	uint8_t res = 0;
1072: 	do {
1073: 		uint8_t byte = val & 127;
1074: 		val >>= 7;
1075: 		if (val != 0) {
1076: 			byte |= 128;
1077: 		}
1078: 		res++;
1079: 	} while (val != 0);
1080: 	return res;
1081: }
