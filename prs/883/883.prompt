847: class ParquetScanFunction : public TableFunction {
848: public:
849: 	ParquetScanFunction()
850: 	    : TableFunction("parquet_scan", {LogicalType::VARCHAR}, parquet_scan_bind, parquet_scan_function, nullptr) {
851: 		supports_projection = true;
