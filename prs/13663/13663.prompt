62: string CreateIndexInfo::ToString() const {
63: 	string result;
64: 
65: 	result += "CREATE";
66: 	D_ASSERT(constraint_type == IndexConstraintType::UNIQUE || constraint_type == IndexConstraintType::NONE);
67: 	if (constraint_type == IndexConstraintType::UNIQUE) {
68: 		result += " UNIQUE";
69: 	}
70: 	result += " INDEX ";
71: 	if (on_conflict == OnCreateConflict::IGNORE_ON_CONFLICT) {
72: 		result += "IF NOT EXISTS ";
