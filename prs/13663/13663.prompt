30: vector<string> CreateIndexInfo::ExpressionsToList() const {
31: 	vector<string> list;
32: 
33: 	for (idx_t i = 0; i < parsed_expressions.size(); i++) {
34: 		auto &expr = parsed_expressions[i];
35: 		auto copy = expr->Copy();
36: 		// column ref expressions are qualified with the table name
37: 		// we need to remove them to reproduce the original query
38: 		RemoveTableQualificationRecursive(copy, table);
39: 		bool add_parenthesis = true;
40: 		if (copy->type == ExpressionType::COLUMN_REF) {
41: 			auto &column_ref = copy->Cast<ColumnRefExpression>();
42: 			if (!column_ref.IsQualified()) {
43: 				// Only when column references are not qualified, i.e (col1, col2)
44: 				// then these expressions do not need to be wrapped in parenthesis
45: 				add_parenthesis = false;
46: 			}
47: 		}
48: 		if (add_parenthesis) {
49: 			list.push_back(StringUtil::Format("(%s)", copy->ToString()));
50: 		} else {
51: 			list.push_back(StringUtil::Format("%s", copy->ToString()));
52: 		}
53: 	}
54: 	return list;
55: }
