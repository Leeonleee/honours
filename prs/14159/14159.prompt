329: template <class OP, class OP_STRING, class OP_VECTOR>
330: unique_ptr<FunctionData> BindMinMax(ClientContext &context, AggregateFunction &function,
331:                                     vector<unique_ptr<Expression>> &arguments) {
332: 	if (arguments[0]->return_type.id() == LogicalTypeId::VARCHAR) {
333: 		auto str_collation = StringType::GetCollation(arguments[0]->return_type);
334: 		if (!str_collation.empty() || !DBConfig::GetConfig(context).options.collation.empty()) {
335: 			// If aggr function is min/max and uses collations, replace bound_function with arg_min/arg_max
336: 			// to make sure the result's correctness.
337: 			string function_name = function.name == "min" ? "arg_min" : "arg_max";
338: 			QueryErrorContext error_context;
339: 			auto func = Catalog::GetEntry(context, CatalogType::SCALAR_FUNCTION_ENTRY, "", "", function_name,
340: 			                              OnEntryNotFound::RETURN_NULL, error_context);
341: 
342: 			auto &func_entry = func->Cast<AggregateFunctionCatalogEntry>();
343: 
344: 			FunctionBinder function_binder(context);
345: 			vector<LogicalType> types {arguments[0]->return_type, arguments[0]->return_type};
346: 			ErrorData error;
347: 			auto best_function = function_binder.BindFunction(func_entry.name, func_entry.functions, types, error);
348: 			if (!best_function.IsValid()) {
349: 				throw BinderException(string("Fail to find corresponding function for collation min/max: ") +
350: 				                      error.Message());
351: 			}
352: 			function = func_entry.functions.GetFunctionByOffset(best_function.GetIndex());
353: 
354: 			// Create a copied child and PushCollation for it.
355: 			arguments.push_back(arguments[0]->Copy());
356: 			ExpressionBinder::PushCollation(context, arguments[1], arguments[0]->return_type);
357: 
358: 			// Bind function like arg_min/arg_max.
359: 			function.arguments[0] = arguments[0]->return_type;
360: 			function.return_type = arguments[0]->return_type;
361: 			return nullptr;
362: 		}
363: 	}
364: 
365: 	auto input_type = arguments[0]->return_type;
366: 	if (input_type.id() == LogicalTypeId::UNKNOWN) {
367: 		throw ParameterNotResolvedException();
368: 	}
369: 	auto name = std::move(function.name);
370: 	function = GetMinMaxOperator<OP, OP_STRING, OP_VECTOR>(input_type);
371: 	function.name = std::move(name);
372: 	function.order_dependent = AggregateOrderDependent::NOT_ORDER_DEPENDENT;
373: 	if (function.bind) {
374: 		return function.bind(context, function, arguments);
375: 	} else {
376: 		return nullptr;
377: 	}
378: }
