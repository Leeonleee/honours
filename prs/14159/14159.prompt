153: struct MinMaxStringState : MinMaxState<string_t> {
154: 	void Destroy() {
155: 		if (isset && !value.IsInlined()) {
156: 			delete[] value.GetData();
157: 		}
158: 	}
159: 
160: 	void Assign(string_t input) {
161: 		if (input.IsInlined()) {
162: 			// inlined string - we can directly store it into the string_t without having to allocate anything
163: 			Destroy();
164: 			value = input;
165: 		} else {
166: 			// non-inlined string, need to allocate space for it somehow
167: 			auto len = input.GetSize();
168: 			char *ptr;
169: 			if (!isset || value.GetSize() < len) {
170: 				// we cannot fit this into the current slot - destroy it and re-allocate
171: 				Destroy();
172: 				ptr = new char[len];
173: 			} else {
174: 				// this fits into the current slot - take over the pointer
175: 				ptr = value.GetDataWriteable();
176: 			}
177: 			memcpy(ptr, input.GetData(), len);
178: 
179: 			value = string_t(ptr, UnsafeNumericCast<uint32_t>(len));
180: 		}
181: 	}
182: };
