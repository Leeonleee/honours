418: static void MinMaxNUpdate(Vector inputs[], AggregateInputData &aggr_input, idx_t input_count, Vector &state_vector,
419:                           idx_t count) {
420: 
421: 	auto &val_vector = inputs[0];
422: 	auto &n_vector = inputs[1];
423: 
424: 	UnifiedVectorFormat val_format;
425: 	UnifiedVectorFormat n_format;
426: 	UnifiedVectorFormat state_format;
427: 	;
428: 	auto val_extra_state = STATE::VAL_TYPE::CreateExtraState(val_vector, count);
429: 
430: 	STATE::VAL_TYPE::PrepareData(val_vector, count, val_extra_state, val_format);
431: 
432: 	n_vector.ToUnifiedFormat(count, n_format);
433: 	state_vector.ToUnifiedFormat(count, state_format);
434: 
435: 	auto states = UnifiedVectorFormat::GetData<STATE *>(state_format);
436: 
437: 	for (idx_t i = 0; i < count; i++) {
438: 		const auto val_idx = val_format.sel->get_index(i);
439: 		if (!val_format.validity.RowIsValid(val_idx)) {
440: 			continue;
441: 		}
442: 		const auto state_idx = state_format.sel->get_index(i);
443: 		auto &state = *states[state_idx];
444: 
445: 		// Initialize the heap if necessary and add the input to the heap
446: 		if (!state.is_initialized) {
447: 			static constexpr int64_t MAX_N = 1000000;
448: 			const auto nidx = n_format.sel->get_index(i);
449: 			if (!n_format.validity.RowIsValid(nidx)) {
450: 				throw InvalidInputException("Invalid input for MIN/MAX: n value cannot be NULL");
451: 			}
452: 			const auto nval = UnifiedVectorFormat::GetData<int64_t>(n_format)[nidx];
453: 			if (nval <= 0) {
454: 				throw InvalidInputException("Invalid input for MIN/MAX: n value must be > 0");
455: 			}
456: 			if (nval >= MAX_N) {
457: 				throw InvalidInputException("Invalid input for MIN/MAX: n value must be < %d", MAX_N);
458: 			}
459: 			state.Initialize(UnsafeNumericCast<idx_t>(nval));
460: 		}
461: 
462: 		// Now add the input to the heap
463: 		auto val_val = STATE::VAL_TYPE::Create(val_format, val_idx);
464: 		state.heap.Insert(aggr_input.allocator, val_val);
465: 	}
466: }
