468: template <class VAL_TYPE, class COMPARATOR>
469: static void SpecializeMinMaxNFunction(AggregateFunction &function) {
470: 	using STATE = MinMaxNState<VAL_TYPE, COMPARATOR>;
471: 	using OP = MinMaxNOperation;
