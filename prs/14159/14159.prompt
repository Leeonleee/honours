473: 	function.state_size = AggregateFunction::StateSize<STATE>;
474: 	function.initialize = AggregateFunction::StateInitialize<STATE, OP>;
475: 	function.combine = AggregateFunction::StateCombine<STATE, OP>;
476: 	function.destructor = AggregateFunction::StateDestroy<STATE, OP>;
477: 
478: 	function.finalize = MinMaxNOperation::Finalize<STATE>;
479: 	function.update = MinMaxNUpdate<STATE>;
