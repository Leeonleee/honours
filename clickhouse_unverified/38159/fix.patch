+	template <typename T, typename... Args, typename = typename std::enable_if_t<std::is_integral<T>::value and not std::is_same<T, std::decay_t<uinteger_t>>::value>>
+	uinteger_t(const T& value, Args... args) :
+		_begin(0),
+		_end(0),
