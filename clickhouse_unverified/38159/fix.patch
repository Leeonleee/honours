+	static digit _divmod(digit x_hi, digit x_lo, digit y, digit* result) {
+	#if defined HAVE____INT128_T
+		if (digit_bits == 64) {
+			auto x = static_cast<__uint128_t>(x_hi) << digit_bits | static_cast<__uint128_t>(x_lo);
+			digit q = x / y;
+			digit r = x % y;
+
+			*result = q;
+			return r;
+		}
+	#endif
+		if (digit_bits == 64) {
+			// quotient
+			digit q = x_lo << 1;
+
+			// remainder
+			digit r = x_hi;
+
+			digit carry = x_lo >> 63;
+			int i;
+
+			for (i = 0; i < 64; i++) {
+				auto tmp = r >> 63;
+				r <<= 1;
+				r |= carry;
+				carry = tmp;
+
+				if (carry == 0) {
+					if (r >= y) {
+						carry = 1;
+					} else {
+						tmp = q >> 63;
+						q <<= 1;
+						q |= carry;
+						carry = tmp;
+						continue;
+					}
+				}
