+        SELECT 
+            *
+        FROM 
+            redis('{address}', 'k', 'k String, v UInt32', 0, 'clickhouse', 10) 
+        """
+    )
+
+    # illegal data type
+    with pytest.raises(QueryRuntimeException):
+        node.query(
+            f"""
+            SELECT 
+                *
+            FROM 
+                redis('{address}', 'k', 'k not_exist_type, v String', 0, 'clickhouse', 10) 
+            """
+        )
+
+    # illegal key
+    with pytest.raises(QueryRuntimeException):
+        node.query(
+            f"""
