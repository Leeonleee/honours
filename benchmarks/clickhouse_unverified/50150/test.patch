+
+    response = node.query(
+        """
+        ALTER TABLE test_update UPDATE m='2023-06-03 00:00:00' WHERE k=1
+        """
+    )
+
+    print("update response: ", response)
+
+    response = TSV.toMat(
+        node.query("SELECT k, m, n FROM test_update WHERE k=1 FORMAT TSV")
