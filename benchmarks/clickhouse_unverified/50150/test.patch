+def test_simple_insert(started_cluster):
+    client = get_redis_connection()
+    address = get_address_for_ch()
+
+    # clean all
+    client.flushall()
+    drop_table("test_simple_insert")
+
+    node.query(
+        f"""
+        CREATE TABLE test_simple_insert(
+            k UInt32, 
